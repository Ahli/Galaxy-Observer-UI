	<Label layoutX="36.0" layoutY="37.0" text="%settings.paths.installDirectory"/>
	<TextField fx:id="sc2Path" layoutX="36.0" layoutY="55.0" maxWidth="1.7976931348623157E308" minWidth="300.0"
