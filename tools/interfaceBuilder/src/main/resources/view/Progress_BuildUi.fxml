<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
			xmlns="http://javafx.com/javafx/17.0.2-ea"
			fx:controller="com.ahli.interfacebuilder.ui.progress.BuildUiController">
	<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
		  AnchorPane.topAnchor="0.0">
		<ScrollPane fx:id="scrollPane" pannable="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" VBox.vgrow="SOMETIMES">
			<TextFlow fx:id="txtArea">
				<padding>
					<Insets left="5.0"/>
				</padding>
			</TextFlow>
		</ScrollPane>
		<Button defaultButton="true" mnemonicParsing="false" onAction="#build" prefWidth="80.0" text="%home.build"
				textAlignment="CENTER" wrapText="true">
			<font>
				<Font name="System Bold" size="14.0"/>
			</font>
		</Button>
	</VBox>
</AnchorPane>
