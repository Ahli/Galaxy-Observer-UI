    <Frame type="Image" name="ShipHighlightTemplate">
        <Texture val="@UI/LotV_Story_SoA_TutorialHighlight"/>
        <DesaturateOnDisable val="false"/>
        <Visible val="false"/>
        
        <Animation name="Highlight">
            <Event event="OnShown" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time="1" value="255" out="Slow"/>
                <Key type="Curve" time="2" value="0" out="Slow"/>
            </Controller>
        </Animation>
        
        <Animation name="HighlightLoop">
            <Event event="OnShown" action="Play"/>
            <Controller type="Animation" end="Loop" animation="Highlight">
                <Key type="Animation" time="2.1" action="Reset"/>
                <Key type="Animation" time="2.1" action="Play"/>
            </Controller>
        </Animation>
        
        <Animation name="HideGracefully">
            <Event event="OnDisabled" action="Play"/>
            <Controller type="Animation" end="Pause" animation="HighlightLoop">
                <Key type="Animation" time="0.1" action="Pause"/>
            </Controller>
        </Animation>
    </Frame>
    
