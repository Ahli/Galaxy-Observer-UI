    <!-- Sat Comm Blur Zone -->
    <Frame type="BlurFrame" name="SatComBlurZoneTemplate">
        <Frame type="Image" name="Darken">
            <Anchor relative="$parent"/>
            <Color val="0,220,150"/>
            <Alpha val="70"/>
        </Frame>

        <Animation name="Blur">
            <Event event="OnShown" action="Reset,Play" frame="$parent/$parent" />

            <Controller type="Blur" end="Pause">
                <Key type="Curve" time="0" value=".2" out="Fast"/>
            </Controller>
        </Animation>
        
        <Animation name="Unhance">
            <Event event="Unhance" action="Reset,Play" frame="$MissionBriefing_DiscoverBox" />
            <Event event="OnShown" action="Reset,Play" frame="$parent" />

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
        </Animation>
    </Frame>
    
