    =============================
    
    DROP POINT CONTAINER TEMPLATE
    
    =============================
    
    
    -->
        
    <Frame type="Frame" name="DropPointContainerTemplate">
        <Width val="400"/>
        <Height val="400"/>
        
        <Frame type="Image" name="DropPointIcon">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="#Nova_Color_Green_01"/>
            <Texture val="Assets/Textures/ui_nova_storymode_briefing_02_map_droppoint.dds"/>
            <Alpha val="0"/>
        </Frame>
        
        <Frame type="Image" name="DropPointCircle1">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="200"/>
            <Height val="200"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="#Nova_Color_Green_01"/>
            <Alpha val="0"/>
            <Visible val="False"/>

            <Texture val="Assets/Textures/ui_nova_storymode_briefing_02_map_droppoint_circle01.dds"/>
            
            <Animation name="ArrivalAnimation">
                <Event event="DropPointArrival" action="Reset,Play" frame="$parent"/>
                <Event event="ResetBriefing" action="Reset" frame="$MissionPreviewPanel"/>
                <Event event="EndBriefingEarly" action="Reset,Play" frame="$MissionPreviewPanel"/>
                
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="True"/>
                </Controller>
                
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="1" event="Pulse"/>
                </Controller>
                
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".4" value="170" in="Slow"/>
                    <Key type="Curve" time=".8" value="170" in="Slow"/>
                </Controller>
                
                <Controller type="Rotation" relative="False" end="Loop" layer="0">
                    <Key type="Curve" time="0" value="0" out="linear"/>
                    <Key type="Curve" time="45" value="360" in="linear"/>
                </Controller>
            </Animation>
            
            <Animation name="Pulse">
                <Event event="Pulse" action="Reset,Play"/>
                <Event event="ResetBriefing" action="Reset,Stop" frame="$MissionPreviewPanel"/>
                
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="170" out="Fast"/>
                    <Key type="Curve" time="4" value="170" in="Slow"/>
                    <Key type="Curve" time="5" value="60" in="Slow"/>
                    <Key type="Curve" time="8" value="60" in="Slow"/>
                    <Key type="Curve" time="9" value="170" in="Slow"/>
                    <Key type="Curve" time="13" value="170" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="DropPointCircle2">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="170"/>
            <Height val="170"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="#Nova_Color_Green_01"/>
            <Alpha val="0"/>
            <Visible val="False"/>

            <Texture val="Assets/Textures/ui_nova_storymode_briefing_02_map_droppoint_circle01.dds"/>
            
            <Animation name="ArrivalAnimation">
                <Event event="DropPointArrival" action="Reset,Play" frame="$parent"/>
                <Event event="ResetBriefing" action="Reset" frame="$MissionPreviewPanel"/>
                <Event event="EndBriefingEarly" action="Reset,Play" frame="$MissionPreviewPanel"/>
                
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="True"/>
                </Controller>
                
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="1" event="Pulse"/>
                </Controller>
                
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".4" value="60" in="Slow"/>
                    <Key type="Curve" time=".8" value="60" in="Slow"/>
                </Controller>
                
                <Controller type="Rotation" relative="False" end="Loop" layer="0">
                    <Key type="Curve" time="0" value="360" out="linear"/>
                    <Key type="Curve" time="45" value="0" in="linear"/>
                </Controller>
                
                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0" sound="@UI_Nova_BriefingDropZoneStart"/>
                </Controller>
            </Animation>
            
            <Animation name="Pulse">
                <Event event="Pulse" action="Reset,Play"/>
                <Event event="ResetBriefing" action="Reset,Stop" frame="$MissionPreviewPanel"/>
                
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="60" out="Fast"/>
                    <Key type="Curve" time="4" value="60" in="Slow"/>
                    <Key type="Curve" time="5" value="150" in="Slow"/>
                    <Key type="Curve" time="8" value="150" in="Slow"/>
                    <Key type="Curve" time="9" value="60" in="Slow"/>
                    <Key type="Curve" time="13" value="60" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Animation name="DropPointArrival">
            <Event event="DropPointArrival" action="Reset,Play"/>
            <Event event="ResetBriefing" action="Reset" frame="$MissionPreviewPanel"/>
            <Event event="EndBriefingEarly" action="Reset,Play" frame="$MissionPreviewPanel"/>
            
            <Controller type="Fade" end="Pause" frame="DropPointIcon">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time=".5" value="255" in="Slow"/>
            </Controller>
        </Animation>
    </Frame>
    
