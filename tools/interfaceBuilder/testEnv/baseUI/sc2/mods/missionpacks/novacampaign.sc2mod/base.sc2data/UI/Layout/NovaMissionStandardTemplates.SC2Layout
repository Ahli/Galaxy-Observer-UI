	<Frame type="AchievementFrame" name="NovaMiniAchievementTemplate">
        <Width val="102"/>
        <Height val="100"/>
        <AcceptsMouse val="true"/>
        <TooltipFrame val="Nova_AchievementTooltip"/>
        <TooltipAnchorPosition val="TopCenter"/>
        <DisabledAlpha val="255"/>

        <Frame type="Image" name="IconImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="72"/>
            <Height val="72"/>
            <TextureType val="Normal"/>
            <AlphaMaskTexture val="Assets/Textures/ui_nova_storymode_missionlaunch_achievementborder_mask.dds"/>
            <DesaturateOnDisable val="False"/>
            <ColorAdjustMode val="None"/>
            <AdjustmentColor val="80,170,155"/>
        </Frame>
        
        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>
            <DesaturateOnDisable val="False"/>
            <Texture val="Assets/Textures/ui_nova_storymode_missionlaunch_achievementborder_normal.dds"/>
            <DisabledAlpha val="255"/>
        </Frame>
        
        <Frame type="Image" name="DisabledImage">
            <Anchor relative="$parent"/>
            <DesaturateOnDisable val="False"/>
            <Texture val="Assets/Textures/ui_nova_storymode_missionlaunch_achievementborder_disabled.dds"/>
            <DisabledAlpha val="255"/>
        </Frame>

        <Frame type="Label" name="NameLabel">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@@AchievementFrameName"/>
        </Frame>

        <Frame type="Label" name="DescriptionLabel">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@@AchievementFrameDescription"/>
        </Frame>

        <Frame type="Label" name="PointsLabel">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@@AchievementFramePoints"/>
        </Frame>
        
        <StateGroup name="DisabledStateGroup">
                        
            <!-- Enabled State -->
            <State name="Enabled">
                <When type="Property" frame="$this" Enabled="True"/>
                <Action type="SetProperty" frame="NormalImage" visible="True"/>
                <Action type="SetProperty" frame="DisabledImage" visible="False"/>
                <Action type="SetProperty" frame="IconImage" colorAdjustMode="None"/>
                <Action type="SetProperty" frame="IconImage" alpha="255"/>
            </State>
            
            <!-- Disabled State -->
            <State name="Enabled">
                <When type="Property" frame="$this" Enabled="False"/>
                <Action type="SetProperty" frame="NormalImage" visible="False"/>
                <Action type="SetProperty" frame="DisabledImage" visible="True"/>
                <Action type="SetProperty" frame="IconImage" colorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="IconImage" alpha="80"/>
            </State>
        </StateGroup>
    </Frame>
	
