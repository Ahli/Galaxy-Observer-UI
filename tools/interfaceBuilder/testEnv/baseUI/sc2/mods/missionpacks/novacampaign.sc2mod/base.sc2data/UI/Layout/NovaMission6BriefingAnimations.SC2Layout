        <Event event="MissionBriefing6_Phase1" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="$MissionPreviewPanel">
            <Key type="Event" time="0" event="ResetBriefing"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="$Mission6_MapConstraint">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$Mission6_MapImage">
            <Key type="Curve" time="0" value="#PosX_N06_BriefingStartPosition" out="Auto"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$Mission6_MapImage">
            <Key type="Curve" time="0" value="#PosX_N06_BriefingStartPosition" out="Auto"/>
