    <!--
    
    =============================
    
    COORDINATE ICON TEMPLATE
    
    =============================
    
    -->
    
    <Frame type="Button" name="CoordinateIconTemplate">
        <Width val="100"/>
        <Height val="100"/>
        <Alpha val="0"/>
        
        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/NovaStory/Briefing/General/CoordinatesIcon"/>
            
            <Animation name="Rotation">
                <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                
                <Controller type="Rotation" relative="False" end="Loop" layer="0">
                    <Key type="Curve" time="0" value="0" out="linear"/>
                    <Key type="Curve" time="35" value="360" in="linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="HoverImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/NovaStory/Briefing/General/CoordinatesIcon"/>
            
            <Animation name="Rotation">
                <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                
                <Controller type="Rotation" relative="False" end="Loop" layer="0">
                    <Key type="Curve" time="0" value="0" out="linear"/>
                    <Key type="Curve" time="35" value="360" in="linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="HighlightImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/NovaStory/Briefing/General/CoordinatesIcon"/>
            <Visible val="False"/>
            
            <Animation name="Show">
                <Event event="OnMouseEnter" action="Reset,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="Reset,Stop" frame="$parent"/>
                
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="True"/>
                </Controller>
            </Animation>
            
            <Animation name="Rotation">
                <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                
                <Controller type="Rotation" relative="False" end="Loop" layer="0">
                    <Key type="Curve" time="0" value="0" out="linear"/>
                    <Key type="Curve" time="35" value="360" in="linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Animation name="Display">
            <Event event="Display" action="Reset,Play"/>
            <Event event="MissionBriefing6_Phase1" action="Reset,Stop" frame="$MissionPreviewPanel"/>
            <Event event="ResetBriefing" action="Reset,Stop" frame="$MissionPreviewPanel"/>
            <Event event="EndBriefingEarly" action="Reset,Play" frame="$MissionPreviewPanel"/>
            
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time=".24" value="255" in="Slow"/>
                <Key type="Curve" time=".48" value="200" in="Slow"/>
            </Controller>
            
            <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause">
                <Key type="Curve" time="0" value="70"/>
                <Key type="Curve" time=".24" value="130"/>
                <Key type="Curve" time=".48" value="100"/>
            </Controller>
            
            <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause">
                <Key type="Curve" time="0" value="70"/>
                <Key type="Curve" time=".24" value="130"/>
                <Key type="Curve" time=".48" value="100"/>
            </Controller>
        </Animation>
    </Frame>
    
