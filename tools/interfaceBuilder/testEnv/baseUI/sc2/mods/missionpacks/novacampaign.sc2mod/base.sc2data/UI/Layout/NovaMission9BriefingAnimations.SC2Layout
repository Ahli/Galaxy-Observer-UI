    -->
    
    <Frame type="Button" name="CoordinateIconTemplate">
        <Width val="100"/>
        <Height val="100"/>
        <Alpha val="0"/>
        
        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/NovaStory/Briefing/General/CoordinatesIcon"/>
            
            <Animation name="Rotation">
                <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                
                <Controller type="Rotation" relative="False" end="Loop" layer="0">
                    <Key type="Curve" time="0" value="0" out="linear"/>
