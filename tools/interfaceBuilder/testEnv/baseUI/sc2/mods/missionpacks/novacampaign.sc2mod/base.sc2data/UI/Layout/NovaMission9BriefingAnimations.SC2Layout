    <!-- 
    
    
    =============================
    
    Dominion DOT TEMPLATE
    
    =============================
    
    
    -->
    
    <Frame type="Image" name="DominionDotTemplate">
        <Alpha val="0"/>
        <Texture val="@UI/NovaStory/Briefing/General/GreenDot"/>
        
        <Animation name="Display">
            <Event event="Display" action="Reset,Play"/>
            <Event event="MissionBriefing9_Phase1" action="Reset,Stop" frame="$MissionPreviewPanel"/>
            <Event event="ResetBriefing" action="Reset,Stop" frame="$MissionPreviewPanel"/>
            
            <Controller type="Fade" name="Display" end="Pause">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time=".3" value="255" out="Fast"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="$this">
                <Key type="Event" time="0" event=""/>
                <Key type="Event" time="6" event="Pulse"/>
            </Controller>
        </Animation>
        
        <Animation name="Pulse">
            <Event event="Pulse" action="Reset,Play"/>
            <Event event="MissionBriefing9_Phase1" action="Reset,Stop" frame="$MissionPreviewPanel"/>
            <Event event="ResetBriefing" action="Reset,Stop" frame="$MissionPreviewPanel"/>
            
            <Controller type="Fade" name="Pulse" end="Loop">
                <Key type="Curve" time="0" value="255" out="Fast"/>
                <Key type="Curve" time=".1" value="255" out="Fast"/>
                <Key type="Curve" time=".5" value="100" out="Fast"/>
                <Key type="Curve" time="1" value="255" out="Fast"/>
                <Key type="Curve" time="5" value="255" out="Fast"/>
            </Controller>
        </Animation>
    </Frame>
    
    
    <!--
    
    =============================
    
    COORDINATE ICON TEMPLATE
    
