        <Frame type="Button" name="DavisIcon">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-465"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-465"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="445"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="445"/>
            <Width val="200"/>
