                </Controller>
                
                <Controller type="Fade" end="Pause" frame="NormalImage">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".24" value="255" in="Slow"/>
                    <Key type="Curve" time=".48" value="160" in="Slow"/>
                </Controller>
                
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause">
                    <Key type="Curve" time="0" value="70"/>
                    <Key type="Curve" time=".24" value="130"/>
                    <Key type="Curve" time=".48" value="100"/>
                </Controller>
                
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause">
                    <Key type="Curve" time="0" value="70"/>
                    <Key type="Curve" time=".24" value="130"/>
                    <Key type="Curve" time=".48" value="100"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
    
