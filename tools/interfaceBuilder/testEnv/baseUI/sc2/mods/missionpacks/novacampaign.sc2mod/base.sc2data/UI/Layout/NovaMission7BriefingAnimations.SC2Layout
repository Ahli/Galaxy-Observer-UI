            <Frame type="Image" name="NormalImage">
                <Anchor relative="$parent"/>
                <Texture val="@UI/NovaStory/Briefing/General/DavisIcon"/>
                <Alpha val="0"/>
            </Frame>
            
            <Frame type="Image" name="HoverImage">
                <Anchor relative="$parent"/>
                <Texture val="@UI/NovaStory/Briefing/General/DavisIcon"/>
            </Frame>
            
            <Animation name="Show">
                <Event event="DisplayDavis" action="Reset,Play" frame="$parent"/>
                <Event event="MissionBriefing5_Phase1" action="Reset,Stop" frame="$MissionPreviewPanel"/>
                <Event event="ResetBriefing" action="Reset,Stop" frame="$MissionPreviewPanel"/>
