<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Frame type="Button" name="TabButtonTemplate">
        <Height val="36"/>
        <ClickSound val="@AC_UI_OnClick01"/>
        <HoverSound val="@AC_UI_MouseOver01"/>
        <Toggleable val="true"/>
        
        <Frame type="Image" name="NormalFrameImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/Glues_Battlenet_TabButton_Left_Normal" layer="0"/>
            <TextureType val="EndCap"/>
            <Tiled val="true"/>
            <DesaturateOnDisable val="false"/>
            
            <Animation name="Normal">
                <Event event="Normal" action="Reset,Play" frame="$parent"/>
                <Event event="OnMouseEnter" action="DirectionReverse,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="DirectionForward,Play" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".01" value="255" in="Fast"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="HoverFrameImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/Glues_Battlenet_TabButton_Left_Over" layer="0"/>
            <TextureType val="EndCap"/>
            <Tiled val="true"/>
            <Alpha val="0"/>
            <DesaturateOnDisable val="false"/>
            
            <Animation name="Hovered">
                <Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".01" value="255" in="Fast"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="SelectedFrameImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/Glues_Battlenet_TabButton_Left_Selected"/>
            <TextureType val="EndCap"/>
            <Visible val="{$parent/@Toggled}"/>
            <DesaturateOnDisable val="false"/>
        </Frame>

        <Frame type="Label" name="Label">
            <Anchor side="Top" relative="$parent" pos="Min" offset="3"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Style val="@Coop_CommanderTabButton"/>

            <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
            <AcceptsMouseTooltip val="true"/>

            <StateGroup name="SelectedStateGroup">
                <State name="SelectedState">
                    <When type="Property" frame="$parent" Toggled="true"/>
                    <Action type="SetProperty" frame="$this" Style="@Coop_CommanderTabButton_Selected"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    <Frame type="Button" name="TabButtonRightTemplate" template="ClubListPanel/TabButtonTemplate">
        <Frame type="Image" name="NormalFrameImage">
            <Texture val="@UI/Glues_Battlenet_TabButton_Right_Normal" layer="0"/>
        </Frame>
        
        <Frame type="Image" name="HoverFrameImage">
            <Texture val="@UI/Glues_Battlenet_TabButton_Right_Over" layer="0"/>
        </Frame>
        
        <Frame type="Image" name="SelectedFrameImage">
            <Texture val="@UI/Glues_Battlenet_TabButton_Right_Selected"/>
        </Frame>
        
        <Frame type="Label" name="Label">
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
            <AcceptsMouseTooltip val="true"/>
        </Frame>
    </Frame>

    <Frame type="BattleClubFrame" name="ClubItemTemplate">
        <UseChatSessionMembers val="true"/>
        
        <Frame type="Frame" name="Container">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="80"/>

            <Frame type="Frame" name="ClubImageContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="7"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Visible val="true"/>

                <StateGroup name="IconStateGroup">
                    <State name="DefaultState">
                        <When type="Property" frame="$parent/$parent" Icon=""/>
                        <Action type="SetProperty" frame="DefaultImage" Visible="true"/>
                        <Action type="SetProperty" frame="ClubImage" Visible="false"/>
                    </State>
                </StateGroup>

                <Frame type="Image" name="ClubFrameImage">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Height val="61"/>
                    <Width val="61"/>
                    <Texture val="@UI/Glues_ClubListPanel_ClubListBackground"/>
                </Frame>

                <Frame type="Image" name="DefaultImage">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
                    <Height val="50"/>
                    <Width val="50"/>
                    <Visible val="false"/>

                    <StateGroup name="CategoryStateGroup">
                        <State name="ClanState">
                            <When type="Property" frame="$parent/$parent/$parent" IsClan="true"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_Icons_ClanToast"/>
                        </State>
                
                        <State name="CommunityState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Community"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_OnlineCommIcon"/>
                        </State>

                        <State name="BarcraftState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Barcraft"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_BarcraftIcon"/>
                        </State>

                        <State name="EsportsTeamsState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="EsportsTeams"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_ProPlayersIcon"/>
                        </State>

                        <State name="CoachingState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Coaching"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_StrategyIcon"/>
                        </State>

                        <State name="CompanyState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Company"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_CompanyIcon"/>
                        </State>

                        <State name="RegionState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Region"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_RegionalIcon"/>
                        </State>

                        <State name="SchoolState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="School"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_SchoolsIcon"/>
                        </State>

                        <State name="ShoutcastState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Shoutcast"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_BroadcastersIcon"/>
                        </State>

                        <State name="OtherState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Other"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_OtherIcon"/>
                        </State>

                        <State name="EsportsLeaguesState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="EsportsLeagues"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_TournamentsIcon"/>
                        </State>

                        <State name="ArcadeState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Arcade"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_ArcadeIcon"/>
                        </State>

                        <State name="IgrState">
                            <When type="Property" frame="$parent/$parent/$parent" Category="Igr"/>
                            <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_IGRIcon"/>
                        </State>
                    </StateGroup>
                </Frame>
                
                <Frame type="Image" name="ClubImage">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
                    <Height val="50"/>
                    <Width val="50"/>
                    <Texture val="{$parent/$parent/$parent/@Icon}"/>
                </Frame>
            </Frame>

            <Frame type="Image" name="ClanIcon">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="2"/>
                <Texture val="@UI/Glues_ClubListPanel_Icons_ClanToast"/>
                <Visible val="False"/>
            </Frame>

            <StateGroup name="IconStateGroup">
                <State name="EmptyClubState">
                    <When type="Property" frame="$parent/$parent" ClubId="0"/>
                    <Action type="SetProperty" frame="ClanIcon" Visible="false"/>
                    <Action type="SetProperty" frame="ClubImageContainer" Visible="false"/>
                </State>
                
                <State name="DefaultClanState">
                    <When type="Property" frame="$parent/$parent" Icon=""/>
                    <When type="Property" frame="$parent/$parent" IsClan="true"/>
                    <Action type="SetProperty" frame="ClanIcon" Visible="true"/>
                    <Action type="SetProperty" frame="ClubImageContainer" Visible="false"/>
                </State>
            </StateGroup>

            <Frame type="Image" name="EventImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="-1"/>
                <Anchor side="Left" relative="$parent/ClubImageContainer/ClubFrameImage" pos="Max" offset="-30"/>
                <Height val="29"/>
                <Width val="30"/>
                <Texture val="@UI/Glues_EventTodayIcon"/>
                <Visible val="false"/>

                <StateGroup name="HasEventStateGroup">
                    <State name="HasEventState">
                        <When type="Property" frame="$parent/$parent" operator="GreaterThan" CurrentEventCount="0"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="Label" name="GroupName">
                <Style val="SocialPlayerName"/>
                <Anchor side="Top" relative="$parent" pos="Min" offset="14"/>
                <Anchor side="Left" relative="$parent/ClanIcon" pos="Max" offset="5"/>
                <Anchor side="Right" relative="$parent/RankImage" pos="Min" offset="-4"/>
                <Height val="22"/>
                <Text val="{$parent/$parent/@DisplayName}"/>
                <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
                <AcceptsMouseTooltip val="true"/>
            </Frame>

            <Frame type="TextFormatFrame" name="StatusFormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/ClubPanel/PlayersOnline"/>
                <Replacement index="0" val="{$parent/$parent/@ChatMemberCount}"/>
                <Replacement index="1" val="{$parent/$parent/@OnlineCount}"/>
            </Frame>

            <Frame type="Label" name="Status">
                <Style val="SocialPlayerStatus"/>
                <Anchor side="Top" relative="$parent/GroupName" pos="Max" offset="3"/>
                <Anchor side="Left" relative="$parent/GroupName" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/GroupName" pos="Max" offset="0"/>
                <Height val="20"/>
                <Text val="{$parent/StatusFormatFrame/@Text}"/>
            </Frame>

            <Frame type="Image" name="RankImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="16"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-17"/>
                <Width val="40"/>
                <Height val="45"/>
                <AcceptsMouseTooltip val="true"/>
                <Visible val="false"/>

                <StateGroup name="RankStateGroup">
                    <State name="OwnerState">
                        <When type="Property" frame="$parent/$parent" IsLocalOwner="true"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_Icons_ClanLeader"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/Club/Ranknames/Owner"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>
                    
                    <State name="OfficerState">
                        <When type="Property" frame="$parent/$parent" IsLocalOfficer="true"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_Icon_ClanMember"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/Club/Ranknames/Officer"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>
                </StateGroup>
            </Frame>
        </Frame>
    </Frame>

    <Frame type="ClubListBoxItem" name="ClubListBoxItem">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Height val="74"/>

        <Frame type="BattleClubFrame" name="ClubItem" template="ClubListPanel/ClubItemTemplate">
            <Anchor relative="$parent"/>
            <ClubId val="{$parent/@ClubId}"/>
        </Frame>
    </Frame>

    <Frame type="ClubEventListBoxItem" name="ClubEventListBoxItem">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        
        <Frame type="Frame" name="TitleContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="40"/>
            <CollapseLayout val="true"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="TitleBorderImage">
                <Anchor relative="$parent" offset="0"/>
                <Texture val="@UI/Glues_Battlenet_PageAssets_ChatPanelCategoryBg"/>
                <TextureType val="Nineslice"/>
            </Frame>
            
            <Frame type="Label" name="TitleLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="3"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="10"/>
                <Style val="ChatMembersTitleLabel"/>
                <AcceptsMouseTooltip val="true"/>
                <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
                <Text val="@UI/ClubListPanel/CurrentEvents"/>

                <StateGroup name="UpcomingStateGroup">
                    <State name="UpcomingState">
                        <When type="Property" frame="$parent/$parent" IsCurrent="false"/>
                        <Action type="SetProperty" frame="$this" Text="@UI/ClubListPanel/UpcomingEvents"/>
                    </State>
                </StateGroup>
            </Frame>
        </Frame>
        
        <Frame type="Frame" name="EventContainer">
            <Anchor side="Top" relative="$parent/TitleContainer" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="74"/>
            <CollapseLayout val="true"/>
            <Visible val="true"/>
            
            <Frame type="BattleClubFrame" name="ClubFrame">
                <Anchor relative="$parent"/>
                <ClubId val="{$parent/$parent/@ClubId}"/>
            </Frame>
        
            <!-- TODO: Put these in a template? -->
            <Frame type="Image" name="IconImage">
                <Anchor side="Top" relative="$parent/IconImageFrame" pos="Min" offset="6"/>
                <Anchor side="Left" relative="$parent/IconImageFrame" pos="Min" offset="6"/>
                <Width val="50"/>
                <Height val="50"/>
                <Texture val="{$parent/ClubFrame/@Icon}"/>
            </Frame>

            <Frame type="Image" name="DefaultIconImage">
                <Anchor side="Top" relative="$parent/IconImageFrame" pos="Min" offset="6"/>
                <Anchor side="Left" relative="$parent/IconImageFrame" pos="Min" offset="6"/>
                <Width val="50"/>
                <Height val="50"/>
                <Visible val="false"/>

                <StateGroup name="CategoryStateGroup">
                    <State name="ClanState">
                        <When type="Property" frame="$parent/ClubFrame" IsClan="true"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_Icons_ClanToast"/>
                    </State>

                    <State name="CommunityState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Community"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_OnlineCommIcon"/>
                    </State>

                    <State name="BarcraftState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Barcraft"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_BarcraftIcon"/>
                    </State>

                    <State name="EsportsTeamsState">
                        <When type="Property" frame="$parent/ClubFrame" Category="EsportsTeams"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_ProPlayersIcon"/>
                    </State>

                    <State name="CoachingState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Coaching"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_StrategyIcon"/>
                    </State>

                    <State name="CompanyState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Company"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_CompanyIcon"/>
                    </State>

                    <State name="RegionState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Region"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_RegionalIcon"/>
                    </State>

                    <State name="SchoolState">
                        <When type="Property" frame="$parent/ClubFrame" Category="School"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_SchoolsIcon"/>
                    </State>

                    <State name="ShoutcastState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Shoutcast"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_BroadcastersIcon"/>
                    </State>

                    <State name="OtherState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Other"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_OtherIcon"/>
                    </State>

                    <State name="EsportsLeaguesState">
                        <When type="Property" frame="$parent/ClubFrame" Category="EsportsLeagues"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_TournamentsIcon"/>
                    </State>

                    <State name="ArcadeState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Arcade"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_ClubListPanel_ArcadeIcon"/>
                    </State>

                    <State name="IgrState">
                        <When type="Property" frame="$parent/ClubFrame" Category="Igr"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues/ClubListPanel_IGRIcon"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="Image" name="IconImageFrame">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="7"/>
                <Height val="61"/>
                <Width val="61"/>
                <Texture val="@UI/Glues_ClubListPanel_ClubListBackground"/>
                <Visible val="true"/>
            </Frame>

            <StateGroup name="IconStateGroup">
                <State name="DefaultState">
                    <When type="Property" frame="$this/ClubFrame" Icon=""/>
                    <Action type="SetProperty" frame="DefaultIconImage" Visible="true"/>
                    <Action type="SetProperty" frame="IconImage" Visible="false"/>
                </State>
            </StateGroup>

            <Frame type="Label" name="Title">
                <Style val="SocialPlayerName"/>
                <Anchor side="Top" relative="$parent" pos="Min" offset="14"/>
                <Anchor side="Left" relative="$parent/IconImage" pos="Max" offset="15"/>
                <Anchor side="Right" relative="$parent/TimeRemaining" pos="Min" offset="-4"/>
                <Height val="22"/>
                <Text val="{$parent/ClubFrame/@DisplayName}"/>
                <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
                <AcceptsMouseTooltip val="true"/>
            </Frame>

            <Frame type="Label" name="Description">
                <Style val="SocialPlayerStatus"/>
                <Anchor side="Top" relative="$parent/Title" pos="Max" offset="3"/>
                <Anchor side="Left" relative="$parent/Title" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/Title" pos="Max" offset="0"/>
                <Height val="20"/>
                <Text val="{$parent/$parent/@Title}"/>
            </Frame>
            
            <Frame type="TextFormatFrame" name="TimeFormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/ClubListPanel/TimeRemainingCurrent"/>
                <Replacement index="0" val="{$parent/$parent/@TimeText}"/>
                
                <StateGroup name="UpcomingStateGroup">
                    <State name="UpcomingState">
                        <When type="Property" frame="$parent/$parent" IsCurrent="false"/>
                        <Action type="SetProperty" frame="$this" FormatText="@UI/ClubListPanel/TimeRemainingUpcoming"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="Label" name="TimeRemaining">
                <Style val="SocialPlayerStatus"/>
                <Anchor side="Top" relative="$parent" pos="Min" offset="14"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-4"/>
                <Text val="{$parent/TimeFormatFrame/@Text}"/>
            </Frame>
        </Frame>
        
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent/TitleContainer" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/EventContainer" pos="Max" offset="0"/>
        </Frame>

        <StateGroup name="VisibleStateGroup">
            <State name="TitleState">
                <When type="Property" frame="$this" ClubId="0"/>
                <Action type="SetProperty" frame="TitleContainer" Visible="true"/>
                <Action type="SetProperty" frame="EventContainer" Visible="false"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="ScrollBar" name="ChatScrollBarTemplate" template="StandardBattlenetTemplates/GluesSocialScrollBarTemplate">
    </Frame>

    <Frame type="Image" name="ClubListBoxBackground">
        <Texture val="@UI/Glues_ClubListPanel_ClubListItemUp"/>
        <TextureType val="Nineslice"/>
        <Alpha val="0"/>
        <DescFlags val="Internal"/>      
    </Frame>
    
    <Frame type="Image" name="ClubListSelected">
        <Texture val="@UI/Glues_Battlenet_PageAssets_MainListItem_Over"/>
        <TextureType val="Nineslice"/>
        <DescFlags val="Internal"/>
    </Frame>

    <Frame type="Image" name="ClubListHover">
        <Texture val="@UI/Glues_Battlenet_PageAssets_MainListItem_Selected"/>
        <TextureType val="Nineslice"/>
        <DescFlags val="Internal"/>
    </Frame>

    <Frame type="ListBox" name="ClubListBoxTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <BackgroundImage val="$root/ClubListPanel/ClubListBoxBackground"/>
        <SelectedImage val="$root/ClubListPanel/ClubListSelected"/>
        <HoverImage val="$root/ClubListPanel/ClubListHover"/>
        <HighlightOnHover val="true"/>
        <ItemHeight val="74"/>
        
        <ItemDesc val="ClubListPanel/ClubListBoxItem"/>
        <ScrollBar val="ScrollBar"/>
        <Insets top="0" left="0" bottom="0" right="0"/>
        <AcceptsMouse val="true"/>

        <Frame type="ScrollBar" name="ScrollBar" template="ClubListPanel/ChatScrollBarTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="-4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="3"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="4"/>
        </Frame>
    </Frame>

    <Frame type="ClubListPanel" name="ClubListPanelTemplate">
        <AcceptsMouse val="true"/>

        <Frame type="BlurFrame" name="BlurEffect">
            <Anchor side="Left" relative="$parent/ClubListContainerBorderImage" pos="Min" offset="5"/>
            <Anchor side="Top" relative="$parent/ClubListContainerBorderImage" pos="Min" offset="5"/>
            <Anchor side="Right" relative="$parent/ClubListContainerBorderImage" pos="Max" offset="-5"/>
            <Anchor side="Bottom" relative="$parent/ClubListContainerBorderImage" pos="Max" offset="-5"/>
            <BlurAmount val=".3"/>

            <Frame type="Image" name="LowImage">
                <Anchor relative="$parent" offset="0"/>
                <Color val="0,0,0"/>
                <Alpha val="150"/>
            </Frame>
        </Frame>
            
        <Frame type="Image" name="ClubListContainerBorderImage">
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_MainContainer"/>
            <TextureType val="Nineslice"/>
        </Frame>
        
        <Frame type="Image" name="ClubListContainerInnerBorderImage">
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
