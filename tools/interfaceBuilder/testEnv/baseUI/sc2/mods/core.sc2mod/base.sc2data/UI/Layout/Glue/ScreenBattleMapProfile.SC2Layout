<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="LobbyTileListItem" name="LobbyListItemTemplate" template="CustomGamesLobbies/LobbyListItemTemplate">
        <ContextMenu val="$parent/$parent/$parent/ItemContextMenu"/>

        <Frame type="Frame" name="NamePaddingFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/BattleMapIcon" pos="Max" offset="10"/>
            <Anchor side="Right" relative="$parent/ExpansionLevelIcon" pos="Max" offset="0"/>
            <CollapseLayout val="true"/>
            <Visible val="{$parent/ExpansionLevelIcon/@Visible}"/>
        </Frame>
        
        <Frame type="Label" name="MapName">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/NamePaddingFrame" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/TitleLabel" pos="Min" offset="-10"/>
        </Frame>
        
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/$parent/$parent/LobbyName" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/$parent/ModeName" pos="Min" offset="-30"/>
        </Frame>
        
        <!-- Mode Name -->
         <Frame type="Label" name="ModeName">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/$parent/$parent/ModeName" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/$parent/PlayerCountIcon" pos="Min" offset="-30"/>
            <Style val="{$parent/TitleLabel/@Style}"/>
            <Text val="{$parent/BattleMapVariantFrame/@ModeName}"/>
        </Frame>

        <Frame type="BattleMapVariantFrame" name="BattleMapVariantFrame">
            <Anchor relative="$parent"/>

            <MapHandle val="{$parent/@MapHandle}"/>
            <VariantIndex val="{$parent/@MapVariantIndex}"/>
        </Frame>

        <Frame type="Label" name="PlayerCount">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/$parent/$parent/PlayerCountIcon" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/$parent/PlayerCountIcon" pos="Mid" offset="0"/>
        </Frame>

        <Frame type="Label" name="HostName">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/$parent/$parent/HostName" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-15"/>
        </Frame>
       
    </Frame>

    <Frame type="TileList" name="LobbyTileListTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

        <AcceptsMouse val="true"/>
        <RequireSelection val="true"/>

        <Frame type="LobbyTileListItem" name="Item0" template="ScreenBattleMapProfile/LobbyListItemTemplate">
            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="3"/>
            </Frame>
        </Frame>

        <Frame type="LobbyTileListItem" name="Item1" template="ScreenBattleMapProfile/LobbyListItemTemplate">
            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent/Item0/AnchorFrame" pos="Max" offset="2"/>
            </Frame>
        </Frame>

        <Frame type="LobbyTileListItem" name="Item2" template="ScreenBattleMapProfile/LobbyListItemTemplate">
            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent/Item1/AnchorFrame" pos="Max" offset="2"/>
            </Frame>
        </Frame>

        <Frame type="LobbyTileListItem" name="Item3" template="ScreenBattleMapProfile/LobbyListItemTemplate">
            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent/Item2/AnchorFrame" pos="Max" offset="2"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="ScrollBarBackground" template="StandardTileListTemplates/StandardTileListScrollBarBackgroundTemplate"/>

        <Frame type="ScrollBar" name="ScrollBar" template="StandardTileListTemplates/StandardTileListScrollBarTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="2"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-12"/>
            <Anchor side="Bottom" relative="$parent/Item3" pos="Max" offset="-2"/>
            <RenderPriority val="900"/>
        </Frame>
    </Frame>

    <Frame type="Frame" name="LobbyListTemplate">
        <Anchor side="Top" relative="$parent/Background" pos="Min" offset="48"/>
        <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent/Background" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent/Background" pos="Max" offset="0"/>

        <Frame type="TileList" name="LobbyListTileList" template="ScreenBattleMapProfile/LobbyTileListTemplate"/>
    </Frame>

    <Frame type="LobbyListFrame" name="GameLobbyListFrameTemplate">
        <Anchor side="Left" relative="$GameInfoAnchorFrame" pos="Min" offset="413"/>
        <Anchor side="Bottom" relative="$GameInfoAnchorFrame" pos="Max" offset="0"/>
        <Width val="1120"/>
        <Height val="375"/>
        <HookupAlias original="LobbyListTileList" alias="LobbyList/LobbyListTileList"/>
        <HookupAlias original="LobbyListTileList/ScrollBar" alias="LobbyList/LobbyListTileList/ScrollBar"/>
        <FilterMapHandle val="{$parent/@MapHandle}"/>

        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="225"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_TableSmallHeaderBg"/>
            <TextureType val="NineSlice"/>
            <Tiled val="True"/>
        </Frame>

        <Frame type="Label" name="MapName">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="18"/>
            <Style val="@Arcade_GameInfo_SectionTitle"/>
            <Text val="@UI/ScreenCustomGames/Lobbies/MapName"/>
        </Frame>

        <Frame type="Label" name="LobbyName">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="400"/>
            <Style val="@Arcade_GameInfo_SectionTitle"/>
            <Text val="@UI/ScreenCustomGames/Lobbies/LobbyName"/>
        </Frame>

        <Frame type="Label" name="ModeName">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="690"/>
            <Style val="@Arcade_GameInfo_SectionTitle"/>
            <Text val="@UI/ScreenCustomGames/Lobbies/ModeName"/>
        </Frame>
        
        <Frame type="Image" name="PlayerCountIcon">
            <Anchor side="Top" relative="$parent/LobbyName" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/LobbyName" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="860"/>
            <Width val="20"/>
            <Height val="24"/>
            <Texture val="@UI/Glues_Icons_NumPlayers"/>
            <Color val="220,238,255"/>
        </Frame>

        <Frame type="Label" name="HostName">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="920"/>
            <Style val="@Arcade_GameInfo_SectionTitle"/>
            <Text val="@UI/ScreenCustomGames/Lobbies/HostName"/>
        </Frame>
        
        <Frame type="Pulldown" name="LobbyListFilterPulldown" template="StandardSmallButtonTemplates/StandardSmallPulldownTemplate">
            <Visible val="false"/>
        </Frame>

        <Frame type="Frame" name="LobbyList" template="ScreenBattleMapProfile/LobbyListTemplate"/>

        <Frame type="GlueButton" name="JoinButton" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
            <Anchor side="Bottom" relative="$GameInfoAnchorFrame" pos="Max" offset="-40"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Width val="260"/>
            <Handle val="GameInfoJoinLobbyButton"/>
            <Text val="@UI/ScreenCustomGames/JoinButton"/>
            <CollapseLayout val="True"/>
            <Visible val="True"/>
        </Frame>

        <Frame type="Button" name="RefreshButton" template="StandardSmallButtonTemplates/StandardSmallIconButtonTemplate">
        
            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent/Background" pos="Min" offset="10"/>
                <Anchor side="Right" relative="$parent/$parent/Background" pos="Max" offset="-10"/>
                <Width val="30"/>
                <Height val="30"/>
            </Frame>

            <Frame type="Image" name="Icon">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Texture val="@UI/Glues_Battlenet_StandardButtons_Small_Icon_Refresh_Normal"/>

                <StateGroup name="DisplayState">
                    <DefaultState val="Normal"/>

                    <State name="Disabled">
                        <When type="StateGroup" frame="$parent" DisabledState="Disabled"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_StandardButtons_Small_Icon_Refresh_Normal"/>
                        <Action type="SetProperty" frame="$this" Color="64,95,113"/>
                    </State>

                    <State name="Pressed">
                        <When type="StateGroup" frame="$parent" PressedState="Pressed"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_StandardButtons_Small_Icon_Refresh_Pressed"/>
                        <Action type="SetProperty" frame="$this" Color="255,255,255"/>
                    </State>

                    <State name="Hover">
                        <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_StandardButtons_Small_Icon_Refresh_Normal"/>
                        <Action type="SetProperty" frame="$this" Color="204,231,255"/>
                    </State>

                    <State name="Normal">
                        <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_StandardButtons_Small_Icon_Refresh_Normal"/>
                        <Action type="SetProperty" frame="$this" Color="141,191,234"/>
                    </State>
                </StateGroup>
            </Frame>

            <Animation name="RefreshLobbyList">
                <Event event="OnClick" frame="$this" action="Reset,Play"/>

                <Controller type="Event" end="Pause" frame="$parent">
                    <Key type="Event" time="0.0" event="RequestRefreshOpenLobbyList"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="BattleLobbyContextMenu" name="ItemContextMenu" template="StandardTemplates/StandardContextMenuTemplate">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <ShowMapInfo val="false"/>
            <ShowCreateOffline val="false"/>
            <MapHandle val="{$parent/@SelectedMapHandle}"/>
            <ModHandle val="{$parent/@SelectedModHandle}"/>
            <AccountId val="{$parent/@SelectedAccountId}"/>
            <LobbyName val="{$parent/@SelectedLobbyName}"/>
        </Frame>
        
        <StateGroup name="VisibleState">
            <DefaultState val="NotVisible"/>
        
            <State name="Visible">
                <When type="Property" frame="$ancestor[@type=ScreenBattleMapProfile]/BattleMapFrame" IsExtensionMod="False"/>
                <Action type="SetProperty" frame="$this" Visible="True"/>
            </State>
            
            <State name="NotVisible">
                <Action type="SetProperty" frame="$this" Visible="False"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="ScreenBattleMapProfile" name="ScreenBattleMapProfile">
        <Anchor relative="$parent"/>
        <Handle val="ScreenBattleMapProfile"/>
        <!-- This must render above ScreenNavigation and other content screens so you can't navigate or interact with other things -->
        <RenderPriority val="650"/>

        <Animation name="Show">
            <Event event="OnHidden" action="Reset,Stop" frame="$this"/>
            <Event event="OnShown" action="Reset,Play" frame="$this"/>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.000" value="0" out="Slow"/>
                <Key type="Curve" time="0.175" value="255" in="Auto"/>
            </Controller>
        </Animation>

        <Animation name="ViewOverview">
            <Event event="ViewOverview" action="Reset,Play" frame="$this"/>

            <Controller type="Property" property="SelectedIndex" end="Pause" frame="TabControl">
                <Key type="Property" time="0" value="0"/>
            </Controller>
        </Animation>
        
        <Frame type="Frame" name="AnchorFrame">
            <Anchor relative="$parent/ExtendedExperienceBG/AnchorFrame"/>
            <Handle val="GameInfoAnchorFrame"/>
        </Frame>
        
        <Frame type="BattleMapFrame" name="BattleMapFrame">
            <Anchor relative="$parent/AnchorFrame"/>
            <MapHandle val="{$ancestor[@type=ScreenBattleMapProfile]/@MapHandle}"/>
        </Frame>

        <!-- This tooltip is just for blocking tooltips behind this panel -->
        <Frame type="StandardTooltip" name="MapProfileTooltip">
            <Frame type="Label" name="Label">
                <Anchor relative="$parent"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="ExtendedExperienceBG" template="StandardExtendedExperienceTemplates/StandardSizeExtendedExperienceTemplate"/>

        <Frame type="Image" name="TitleBackground">
            <Anchor side="Top" relative="$parent/ExtendedExperienceBG/AnchorFrame" pos="Min" offset="57"/>
            <Anchor side="Left" relative="$parent/ExtendedExperienceBG/AnchorFrame" pos="Min" offset="32"/>
            <Width val="376"/>
            <Height val="53"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_ListHeaderBg"/>
            <TextureType val="Nineslice"/>
            <Tiled val="True"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/TitleBackground" pos="Min" offset="1"/>
            <Anchor side="Bottom" relative="$parent/TitleBackground" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/TitleBackground" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent/TitleBackground" pos="Max" offset="-20"/>
            <Style val="@Arcade_Title"/>

            <StateGroup name="TitleStateGroup">
                <State name="ArcadeState">
                    <When type="AnimationState" frame="$parent" ArcadeState="Arcade"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/BattleMapProfilePanel/Title"/>
                </State>

                <State name="ModState">
                    <When type="AnimationState" frame="$parent" ModState="Mod"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/BattleMapProfilePanel/TitleMod"/>
                </State>

                <State name="NotArcadeState">
                    <When type="AnimationState" frame="$parent" ArcadeState="NotArcade"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/BattleMapProfilePanel/Title"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Image" name="PageHeaderBackground">
            <Anchor side="Top" relative="$parent/TitleBackground" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TitleBackground" pos="Max" offset="5"/>
            <Width val="1120"/>
            <Height val="84"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_ListHeaderBg"/>
            <TextureType val="Nineslice"/>
            <Tiled val="True"/>
        </Frame>

        <Frame type="Frame" name="IconImageAnchor">
            <Anchor side="Top" relative="$parent/IconImageFrame" pos="Min" offset="6"/>
            <Anchor side="Left" relative="$parent/IconImageFrame" pos="Min" offset="6"/>
            <Width val="91"/>
            <Height val="57"/>
        </Frame>

        <Frame type="BattleMapIcon" name="IconImage">
            <Anchor relative="$parent/IconImageAnchor"/>
            <CollapseLayout val="true"/>
            <MapHandle val="{$parent/@MapHandle}"/>

            <Frame type="Image" name="Image">
                <Anchor relative="$parent"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="IconImageFrame">
            <Anchor side="Top" relative="$parent/PageHeaderBackground" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/PageHeaderBackground" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/PageHeaderBackground" pos="Min" offset="8"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_IconFrame"/>
            <Width val="103"/>
            <Height val="70"/>
            <CollapseLayout val="true"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="Frame" name="IconImageFrameAnchor">
            <Anchor side="Top" relative="$parent/PageHeaderBackground" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/PageHeaderBackground" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/IconImageFrame" pos="Max" offset="0"/>
            <Width val="12"/>
            <Height val="1"/>
            <CollapseLayout val="true"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="Label" name="MapNameLabel">
            <Anchor side="Top" relative="$parent/PageHeaderBackground" pos="Min" offset="19"/>
            <Anchor side="Left" relative="$parent/IconImageFrameAnchor" pos="Max" offset="15"/>
            <Style val="@Arcade_GameInfo_Title"/>
            <AcceptsMouseTooltip val="true"/>
            <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
            <PreserveAnchorOffset val="true"/>
            <AutoSizeClamps maxWidth="520"/>
            <Text val="{$this/BattleMapFrame/@NameWithVersion}"/>

            <Frame type="BattleMapFrame" name="BattleMapFrame">
                <Anchor relative="$parent"/>
                <MapHandle val="{$parent/$parent/@MapHandle}"/>
            </Frame>
        </Frame>

        <Frame type="BattleMapContextMenu" name="MapContextMenu" template="StandardTemplates/StandardContextMenuTemplate">
            <Anchor side="Top" relative="$parent/OptionsButton" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/OptionsButton" pos="Min" offset="0"/>
            <UseAnchors val="true"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>

        <Frame type="Button" name="OptionsButton" template="StandardBattlenetTemplates/OptionsButton">
            <Anchor side="Top" relative="$parent/MapNameLabel" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/MapNameLabel" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Max" offset="5"/>
            <ContextMenu val="$parent/MapContextMenu"/>
            <ContextMenuButtons val="Left"/>
        </Frame>

        <Frame type="ProgressBar" name="RatingProgressBar">
            <Anchor side="Top" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="0"/>
            <Width val="64"/>
            <Height val="14"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_RatingStarsMini"/>
            <Color val="FFFFFF"/>
            <ColorStep val="FFFFFF"/>
            <MinValue val="0"/>
            <MaxValue val="5"/>
        </Frame>

        <Frame type="Button" name="BookmarkButton" template="StandardBattlenetTemplates/BattlenetSmallButtonTemplate">
            <Anchor side="Top" relative="$parent/PageHeaderBackground" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/PageHeaderBackground" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/PageHeaderBackground" pos="Max" offset="-20"/>
            <Text val="@UI/BattleMapProfilePanel/Bookmark"/>
            <Width val="175"/>
            <HoverSound val="@UI_HoverTick"/>
        </Frame>

        <Frame type="Button" name="JoinChatButton" template="StandardBattlenetTemplates/BattlenetSmallButtonTemplate">
            <Anchor side="Top" relative="$parent/BookmarkButton" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/BookmarkButton" pos="Min" offset="0"/>
            <Text val="@UI/BattleMapProfilePanel/JoinChat"/>
            <Width val="175"/>
            <HoverSound val="@UI_HoverTick"/>
        </Frame>

        <Frame type="Button" name="PinButton" template="StandardBattlenetTemplates/BattlenetSmallButtonTemplate">
            <Anchor side="Top" relative="$parent/JoinChatButton" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/JoinChatButton" pos="Min" offset="0"/>
            <Text val="@UI/ScreenCustomGames/GameInfo/Pin"/>
            <Width val="175"/>
            <HoverSound val="@UI_HoverTick"/>

            <StateGroup name="CanPinState">
                <State name="CantPin">
                    <When type="Property" frame="$parent/PinFrame" IsPinned="false"/>
                    <When type="Property" frame="$parent/PinFrame" PinCount="3"/>
                    <Action type="SetProperty" frame="$this" Enabled="false"/>
                    <Action type="SetProperty" frame="$this" Tooltip="@UI/ScreenCustomGames/GameInfo/MaxPins"/>
                </State>
            </StateGroup>

            <StateGroup name="PinnedState">
                <DefaultState val="NotPinned"/>

                <State name="Pinned">
                    <When type="Property" frame="$parent/PinFrame" IsPinned="true"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCustomGames/GameInfo/Unpin"/>
                </State>

                <State name="NotPinned">
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCustomGames/GameInfo/Pin"/>
                </State>
            </StateGroup>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="Property" frame="$ancestor[@type=ScreenBattleMapProfile]/BattleMapFrame" IsExtensionMod="False"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="BattleMapPinFrame" name="PinFrame">
            <Anchor relative="$parent"/>
            <SelectedMapHandle val="{$parent/@MapHandle}"/>

            <Animation name="Show">
                <Event event="OnClick" action="Reset,Play" frame="$parent/PinButton"/>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0" event="TogglePin"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="TabControl" name="TabControl">
            <Anchor relative="$parent"/>
            <TabListItem val="$parent/OverviewButton"/>
            <TabListItem val="$parent/HowToPlayButton"/>
            <TabListItem val="$parent/PatchNotesButton"/>
            <TabListItem val="$parent/ReviewsButton"/>
            <PanelListItem val="$parent/OverviewPanel"/>
            <PanelListItem val="$parent/HowToPlayPanel"/>
            <PanelListItem val="$parent/PatchNotesPanel"/>
            <PanelListItem val="$parent/ReviewsPanel"/>
        </Frame>

        <Frame type="Button" name="OverviewButton" template="StandardBattlenetTemplates/BattlenetVerticalTabButtonTemplate">
            <Anchor side="Top" relative="$parent/TitleBackground" pos="Max" offset="-1"/>
            <Anchor side="Left" relative="$parent/TitleBackground" pos="Min" offset="4"/>
            <Text val="@UI/BattleMapProfilePanel/Overview"/>
            <Width val="370"/>
            <HoverSound val="@UI_HoverTick"/>
            <Toggleable val="true"/>
        </Frame>

        <Frame type="Button" name="HowToPlayButton" template="StandardBattlenetTemplates/BattlenetVerticalTabButtonTemplate">
            <Anchor side="Top" relative="$parent/OverviewButton" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/OverviewButton" pos="Min" offset="0"/>
            <Text val="@UI/BattleMapProfilePanel/HowToPlay"/>
            <Width val="370"/>
            <HoverSound val="@UI_HoverTick"/>
            <Toggleable val="true"/>

            <StateGroup name="HowToPlayStateGroup">
                <State name="EmptyState">
                    <When type="AnimationState" frame="$parent/HowToPlayPanel/ListBox" ListBoxState="Empty"/>
                    <Action type="SetProperty" frame="$this" Enabled="false"/>
                    <Action type="SetProperty" frame="$this" Tooltip="@UI/BattleMapProfilePanel/HowToPlayDisabled"/>
                </State>

                <State name="NotEmptyState">
                    <When type="AnimationState" frame="$parent/HowToPlayPanel/ListBox" ListBoxState="NotEmpty"/>
                    <Action type="SetProperty" frame="$this" Enabled="true"/>
                    <Action type="SetProperty" frame="$this" Tooltip=""/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Button" name="PatchNotesButton" template="StandardBattlenetTemplates/BattlenetVerticalTabButtonTemplate">
            <Anchor side="Top" relative="$parent/HowToPlayButton" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/HowToPlayButton" pos="Min" offset="0"/>
            <Text val="@UI/BattleMapProfilePanel/PatchNotes"/>
            <Width val="370"/>
            <HoverSound val="@UI_HoverTick"/>
            <Toggleable val="true"/>

            <StateGroup name="PatchNotesStateGroup">
                <State name="EmptyState">
                    <When type="AnimationState" frame="$parent/PatchNotesPanel/ListBox" ListBoxState="Empty"/>
                    <Action type="SetProperty" frame="$this" Enabled="false"/>
                    <Action type="SetProperty" frame="$this" Tooltip="@UI/BattleMapProfilePanel/PatchNotesDisabled"/>
                </State>

                <State name="NotEmptyState">
                    <When type="AnimationState" frame="$parent/PatchNotesPanel/ListBox" ListBoxState="NotEmpty"/>
                    <Action type="SetProperty" frame="$this" Enabled="true"/>
                    <Action type="SetProperty" frame="$this" Tooltip=""/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Button" name="ReviewsButton" template="StandardBattlenetTemplates/BattlenetVerticalTabButtonTemplate">
            <Anchor side="Top" relative="$parent/PatchNotesButton" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PatchNotesButton" pos="Min" offset="0"/>
            <Text val="@UI/BattleMapProfilePanel/Reviews"/>
            <Width val="370"/>
            <HoverSound val="@UI_HoverTick"/>
            <Toggleable val="true"/>
        </Frame>

        <Frame type="BattleMapProfileOverviewPanel" name="OverviewPanel" template="BattleMapProfileOverviewPanel/BattleMapProfileOverviewPanelTemplate">
            <Anchor side="Top" relative="$parent/PageHeaderBackground" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PageHeaderBackground" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/PageHeaderBackground" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/GameLobbyListFrame" pos="Min" offset="0"/>
            <Visible val="false"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>

        <Frame type="BattleMapProfileHowToPlayPanel" name="HowToPlayPanel" template="BattleMapProfileHowToPlayPanel/BattleMapProfileHowToPlayPanelTemplate">
            <Anchor side="Top" relative="$parent/OverviewPanel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/OverviewPanel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/OverviewPanel" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/GameLobbyListFrame" pos="Min" offset="0"/>
            <Visible val="false"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>

        <Frame type="BattleMapProfilePatchNotesPanel" name="PatchNotesPanel" template="BattleMapProfilePatchNotesPanel/BattleMapProfilePatchNotesPanelTemplate">
            <Anchor side="Top" relative="$parent/OverviewPanel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/OverviewPanel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/OverviewPanel" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/GameLobbyListFrame" pos="Min" offset="0"/>
            <Visible val="false"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>

        <Frame type="BattleMapProfileReviewsPanel" name="ReviewsPanel" template="BattleMapProfileReviewsPanel/BattleMapProfileReviewsPanelTemplate">
            <Visible val="false"/>
            <MapHandle val="{$parent/@MapHandle}"/>
            <!-- This needs to render above the close button for when you're writing a review -->
            <RenderPriority val="800"/>

            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent/OverviewPanel" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent/OverviewPanel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent/OverviewPanel" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/$parent/GameLobbyListFrame" pos="Min" offset="0"/>
            </Frame>

            <Frame type="Button" name="ReviewButton" template="StandardBattlenetTemplates/BattlenetSmallButtonTemplate">
                <StateGroup name="TitleStateGroup">
                    <State name="ArcadeState">
                        <When type="AnimationState" frame="$parent/$parent" ArcadeState="Arcade"/>
                        <Action type="SetProperty" frame="$this" Text="@UI/BattleMapProfileReviewsPanel/Review"/>
                    </State>

                    <State name="ModState">
                        <When type="AnimationState" frame="$parent/$parent" ModState="Mod"/>
                        <Action type="SetProperty" frame="$this" Text="@UI/BattleMapProfileReviewsPanel/ReviewMod"/>
                    </State>

                    <State name="NotArcadeState">
                        <When type="AnimationState" frame="$parent/$parent" ArcadeState="NotArcade"/>
                        <Action type="SetProperty" frame="$this" Text="@UI/BattleMapProfileReviewsPanel/ReviewMap"/>
                    </State>
                </StateGroup>
            </Frame>
        </Frame>

        <Frame type="LobbyListFrame" name="GameLobbyListFrame" template="ScreenBattleMapProfile/GameLobbyListFrameTemplate"/>

        <Frame type="ExpansionPulldown" name="ExpansionPulldown" template="StandardBattlenetTemplates/BattlenetLargePulldownTemplate">
            <Anchor side="Bottom" relative="$GameInfoAnchorFrame" pos="Max" offset="-44"/>
            <Anchor side="Left" relative="$parent/CreateModButton" pos="Max" offset="8"/>
            <Width val="288"/>
            <RequiredExpansionLevel val="{$parent/BattleMapFrame/@ExpansionLevel}"/>
        </Frame>

        <Frame type="CustomGamePlayButton" name="CreateButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Bottom" relative="$GameInfoAnchorFrame" pos="Max" offset="-40"/>
            <Anchor side="Left" relative="$GameInfoJoinLobbyButton" pos="Max" offset="8"/>
            <Width val="260"/>
            <Text val="@UI/ScreenCustomGames/CreateButton"/>
            <CollapseLayout val="true"/>
            <MapHandle val="{$parent/@MapHandle}"/>
            <ExpansionTag val="{$parent/ExpansionPulldown/@ExpansionTag}"/>
            <VariantIndex val="{$parent/VariantPulldown/@VariantIndex}"/>
        </Frame>

        <Frame type="CustomGamePlayButton" name="AutoJoinButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Bottom" relative="$GameInfoAnchorFrame" pos="Max" offset="-40"/>
            <Anchor side="Left" relative="$parent/CreateButton" pos="Max" offset="8"/>
            <Width val="260"/>
            <Text val="@UI/ScreenCustomGames/AutoJoinButton"/>
            <Tooltip val="@UI/ScreenCustomGames/AutoJoinTooltip"/>
            <TooltipDelayed val="True"/>
            <AcceptsMouseTooltip val="True"/>
            <MapHandle val="{$parent/@MapHandle}"/>
            <UseDefaultVariant val="False"/>
            <VariantIndex val="{$parent/VariantPulldown/@VariantIndex}"/>
            <PlayMode val="JoinOrCreate"/>
            <CollapseLayout val="True"/>

            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="Property" frame="$parent/BattleMapFrame" IsArcade="True"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="BattleMapVariantPulldown" name="VariantPulldown" template="StandardBattlenetTemplates/BattlenetLargePulldownTemplate">
            <Anchor side="Bottom" relative="$GameInfoAnchorFrame" pos="Max" offset="-44"/>
            <Anchor side="Left" relative="$parent/AutoJoinButton" pos="Max" offset="8"/>
            <Width val="260"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>

        <Frame type="CustomGamePlayButton" name="TutorialButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Top" relative="$parent/CreateButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TitleBackground" pos="Min" offset="4"/>
            <Width val="370"/>
            <Text val="@UI/BattleMapProfilePanel/Tutorial"/>
            <CollapseLayout val="true"/>
            <MapHandle val="{$parent/@TutorialHandle}"/>
            <VariantIndex val="{$parent/@TutorialVariantIndex}"/>
            <ExpansionTag val="{$parent/ExpansionPulldown/@ExpansionTag}"/>
        </Frame>

        <Frame type="Button" name="CreateModButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Top" relative="$parent/CreateButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/CreateButton" pos="Max" offset="8"/>
            <Width val="260"/>
            <Text val="@UI/ScreenCustomGames/CreateModButton"/>
            <HoverSound val="@UI_HoverTick"/>
            <CollapseLayout val="true"/>
            <Enabled val="{$parent/CreateButton/@Enabled}"/>
            <Tooltip val="{$parent/CreateButton/@Tooltip}"/>
        </Frame>

        <Frame type="Button" name="CloseButton" template="StandardBattlenetTemplates/CloseButton">
            <Anchor side="Top" relative="$parent/ExtendedExperienceBG/AnchorFrame" pos="Min" offset="11"/>
            <Anchor side="Right" relative="$parent/ExtendedExperienceBG/AnchorFrame" pos="Max" offset="-14"/>
            <Shortcut val="@UI/ScreenNavigationBack_Hotkey"/>
            <ClickSound val="@UI_BnetDefaultClick"/>
        </Frame>

        <Frame type="Frame" name="IdleSpinner" template="StandardBattlenetTemplates/IdleSpinnerTemplate">
            <Anchor relative="$parent/ExtendedExperienceBG/AnchorFrame"/>
        </Frame>
        
        <Frame type="CustomGameModListPanel" name="ModListPanel" template="CustomGameModListPanel/CustomGameModListPanelTemplate">
            <Handle val="ModListPanel"/>
            <Anchor relative="$parent"/>
            <RenderPriority val="900"/>
            <Visible val="false"/>
            <MapHandle val="{$parent/@MapHandle}"/>
            
            <Animation name="Show">
                <Event event="OnClick" action="Reset,Play" frame="$parent/CreateModButton"/>
                <Event event="OnClick" action="Reset,Stop" frame="CloseButton"/>
                <Event event="Loading" action="Reset,Stop" frame="$parent"/>
                <Event event="OnHidden" action="Reset,Stop" frame="$parent"/>
                <Event event="ViewOverview" action="Reset,Stop" frame="$parent"/>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.000" value="false"/>
                    <Key type="Bool" time="0.001" value="true"/>
                    <Key type="Bool" time="0.175" value="true"/>
                </Controller>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0.000" value="0" out="Slow"/>
                    <Key type="Curve" time="0.175" value="255" in="Auto"/>
                </Controller>
            </Animation>

            <Frame type="CustomGamePlayButton" name="CreateButton">
                <ExpansionTag val="{$parent/$parent/ExpansionPulldown/@ExpansionTag}"/>
                <VariantIndex val="{$parent/$parent/VariantPulldown/@VariantIndex}"/>
            </Frame>
        </Frame>

        <StateGroup name="TutorialLoadingStateGroup">
            <State name="LoadingState">
                <When type="AnimationState" frame="$this" LoadingState="Loading"/>
                <Action type="SetProperty" frame="TutorialButton" Visible="false"/>
            </State>

            <State name="LoadedTutorialState">
                <When type="AnimationState" frame="$this" LoadingState="Loaded"/>
                <When type="AnimationState" frame="$this" TutorialState="HasTutorial"/>
                <When type="AnimationState" frame="$this" ArcadeState="Arcade"/>
                <Action type="SetProperty" frame="TutorialButton" Visible="true"/>
            </State>

            <State name="LoadedNoTutorialState">
                <When type="AnimationState" frame="$this" LoadingState="Loaded"/>
                <Action type="SetProperty" frame="TutorialButton" Visible="false"/>
            </State>
        </StateGroup>

        <StateGroup name="ArcadeLoadingStateGroup">
            <State name="LoadingState">
                <When type="AnimationState" frame="$this" LoadingState="Loading"/>
                <Action type="SetProperty" frame="CreateButton" Visible="false"/>
                <Action type="SetProperty" frame="CreateModButton" Visible="false"/>
                <Action type="SetProperty" frame="ExpansionPulldown" Visible="false"/>
                <Action type="SetProperty" frame="VariantPulldown" Visible="false"/>
                <Action type="SetProperty" frame="IconImage" Visible="false"/>
                <Action type="SetProperty" frame="IconImageFrame" Visible="false"/>
                <Action type="SetProperty" frame="IconImageFrameAnchor" Visible="true"/>
            </State>

            <State name="LoadedModState">
                <When type="AnimationState" frame="$this" LoadingState="Loaded"/>
                <When type="AnimationState" frame="$this" ModState="Mod"/>
                <Action type="SetProperty" frame="CreateButton" Visible="false"/>
                <Action type="SetProperty" frame="CreateModButton" Visible="false"/>
                <Action type="SetProperty" frame="ExpansionPulldown" Visible="false"/>
                <Action type="SetProperty" frame="VariantPulldown" Visible="false"/>
                <Action type="SetProperty" frame="IconImage" Visible="false"/>
                <Action type="SetProperty" frame="IconImageFrame" Visible="false"/>
                <Action type="SetProperty" frame="IconImageFrameAnchor" Visible="false"/>
            </State>

            <State name="LoadedArcadeState">
                <When type="AnimationState" frame="$this" LoadingState="Loaded"/>
                <When type="AnimationState" frame="$this" ArcadeState="Arcade"/>
                <Action type="SetProperty" frame="CreateButton" Visible="true"/>
                <Action type="SetProperty" frame="CreateModButton" Visible="false"/>
                <Action type="SetProperty" frame="ExpansionPulldown" Visible="false"/>
                <Action type="SetProperty" frame="VariantPulldown" Visible="true"/>
                <Action type="SetProperty" frame="IconImage" Visible="true"/>
                <Action type="SetProperty" frame="IconImageFrame" Visible="true"/>
                <Action type="SetProperty" frame="IconImageFrameAnchor" Visible="false"/>
            </State>

            <State name="LoadedMeleeState">
                <When type="AnimationState" frame="$this" LoadingState="Loaded"/>
                <When type="AnimationState" frame="$this" ArcadeState="NotArcade"/>
                <Action type="SetProperty" frame="CreateButton" Visible="true"/>
                <Action type="SetProperty" frame="CreateModButton" Visible="true"/>
                <Action type="SetProperty" frame="ExpansionPulldown" Visible="true"/>
                <Action type="SetProperty" frame="VariantPulldown" Visible="false"/>
                <Action type="SetProperty" frame="IconImage" Visible="false"/>
                <Action type="SetProperty" frame="IconImageFrame" Visible="false"/>
                <Action type="SetProperty" frame="IconImageFrameAnchor" Visible="true"/>
            </State>
        </StateGroup>

        <StateGroup name="LoadingStateGroup">
            <State name="LoadingState">
                <When type="AnimationState" frame="$this" LoadingState="Loading"/>
                <Action type="SetProperty" frame="MapNameLabel" Visible="false"/>
                <Action type="SetProperty" frame="RatingProgressBar" Visible="false"/>
                <Action type="SetProperty" frame="JoinChatButton" Visible="false"/>
                <Action type="SetProperty" frame="BookmarkButton" Visible="false"/>
                <Action type="SetProperty" frame="OverviewButton" Visible="false"/>
                <Action type="SetProperty" frame="HowToPlayButton" Visible="false"/>
                <Action type="SetProperty" frame="PatchNotesButton" Visible="false"/>
                <Action type="SetProperty" frame="ReviewsButton" Visible="false"/>
                <Action type="SetProperty" frame="IdleSpinner" Visible="true"/>
            </State>

            <State name="LoadedState">
                <When type="AnimationState" frame="$this" LoadingState="Loaded"/>
                <Action type="SetProperty" frame="MapNameLabel" Visible="true"/>
                <Action type="SetProperty" frame="RatingProgressBar" Visible="true"/>
                <Action type="SetProperty" frame="JoinChatButton" Visible="true"/>
                <Action type="SetProperty" frame="BookmarkButton" Visible="true"/>
                <Action type="SetProperty" frame="OverviewButton" Visible="true"/>
                <Action type="SetProperty" frame="HowToPlayButton" Visible="true"/>
                <Action type="SetProperty" frame="PatchNotesButton" Visible="true"/>
                <Action type="SetProperty" frame="ReviewsButton" Visible="true"/>
                <Action type="SetProperty" frame="IdleSpinner" Visible="false"/>
            </State>
        </StateGroup>
    </Frame>
</Desc>
