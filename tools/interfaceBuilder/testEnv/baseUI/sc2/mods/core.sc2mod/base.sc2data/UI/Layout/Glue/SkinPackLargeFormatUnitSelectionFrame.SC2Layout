            <Anchor relative="$parent"/>
            <FormatText val=""/>
            <Replacement index="0" val="{$parent/SkinCacheFrame/@NameText}"/>
            <Replacement index="1" val="{$parent/UnitCacheFrame/@NameText}"/>
