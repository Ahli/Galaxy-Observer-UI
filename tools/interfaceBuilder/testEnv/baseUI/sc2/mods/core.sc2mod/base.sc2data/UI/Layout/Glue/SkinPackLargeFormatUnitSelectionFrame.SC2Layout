        <!--
        
        STANDARD STATEGROUPS
        
        -->
                
        <!-- ToggledState -->
        <StateGroup name="ToggledState">
            <DefaultState val="NotToggled"/>
            <State name="Toggled">
                <When type="Property" frame="$this" Toggled="True"/>
            </State>
            <State name="NotToggled">
            </State>
        </StateGroup>
                
        <!-- HoveredState -->
        <StateGroup name="HoveredState">
            <DefaultState val="NotHovered"/>
            <State name="Hovered">
                <When type="Property" frame="$this" Hovering="True"/>
            </State>
            <State name="NotHovered"/>
        </StateGroup>
                
        <!-- PressedState -->
        <StateGroup name="PressedState">
            <DefaultState val="NotPressed"/>
            <State name="Pressed">
                <When type="Property" frame="$this" Pushed="True"/>
            </State>
            <State name="NotPressed">
            </State>
        </StateGroup>
                
        <!-- DisabledState -->
        <StateGroup name="DisabledState">
            <DefaultState val="NotDisabled"/>
            <State name="Disabled">
            </State>
            <State name="NotDisabled"/>
        </StateGroup>
                
        <!-- EquippedState -->
        <StateGroup name="EquippedState">
            <DefaultState val="NotEquipped"/>
            <State name="Equipped">
                <When type="Property" frame="$this" Equipped="True"/>
            </State>
            <State name="NotEquipped">
            </State>
        </StateGroup>
                
        <!-- UnitState -->
        <StateGroup name="UnitState">
            <DefaultState val="NotUnit"/>
            <State name="Unit">
                <When type="Property" frame="UnitCacheFrame" Valid="True"/>
            </State>
            <State name="NotUnit">
            </State>
        </StateGroup>
                
        <!-- EmptyState -->
        <StateGroup name="EmptyState">
            <DefaultState val="NotEmpty"/>
            <State name="Empty">
                <When type="Property" frame="$this" HasData="False"/>
            </State>
            <State name="NotEmpty">
            </State>
        </StateGroup>
                
        <!-- HoverSoundState -->
        <StateGroup name="HoverSoundState">
            <DefaultState val="DoNotPlay"/>
            <State name="Play">
                <When type="StateGroup" HoveredState="Hovered"/>
                <When type="StateGroup" EquippedState="NotEquipped"/>
                <Action type="PlaySound" sound="@UI_HoverTick"/>
            </State>
            <State name="DoNotPlay"/>
        </StateGroup>

        <!-- DisplayState -->
        <StateGroup name="DisplayState">
            <DefaultState val="Empty"/>
            
            <!-- <State name="EquippedHovered">
                <When type="StateGroup" frame="$this" HoveredState="Hovered"/>
                <When type="StateGroup" frame="$this" EquippedState="Equipped"/>
                <When type="StateGroup" frame="$this" UnitState="Unit"/>
                <When type="StateGroup" frame="$this" EmptyState="NotEmpty"/>
                
                <Action type="SetProperty" frame="BorderInner" Color="239,182,85"/>
                <Action type="SetProperty" frame="IconImage" Visible="True"/>
                <Action type="SetProperty" frame="ClearSelectionIcon" Visible="False"/>
                <Action type="SetProperty" frame="EquippedIcon" Visible="True"/>
                
                <Action type="SetProperty" frame="$this" SelectedSound="@UI_FileSelectionSelect"/>
                <Action type="SetProperty" frame="$this" Enabled="False"/>
                <Action type="SetProperty" frame="$this/FormattedTooltip" FormatText="@UI/ScreenCollection/Skins/LargeFormatPossibleUnitTooltip/InSlot"/>
            </State> -->
            
            <State name="Equipped">
                <When type="StateGroup" frame="$this" EquippedState="Equipped"/>
                <When type="StateGroup" frame="$this" UnitState="Unit"/>
                <When type="StateGroup" frame="$this" EmptyState="NotEmpty"/>
                
                <Action type="SetProperty" frame="BorderInner" Color="198,133,22"/>
                <Action type="SetProperty" frame="IconImageHighlight" Alpha="0"/>
                <Action type="SetProperty" frame="IconImage" Visible="True"/>
                <Action type="SetProperty" frame="ClearSelectionIcon" Visible="False"/>
                <Action type="SetProperty" frame="EquippedIcon" Visible="True"/>
                
                <Action type="SetProperty" frame="$this" SelectedSound="@UI_FileSelectionSelect"/>
                <Action type="SetProperty" frame="$this" Enabled="False"/>
                <Action type="SetProperty" frame="$this/FormattedTooltip" FormatText="@UI/ScreenCollection/Skins/LargeFormatPossibleUnitTooltip/InSlot"/>
            </State>

            <State name="NormalHovered">
                <When type="StateGroup" frame="$this" HoveredState="Hovered"/>
                <When type="StateGroup" frame="$this" UnitState="Unit"/>
                <When type="StateGroup" frame="$this" EmptyState="NotEmpty"/>
                
                <Action type="SetProperty" frame="BorderInner" Color="88,163,234"/>
                <Action type="SetProperty" frame="IconImageHighlight" Alpha="120"/>
                <Action type="SetProperty" frame="IconImage" Visible="True"/>
                <Action type="SetProperty" frame="ClearSelectionIcon" Visible="False"/>
                <Action type="SetProperty" frame="EquippedIcon" Visible="False"/>
                
                <Action type="SetProperty" frame="$this" SelectedSound="@UI_FileSelectionSelect"/>
                <Action type="SetProperty" frame="$this" Enabled="True"/>
                <Action type="SetProperty" frame="$this/FormattedTooltip" FormatText="@UI/ScreenCollection/Skins/LargeFormatPossibleUnitTooltip/Standard"/>
            </State>

            <State name="Normal">
                <When type="StateGroup" frame="$this" UnitState="Unit"/>
                <When type="StateGroup" frame="$this" EmptyState="NotEmpty"/>
                
                <Action type="SetProperty" frame="BorderInner" Color="36,107,169"/>
                <Action type="SetProperty" frame="IconImageHighlight" Alpha="0"/>
                <Action type="SetProperty" frame="IconImage" Visible="True"/>
                <Action type="SetProperty" frame="ClearSelectionIcon" Visible="False"/>
                <Action type="SetProperty" frame="EquippedIcon" Visible="False"/>
                
                <Action type="SetProperty" frame="$this" SelectedSound="@UI_FileSelectionSelect"/>
                <Action type="SetProperty" frame="$this" Enabled="True"/>
                <Action type="SetProperty" frame="$this/FormattedTooltip" FormatText="@UI/ScreenCollection/Skins/LargeFormatPossibleUnitTooltip/Standard"/>
            </State>

            <State name="ClearHovered">
                <When type="StateGroup" frame="$this" HoveredState="Hovered"/>
                <When type="StateGroup" frame="$this" EmptyState="NotEmpty"/>
                
                <Action type="SetProperty" frame="BorderInner" Color="88,163,234"/>
                <Action type="SetProperty" frame="IconImageHighlight" Alpha="0"/>
                <Action type="SetProperty" frame="IconImage" Visible="False"/>
                <Action type="SetProperty" frame="ClearSelectionIcon" Visible="True"/>
                <Action type="SetProperty" frame="EquippedIcon" Visible="False"/>
                
                <Action type="SetProperty" frame="$this" SelectedSound="@UI_DropdownClose"/>
                <Action type="SetProperty" frame="$this" Enabled="True"/>
                <Action type="SetProperty" frame="$this/FormattedTooltip" FormatText="@UI/ScreenCollection/Skins/LargeFormatPossibleUnitTooltip/ClearSlot"/>
            </State>

            <State name="Clear">
                <When type="StateGroup" frame="$this" EmptyState="NotEmpty"/>
                
                <Action type="SetProperty" frame="BorderInner" Color="36,107,169"/>
                <Action type="SetProperty" frame="IconImageHighlight" Alpha="0"/>
                <Action type="SetProperty" frame="IconImage" Visible="False"/>
                <Action type="SetProperty" frame="ClearSelectionIcon" Visible="True"/>
                <Action type="SetProperty" frame="EquippedIcon" Visible="False"/>
                
                <Action type="SetProperty" frame="$this" SelectedSound="@UI_DropdownClose"/>
                <Action type="SetProperty" frame="$this" Enabled="True"/>
                <Action type="SetProperty" frame="$this/FormattedTooltip" FormatText="@UI/ScreenCollection/Skins/LargeFormatPossibleUnitTooltip/ClearSlot"/>
            </State>
            
            <State name="Empty">
                <Action type="SetProperty" frame="BorderInner" Color="21,42,63"/>
                <Action type="SetProperty" frame="IconImageHighlight" Alpha="0"/>
                <Action type="SetProperty" frame="IconImage" Visible="False"/>
                <Action type="SetProperty" frame="ClearSelectionIcon" Visible="False"/>
                <Action type="SetProperty" frame="EquippedIcon" Visible="False"/>
                
                <Action type="SetProperty" frame="$this" Enabled="False"/>
                <Action type="SetProperty" frame="$this/FormattedTooltip" FormatText=""/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="SkinPackLargeFormatTileListItem" name="SelectedUnitCacheTileListItemTemplate" template="StandardSmallButtonTemplates/StandardSmallIconTileListItemTemplate">
        <SelectedSound val="@UI_FileSelectionSelect"/>
        
        <Frame type="Frame" name="AnchorFrame">
            <Width val="118"/>
            <Height val="88"/>
        </Frame>
        
        <!-- Background -->
        <Frame type="Image" name="Background">
            
            <StateGroup name="ColorState">
                
                <State name="Disabled">
                    <Action type="SetProperty" frame="$this" LayerColor="6,15,25" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="89" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerColor="22,29,37" index="3"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="0" index="3"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- Background Highlight -->
        <Frame type="Image" name="BackgroundHighlight">
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="3"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="-3"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="3"/>
            <Height val="88"/>
            <Visible val="False"/>
            <RenderPriority val="511"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Glues_Battlenet_StandardButtons_Small_Mask" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <TextureType val="NineSlice" layer="0"/>
            
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_TextBackground_Small_Normal" layer="1"/>
            
            <StateGroup name="DisplayState">
                <DefaultState val="Normal"/>
                
                <State name="Hovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_PageAssets_GraphicListItem_TextBackground_Small_Hover" index="1"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_PageAssets_GraphicListItem_TextBackground_Small_Normal" index="1"/>
                </State>
            </StateGroup>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$parent" PressedState="NotPressed"/>
                    <When type="StateGroup" frame="$parent" DisabledState="NotDisabled"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="UnitCacheFrame" name="UnitCacheFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Unit val="{$parent/@Unit}"/>
        </Frame>

        <Frame type="RewardCacheFrame" name="RewardCacheFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Reward val="{$parent/@Reward}"/>
        </Frame>

        <Frame type="Image" name="IconImage">
            <Anchor side="Top" relative="$this" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-14"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Height val="76"/>
            <Width val="76"/>
            <DesaturateOnDisable val="False"/>
            <ColorAdjustMode val="None"/>
            <LightAdjustColor val="234d7b"/>
            <DarkAdjustColor val="0a2646"/>
            
            <StateGroup name="DefaultSkinState">
                <DefaultState val="NotDefaultSkin"/>

                <State name="DefaultSkin">
                    <When type="Property" frame="$parent/RewardCacheFrame" Default="True"/>
                    <Action type="SetProperty" frame="$this" Texture="{$parent/UnitCacheFrame/@Icon}"/>
                    <Action type="SetProperty" frame="$this" ColorAdjustMode="Desaturate"/>
                </State>

                <State name="NotDefaultSkin">
                    <Action type="SetProperty" frame="$this" Texture="{$parent/RewardCacheFrame/@Skin[VariationIcon]}"/>
                    <Action type="SetProperty" frame="$this" ColorAdjustMode="None"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Image" name="IconImageHighlight">
            <Anchor relative="$parent/IconImage"/>
            <Texture val="{$parent/IconImage/@Texture}"/>
            <Visible val="{$parent/IconImage/@Visible}"/>
            <ColorAdjustMode val="{$parent/IconImage/@ColorAdjustMode}"/>
            <LightAdjustColor val="{$parent/IconImage/@LightAdjustColor}"/>
            <DarkAdjustColor val="{$parent/IconImage/@DarkAdjustColor}"/>
            <DesaturateOnDisable val="False"/>
            <BlendMode val="Add"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Image" name="InvalidIconImageShadow">
            <Anchor side="Top" relative="$parent/InvalidIconImage" pos="Mid" offset="1"/>
            <Anchor side="Bottom" relative="$parent/InvalidIconImage" pos="Mid" offset="1"/>
            <Anchor side="Left" relative="$parent/InvalidIconImage" pos="Mid" offset="1"/>
            <Anchor side="Right" relative="$parent/InvalidIconImage" pos="Mid" offset="1"/>
            <Height val="28"/>
            <Width val="28"/>
            <Visible val="{$parent/InvalidIconImage/@Visible}"/>
            <Texture val="@UI/Glues_Battlenet_StandardButtons_Large_Icon_Plus"/>
            <Color val="0,0,0"/>
            <Alpha val="{$parent/InvalidIconImage/@Alpha}"/>
        </Frame>

        <Frame type="Image" name="InvalidIconImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Height val="28"/>
            <Width val="28"/>
            <LayerCount val="4"/>
            <Texture val="@UI/Glues_Battlenet_StandardButtons_Large_Icon_Plus" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <Texture val="Assets/Textures/White.dds" layer="1"/>
            <Texture val="@UI/Glues_Battlenet_StandardButtons_Large_Icon_Plus" layer="2"/>
            <AlphaLayer val="True" layer="2"/>
            <Texture val="@UI/Glues_Battlenet_StandardButtons_Small_Background_Gradient" layer="3"/>
            <TextureCoords top="1" left="0" bottom="0" right="1" layer="3"/>
            <LayerColor val="255,255,255" layer="3"/>
            <LayerAlpha val="120" layer="3"/>
            <DesaturateOnDisable val="False"/>
        </Frame>
                
        <!-- HoverSoundState -->
        <StateGroup name="HoverSoundState">
            <DefaultState val="DoNotPlay"/>
            <State name="Play">
                <When type="Property" Selected="False"/>
                <When type="Property" Hovered="True"/>
                <When type="StateGroup" EnabledType="Enabled"/>
                <Action type="PlaySound" sound="@UI_HoverTick"/>
            </State>
            <State name="DoNotPlay"/>
        </StateGroup>

        <StateGroup name="UnitSetStateGroup">
            <DefaultState val="UnitNotValid"/>

            <State name="UnitValid">
                <When type="Property" frame="UnitCacheFrame" Valid="true"/>
                <Action type="SetProperty" frame="IconImage" visible="true"/>
                <Action type="SetProperty" frame="InvalidIconImage" visible="false"/>
            </State>

            <State name="UnitNotValid">
                <Action type="SetProperty" frame="IconImage" visible="false"/>
                <Action type="SetProperty" frame="InvalidIconImage" visible="true"/>
            </State>
        </StateGroup>
        
        <StateGroup name="EnabledType">
            <State name="Enabled"/>
            <State name="Disabled_PreviousSlotEmpty"/>
            <!--
            <State name="Disabled_NoSkinsEquipped"/>
            <State name="Disabled_AllEquippedSkinsInUse"/>
            -->
        </StateGroup>
        
        <StateGroup name="SelectedType">
            <State name="Selected">
                <When type="Property" frame="$this" Selected="True"/>
            </State>
            <State name="Unselected">
                <When type="Property" frame="$this" Selected="False"/>
            </State>
        </StateGroup>
        
        <StateGroup name="PressedChecker">
            <DefaultState val="NotPressed"/>
            <State name="PressedSelected">
                <When type="StateGroup" frame="$this" SelectedType="Selected"/>
                <Action type="SetProperty" frame="$this/AnchorFrame" Height="104"/>
            </State>
            <State name="PressedReal">
                <When type="Property" frame="$this" Pushed="True"/>
                <Action type="SetProperty" frame="$this/AnchorFrame" Height="88"/>
            </State>
            <State name="NotPressed">
                <Action type="SetProperty" frame="$this/AnchorFrame" Height="88"/>
            </State>
        </StateGroup>
        
        <StateGroup name="PressedState">
            <DefaultState val="Pressed"/>
            <State name="Pressed">
            </State>
            <State name="NotPressed">
                <When type="StateGroup" frame="$this" PressedChecker="NotPressed"/>
            </State>
        </StateGroup>
        
        <StateGroup name="EnabledStateFunction">
            <DefaultState val="Normal"/>
            
            <State name="Disabled_PreviousSlotEmpty">
                <When type="StateGroup" frame="$this" EnabledType="Disabled_PreviousSlotEmpty"/>
                <Action type="SetProperty" frame="$this" Enabled="False"/>
                <Action type="SetProperty" frame="$this" AcceptsMouse="False"/>
                <Action type="SetProperty" frame="$this" Tooltip=""/>
                <Action type="SetProperty" frame="$this/InvalidIconImage" Alpha="0"/>
                <Action type="SetProperty" frame="$this/IconImageHighlight" Alpha="0"/>
            </State>

            <State name="Enabled_Selected">
                <When type="StateGroup" frame="$this" SelectedType="Selected"/>
                <Action type="SetProperty" frame="$this" Enabled="True"/>
                <Action type="SetProperty" frame="$this" AcceptsMouse="True"/>
                <Action type="SetProperty" frame="$this/InvalidIconImage" Alpha="0"/>
                <Action type="SetProperty" frame="$this/IconImageHighlight" Alpha="0"/>
            </State>

            <State name="Hover">
                <When type="StateGroup" frame="$this" HoveredState="Hovered"/>
                <Action type="SetProperty" frame="$this" Enabled="True"/>
                <Action type="SetProperty" frame="$this" AcceptsMouse="True"/>
                <Action type="SetProperty" frame="$this/InvalidIconImage" LayerColor="141,208,255" index="1"/>
                <Action type="SetProperty" frame="$this/IconImageHighlight" Alpha="100"/>
            </State>
            
            <State name="Normal">
                <Action type="SetProperty" frame="$this" Enabled="True"/>
                <Action type="SetProperty" frame="$this" AcceptsMouse="True"/>
                <Action type="SetProperty" frame="$this/InvalidIconImage" LayerColor="65,131,199" index="1"/>
                <Action type="SetProperty" frame="$this/IconImageHighlight" Alpha="0"/>
            </State>
        </StateGroup>
    </Frame>

