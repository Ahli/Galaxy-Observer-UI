<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
<!--
||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||

CONSTANTS

||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||
-->
    
    <Constant name="Width_EntrantFrame" val="284"/>
    <Constant name="Width_MatchFrame" val="284"/>
    <Constant name="Color_Lines_Lose" val="48,127,194"/>
    <Constant name="Color_Lines_Win" val="48,127,194"/>
    <Constant name="Alpha_Lines_Lose" val="65"/>
    <Constant name="Size_Lines" val="2"/>
    <Constant name="Alpha_Lines_Default" val="0"/>
    <Constant name="Visible_Lines_Default" val="False"/>
    <Constant name="Time_Lines_FadeIn" val="0"/>
    

<!--
||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||

BRACKET FRAME TEMPLATES

||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||
-->

    <!--
        
    ====================================================================
    
    ANIMATION STATES
    
    ====================================================================
    
    -->
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    ENTRANT STATE ANIMATIONS
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
    
    <!-- Default -->
    <Animation name="BracketEntrantStateAnimationTemplate_Default">
        
        <Controller type="Visibility" end="Pause" frame="Background_Empty">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="Background_Active">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="Background_Winner">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Event" name="LineAlpha" end="Stop">
            <Key type="Event" time="0" event="HideLines"/>
        </Controller>
        
        <Controller type="Event" name="LineContainerVisibility" end="Stop">
            <Key type="Event" time="0" event="ShowLineContainer"/>
        </Controller>
    </Animation>
    
    
    <!-- Empty -->
    <Animation name="BracketEntrantStateAnimationTemplate_Empty" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Default">
        <Event event="Empty" action="Reset,Play"/>
        
        <Controller type="Visibility" end="Pause" frame="Background_Empty">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Event" name="LineContainerVisibility" end="Stop">
            <Key type="Event" time="0" event="HideLineContainer"/>
        </Controller>
    </Animation>

    
    <!-- Undecided -->
    <Animation name="BracketEntrantStateAnimationTemplate_Undecided" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Default">
        <Event event="Undecided" action="Reset,Play"/>

        <Controller type="Visibility" end="Pause" frame="Background_Active">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
    </Animation>

    
    <!-- Won -->
    <Animation name="BracketEntrantStateAnimationTemplate_Won" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Default">
        <Event event="Won" action="Reset,Play"/>
        
        <Controller type="Visibility" end="Pause" frame="Background_Active">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Event" name="LineAlpha" end="Stop">
            <Key type="Event" time="0" event="ShowLines"/>
        </Controller>
    </Animation>

    
    <!-- Lost -->
    <Animation name="BracketEntrantStateAnimationTemplate_Lost" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Default">
        <Event event="Lost" action="Reset,Play"/>
        
        <Controller type="Visibility" end="Pause" frame="Background_Empty">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
    </Animation>

    
    <!-- Bye -->
    <Animation name="BracketEntrantStateAnimationTemplate_Bye" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Default">
        <Event event="Bye" action="Reset,Play"/>
        
        <Controller type="Visibility" end="Pause" frame="Background_Empty">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
    </Animation>

    
    <!-- Eliminated -->
    <Animation name="BracketEntrantStateAnimationTemplate_Eliminated" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Default">
        <Event event="Eliminated" action="Reset,Play"/>
        
        <Controller type="Visibility" end="Pause" frame="Background_Empty">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
    </Animation>
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    TOURNAMENT TYPE STATE ANIMATIONS
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
    
    <!-- 1v1 -->
    <Animation name="BracketEntrantStateAnimationTemplate_1v1">
        <Event event="1v1" action="Reset,Play"/>

    </Animation>

    <!-- 2v2 -->
    <Animation name="BracketEntrantStateAnimationTemplate_2v2">
        <Event event="2v2" action="Reset,Play"/>

    </Animation>

    <!-- Tandem -->
    <Animation name="BracketEntrantStateAnimationTemplate_Tandem">
        <Event event="Tandem" action="Reset,Play"/>

    </Animation>
    
    
    <!--
        
    ====================================================================
    
    ENTRANT FRAME TEMPLATES
    
    ====================================================================
    
    -->
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    BASE ENTRANT PLAYER FRAME TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="TournamentEntrantPlayerFrame" name="BracketTournamentEntrantPlayerFrameTemplate" template="TournamentEntrantPlayerFrame/TournamentEntrantPlayerFrameTemplate">
        <Width val="#Width_EntrantFrame"/>
        <Height val="52"/>

        <Frame type="Label" name="NameLabel">
            <Height val="30"/>
            <AutoSizeClamps maxWidth="192"/>
            <ContextMenu val="$parent/$parent/$parent/$parent/$parent/TournamentBattleUserContextMenu"/>
        </Frame>
    </Frame>

    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    LEFT ENTRANT FRAME TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="TournamentMatchEntrantFrame" name="TournamentMatchEntrantFrameTemplate_Left">
    
        <!-- 
        
        ~~~~~~~~~~~~~~~~~~~~~~~
        EVENTS
        ~~~~~~~~~~~~~~~~~~~~~~~
        
        The following events are being sent to this frame:

        Empty - There is no entrant in this frame yet.
        Undecided - This entrant's match is undecided.
        Won - This entrant won their match.
        Lost - This entrant lost their match.
        Eliminated - This entrant was eliminated in this match or a previous match.
        Bye - This entrant is empty and gave its opponent a bye.
        
        Self - The viewer is in this entrant slot.
        NotSelf - The viewer is not in this entrant slot.
        
        1v1 - The tournament style is 1v1
        2v2 - The tournament style is 2v2
        Tandem - The tournament style is Archon Mode
        
        In this case these events are being listened to both on this frame and on the TournnamentEntrantPlayerFrame/TournamentEntrantPlayerFrameTemplate
        which is being used in this template as "EntrantPlayerFrame0" and possibly "EntrantPlayerFrame1" if support for archon mode or 2v2 is added.
        
        The Self/NotSelf events are actually not used here at all, and instead is only listened to by children of this frame.
        
        -->
        
        <Width val="#Width_EntrantFrame"/>
        <Height val="64"/>

        <Frame type="Image" name="Background_Empty">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Single_Left_Empty"/>
            <TextureType val="Normal"/>
        </Frame>
        
        <Frame type="Image" name="Background_Active">
            <Anchor relative="$parent/Background_Empty"/>
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Single_Left_Active"/>
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Image" name="Background_Winner">
            <Anchor relative="$parent/Background_Empty"/>
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Single_Left_Winner"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="TournamentEntrantPlayerFrame" name="EntrantPlayerFrame0" template="TournamentBracketStage/BracketTournamentEntrantPlayerFrameTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="1"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="1"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="9"/>
        </Frame>

        <!-- Entrant State Animations -->
        <Animation name="BracketEntrant_Empty" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Empty"/>
        <Animation name="BracketEntrant_Won" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Won"/>
        <Animation name="BracketEntrant_Lost" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Lost"/>
        <Animation name="BracketEntrant_Undecided" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Undecided"/>
        <Animation name="BracketEntrant_Eliminated" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Eliminated"/>
        <Animation name="BracketEntrant_Bye" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Bye"/>
        
        <!-- Tournament Type State Animations -->
        <Animation name="BracketEntrant_1v1" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_1v1"/>
        <Animation name="BracketEntrant_2v2" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_2v2"/>
        <Animation name="BracketEntrant_Tandem" template="TournamentBracketStage/BracketEntrantStateAnimationTemplate_Tandem"/>
    </Frame>
    
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    RIGHT ENTRANT FRAME TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="TournamentMatchEntrantFrame" name="TournamentMatchEntrantFrameTemplate_Right" template="TournamentBracketStage/TournamentMatchEntrantFrameTemplate_Left">
    
        <Frame type="Image" name="Background_Empty">
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Single_Right_Empty"/>
        </Frame>
        
        <Frame type="Image" name="Background_Active">
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Single_Right_Active"/>
        </Frame>
        
        <Frame type="Image" name="Background_Winner">
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Single_Right_Winner"/>
        </Frame>
        
        <Frame type="TournamentEntrantPlayerFrame" name="EntrantPlayerFrame0" template="TournamentBracketStage/BracketTournamentEntrantPlayerFrameTemplate">
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
        </Frame>
    </Frame>
    
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    TOURNAMENT WINNER CALLOUT TOP TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
    
    <Frame type="Frame" name="TournamentWinnerCalloutTemplate_Top">
        <Width val="700"/>
        <Height val="300"/>
        <Visible val="False"/>
        
        <Frame type="Image" name="WinnerLabelFlare">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-52"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-52"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Winner_TextFlare"/>
            
            <Animation name="Pulse">
                <Event event="OnShown" action="Reset,Play"/>
                <Controller type="Fade" end="PingPong">
                    <Key type="Curve" time="0" value="80" out="Slow"/>
                    <Key type="Curve" time="2" value="255" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="WinnerLabel">
            <Anchor side="Top" relative="$parent/WinnerLabelFlare" pos="Mid" offset="-3"/>
            <Anchor side="Bottom" relative="$parent/WinnerLabelFlare" pos="Mid" offset="-3"/>
            <Anchor side="Left" relative="$parent/WinnerLabelFlare" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/WinnerLabelFlare" pos="Mid" offset="0"/>
            <Style val="@Tournament_WinnerLabel"/>
            <Text val="@UI/TournamentMatchFrames/MatchStatusWinner"/>
        </Frame>
    </Frame>
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    TOURNAMENT WINNER CALLOUT BOTTOM TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
    
    <Frame type="Frame" name="TournamentWinnerCalloutTemplate_Bottom" template="TournamentBracketStage/TournamentWinnerCalloutTemplate_Top">
        <Width val="700"/>
        <Height val="300"/>
        <Visible val="False"/>
        
        <Frame type="Image" name="WinnerLabelFlare">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="59"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="59"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        </Frame>
    </Frame>
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    FINALS MATCH TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="TournamentMatchFrame" name="FinalsTournamentMatchFrameTemplate">
        <Height val="124"/>
        <Width val="334"/>

        <Animation name="NotStarted">
            <Event event="NotStarted" action="Reset,Play"/>

            <Controller type="Text" end="Pause" frame="StatusLabel">
                <Key type="Text" time="0" text="@UI/TournamentMatchFrames/MatchStatusWaiting"/>
            </Controller>

            <Controller type="Style" end="Pause" style1="Tournament_MatchStatusWaiting" style2="Tournament_MatchStatusWaiting" frame="StatusLabel">
                <Key type="Curve" time="0" value="0"/>
            </Controller>
        </Animation>

        <Animation name="InProgress">
            <Event event="InProgress" action="Reset,Play"/>

            <Controller type="Text" end="Pause" frame="StatusLabel">
                <Key type="Text" time="0" text="@UI/TournamentMatchFrames/MatchStatusInProgress"/>
            </Controller>

            <Controller type="Style" end="Pause" style1="Tournament_MatchStatusProgress" style2="Tournament_MatchStatusProgress" frame="StatusLabel">
                <Key type="Curve" time="0" value="0"/>
            </Controller>
        </Animation>

        <Animation name="Complete">
            <Event event="Complete" action="Reset,Play"/>

            <Controller type="Text" end="Pause" frame="StatusLabel">
                <Key type="Text" time="0" text="@UI/TournamentMatchFrames/MatchStatusComplete"/>
            </Controller>

            <Controller type="Style" end="Pause" style1="Tournament_MatchStatusComplete" style2="Tournament_MatchStatusComplete" frame="StatusLabel">
                <Key type="Curve" time="0" value="0"/>
            </Controller>
        </Animation>
        
        <Animation name="Empty">
            <Event event="Empty" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
        </Animation>

        <Frame type="Label" name="StatusLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="3"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
            <Style val="@Tournament_MatchStatusWaiting"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="TournamentMatchEntrantFrame" name="EntrantFrame0" template="TournamentBracketStage/TournamentMatchEntrantFrameTemplate_Left">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            
            <Animation name="TournamentWinnerCalloutShow">
                <Event event="Won" action="Reset,Play"/>
                
                <Controller type="Visibility" name="WinnerBorderVisibility" end="Pause" frame="Background_Winner">
                    <Key type="Bool" time="0.01" value="True"/>
                </Controller>
            </Animation>
            
            <Animation name="TournamentWinnerCalloutHide">
                <Event event="Empty" action="Reset,Play"/>
                <Event event="Undecided" action="Reset,Play"/>
                <Event event="Lost" action="Reset,Play"/>
                <Event event="Bye" action="Reset,Play"/>
                <Event event="Eliminated" action="Reset,Play"/>
                
                <Controller type="Visibility" name="WinnerBorderVisibility" end="Pause" frame="Background_Winner">
                    <Key type="Bool" time="0" value="False"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="TournamentMatchEntrantFrame" name="EntrantFrame1" template="TournamentBracketStage/TournamentMatchEntrantFrameTemplate_Right">
            <Anchor side="Top" relative="$parent/EntrantFrame0" pos="Max" offset="-4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            
            <Animation name="TournamentWinnerCalloutShow">
                <Event event="Won" action="Reset,Play"/>
                
                <Controller type="Visibility" name="WinnerBorderVisibility" end="Pause" frame="Background_Winner">
                    <Key type="Bool" time="0.01" value="True"/>
                </Controller>
            </Animation>
            
            <Animation name="TournamentWinnerCalloutHide">
                <Event event="Empty" action="Reset,Play"/>
                <Event event="Undecided" action="Reset,Play"/>
                <Event event="Lost" action="Reset,Play"/>
                <Event event="Bye" action="Reset,Play"/>
                <Event event="Eliminated" action="Reset,Play"/>
                
                <Controller type="Visibility" name="WinnerBorderVisibility" end="Pause" frame="Background_Winner">
                    <Key type="Bool" time="0" value="False"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    LEFT MATCH TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="TournamentMatchFrame" name="TournamentMatchFrameTemplate_Left">
        <Height val="124"/>
        <Width val="#Width_MatchFrame"/>

        <Frame type="TournamentMatchEntrantFrame" name="EntrantFrame0" template="TournamentBracketStage/TournamentMatchEntrantFrameTemplate_Left">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        </Frame>

        <Frame type="TournamentMatchEntrantFrame" name="EntrantFrame1" template="TournamentBracketStage/TournamentMatchEntrantFrameTemplate_Left">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        </Frame>
    </Frame>
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    RIGHT MATCH TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
    
    <Frame type="TournamentMatchFrame" name="TournamentMatchFrameTemplate_Right" template="TournamentBracketStage/TournamentMatchFrameTemplate_Left">
    
        <Frame type="TournamentMatchEntrantFrame" name="EntrantFrame0" template="TournamentBracketStage/TournamentMatchEntrantFrameTemplate_Right"/>

        <Frame type="TournamentMatchEntrantFrame" name="EntrantFrame1" template="TournamentBracketStage/TournamentMatchEntrantFrameTemplate_Right"/>
    </Frame>
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    BRACKET LINE TEMPLATES
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
    
    <!-- Horizontal Line - Lose -->
                    
    <Frame type="Image" name="HorizontalLineLoseTemplate">
        <Height val="#Size_Lines"/>
        <Color val="#Color_Lines_Lose"/>
        <Alpha val="#Alpha_Lines_Lose"/>
    </Frame>
    
    <!-- Horizontal Line - Win -->
    
    <Frame type="Image" name="HorizontalLineWinTemplate">
        <Height val="#Size_Lines"/>
        <Color val="#Color_Lines_Win"/>
    </Frame>
    
    <!-- Vertical Line - Lose -->
    
    <Frame type="Image" name="VerticalLineLoseTemplate">
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <Width val="#Size_Lines"/>
        <Color val="#Color_Lines_Lose"/>
        <Alpha val="#Alpha_Lines_Lose"/>
    </Frame>
    
    <!-- Vertical Line - Win -->
    
    <Frame type="Image" name="VerticalLineWinTemplate">
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <Width val="#Size_Lines"/>
        <Color val="#Color_Lines_Win"/>
    </Frame>
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    ROUND HIGHLIGHT TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
                    
    <Frame type="Frame" name="RoundHighlightTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        
        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent" pos="Min" offset="50"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-100"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Alpha val="45"/>
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Highlight_Background"/>
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Image" name="BorderLeft">
            <Anchor side="Top" relative="$parent" pos="Min" offset="50"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-100"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="2"/>
            <Alpha val="35"/>
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Highlight_Border"/>
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Image" name="BorderRight">
            <Anchor side="Top" relative="$parent" pos="Min" offset="50"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-100"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Width val="2"/>
            <Alpha val="35"/>
            <Texture val="@UI/SC2_UI_Tournament_Bracket_Highlight_Border"/>
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Label" name="RoundTitle">
            <Anchor side="Top" relative="$parent" pos="Min" offset="100"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Style val="@Tournament_BracketRoundTitle"/>
        </Frame>
        
        <Animation name="Show">
            <Event event="Show" action="Reset,Play"/>
            <Event event="Hide" action="Reset"/>
             
            <Controller type="Visibility" end="Pause" frame="Background">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="BorderLeft">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="BorderRight">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Style" end="Pause" style1="Tournament_BracketRoundTitleHighlight" style2="Tournament_BracketRoundTitleHighlight" frame="RoundTitle">
                <Key type="Curve" time="0" value="0"/>
            </Controller>
        </Animation>
    </Frame>

    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    COMPLETE BRACKET TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="TournamentBracketFrame" name="TournamentBracketFrameTemplate">
        <Height val="650"/>

        <!-- Empty -->
        <Animation name="Empty">
            <Event event="Empty" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>

        <!-- Inactive -->
        <Animation name="Inactive">
            <Event event="Inactive" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>

        <!-- Active -->
        <Animation name="Active">
            <Event event="Active" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
        </Animation>

        <!-- Current Round Animations -->
        <Animation name="InvalidRoundCurrent">
            <Event event="InvalidRoundCurrent" action="Reset,Play"/>
        </Animation>

        <Animation name="Round1Current">
            <Event event="Round1Current" action="Reset,Play"/>
        </Animation>

        <Animation name="Round2Current">
            <Event event="Round2Current" action="Reset,Play"/>
        </Animation>

        <Animation name="Round3Current">
            <Event event="Round3Current" action="Reset,Play"/>
        </Animation>

        <Frame type="Image" name="OuterBackground" template="StandardTemplates/StandardOuterBorderTemplate">
            <Anchor relative="$parent"/>
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Frame" name="RoundHighlightGrandFinals" template="TournamentBracketStage/RoundHighlightTemplate">
            <Anchor side="Left" relative="$parent/MatchFrame0" pos="Min" offset="-11"/>
            <Anchor side="Right" relative="$parent/MatchFrame0" pos="Max" offset="11"/>
            
            <Frame type="Label" name="RoundTitle">
                <Text val="@UI/TournamentFrame/BracketStageFinals"/>
            </Frame>
            
            <Animation name="Current">
                <Event event="Round3Current" action="Reset,Play" frame="$parent"/>
                 
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="Show"/>
                </Controller>
            </Animation>
            
            <Animation name="Hide">
                <Event event="Round1Current" action="Reset,Play" frame="$parent"/>
                <Event event="Round2Current" action="Reset,Play" frame="$parent"/>
                
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="Hide"/>
                </Controller>
            </Animation>
        </Frame>
        
        
        <Frame type="Frame" name="RoundHighlightSemiFinals1" template="TournamentBracketStage/RoundHighlightTemplate">
            <Anchor side="Left" relative="$parent/MatchFrame1" pos="Min" offset="-11"/>
            <Anchor side="Right" relative="$parent/MatchFrame1" pos="Max" offset="11"/>
            
            <Frame type="Label" name="RoundTitle">
                <Text val="@UI/TournamentFrame/BracketStageSemiFinals"/>
