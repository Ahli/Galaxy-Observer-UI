                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Texture val="@UI/HelpMenuDialogWelcomeNewUserSubHeaderBg"/>
            </Frame>
        
            <Frame type="Label" name="OtherChangesHeader">
                <Anchor side="Top" relative="$parent/SecondSubHeaderBg" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/SecondSubHeaderBg" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/SecondSubHeaderBg" pos="Min" offset="223"/>
                <Style val="@HelpNewUserWelcomePanelSubHeader"/>
                <Text val="@UI/HelpReturnPanel_Text_OtherChanges"/>
            </Frame>
        
            <Frame type="Frame" name="OtherChangesItem1" template="ReturningUserDialog/HelpReturnIconItemTemplate">
                <Anchor side="Top" relative="$parent/SecondSubHeaderBg" pos="Max" offset="15"/>
                <Anchor side="Left" relative="$parent/OtherChangesHeader" pos="Min" offset="10"/>
            
                <Frame type="Image" name="HelpReturnIcon">
                    <Texture val="@UI/HelpMenuDialogWelcomeReturningUserIcon_6"/>
                </Frame>
            
                <Frame type="Label" name="HelpReturnText">
                    <Text val="@UI/HelpReturnPanel_Text_6"/>
                </Frame>
            </Frame>
        
            <Frame type="Frame" name="OtherChangesItem2" template="ReturningUserDialog/HelpReturnIconItemTemplate">
                <Anchor side="Top" relative="$parent/OtherChangesItem1" pos="Max" offset="5"/>
                <Anchor side="Left" relative="$parent/OtherChangesItem1" pos="Min" offset="0"/>
            
                <Frame type="Image" name="HelpReturnIcon">
                    <Texture val="@UI/HelpMenuDialogWelcomeReturningUserIcon_7"/>
                </Frame>
            
                <Frame type="Label" name="HelpReturnText">
                    <Text val="@UI/HelpReturnPanel_Text_7"/>
                </Frame>
            </Frame>
        
            <Frame type="Frame" name="OtherChangesItem3" template="ReturningUserDialog/HelpReturnIconItemTemplate">
                <Anchor side="Top" relative="$parent/OtherChangesItem2" pos="Max" offset="5"/>
                <Anchor side="Left" relative="$parent/OtherChangesItem2" pos="Min" offset="0"/>
            
                <Frame type="Image" name="HelpReturnIcon">
                    <Texture val="@UI/HelpMenuDialogWelcomeReturningUserIcon_8"/>
                </Frame>
            
                <Frame type="Label" name="HelpReturnText">
                    <Text val="@UI/HelpReturnPanel_Text_8"/>
                </Frame>
            </Frame>
            
            <Frame type="Label" name="HelpReturnCallToActionText">
                <Anchor side="Top" relative="$parent/OtherChangesItem3" pos="Max" offset="55"/>
                <Anchor side="Left" relative="$parent/OtherChangesItem3/HelpReturnText" pos="Min" offset="0"/>
                <Text val="@UI/HelpReturnPanel_Text_9"/>
                <Style val="HelpReturnPanelActionText"/>
            </Frame>
        
            <Frame type="Button" name="ContinueButton" template="StandardTemplates/StandardButtonAlternateTemplate">
                <Anchor side="Top" relative="$parent/OtherChangesItem3" pos="Max" offset="150"/>
                <Anchor side="Left" relative="$parent/OtherChangesItem3" pos="Min" offset="0"/>
                <Width val="313"/>
                <Text val="@UI/CampaignPanelContinue_Control"/>
            </Frame>
        
            <Frame type="GlueOptionsButton" name="SetOptionsButton" template="StandardTemplates/StandardButtonTemplate">
                <Anchor side="Top" relative="$parent/ContinueButton" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/ContinueButton" pos="Max" offset="5"/>
                <Width val="313"/>
                <Text val="@UI/HelpReturnPanel_Options"/>
            </Frame>
        </Frame>
        
        <Frame type="Image" name="FullscreenBlackout">
            <Anchor relative="$parent"/>
            <Color val="0, 0, 0"/>
            <Alpha val="255"/>
        </Frame>
