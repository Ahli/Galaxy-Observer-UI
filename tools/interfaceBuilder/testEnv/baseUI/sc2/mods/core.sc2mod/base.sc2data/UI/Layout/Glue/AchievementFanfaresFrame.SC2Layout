                    <Key type="Bool" time=".55" value="True"/>
                </Controller>
            </Animation>
        </Frame>

        <Animation name="ShowAnimation" template="AchievementFanfaresFrame/FanfareAnimationTemplate"/>

        <!-- State group to change the fanfare racial art style -->
