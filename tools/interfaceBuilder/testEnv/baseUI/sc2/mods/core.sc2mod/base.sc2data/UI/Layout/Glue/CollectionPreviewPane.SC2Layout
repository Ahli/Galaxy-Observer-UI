<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Constant name="VoiceLineLeftOffset" val="3"/>
    <Constant name="VoiceLineLeftOffset16x9" val="4"/>
    
    <!--
    ===========
    Grid Animations
    ===========
    
    Some shared animations for the background grid
    -->
                
    <Animation name="GridWipeTemplate">
        <Event event="OnShown" action="Play"/>
        
        <Controller type="LayerUV" layer="0" side="Top" relative="true" sidelock="false" end="Loop" >
            <Key type="Curve" time="0.000" value="0.000" inOut="Linear"/>
            <Key type="Curve" time="1.667" value="0.000" inOut="Linear"/>
            <Key type="Curve" time="5.000" value="8.250" inOut="Linear"/>
            <Key type="Curve" time="8.000" value="8.250" inOut="Linear"/>
        </Controller>
        
        <Controller type="LayerUV" layer="0" side="Bottom" relative="true" sidelock="false" end="Loop" >
            <Key type="Curve" time="0.000" value="0.000" inOut="Linear"/>
            <Key type="Curve" time="1.667" value="0.000" inOut="Linear"/>
            <Key type="Curve" time="5.000" value="8.250" inOut="Linear"/>
            <Key type="Curve" time="8.000" value="8.250" inOut="Linear"/>
        </Controller>
    </Animation>
    
    <!--
    ====================
    Background Streak Animations
    ====================
    -->
    
    <Animation name="StreakLeftToRightTemplate">
        <Event event="OnShown" action="Stop"/>
        <Event event="PlayStreak" action="Reset,Play"/>
        
        <Controller type="Anchor" side="Right" relative="true" end="Pause">
            <Key type="Curve" time="0" value="0" inOut="Linear"/>
            <Key type="Curve" time="8" value="4112" inOut="Linear"/>
        </Controller>
    </Animation>
    
    <Animation name="StreakRightToLeftTemplate">
        <Event event="OnShown" action="Stop"/>
        <Event event="PlayStreak" action="Reset,Play"/>
        
        <Controller type="Anchor" side="Left" relative="true" end="Pause">
            <Key type="Curve" time="0" value="0" inOut="Linear"/>
            <Key type="Curve" time="8" value="-4112" inOut="Linear"/>
        </Controller>
    </Animation>
    
    <Animation name="StreakTopToBottomTemplate">
        <Event event="OnShown" action="Stop"/>
        <Event event="PlayStreak" action="Reset,Play"/>
        
        <Controller type="Anchor" side="Bottom" relative="true" end="Pause">
            <Key type="Curve" time="0" value="0" inOut="Linear"/>
            <Key type="Curve" time="8" value="4112" inOut="Linear"/>
        </Controller>
    </Animation>
    
    <Animation name="StreakBottomToTopTemplate">
        <Event event="OnShown" action="Stop"/>
        <Event event="PlayStreak" action="Reset,Play"/>
        
        <Controller type="Anchor" side="Top" relative="true" end="Pause">
            <Key type="Curve" time="0" value="0" inOut="Linear"/>
            <Key type="Curve" time="8" value="-4112" inOut="Linear"/>
        </Controller>
    </Animation>
    
    <Frame type="Image" name="BackgroundStreakHorizontalTemplate">
        <Width val="180"/>
        <Height val="2"/>
        <RotationPosition val="MiddleCenter"/>
        <Texture val="@UI/SC2_UI_Collection_Announcers_Background_Streak"/>
        <Color val="6,82,117"/>
        <Alpha val="64"/>
        <BlendMode val="Add"/>
    </Frame>
    
    <Frame type="Image" name="BackgroundStreakVerticalTemplate" template="CollectionPreviewPane/BackgroundStreakHorizontalTemplate">
        <Rotation val="90"/>
    </Frame>
    
    <!--
    ==========================
    Collection Preview Pane Template
    ==========================
    -->
        
    <Frame type="Frame" name="CollectionPreviewPaneTemplate">
            
        <!-- Outer background -->
        
        <Frame type="BlurFrame" name="BackgroundBlur">
            <Anchor relative="$parent/BackgroundFill"/>
            <BlurAmount val="0.35"/>
            <Alpha val="255"/>
        </Frame>
        
        <Frame type="Image" name="BackgroundFill">
            <Anchor side="Top" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-4"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-4"/>
            <Texture val="Assets/Textures/white.dds"/>
            <Color val="9,12,21"/>
            <Alpha val="225"/>
        </Frame>
    
        <Frame type="Image" name="BorderImage">
            <Anchor relative="$parent"/>
            <RenderPriority val="530"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Outer" layer="0"/>
            <TextureType val="NineSlice" layer="0"/>
            <LayerColor val="0,0,0" layer="0"/>
            
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Inner" layer="1"/>
            <TextureType val="NineSlice" layer="1"/>
            <LayerColor val="29,46,69" layer="1"/>
            
            <StateGroup name="AspectRatioState">
                <DefaultState val="Not16x9"/>
                
                <State name="16x9">
                    <When type="StateGroup" frame="$GlueUI" AspectRatioState="16x9"/>
                    <Action type="SetProperty" Texture="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Outer_16x9" index="0"/>
                    <Action type="SetProperty" Texture="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Inner_16x9" index="1"/>
                </State>
                
                <State name="Not16x9">
                    <Action type="SetProperty" Texture="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Outer" index="0"/>
                    <Action type="SetProperty" Texture="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Inner" index="1"/>
                </State>
            </StateGroup>
        </Frame>
            
        <!-- Inner container, houses the visible content space -->
        
        <Frame type="Frame" name="DisplayContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            
            <!--
            ============
            Preview Container
            ============
            
            The content being previewed should be placed in this frame
            -->
            
            <Frame type="Frame" name="PreviewContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            </Frame>
        </Frame>
    </Frame>
    
    <!--
    ================================
    Collection Preview Pane Header Template
    ================================
    -->
        
    <Frame type="Frame" name="CollectionPreviewPaneHeaderTemplate" template="CollectionPreviewPane/CollectionPreviewPaneTemplate">
            
        <!-- Outer background -->
        
        <Frame type="Image" name="HeaderFill">
            <Anchor side="Top" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Bottom" relative="$parent/DividerImage" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-4"/>
            <Texture val="Assets/Textures/white.dds"/>
            <Color val="46,112,168"/>
            <Alpha val="51"/>
        </Frame>
        
        <Frame type="Image" name="DividerImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="39"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-4"/>
            <Height val="4"/>
            <RenderPriority val="530"/>
            <LayerCount val="2"/>
                
            <Texture val="Assets/Textures/white.dds" layer="0"/>
            <LayerColor val="0,0,0" layer="0"/>
            
            <Texture val="Assets/Textures/white.dds" layer="1"/>
            <LayerColor val="0,0,0" layer="1"/>
            <LayerVisible val="false" layer="1"/>
                
            <StateGroup name="AspectRatioState">
                <DefaultState val="Not16x9"/>
                
                <State name="16x9">
                    <When type="StateGroup" frame="$GlueUI" AspectRatioState="16x9"/>
                    <Action type="SetProperty" LayerVisible="true" index="1"/>
                    <Action type="SetProperty" Height="5"/>
                </State>
                
                <State name="Not16x9">
                    <Action type="SetProperty" LayerVisible="false" index="1"/>
                    <Action type="SetProperty" Height="4"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Image" name="DividerInnerImage">
            <Anchor side="Top" relative="$parent/DividerImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/DividerImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="3"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-3"/>
            <Height val="2"/>
            <RenderPriority val="530"/>
            <LayerCount val="2"/>
            
            <Texture val="Assets/Textures/white.dds" layer="0"/>
            <LayerColor val="29,46,69" layer="0"/>
            
            <Texture val="Assets/Textures/white.dds" layer="1"/>
            <LayerColor val="29,46,69" layer="1"/>
            <LayerVisible val="false" layer="1"/>
                
            <StateGroup name="AspectRatioState">
                <DefaultState val="Not16x9"/>
                
                <State name="16x9">
                    <When type="StateGroup" frame="$GlueUI" AspectRatioState="16x9"/>
                    <Action type="SetProperty" LayerVisible="true" index="1"/>
                </State>
                
                <State name="Not16x9">
                    <Action type="SetProperty" LayerVisible="false" index="1"/>
                </State>
            </StateGroup>
        </Frame>
            
        <!-- Inner container, houses the visible content space -->
        
        <Frame type="Frame" name="HeaderContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
            <Anchor side="Bottom" relative="$parent/DividerImage" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
        </Frame>
        
        <Frame type="Frame" name="DisplayContainer">
            <Anchor side="Top" relative="$parent/DividerImage" pos="Max" offset="0"/>
        </Frame>
    </Frame>
    
    <!--
    ==============================
    Collection Preview Pane Animated Template
    ==============================
    -->
        
    <Frame type="Frame" name="CollectionPreviewPaneAnimatedTemplate" template="CollectionPreviewPane/CollectionPreviewPaneTemplate">
            
        <!-- Outer background -->
    
        <Frame type="Frame" name="DisplayContainer">
            
            <!-- Inner container, houses the visible content space -->
            
            <Frame type="Frame" name="BackgroundContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
                <RenderPriority val="508"/>
            
                <Frame type="Image" name="BackgroundFill">
                    <Anchor relative="$parent"/>
                    <Texture val="Assets/Textures/white.dds"/>
                    <Color val="0,4,10"/>
                </Frame>
                
                <!-- Background glows-->
                
                <Frame type="Image" name="BackgroundBottomGlow">
                    <Anchor side="Top" relative="$parent" pos="-20%" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="340%" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="-80%" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="180%" offset="0"/>
                    <Texture val="@UI/SC2_UI_Collection_VoicePack_CharacterFrame_GenericEffect_RadialGradient_256x256"/>
                    <Alpha val="120"/>
                    <Color val="38,83,148"/>
                    
                    <Animation name="GlowPulse">
                        <Event event="OnShown" action="Reset,Play"/>
                            
                        <Controller type="Property" property="LayerAlpha" curve="True" index="0" end="Loop">
                            <Key type="Property" time="0.000" value="120" inOut="Slow"/>
                            <Key type="Property" time="2.000" value="200" inOut="Slow"/>
                            <Key type="Property" time="4.000" value="120" inOut="Slow"/>
                            <Key type="Property" time="6.000" value="200" inOut="Slow"/>
                            <Key type="Property" time="8.000" value="120" inOut="Slow"/>
                        </Controller>
                    </Animation>
                    
                </Frame>
                
                <Frame type="Image" name="BackgroundTopLeftGlow">
                    <Anchor side="Top" relative="$parent" pos="-185%" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="65%" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="-355%" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="235%" offset="0"/>
                    <Texture val="@UI/SC2_UI_Collection_VoicePack_CharacterFrame_GenericEffect_RadialGradient_256x256"/>
                    <Alpha val="180"/>
                    <Color val="68,83,125"/>
                </Frame>
                
                <!-- Background smoke -->
            
                <Frame type="Image" name="BackgroundSmoke">
                    <Anchor relative="$parent"/>
                    <LayerCount val="3"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_Masks_01" layer="0"/>
                    <AlphaChannel val="Red" layer="0"/>
                    <AlphaLayer val="true" layer="0"/>
                    <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
                    <WrapUV val="true" layer="0"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_Masks_01" layer="1"/>
                    <AlphaChannel val="InverseRed" layer="1"/>
                    <AlphaLayer val="true" layer="1"/>
                    <TextureCoords top="0" left="0" bottom="0.7" right="0.5" layer="1"/>
                    <WrapUV val="true" layer="1"/>
                    
                    <Texture val="Assets/Textures/white.dds" layer="2"/>
                    <LayerColor val="24,66,96" layer="2"/>
                    <LayerAlpha val="102" layer="2"/>
                    
                    <Animation name="SmokeBillow">
                        <Event event="OnShown" action="Play"/>
                        
                        <Controller type="LayerUV" layer="0" side="Top" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" inOut="Linear"/>
                            <Key type="Curve" time="15" value="1" inOut="Linear"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="0" side="Bottom" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" inOut="Linear"/>
                            <Key type="Curve" time="15" value="1" inOut="Linear"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="1" side="Top" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" inOut="Linear"/>
                            <Key type="Curve" time="20" value="-1" inOut="Linear"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="1" side="Bottom" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" inOut="Linear"/>
                            <Key type="Curve" time="20" value="-1" inOut="Linear"/>
                        </Controller>
                    </Animation>
                </Frame>
                
                <!-- Static background grid -->
            
                <Frame type="Image" name="BackgroundGridSmall">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Width val="720"/>
                    <Height val="720"/>
                    <LayerCount val="2"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_GridPattern_Mask" layer="0"/>
                    <AlphaChannel val="Alpha" layer="0"/>
                    <AlphaLayer val="true" layer="0"/>
                    <Tiled val="true" layer="0"/>
                    
                    <Texture val="Assets/Textures/white.dds" layer="1"/>
                    <LayerAlpha val="20" layer="1"/>
                    <LayerColor val="62,146,181" layer="1"/>
                </Frame>
            
                <Frame type="Image" name="BackgroundGridLargeGlow">
                    <Anchor relative="$parent/BackgroundGridSmall"/>
                    <LayerCount val="2"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_GridPattern_Mask" layer="0"/>
                    <AlphaChannel val="Red" layer="0"/>
                    <AlphaLayer val="true" layer="0"/>
                    <Tiled val="true" layer="0"/>
                    
                    <Texture val="Assets/Textures/white.dds" layer="1"/>
                    <LayerAlpha val="10" layer="1"/>
                    <LayerColor val="255,255,255" layer="1"/>
                </Frame>
                
                <!-- The wave that moves across the large grid -->
            
                <Frame type="Image" name="BackgroundGridLargeWave">
                    <Anchor relative="$parent/BackgroundGridSmall"/>
                    <LayerCount val="3"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_Masks_01" layer="0"/>
                    <AlphaLayer val="true" layer="0"/>
                    <AlphaChannel val="Blue" layer="0"/>
                    <TextureCoords top="-.75" left="1" bottom="0" right="1.5" layer="0"/>
                    <WrapUV val="false" layer="0"/>
                    <LayerAlpha val="128" layer="0"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_GridPattern_Mask" layer="1"/>
                    <AlphaChannel val="Green" layer="1"/>
                    <AlphaLayer val="true" layer="1"/>
                    <Tiled val="true" layer="1"/>
                    
                    <Texture val="Assets/Textures/white.dds" layer="2"/>
                    <LayerAlpha val="64" layer="2"/>
                    <LayerColor val="52,135,209" layer="2"/>
                    <BlendMode val="Add"/>
                    
                    <Animation name="LargeGridWave">
                        <Event event="OnShown" action="Play"/>
                        
                        <Controller type="LayerUV" layer="0" side="Top" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" inOut="Linear"/>
                            <Key type="Curve" time="5.333" value="1.75" inOut="Linear"/>
                            <Key type="Curve" time="9.167" value="1.75" inOut="Linear"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="0" side="Bottom" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" inOut="Linear"/>
                            <Key type="Curve" time="5.333" value="1.75" inOut="Linear"/>
                            <Key type="Curve" time="9.167" value="1.75" inOut="Linear"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="0" side="Left" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" inOut="Linear"/>
                            <Key type="Curve" time="5.333" value="-1.5" inOut="Linear"/>
                            <Key type="Curve" time="9.167" value="-1.5" inOut="Linear"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="0" side="Right" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" inOut="Linear"/>
                            <Key type="Curve" time="5.333" value="-1.5" inOut="Linear"/>
                            <Key type="Curve" time="9.167" value="-1.5" inOut="Linear"/>
                        </Controller>
                    </Animation>
                </Frame>
                
                <!-- Background dots -->
            
                <Frame type="Image" name="BackgroundGridDots">
                    <Anchor relative="$parent/BackgroundGridSmall"/>
                    <LayerCount val="2"/>
                    <BlendMode val="Add"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_GridPattern_Mask" layer="0"/>
                    <AlphaChannel val="Blue" layer="0"/>
                    <AlphaLayer val="true" layer="0"/>
                    <Tiled val="true" layer="0"/>
                    
                    <Texture val="Assets/Textures/white.dds" layer="1"/>
                    <LayerAlpha val="102" layer="1"/>
                    <LayerColor val="7,15,33" layer="1"/>
                </Frame>
            
                <Frame type="Image" name="BackgroundGridDotsTwinkle">
                    <Anchor relative="$parent/BackgroundGridDots"/>
                    <BlendMode val="Add"/>
                    <LayerCount val="3"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_Masks_01" layer="0"/>
                    <AlphaLayer val="true" layer="0"/>
                    <AlphaChannel val="Green" layer="0"/>
                    <TextureCoords top="0" left="0" bottom="2" right="1" layer="0"/>
                    <WrapUV val="true" layer="0"/>
                    
                    <Texture val="@UI/SC2_UI_Collection_Announcers_Background_GridPattern_Mask" layer="1"/>
                    <AlphaChannel val="Blue" layer="1"/>
                    <AlphaLayer val="true" layer="1"/>
                    <Tiled val="true" layer="1"/>
                    
                    <Texture val="Assets/Textures/white.dds" layer="2"/>
                    <LayerAlpha val="63" layer="2"/>
                    <LayerColor val="18,39,85" layer="2"/>
                    
                    <Animation name="TwinkleDots">
                        <Event event="OnShown" action="Play"/>
                        
                        <Controller type="LayerUV" layer="0" side="Top" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" out="fast"/>
                            <Key type="Curve" time="8" value="-2" in="fast"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="0" side="Bottom" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" out="fast"/>
                            <Key type="Curve" time="8" value="-2" in="fast"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="0" side="Left" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" out="fast"/>
                            <Key type="Curve" time="8" value="-1" in="fast"/>
                        </Controller>
                        
                        <Controller type="LayerUV" layer="0" side="Right" relative="true" sidelock="false" end="Loop" >
                            <Key type="Curve" time="0" value="0" out="fast"/>
                            <Key type="Curve" time="8" value="-1" in="fast"/>
                        </Controller>
                    </Animation>
                </Frame>
                
                <!--
                ================
                Background Streaks
                ================
                
                These are the streaks in the background. They streak when given the PlayStreak event
                -->
                
                <!-- Horizontal streaks -->
                
                <Frame type="Image" name="BackgroundStreakHorizontal1" template="CollectionPreviewPane/BackgroundStreakHorizontalTemplate">
                    <Anchor side="Top" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="-240"/>
                    <Anchor side="Bottom" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="-240"/>
                    <Anchor side="Right" relative="$ancestor[@name=DisplayContainer]" pos="Min" offset="0"/>
                    
                    <Animation name="PlayStreak" template="CollectionPreviewPane/StreakLeftToRightTemplate"/>
                </Frame>
                
                <Frame type="Image" name="BackgroundStreakHorizontal2" template="CollectionPreviewPane/BackgroundStreakHorizontalTemplate">
                    <Anchor side="Top" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="-180"/>
                    <Anchor side="Bottom" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="-180"/>
                    <Anchor side="Left" relative="$ancestor[@name=DisplayContainer]" pos="Max" offset="0"/>
                    
                    <Animation name="PlayStreak" template="CollectionPreviewPane/StreakRightToLeftTemplate"/>

                    <Animation name="StreakInitial">
                        <Event event="PlayStreak" action="Stop"/>
                        <Event event="OnShown" action="Reset,Play"/>
                        
                        <Controller type="Anchor" side="Left" relative="true" end="Pause">
                            <Key type="Curve" time="0" value="-292" inOut="Linear"/>
                            <Key type="Curve" time="7.432" value="-4112" inOut="Linear"/>
                        </Controller>
                    </Animation>
                </Frame>
                
                <Frame type="Image" name="BackgroundStreakHorizontal3" template="CollectionPreviewPane/BackgroundStreakHorizontalTemplate">
                    <Anchor side="Top" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="-120"/>
                    <Anchor side="Bottom" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="-120"/>
                    <Anchor side="Left" relative="$ancestor[@name=DisplayContainer]" pos="Max" offset="0"/>
                    
                    <Animation name="PlayStreak" template="CollectionPreviewPane/StreakRightToLeftTemplate"/>
                </Frame>
                
                <Frame type="Image" name="BackgroundStreakHorizontal4" template="CollectionPreviewPane/BackgroundStreakHorizontalTemplate">
                    <Anchor side="Top" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="60"/>
                    <Anchor side="Bottom" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="60"/>
                    <Anchor side="Right" relative="$ancestor[@name=DisplayContainer]" pos="Min" offset="0"/>
                    
                    <Animation name="PlayStreak" template="CollectionPreviewPane/StreakLeftToRightTemplate" speed="1.2"/>
                </Frame>
                
                <Frame type="Image" name="BackgroundStreakHorizontal5" template="CollectionPreviewPane/BackgroundStreakHorizontalTemplate">
                    <Anchor side="Top" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="120"/>
                    <Anchor side="Bottom" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="120"/>
                    <Anchor side="Left" relative="$ancestor[@name=DisplayContainer]" pos="Max" offset="0"/>
                    
                    <Animation name="PlayStreak" template="CollectionPreviewPane/StreakRightToLeftTemplate"/>
                </Frame>
                
                <Frame type="Image" name="BackgroundStreakHorizontal6" template="CollectionPreviewPane/BackgroundStreakHorizontalTemplate">
                    <Anchor side="Top" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="240"/>
                    <Anchor side="Bottom" relative="$ancestor[@name=DisplayContainer]" pos="Mid" offset="240"/>
                    <Anchor side="Right" relative="$ancestor[@name=DisplayContainer]" pos="Min" offset="0"/>
                    
                    <Animation name="PlayStreak" template="CollectionPreviewPane/StreakLeftToRightTemplate" speed="1.5"/>
                </Frame>
                
                <!-- Vertical streaks -->
                
                <Frame type="Image" name="BackgroundStreakVertical1" template="CollectionPreviewPane/BackgroundStreakVerticalTemplate">
                    <Anchor side="Top" relative="$ancestor[@name=DisplayContainer]" pos="Max" offset="89"/>
