        <Anchor relative="$parent"/>
        <Handle val="ScreenNewUser"/>
        <DisplayBackgroundModel val="False"/>
        <DisplayForegroundUI val="false"/>
        <!-- This must render above normal content screens so it is always accessible -->
        <RenderPriority val="600"/>
        <SelectedTabIndex val="{$this/TabControl/@SelectedIndex}"/>

        <StateGroup name="BackgroundStateGroup">
            <State name="BackVisible">
                <When type="Property" frame="BackFrame" Visible="true"/>
                <Action type="SetProperty" frame="BackgroundCutsceneContainer" Visible="false"/>
                <Action type="SetProperty" frame="BackgroundContainer" Visible="false"/>
            </State>

            <State name="NotVisible">
                <When type="Property" frame="BackFrame" Visible="false"/>
                <Action type="SetProperty" frame="BackgroundCutsceneContainer" Visible="true"/>
                <Action type="SetProperty" frame="BackgroundContainer" Visible="true"/>
