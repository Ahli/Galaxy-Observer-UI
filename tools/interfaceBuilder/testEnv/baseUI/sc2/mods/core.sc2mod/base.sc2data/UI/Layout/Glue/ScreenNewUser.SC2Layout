    <!-- Background Hex Shimmer Template -->
    <Frame type="Image" name="BackgroundHexShimmerTemplate">
        <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent/AnchorFrame" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent/AnchorFrame" pos="Mid" offset="0"/>
        <Width val="4246"/>
        <LayerCount val="2"/>
        
        <Texture val="@UI/SC2_UI_NewPlayerXP_Hexagon_ShimmerMask" layer="0"/>
        <AlphaLayer val="True" layer="0"/>
        <TextureCoords top="0" bottom="1" left="1" right="4.452" layer="0"/>
        
        <Texture val="@UI/SC2_UI_NewPlayerXP_Hexagon" layer="1"/>
        <Tiled val="True" layer="1"/>
        <LayerColor val="35,123,255" layer="1"/>
        <LayerAlpha val="61" layer="1"/>
        
        <Animation name="Shimmer">
            <Event event="OnShown" action="Reset,Play"/>
     
            <Controller type="LayerUV" layer="0" side="Left" relative="True" sidelock="False" end="Loop">
                <Key type="Curve" time="0" value="0" out="fast"/>
                <Key type="Curve" time="5" value="-4.452" in="fast"/>
                <Key type="Curve" time="10" value="-4.452" in="fast"/>
            </Controller>
     
            <Controller type="LayerUV" layer="0" side="Right" relative="True" sidelock="False" end="Loop">
                <Key type="Curve" time="0" value="0" out="fast"/>
                <Key type="Curve" time="5" value="-4.452" in="fast"/>
                <Key type="Curve" time="10" value="-4.452" in="fast"/>
            </Controller>
        </Animation>
    </Frame>
    
