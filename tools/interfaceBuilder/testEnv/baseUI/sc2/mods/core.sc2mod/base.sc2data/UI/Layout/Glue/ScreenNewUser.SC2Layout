<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <!--
    =================================
    Animation Templates
    =================================
    -->
    
    <!-- Receives the event to display the reminder dialog -->
    <Animation name="ShowHideReminderAnimTemplate">
        <Event event="OnShown" action="Reset"/>
        <Event event="ActivateReminderDialogRequest" action="Play"/>
        
        <Controller type="Visibility" frame="ReminderDialog" end="Pause">
            <Key type="Bool" time="0" value="False"/>
            <Key type="Bool" time="0" value="True"/>
        </Controller>
    </Animation>
    
    <!-- Go to Campaign on click -->
    <Animation name="GoCampaignAnimTemplate">
        <Event event="OnClick" frame="$this" action="Reset,Play"/>

        <Controller type="Event" end="Pause" frame="$ScreenNewUser">
            <Key type="Event" time="0.0" event="ActivateCampaignScreenRequest"/>
        </Controller>
    </Animation>
    
    <!-- Go to Coop on click -->
    <Animation name="GoCoopAnimTemplate">
        <Event event="OnClick" frame="$this" action="Reset,Play"/>

        <Controller type="Event" end="Pause" frame="$ScreenNewUser">
            <Key type="Event" time="0.0" event="ActivateCoopScreenRequest"/>
        </Controller>
    </Animation>
    
    <!-- Go to Homer screen on click -->
    <Animation name="GoHomeAnimTemplate">
        <Event event="OnClick" frame="$this" action="Reset,Play"/>

        <Controller type="Event" end="Pause" frame="$ScreenNewUser">
            <Key type="Event" time="0.0" event="ActivateHomeScreenRequest"/>
        </Controller>
    </Animation>
    
    <!-- Go to New User on click -->
    <Animation name="GoNewUserAnimTemplate">
        <Event event="OnClick" frame="$this" action="Reset,Play"/>

        <Controller type="Event" end="Pause" frame="$ScreenNewUser">
            <Key type="Event" time="0.0" event="ActivateNewUserScreenRequest"/>
        </Controller>
    </Animation>
    
    <!-- Go to Tutorial on click -->
    <Animation name="GoTutorialAnimTemplate">
        <Event event="OnClick" frame="$this" action="Reset,Play"/>

        <Controller type="Event" end="Pause" frame="$ScreenNewUser">
            <Key type="Event" time="0.0" event="LaunchTutorialRequest"/>
        </Controller>
    </Animation>
    
    <!-- Go to Versus on click -->
    <Animation name="GoVersusAnimTemplate">
        <Event event="OnClick" frame="$this" action="Reset,Play"/>

        <Controller type="Event" end="Pause" frame="$ScreenNewUser">
            <Key type="Event" time="0.0" event="ActivateMultiplayerScreenRequest"/>
        </Controller>
    </Animation>
    
    <!-- Show reminder on click -->
    <Animation name="DisplayReminderAnimTemplate">
        <Event event="OnClick" frame="$this" action="Reset,Play"/>

        <Controller type="Event" end="Pause" frame="$ScreenNewUser">
            <Key type="Event" time="0.0" event="ActivateReminderDialogRequest"/>
        </Controller>
    </Animation>

    <!--
    =================================
    BACKGROUND CUTSCENE CONTAINER TEMPLATE
    =================================
    
    Fills the background with a cutscene
    -->
    
    <Frame type="Frame" name="BackgroundCutsceneContainerTemplate">
        <Anchor relative="$parent"/>
        <RenderType val="HDR"/>

        <Frame type="CutsceneFrame" name="BackgroundCutsceneFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <RenderType val="HDR"/>
            <ToneMapping val="True"/>
            <File val="Cutscenes/UI_Screens_CampaignMode_Background.SC2Cutscene"/>
            <AutoPlay val="true"/>
            <Fading val="true"/>
        </Frame>
    </Frame>
    
