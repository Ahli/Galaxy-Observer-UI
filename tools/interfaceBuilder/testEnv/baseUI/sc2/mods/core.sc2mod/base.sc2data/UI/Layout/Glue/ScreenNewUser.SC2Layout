            <!-- Clicking button closes goes to new user screen, deactivating other (coop/versus/campaign) and transitions to the progress screen -->
            <Animation name="ClickAnim" template="ScreenNewUser/GoNewUserAnimTemplate"/>
        </Frame>
    </Frame>

