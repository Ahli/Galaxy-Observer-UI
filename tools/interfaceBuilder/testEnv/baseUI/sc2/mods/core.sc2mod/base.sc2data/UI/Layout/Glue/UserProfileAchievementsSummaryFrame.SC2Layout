    <Frame type="AchievementInfoFrame" name="ShowcaseSelectAchievementFrameTemplate">
        <Width val="595"/>
        <Height val="85"/>
        
        <Frame type="Frame" name="Background" template="UserProfileAchievementsSummaryFrame/MiniAchievementBackgroundTemplate"/>

        <Frame type="Image" name="Icon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="13"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="19"/>
            <Width val="47"/>
            <Height val="47"/>
        </Frame>

        <Frame type="Image" name="IconBorder">
            <Anchor side="Top" relative="$parent/Icon" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Icon" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/Icon" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/Icon" pos="Mid" offset="0"/>
            <Width val="59"/>
            <Height val="59"/>
            <TextureType val="NineSlice"/>
            <Texture val="@UI/SC2_UI_Collection_AchievementIconFrame_Normal"/>
        </Frame>

        <Frame type="Label" name="Title">
            <Anchor side="Top" relative="$parent" pos="Min" offset="23"/>
            <Anchor side="Left" relative="$parent/IconBorder" pos="Max" offset="9"/>
            <Anchor side="Right" relative="$parent/Checkbox" pos="Max" offset="-10"/>
            <Style val="Profile_Achievement_MiniTitle_Showcase_Unselected"/>
            <Text val="{$parent/@TitleText}"/>
        </Frame>
        
        <Frame type="Image" name="Checkbox">
            <Anchor side="Top" relative="$parent/Title" pos="Mid" offset="3"/>
            <Anchor side="Bottom" relative="$parent/Title" pos="Mid" offset="3"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-21"/>
            <Width val="34"/>
            <Height val="35"/>
            <Texture val="@UI/SC2_UI_UserProfile_Achievements_Showcase_Checkbox"/>
            <TextureCoords top="0" bottom=".5" left="0" right="1"/>
        </Frame>
        
        <Frame type="Image" name="Checkmark">
            <Anchor side="Top" relative="$parent/Checkbox" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Checkbox" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/Checkbox" pos="Mid" offset="1"/>
            <Anchor side="Right" relative="$parent/Checkbox" pos="Mid" offset="1"/>
            <Width val="28"/>
            <Height val="28"/>
            <Texture val="@UI/SC2_UI_UserProfile_Achievements_Showcase_Checkmark"/>
        </Frame>
        
        <Frame type="Frame" name="Frame" template="UserProfileAchievementsSummaryFrame/MiniAchievementFrameTemplate"/>
        
        <StateGroup name="SelectionState">
            <DefaultState val="NotSelected"/>
            
            <State name="Selected">
                <When type="Property" frame="$parent" Selected="True"/>
                <Action type="SetProperty" frame="Title" Style="Profile_Achievement_MiniTitle_Showcase"/>
                <Action type="SetProperty" frame="Frame" Color="255,255,255"/>
                <Action type="SetProperty" frame="Checkmark" Visible="True"/>
                <Action type="SetState" frame="Background" group="GlowState" state="On"/>
            </State>
            
            <State name="Hovered">
                <When type="Property" frame="$parent" Hovered="True"/>
                <Action type="SetProperty" frame="Title" Style="Profile_Achievement_MiniTitle_Showcase"/>
                <Action type="SetProperty" frame="Frame" Color="255,255,255"/>
                <Action type="SetProperty" frame="Checkmark" Visible="False"/>
                <Action type="SetState" frame="Background" group="GlowState" state="Off"/>
            </State>
            
            <State name="NotSelected">
                <Action type="SetProperty" frame="Title" Style="Profile_Achievement_MiniTitle_Showcase_Unselected"/>
                <Action type="SetProperty" frame="Frame" Color="100,100,170"/>
                <Action type="SetProperty" frame="Checkmark" Visible="False"/>
                <Action type="SetState" frame="Background" group="GlowState" state="Off"/>
            </State>
        </StateGroup>
    </Frame>

