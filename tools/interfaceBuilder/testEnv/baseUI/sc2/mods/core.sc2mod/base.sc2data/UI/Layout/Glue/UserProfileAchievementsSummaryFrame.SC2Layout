                    <When type="StateGroup" frame="$parent" AchievementState="Chosen"/>
                    <Action type="SetProperty" texture="@UI/SC2_UI_UserProfile_Achievements_Showcase_Frame_Hover"/>
                    <Action type="SetProperty" frame="HoverAdd" visible="False"/>
