                <State name="NotEmptyNotHovered">
                    <When type="StateGroup" frame="$parent" AchievementState="Chosen"/>
                    <Action type="SetProperty" texture="@UI/SC2_UI_UserProfile_Achievements_Showcase_Frame_Normal"/>
                    <Action type="SetProperty" frame="HoverAdd" visible="False"/>
                </State>
                
                <State name="EmptyHoveredMine">
                    <When type="Property" frame="$parent/SetAchievementButton" Hovering="True"/>
                    <When type="StateGroup" frame="$parent" UserState="LocalUser"/>
                    <Action type="SetProperty" texture="@UI/SC2_UI_UserProfile_Achievements_Showcase_Frame_Empty"/>
