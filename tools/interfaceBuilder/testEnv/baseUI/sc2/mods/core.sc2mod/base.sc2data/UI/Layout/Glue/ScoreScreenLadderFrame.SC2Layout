                <Anchor side="Left" relative="$parent/EnemyMMRTitleLabel" pos="Min" offset="2"/>
                <Anchor side="Right" relative="$parent/$parent/InnerBorderBottomRight" pos="Max" offset="-20"/>
                <Anchor side="Bottom" relative="$parent/EnemyTeamNameLabel" pos="Min" offset="-6"/>
                <Style val="@ScoreScreenLadderPlacementMMR"/>
                <Text val="@UI/ScoreScreenLadder/EnemyMMRLabelLeaguePlacement"/>
