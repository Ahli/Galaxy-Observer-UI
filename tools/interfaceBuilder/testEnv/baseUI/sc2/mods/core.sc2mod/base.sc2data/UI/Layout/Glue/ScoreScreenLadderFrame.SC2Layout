        <Frame type="Frame" name="MMRFrame">
            <Anchor side="Top" relative="$parent/InnerBorderBottomLeft" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            
            <Frame type="Frame" name="YourMMRResultIcon" template="ScoreScreenLadderFrame/MMRResultIcon">
                <Anchor side="Top" relative="$parent" pos="Min" offset="3"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            </Frame>
            
            <Frame type="Label" name="YourMMRTitleLabel">
                <Anchor side="Bottom" relative="$parent/YourMMRResultIcon" pos="Mid" offset="-6"/>
                <Anchor side="Left" relative="$parent/YourMMRResultIcon" pos="Max" offset="5"/>
                <Style val="@ScoreScreenLadderYourMMRTitle"/>
                <Text val="@UI/ScoreScreenLadder/YourMMRTitle"/>
            </Frame>

            <Frame type="Label" name="YourMMRLabelPlacementMatch">
                <Anchor side="Top" relative="$parent/YourMMRTitleLabel" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/YourMMRTitleLabel" pos="Min" offset="2"/>
                <Anchor side="Right" relative="$parent/$parent/InnerBorderBottomLeft" pos="Max" offset="-20"/>
                <Anchor side="Bottom" relative="$parent/EnemyMMRLabelPlacementMatch" pos="Max" offset="0"/>
                <Style val="@ScoreScreenLadderPlacementMMR"/>
                <Text val="@UI/ScoreScreenLadder/MMRLabelLeaguePlacement"/>
                <Visible val="False"/>
            </Frame>

            <Frame type="CountdownLabel" name="YourMMRLabel">
                <Anchor side="Top" relative="$parent/YourMMRTitleLabel" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/YourMMRTitleLabel" pos="Max" offset="10"/>
                <FormatNumber val="false"/>
                <CountdownTime val="5000"/>
                <Style val="@ScoreScreenLadderYourMMR"/>
                <CountdownSound val="AC_UI_XPBarFillLoop"/>

                <Animation name="CountUp">
                    <Event event="Begin" action="Restart,Play" frame="$parent/$parent"/>
                    <Event event="Reset" action="Restart,Pause" frame="$parent/$parent"/>
                    
                    <Controller type="Counter" end="Pause">
                        <Key type="Curve" time="0.0" value="0.0" out="Slow"/>
                        <Key type="Curve" time="2.0" value="1.0" in="Slow"/>
                        <Key type="Curve" time="2.1" value="1.0" in="Slow"/>
                    </Controller>

                    <Controller type="Property" frame="$parent/$parent" property="AnimationFinished" end="Pause">
                        <Key type="Property" time="0" value="False"/>
                        <Key type="Property" time="2.5" value="True"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Label" name="YourMMRGainedLabel">
                <Anchor side="Top" relative="$parent/YourMMRTitleLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/YourMMRTitleLabel" pos="Min" offset="2"/>
