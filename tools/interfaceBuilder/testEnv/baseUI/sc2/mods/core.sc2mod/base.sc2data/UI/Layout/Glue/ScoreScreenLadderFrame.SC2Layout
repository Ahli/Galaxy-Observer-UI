            <Height val="640"/>
        </Frame>
        
        <Frame type="Image" name="InnerBorderTop">
            <Anchor side="Top" relative="$parent/OuterBorder" pos="Min" offset="8"/>
            <Anchor side="Left" relative="$parent/OuterBorder" pos="Min" offset="8"/>
            <Anchor side="Right" relative="$parent/OuterBorder" pos="Max" offset="-8"/>
            <Texture val="@UI/Battlenet_Glue_Ladder_Score_InnerContainer"/>
            <TextureType val="Nineslice"/>
            <Height val="505"/>
        </Frame>
        
        <Frame type="Image" name="InnerBorderBottomLeft">
            <Anchor side="Bottom" relative="$parent/OuterBorder" pos="Max" offset="-8"/>
            <Anchor side="Left" relative="$parent/OuterBorder" pos="Min" offset="8"/>
            <Anchor side="Right" relative="$parent/OuterBorder" pos="Mid" offset="4"/>
            <Texture val="@UI/Battlenet_Glue_Ladder_Score_InnerContainer"/>
            <TextureType val="Nineslice"/>
            <Height val="152"/>
            <TextureCoords top="1" left="1" bottom="0" right="0"/>
        </Frame>
        
        <Frame type="Image" name="InnerBorderBottomRight">
            <Anchor side="Bottom" relative="$parent/OuterBorder" pos="Max" offset="-8"/>
            <Anchor side="Left" relative="$parent/OuterBorder" pos="Mid" offset="-1"/>
            <Anchor side="Right" relative="$parent/OuterBorder" pos="Max" offset="-8"/>
            <Texture val="@UI/Battlenet_Glue_Ladder_Score_InnerContainer"/>
