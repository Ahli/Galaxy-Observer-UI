                <Anchor side="Left" relative="$parent/EnemyMMRTitleLabel" pos="Min" offset="2"/>
                <Anchor side="Right" relative="$parent/$parent/InnerBorderBottomRight" pos="Max" offset="-20"/>
                <Anchor side="Bottom" relative="$parent/$parent/InnerBorderBottomRight" pos="Max" offset="-10"/>
                <Style val="@ScoreScreenLadderTeamName"/>
                <Text val="[PH Player 1, Player 2, Player 3]"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="DivisionFrame">
            <Anchor side="Top" relative="$parent/InnerBorderTop" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/InnerBorderTop" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/InnerBorderTop" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/InnerBorderTop" pos="Max" offset="0"/>
            
            <Frame type="Image" name="LeagueTitleBg">
