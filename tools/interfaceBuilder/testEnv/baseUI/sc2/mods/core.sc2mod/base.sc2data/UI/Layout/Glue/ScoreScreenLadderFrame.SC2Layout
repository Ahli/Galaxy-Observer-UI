                <Anchor side="Left" relative="$parent/$parent/InnerBorderBottomRight" pos="Min" offset="10"/>
            </Frame>

            <Frame type="Label" name="EnemyMMRTitleLabel">
                <Anchor side="Bottom" relative="$parent/EnemyMMRResultIcon" pos="Mid" offset="-6"/>
                <Anchor side="Left" relative="$parent/EnemyMMRResultIcon" pos="Max" offset="5"/>
