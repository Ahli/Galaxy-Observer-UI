                <Anchor side="Left" relative="$parent/LeagueTierLabel" pos="Min" offset="2"/>
                <Width val="360"/>
                <Style val="@ScoreScreenLadderTeamName"/>
                <Text val="[PH Player 1, Player 2, Player 3]"/>
                <Visible val="True"/>
                <CollapseLayout val="true"/>
            </Frame>
            
            <Frame type="Label" name="TieMatchLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="45"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="55"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-55"/>
