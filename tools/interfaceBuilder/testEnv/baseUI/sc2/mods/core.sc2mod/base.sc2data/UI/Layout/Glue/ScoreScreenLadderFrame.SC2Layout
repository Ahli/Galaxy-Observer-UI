            </Frame>

            <Frame type="Label" name="LeaguePromotionTitle">
                <Anchor side="Bottom" relative="$parent/MMRProgressBar" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent/MMRProgressBar" pos="Min" offset="20"/>
                <Style val="@LeaguePromotionTitle"/>
                <Text val="@UI/ScoreScreenLadder/LeaguePromotionTitle"/>
            </Frame>
            
            <Frame type="Label" name="PointsEarnedTitle">
                <Anchor side="Top" relative="$parent/MMRProgressBar" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/MMRProgressBar" pos="Min" offset="20"/>
                <Style val="@BonusValueTitle"/>
                <Text val="@UI/ScoreScreenLadder/PointsEarnedTitle"/>
            </Frame>
            
            <Frame type="Label" name="PointsEarnedValue">
                <Anchor side="Top" relative="$parent/PointsEarnedTitle" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/PointsEarnedTitle" pos="Max" offset="5"/>
                <Anchor side="Right" relative="$parent/MMRProgressBar" pos="Max" offset="-20"/>
                <Style val="@BonusValueText"/>
            </Frame>
            
            <Frame type="Frame" name="DemotionWarning">
                <Anchor side="Top" relative="$parent/MMRProgressBar" pos="Min" offset="-55"/>
                <Anchor side="Left" relative="$parent/MMRProgressBar" pos="Max" offset="-102"/>
                <Width val="194"/>
                <Height val="189"/>
                <Visible val="False"/>
                
                <Frame type="Image" name="WarningIcon">
                    <Anchor relative="$parent"/>
                    <Texture val="@UI/Battlenet_Glue_Ladder_Score_WarningIcon"/>
                    
                    <Animation name="Pulse">
                        <Event event="OnShown" action="Reset,Play"/>
                        <Controller type="Fade" end="Loop">
                            <Key type="Curve" time="0" value="255" out="Fast"/>
                            <Key type="Curve" time="1" value="150" in="Slow"/>
                            <Key type="Curve" time="2" value="255" out="Fast"/>
                        </Controller>
                    </Animation>
                </Frame>
                
                <Frame type="Image" name="Shimmer">
                    <Anchor relative="$parent"/>
                    <LayerCount val="2"/>
                    <Alpha val="150"/>
            
                    <Texture val="@UI/Battlenet_Glue_Ladder_Score_WarningIcon_Mask" layer="0"/>
                    <TextureType val="Normal" layer="0"/>
                    <AlphaLayer val="true" layer="0"/>
                    
                    <Texture val="@UI/Battlenet_Glue_Ladder_Score_WarningIcon_Shimmer" layer="1"/>
                    <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
                    <BlendMode val="Add"/>
                    
                    <Animation name="Shimmer">
                        <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                        <Event event="OnShown" action="Play" frame="$parent"/>
                 
                        <Controller type="LayerUV" layer="1" side="Top" relative="False" sidelock="True" end="Loop" >
                            <Key type="Curve" time="0" value="-1" out="fast"/>
                            <Key type="Curve" time="1.0" value="1" in="fast"/>
                            <Key type="Curve" time="4.0" value="1" in="fast"/>
                        </Controller>
                    </Animation>
                </Frame>
            </Frame>
            
            <Frame type="ProgressBar" name="MMRProgressBar">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-70"/>
                <Anchor side="Left" relative="$parent/LeagueIconGlow" pos="Max" offset="40"/>
                <Width val="1032"/>
                <Height val="84"/>
                <UseFillContainer val="true"/>

                <Frame type="Image" name="BorderImage">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="18"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="18"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="-18"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="-18"/>
                    <Texture val="@UI/Battlenet_Glue_Ladder_Score_ProgressBorder"/>
                    <TextureType val="Nineslice"/>
                    <Tiled val="True"/>
                </Frame>

                <Frame type="Frame" name="FillImageContainer">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

                    <Frame type="Image" name="FillImage">
                        <Anchor relative="$parent/$parent"/>
                        <Texture val="@UI/Battlenet_Glue_Ladder_Score_ProgressFill"/>
                        <TextureType val="EndCap"/>
                    </Frame>
                    
                    <Frame type="Image" name="FillImageAccent">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="-1"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-1"/>
                        <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="-24"/>
                        <Texture val="@UI/Battlenet_Glue_Ladder_Score_ProgressAccent"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <Frame type="Image" name="ProgressTooltipIcon">
                <Anchor side="Top" relative="$parent/LeaguePromotionTitle" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/LeaguePromotionTitle" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/LeaguePromotionTitle" pos="Max" offset="10"/>
                <Texture val="@UI/Glues_Icons_GenericInfo"/>
                <AcceptsMouseTooltip val="true"/>
                <Tooltip val="@UI/ScoreScreenLadder/PromotionTooltip"/>
                <Height val="43"/>
                <Width val="43"/>
            </Frame>

            <Frame type="Frame" name="WarningTooltipFrame">
                <Anchor side="Top" relative="$parent/DemotionWarning" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/DemotionWarning" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/DemotionWarning" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/DemotionWarning" pos="Mid" offset="0"/>
                <Width val="100"/>
                <Height val="100"/>
                <Visible val="{$parent/DemotionWarning/@Visible}"/>
                <AcceptsMouseTooltip val="true"/>
                <Tooltip val="@UI/ScoreScreenLadder/DemotionWarningTooltip"/>
            </Frame>

            <Frame type="Label" name="CurrentLeagueRankLabel">
                <Anchor side="Top" relative="$parent/MMRProgressBar" pos="Min" offset="3"/>
                <Anchor side="Bottom" relative="$parent/MMRProgressBar" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/MMRProgressBar" pos="Min" offset="38"/>
                <Anchor side="Right" relative="$parent/MMRProgressBar" pos="Max" offset="0"/>
                <Style val="@CurrentLeagueRankTitle"/>
                <Text val="{$parent/$parent/@CurrentLeagueName}"/>
            </Frame>
            
            <Frame type="Image" name="LeagueIconGlow">
                <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
                <Texture val="@UI/Battlenet_Glue_Ladder_Score_LeagueIconGlow"/>
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Image" name="LeagueIconGlow_Unranked">
                <Anchor side="Top" relative="$parent" pos="Min" offset="9"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
                <Texture val="@UI/Battlenet_Glue_Ladder_Score_LeagueIconGlow_Unranked"/>
                <Visible val="False"/>
            </Frame>
            
            <Frame type="MatchmakingLeagueIcon" name="LeagueIcon" template="MatchmakingLeagueIcon/MatchmakingLeagueIconTemplate">
                <Anchor side="Top" relative="$parent/LeagueIconGlow" pos="Min" offset="33"/>
                <Anchor side="Left" relative="$parent/LeagueIconGlow" pos="Mid" offset="-11"/>
                <Anchor side="Right" relative="$parent/LeagueIconGlow" pos="Mid" offset="-11"/>
            </Frame>

            <Frame type="Button" name="ViewLadderButton">
                <Anchor relative="$parent/LeagueIcon"/>
            </Frame>
        </Frame>

        <StateGroup name="TeamVisibilityState">
            <DefaultState val="SoloGame"/>

            <State name="Tie">
                <When type="AnimationState" ScoreScreenGameResult="ScoreScreenTie"/>
                <Action type="SetProperty" frame="DivisionFrame/TeamNameLabel" visible="False"/>
            </State>

            <State name="NoData">
                <When type="AnimationState" ScoreScreenPlacementMatch="ScoreScreenNoLocalData"/>
                <Action type="SetProperty" frame="DivisionFrame/TeamNameLabel" visible="False"/>
            </State>

            <State name="MatchInProgress">
                <When type="AnimationState" ScoreScreenMatchProgress="ScoreScreenMatchInProgress"/>
                <Action type="SetProperty" frame="DivisionFrame/TeamNameLabel" visible="False"/>
            </State>
                
            <State name="TeamGame">
                <When type="AnimationState" ScoreScreenIsTeamGame="ScoreScreenTeamGame"/>
                <Action type="SetProperty" frame="DivisionFrame/TeamNameLabel" visible="True"/>
            </State>

            <State name="SoloGame">
                <Action type="SetProperty" frame="DivisionFrame/TeamNameLabel" visible="False"/>
            </State>
        </StateGroup>

        <StateGroup name="LeagueIconGlow">
            <DefaultState val="Ranked"/>

            <State name="NoData">
                <When type="AnimationState" ScoreScreenPlacementMatch="ScoreScreenNoLocalData"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueIconGlow" visible="False"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueIconGlow_Unranked" visible="False"/>
            </State>

            <State name="MatchInProgress">
                <When type="AnimationState" ScoreScreenMatchProgress="ScoreScreenMatchInProgress"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueIconGlow" visible="False"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueIconGlow_Unranked" visible="False"/>
            </State>

            <State name="Unranked">
                <When type="AnimationState" frame="DivisionFrame/LeagueIcon" LeagueIcon="Unranked"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueIconGlow_Unranked" visible="True"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueIconGlow" visible="False"/>
            </State>

            <State name="Ranked">
                <Action type="SetProperty" frame="DivisionFrame/LeagueIconGlow" visible="True"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueIconGlow_Unranked" visible="False"/>
            </State>
        </StateGroup>

        <StateGroup name="GrandmasterScoreScreen">
            <DefaultState val="NotGrandmaster"/>

            <State name="Grandmaster">
                <When type="AnimationState" frame="DivisionFrame/LeagueIcon" LeagueIcon="Grandmaster"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueTierLabel" visible="False"/>
                <Action type="SetProperty" frame="DivisionFrame/PointsEarnedTitle" visible="False"/>
                <Action type="SetProperty" frame="DivisionFrame/PointsEarnedValue" visible="False"/>
                <Action type="SetProperty" frame="DivisionFrame/LeaguePromotionTitle" Text="@UI/ScoreScreenLadder/LeaguePromotionGMTitle"/>
            </State>

            <State name="NotGrandmaster">
                <Action type="SetProperty" frame="DivisionFrame/LeagueTierLabel" visible="True"/>
                <Action type="SetProperty" frame="DivisionFrame/PointsEarnedTitle" visible="True"/>
                <Action type="SetProperty" frame="DivisionFrame/PointsEarnedValue" visible="True"/>
                <Action type="SetProperty" frame="DivisionFrame/LeaguePromotionTitle" Text="@UI/ScoreScreenLadder/LeaguePromotionTitle"/>
            </State>
        </StateGroup>

        <StateGroup name="MatchState">
            <DefaultState val="MatchFinishedNotPlacement"/>

            <State name="Tie">
                <When type="AnimationState" ScoreScreenGameResult="ScoreScreenTie"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueLabel" visible="False"/>
                <Action type="SetProperty" frame="DivisionFrame/LeaguePlacementCountLabel" visible="False"/>
                <Action type="SetProperty" frame="DivisionFrame/LeagueIcon" visible="False"/>
