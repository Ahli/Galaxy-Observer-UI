                <Text val="@UI/ScoreScreenLadder/MMRPointsGainedTitle"/>
            </Frame>
            
            <Frame type="Label" name="EnemyMMRGainedValue">
                <Anchor side="Top" relative="$parent/EnemyMMRGainedLabel" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/EnemyMMRGainedLabel" pos="Max" offset="5"/>
                <Anchor side="Right" relative="$parent/$parent/InnerBorderBottomRight" pos="Max" offset="-20"/>
                <Style val="@BonusValueText"/>
