                <Anchor side="Bottom" relative="$parent/LeagueLabel" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/LeagueLabel" pos="Min" offset="-30"/>
                <Anchor side="Right" relative="$parent/LeagueLabel" pos="Max" offset="30"/>
