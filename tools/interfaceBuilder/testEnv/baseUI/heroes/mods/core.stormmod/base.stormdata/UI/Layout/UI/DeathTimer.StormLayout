<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    
    <!-- Hero portrait frames for announcement -->
    <Frame type="Frame" name="DeadPlayerAnnouncementTemplate">
        <Width val="85"/>
        <Height val="79"/>
        
        <Frame type="Image" name="PortraitImage">
            <Width val="60"/>
            <Height val="60"/>
            <Anchor side="Top" relative="$parent/BorderImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BorderImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BorderImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BorderImage" pos="Mid" offset="0"/>
            <AlphaMaskTexture val="@UI/HUD_BG_PortraitFrame_AlphaMask"/>
        </Frame>
        
        <Frame type="Image" name="BorderImageFaded">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Texture val="@UI/HUD_DeathAnnouncement_Portrait_BlueFaded"/>
            <Alpha val="150"/>
        </Frame>
        
        <Frame type="Image" name="BorderImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Texture val="@UI/HUD_DeathAnnouncement_Portrait_Blue"/>
            
            <!-- Hide this frame initially.  Animations will cause it to show. -->
            <Visible val="false"/>
            
            <!-- Show when the hero frame is transitioning in as the topmost frame. -->
            <Animation name="TransitionInTopmost">
                <Event event="TransitionInTopmost" action="Restart,Play" frame="$parent/$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>
            
            <!-- Fade in when the hero frame transitions to the top of the stack. -->
            <Animation name="TransitionToTop">
                <Event event="TransitionToTop" action="Restart,Play" frame="$parent/$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.75" value="255"/>
                </Controller>
            </Animation>
            
            <!-- Hide when the hero frame resets to the bottom of the stack. -->
            <Animation name="ResetToBottom">
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="CrossImage">
            <Anchor side="Top" relative="$parent/PortraitImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/PortraitImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/PortraitImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/PortraitImage" pos="Mid" offset="0"/>
            <!-- the texture for this is set to the appropriate color for each team in script -->
            <Texture val="@UI/HUD_DeathTimer_CrossBlue"/>
            
            <!-- Fade in when the hero frame is transitioning in as the topmost frame. -->
            <Animation name="TransitionInTopmost">
                <Event event="TransitionInTopmost" action="Restart,Play" frame="$parent/$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.75" value="255"/>
                </Controller>
            </Animation>
            
            <!-- Fade in when the hero frame transitions to the top of the stack. -->
            <Animation name="TransitionToTop">
                <Event event="TransitionToTop" action="Restart,Play" frame="$parent/$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.75" value="255"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
    
    <Frame type="Frame" name="DeadPlayerAnnouncementWithTextLeftTemplate">
        <Width val="320"/>
        <Height val="79"/>
        
        <!-- Hide this frame initially.  Animations will cause it to show. -->
        <Visible val="false"/>
        
        <Frame type="Frame" name="DeadPlayerFrame" template="DeathTimer/DeadPlayerAnnouncementTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        </Frame>
        
        <Frame type="Label" name="HeroNameLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent/DeadPlayerFrame" pos="Max" offset="0"/>
            <!-- the font style for this is set to the opposite color for team 2 in script -->
            <Style val="Storm_DeathTimer_DeathAnnouncementHeroNameLeftBlue"/>
            <Text val="Hero Name"/>
            
            <!-- Hide this frame initially.  Animations will cause it to show. -->
            <Visible val="false"/>
            
            <!-- Show if the hero frame is transitioning in as the topmost frame. -->
            <Animation name="TransitionInTopmost">
                <Event event="TransitionInTopmost" action="Restart,Play" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>
            
            <!-- Fade in when the hero frame transitions to the top of the stack. -->
            <Animation name="TransitionToTop">
                <Event event="TransitionToTop" action="Restart,Play" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.75" value="255"/>
                </Controller>
            </Animation>
            
            <!-- Hide when the frame transitions out. -->
            <Animation name="TransitionOut">
                <Event event="TransitionOut" action="Restart,Play" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="KillersLabel">
            <Anchor side="Top" relative="$parent/HeroNameLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/HeroNameLabel" pos="Min" offset="0"/>
            <!-- the font style for this is set to the opposite color for team 2 in script -->
            <Style val="Storm_DeathTimer_DeathAnnouncementKilledByLeftBlue"/>
            <Text val="Killed By Enemies"/>
            
            <!-- Hide this frame initially.  Animations will cause it to show. -->
            <Visible val="false"/>
            
            <!-- Show if the hero frame is transitioning in as the topmost frame. -->
            <Animation name="TransitionInTopmost">
                <Event event="TransitionInTopmost" action="Restart,Play" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>
            
            <!-- Fade in when the hero frame transitions to the top of the stack. -->
            <Animation name="TransitionToTop">
                <Event event="TransitionToTop" action="Restart,Play" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.75" value="255"/>
                </Controller>
            </Animation>
            
            <!-- Hide when the frame transitions out. -->
            <Animation name="TransitionOut">
                <Event event="TransitionOut" action="Restart,Play" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        <!-- Transition in as the topmost frame. -->
        <Animation name="TransitionInTopmost">
            <Event event="TransitionInTopmost" action="Restart,Play"/>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="255"/>
            </Controller>
        </Animation>
        
        <!-- Transition in as a non-topmost frame. -->
        <Animation name="TransitionIn">
            <Event event="TransitionIn" action="Restart,Play"/>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="255"/>
            </Controller>
        </Animation>
        
        <!-- Shift the frame up one position in the stack. -->
        <Animation name="ShiftUp">
            <Event event="ShiftUp" action="RefreshBaseValue,Restart,Play"/>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="1" value="7"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="1" value="-5"/>
            </Controller>
        </Animation>
        
        <!-- Transition the frame out.  This will automatically call ResetToBottom when done. -->
        <Animation name="TransitionOut">
            <Event event="TransitionOut" action="RefreshBaseValue,Restart,Play"/>
            <Controller type="Blur" end="Pause">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0.25" value="1"/>
            </Controller>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="255" inout="Linear,Step"/>
                <Key type="Curve" time="0.25" value="255" inout="Step,Slow"/>
                <Key type="Curve" time="0.5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time="0.5" value="160" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Animation" end="Pause" animation="ResetToBottom">
                <Key type="Animation" time="0.5" action="RefreshBaseValue"/>
                <Key type="Animation" time="0.5" action="Restart"/>
                <Key type="Animation" time="0.5" action="Play"/>
            </Controller>
        </Animation>
        
        <!-- Reset the frame to the bottom of the stack.  This should only be called from TransitionOut. -->
        <Animation name="ResetToBottom">
            <!-- Pass the anim call along to some children so they can reset themselves. -->
            <Controller type="Animation" end="Pause" frame="DeadPlayerFrame/BorderImage" animation="ResetToBottom">
                <Key type="Animation" time="0" action="Restart"/>
                <Key type="Animation" time="0" action="Play"/>
            </Controller>
            <!-- Reset the frame. -->
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0" value="-223"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0" value="45"/>
            </Controller>
            <Controller type="Blur" end="Pause">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0" value="0"/>
            </Controller>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>
    </Frame>
    
