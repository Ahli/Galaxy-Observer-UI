    <!-- Hero portrait frames for timer bar -->
    <Frame type="Frame" name="DeadPlayerTimerBarTemplate">
        <Width val="460"/>
        <Height val="115"/>
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <UseAlternateTime val="true"/>
        
        <Frame type="Image" name="PortraitImage">
            <Anchor relative="$parent/BorderImage"/>
            <ImageMaskTexture val="@UI/HUD_DeathTimer_PortraitAlphaMask"/>
            <AcceptsMouse val="true"/>
            <SubpixelRendering val="True"/>
        </Frame>
        
        <Frame type="Image" name="ArrowImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/BorderImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BorderImage" pos="Mid" offset="0"/>
            <Texture val="@UI/HUD_DeathTimer_PortraitArrow"/>
            <SubpixelRendering val="True"/>

            <Animation name="ShowArrow">
                <Event event="SetLayer1" action="Reset,Play" frame="$parent"/>
                <Event event="Reset" action="Stop"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.0" value="true"/>
                </Controller>
            </Animation>

            <Animation name="HideArrow">
                <Event event="SetLayer2" action="Reset,Play" frame="$parent"/>
