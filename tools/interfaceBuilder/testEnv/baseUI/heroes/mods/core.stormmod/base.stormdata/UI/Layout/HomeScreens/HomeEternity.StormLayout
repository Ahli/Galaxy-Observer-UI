        <Frame type="CutsceneFrame" name="CometCutscene">
            <Anchor relative="$parent/BackgroundImage"/>
            <RenderType val="HDR"/>
            <AutoPlay val="True"/>
            <RenderPriority val="511"/>
            <Visible val="True"/>

            <Animation name="OnShown">
                <Event event="OnShown" action="Restart,Play"/>
                <Controller type="Cutscene" end="Loop">
                    <Key type="Cutscene" time="0" action="Stop"/>
                    <Key type="Cutscene" time="0" action="SetCutscene" file="Cutscenes/Home_EternityComet.StormCutscene"/>
                    <Key type="Cutscene" time="0" action="Bookmark" bookmark="Start"/>
                    <Key type="Cutscene" time="5" action="Play"/>
                    <Key type="Cutscene" time="15" action="Stop"/>
                </Controller>
                <Controller type="Visibility" end="Loop" frame="$parent/GoblinTriggerImage">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="9.3" value="True"/>
                    <Key type="Bool" time="10" value="False"/>
                    <Key type="Bool" time="15" value="False"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="CutsceneFrame" name="GoblinCutscene">
            <Anchor relative="$parent/BackgroundImage"/>
            <AutoPlay val="False"/>
            <RenderType val="HDR"/>
            <RenderPriority val="511"/>
            <Visible val="False"/>

            <Animation name="OnShown">
