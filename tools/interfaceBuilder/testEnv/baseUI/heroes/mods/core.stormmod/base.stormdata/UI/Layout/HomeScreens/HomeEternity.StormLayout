                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" time="0.0" action="Stop"/>
                    <Key type="Cutscene" time="0.0" action="SetCutscene" file="Cutscenes/Home_EternityGoblin.StormCutscene"/>
                    <Key type="Cutscene" time="0.0" action="Bookmark" bookmark="Start"/>
