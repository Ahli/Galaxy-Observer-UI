        <Frame type="Button" name="SwapButton" template="HeroDraftSlotFrame/SwapButtonTemplate">
            <Anchor side="Right" relative="$parent/BorderImage" pos="Max" offset="-5"/>
            <Anchor side="Bottom" relative="$parent/BorderImage" pos="Max" offset="-45"/>

            <Animation name="Show">
                <Event event="OnShown" action="Reset, Play"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                    <Key type="Curve" time=".7" value="255" inout="Auto,Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="SwapHighlight">
            <Anchor side="Top" relative="$parent/BorderImage" pos="Mid" offset="-25"/>
            <Anchor side="Bottom" relative="$parent/BorderImage" pos="Mid" offset="-25"/>
            <Anchor side="Left" relative="$parent/BorderImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BorderImage" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Draft_Swap_Hero_Frame_Inside_Highlight"/>
            <Alpha val="0"/>

            <Animation name="Fade">
                <Event event="PulseStop" action="DirectionReverse, Play" frame="$this"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.5" value="255"/>
                </Controller>
            </Animation>

            <Animation name="PingPong">
                <Event event="PulseStart" action="Reset, Play" frame="$this"/>
                <Event event="PulseStop" action="Pause" frame="$this"/>

                <Controller type="Animation" end="PingPong" animation="Fade">
                    <Key type="Animation" time="0" action="DirectionForward"/>
                    <Key type="Animation" time="0" action="Play"/>
                    <Key type="Animation" time="0.5" action="DirectionReverse"/>
                    <Key type="Animation" time="0.5" action="Play"/>
                </Controller>
            </Animation>

            <StateGroup name="ButtonHover">
                <DefaultState val="NotHovering"/>

                <State name="NotHovering">
                    <Action type="SendEvent" event="PulseStop"/>
                </State>

                <State name="Hovering">
                    <When type="Property" frame="$parent/SwapButton" Hovering="True"/>
                    <When type="Property" frame="$parent/SwapButton" Enabled="True"/>
                    <When type="Property" frame="$parent/SwapButton" Visible="True"/>
                    <Action type="SendEvent" event="PulseStart"/>
                </State>
            </StateGroup>

            <StateGroup name="SelfHover">
                <DefaultState val="NotHovering"/>

                <State name="NotHovering">
                    <When type="Property" frame="$parent" IsSelf="True"/>
                    <When type="Property" frame="$ancestor[@type=BattleLobbyTeamPanel]" HasSwapHovering="False"/>
                    <Action type="SendEvent" event="PulseStop"/>
                </State>

                <State name="Hovering">
                    <When type="Property" frame="$parent" IsSelf="True"/>
                    <When type="Property" frame="$ancestor[@type=BattleLobbyTeamPanel]" HasSwapHovering="True"/>
                    <Action type="SendEvent" event="PulseStart"/>
                </State>
            </StateGroup>
        </Frame>

