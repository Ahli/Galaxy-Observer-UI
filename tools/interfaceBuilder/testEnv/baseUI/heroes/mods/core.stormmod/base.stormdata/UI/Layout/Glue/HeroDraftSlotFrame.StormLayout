                <Action type="SetProperty" frame="HeroLabel" Visible="false"/>
                <Action type="SetProperty" frame="HeroLabelBackgroundImage" Visible="false"/>
            </State>

            <State name="TournamentDisabled">
                <When type="StateGroup" HeroLabelState="Disabled"/>
                <When type="Property" frame="$ancestor[@type=HeroDraftPanel]" IsTournamentSpectator="True"/>
                <When type="Property" operator="NotEqual" SlotState="Countdown"/>

                <Action type="SetProperty" frame="HeroLabel" Visible="false"/>
                <Action type="SetProperty" frame="HeroLabelBackgroundImage" Visible="false"/>
            </State>
        </StateGroup>
    </Frame>

