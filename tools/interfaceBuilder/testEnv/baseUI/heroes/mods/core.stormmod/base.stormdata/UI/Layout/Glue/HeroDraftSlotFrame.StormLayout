<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Constant name="DraftNameRotationLeft" val="30.50"/>
    <Constant name="DraftNameRotationRight" val="-30.50"/>
    <Constant name="DraftSwapButtonOffset" val="-25"/>

    <Frame type="Button" name="SwapButtonTemplate">
        <Width val="68"/>
        <Height val="68"/>
        <Unclipped val="True"/>
        <Tooltip val="@UI/Draft/SwapButtonTooltip"/>
        <TooltipAnchorPosition val="MiddleRight"/>
        <ClickSound val="@UI_BNet_Generic_01"/>
        <DisplayType val="ImageLayer"/>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <LayerCount val="1"/>
            <Texture val="@UI/Storm_UI_Draft_Swap_Normal" layer="0"/>
            <StateCount val="1"/>
            <DesaturateOnDisable val="True"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <ColorAdjustMode val="AddSelf"/>
            <AdjustmentColor val="150,255,255,255"/>
            <LayerCount val="1"/>
            <Texture val="@UI/Storm_UI_Draft_Swap_Normal" layer="0"/>
            <TextureType val="Nineslice" layer="0"/>
            <Tiled val="true"/>
            <StateCount val="1"/>
            <DesaturateOnDisable val="false"/>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
        </Frame>

        <StateGroup name="SwapVisibility">
            <DefaultState val="NotSwapping"/>

            <State name="NotSwapping">
                <Action type="SetProperty" Visible="False"/>
            </State>

            <State name="SwapPhase">
                <When type="Property" frame="$ancestor[@type=HeroDraftSlotFrame]" SlotState="Swapping"/>
                <When type="Property" frame="$ancestor[@type=HeroDraftSlotFrame]" IsSwappable="True"/>
                <Action type="SetProperty" Visible="True"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="Image" name="UnknownSelectionTemplate">
        <Anchor side="Top" relative="$parent/Cutscene" pos="Mid" offset="-100"/>
        <Anchor side="Bottom" relative="$parent/Cutscene" pos="Mid" offset="-100"/>
        <Anchor side="Left" relative="$parent/Cutscene" pos="Mid" offset="-6"/>
        <Anchor side="Right" relative="$parent/Cutscene" pos="Mid" offset="-6"/>
        <Texture val="@UI/storm_ui_draft_blue_spinner"/>
        <Visible val="False"/>

        <Animation name="UnknownSelectionSpinner">
            <Event event="OnShown" action="Play,DirectionForward" frame="$parent"/>
            <Event event="HiddenSelection" action="Play,DirectionForward" frame="$parent"/>

            <Controller type="Rotation" relative="False" end="Loop">
                <Key type="Curve" time="0" value="0" inout="Linear,linear"/>
                <Key type="Curve" time="5" value="-360" inout="linear,Linear"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="Image" name="PickingHighlightTemplate">
        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <Width val="284"/>
        <Height val="304"/>
        <LayerCount val="2"/>
        <BlendMode val="Normal"/>
        <Alpha val="0"/>

        <Texture val="@UI/Storm_UI_Glues_Draft_Portrait_Selecting_Glow_Mask" layer="0"/>
        <TextureCoords top="0" left=".6" bottom="1" right=".3" layer="0"/>
        <AlphaLayer val="True" layer="0"/>
        <WrapUV val="True" layer="0"/>

        <Texture val="@UI/Storm_UI_Glues_Draft_Portrait_Selecting_Glow_Blue" layer="1"/>
        <WrapUV val="True" layer="1"/>

        <Animation name="PreviewSelectPicking">
            <Event event="MyTurn" action="Reset,DirectionForward,Play" frame="$parent"/>
            <Event event="EnemyTeamTurn" action="Stop" frame="$parent"/>
            <Event event="MyTeamTurn" action="Stop" frame="$parent"/>
            <Event event="ResetActiveState" action="Stop" frame="$parent"/>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".326" value="255" inout="Linear,Slow"/>
            </Controller>
            <Controller type="LayerUV" layer="0" side="Left" relative="True" sidelock="True" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="1.956" value="1" inout="Linear"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="Frame" name="BanChooserFrameTemplate">
        <Width val="80"/>
        <Height  val="80"/>
        <Visible val="false"/>

        <Frame type="Image" name="PulseImage">
            <Anchor side="Top" relative="$parent/BanChooserIcon" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BanChooserIcon" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BanChooserIcon" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BanChooserIcon" pos="Mid" offset="0"/>
            <Unclipped val="true"/>
            <Texture val="@UI/storm_ui_draft_leader_icon_pulse"/>

            <Animation name="BanIsActive">
                <Event event="OnShown" action="Reset,Play"/>
                <Event event="OnHidden" action="Stop"/>

                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="PingPong" >
                    <Key type="Curve" time="0" value="90" inout="Linear,Auto"/>
                    <Key type="Curve" time=".6" value="70"  inout="Auto,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="PingPong" >
                    <Key type="Curve" time="0" value="90" inout="Linear,Auto"/>
                    <Key type="Curve" time=".6" value="70"  inout="Auto,Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="BanChooserIcon">
            <Anchor relative="$parent"/>
            <Texture val="@UI/storm_ui_draft_leader_icon_active"/>
        </Frame>

        <Frame type="Frame" name="ShineEffectFrame">
            <Anchor relative="$parent"/>

            <Frame type="Image" name="GlareImage">
                <Anchor relative="$parent"/>
                <BlendMode val="Add"/>
                <LayerCount val="2"/>

                <Texture val="@UI/storm_ui_draft_leader_icon_mask" layer="0"/>
                <AlphaLayer val="True" layer="0"/>
                <TextureType val="Normal" layer="0"/>

                <Texture val="@UI/storm_ui_draft_leader_icon_uvscroll" layer="1"/>
                <TextureType val="Normal" layer="1"/>
                <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
                <WrapUV val="false" layer="1"/>


                <Animation name="UVScroll">
                    <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                    <Event event="OnHidden" action="Stop" frame="$parent"/>

                    <Controller type="LayerUV" layer="1" side="Top" relative="False" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="-1" inout="Linear"/>
                        <Key type="Curve" time="1.0" value="1" inout="Linear"/>
                        <Key type="Curve" time="5.0" value="1" inout="Linear"/>
                    </Controller>
                    <Controller type="Fade" end="Loop">
                        <Key type="Curve" time="0" value="200" inout="Linear,Auto"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Image" name="Flare">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="-19"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-19"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>

                <Texture val="@UI/storm_ui_draft_leader_icon_flare"/>
                <BlendMode val="Add"/>
                <Animation name="Flicker">
                    <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                    <Event event="OnHidden" action="Stop" frame="$parent"/>

                    <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Loop">
                        <Key type="Curve" time="0" value="1"/>
                        <Key type="Curve" time=".5" value="1" inout="Linear"/>
                        <Key type="Curve" time="1.0" value="100" inout="Linear,Slow"/>
                        <Key type="Curve" time="1.5" value="1"/>
                        <Key type="Curve" time="5" value="1"/>
                    </Controller>
                    <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Loop">
                        <Key type="Curve" time="0" value="1"/>
                        <Key type="Curve" time=".5" value="1" inout="Linear"/>
                        <Key type="Curve" time="1.0" value="100" inout="Linear,Slow"/>
                        <Key type="Curve" time="1.5" value="1"/>
                        <Key type="Curve" time="5" value="1"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>

        <StateGroup name="BanChooserState">
            <DefaultState val="BanChooserOff"/>

            <State name="BanChooserOff">
                <Action type="SetProperty" frame="$this" visible="false"/>
                <Action type="SetProperty" frame="BanChooserIcon" visible="false"/>
                <Action type="SetProperty" frame="PulseImage" visible="false"/>
                <Action type="SetProperty" frame="ShineEffectFrame" visible="false"/>
            </State>
            <State name="BanChooserBanning">
                <Action type="SetProperty" frame="$this" visible="true"/>
                <Action type="SetProperty" frame="PulseImage" visible="true"/>
                <Action type="SetProperty" frame="ShineEffectFrame" visible="true"/>
                <Action type="SetProperty" frame="BanChooserIcon" visible="true"/>
                <Action type="SetProperty" frame="BanChooserIcon" texture="@UI/storm_ui_draft_leader_icon_active"/>
            </State>
            <State name="BanChooserOnMyTeamTurn">
                <Action type="SetProperty" frame="$this" visible="true"/>
                <Action type="SetProperty" frame="PulseImage" visible="false"/>
                <Action type="SetProperty" frame="ShineEffectFrame" visible="false"/>
                <Action type="SetProperty" frame="BanChooserIcon" visible="true"/>
                <Action type="SetProperty" frame="BanChooserIcon" texture="@UI/storm_ui_draft_leader_icon_normal"/>
            </State>
            <State name="BanChooserOn">
                <Action type="SetProperty" frame="$this" visible="true"/>
                <Action type="SetProperty" frame="PulseImage" visible="false"/>
                <Action type="SetProperty" frame="ShineEffectFrame" visible="false"/>
                <Action type="SetProperty" frame="BanChooserIcon" visible="true"/>
                <Action type="SetProperty" frame="BanChooserIcon" texture="@UI/storm_ui_draft_leader_icon_disabled"/>
            </State>
        </StateGroup>

        <Frame type="Frame" name="TooltipAnchorFrame">
            <Anchor relative="$parent"/>
            <Tooltip val="@UI/Draft/BanChooser"/>
            <TooltipAnchorPosition val="TopCenter"/>
            <AcceptsMouse val="true"/>
        </Frame>
    </Frame>

    <Frame type="CutsceneFrame" name="HeroSelectedCutsceneTemplate">
        <File val="Cutscenes/DraftUI_NamePlate.StormCutscene"/>
        <RenderType val="HDR"/>
        <AutoPlay val="true"/>
        <AcceptsMouse val="false"/>
        <Width val="700"/>
        <Height val="700"/>
        <Unclipped val="True"/>
        <IgnoreParentAlpha val="true"/>
    </Frame>

    <Frame type="HeroDraftSlotFrame" name="HeroDraftSlotFrameTemplate">
        <Width val="412"/>
        <Height val="412"/>
        <AcceptsMouse val="false"/>

        <Animation name="Intro">
            <Event event="Intro" action="Restart,Play"/>
            <Event event="Reset" action="Stop" frame="$parent"/>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".2" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="NameLabelBackgroundImage">
                <Key type="Curve" time="0" value="-40" inout="Linear"/>
                <Key type="Curve" time=".6" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="NameLabelBackgroundImage">
                <Key type="Curve" time="0.0" value="0" inout="Linear"/>
                <Key type="Curve" time=".6" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="PortraitContainer">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="PortraitContainer/UnknownSelection">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="PortraitContainer/Cutscene">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="PortraitContainer/DisabledBackgroundImage">
                <Key type="Curve" time="0" value="0" inout="Linear,Step"/>
                <Key type="Curve" time="1.0" value="0" inout="Linear"/>
                <Key type="Curve" time="2.0" value="255" inout="Linear,Slow"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="PortraitContainer/BackgroundImage">
                <Key type="Curve" time="0" value="0" inout="Linear,Step"/>
                <Key type="Curve" time="1.0" value="0" inout="Linear"/>
                <Key type="Curve" time="1.5" value="255" inout="Linear"/>
                <Key type="Curve" time="2.0" value="0" inout="Linear,Slow"/>
            </Controller>
            <Controller type="Property" property="AcceptsMouse" end="Pause">
                <Key type="Property" time="0" value="False"/>
                <Key type="Property" time="3.0" value="True"/>
            </Controller>
        </Animation>

        <Animation name="ReadySelected">
            <Event event="Ready" action="Reset,Play"/>
            <Event event="NotReady" action="Stop" frame="$parent"/>

            <Controller type="Event" end="Stop">
                <Key type="Event" time="0.0" event="Null"/>
                <Key type="Event" time="0.6" event="Selected"/>
            </Controller>
        </Animation>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="9"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="9"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="-13"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="-13"/>
            <Alpha val="50"/>
            <Width val="232"/>
            <Height val="252"/>
            <HitTestTexture val="@UI/Storm_UI_Glues_Draft_Portrait_Hittest_Left"/>
            <TooltipAnchorPosition val="MiddleRight"/>
        </Frame>

        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="232"/>
            <Height val="256"/>
        </Frame>

        <StateGroup name="MyTurnState">
            <DefaultState val="DimmedBorder"/>

            <!-- DraftNotStarted -->
            <State name="DraftNotStarted">
                <When type="AnimationState" ActiveState="DraftNotStarted"/>
                <Action type="SendEvent" event="ResetActiveState"/>
            </State>

            <!-- Actively Banning -->
            <State name="ActiveBorder">
                <When type="AnimationState" ActiveState="Active"/>
                <Action type="SendEvent" event="MyTurn"/>
            </State>
            <!-- My team's turn -->
            <State name="NormalBorder">
                <When type="AnimationState" TeamState="MyTeam"/>
                <Action type="SendEvent" event="MyTeamTurn"/>
            </State>
            <!-- Show active border for both team when counting down -->
            <State name="NormalBorder2">
                <When type="Property" SlotState="Countdown"/>
                <Action type="SendEvent" event="MyTeamTurn"/>
            </State>
            <!-- Show active border for both team when Swapping -->
            <State name="NormalBorder3">
                <When type="Property" SlotState="Swapping"/>
                <Action type="SendEvent" event="MyTeamTurn"/>
            </State>
            <!-- Default -->
            <State name="DimmedBorder">
                <Action type="SendEvent" event="EnemyTeamTurn"/>
            </State>
        </StateGroup>

        <StateGroup name="SlotStateController">
            <State name="PreviewSelect">
                <When type="Property" SlotState="PreviewSelect"/>
                <Action type="SetState" group="SlotVisualState" state="PreviewSelect"/>
            </State>

            <State name="PreviewSelectNotYourTurn">
                <When type="Property" SlotState="PreviewSelectNotYourTurn"/>
                <Action type="SetState" group="SlotVisualState" state="PreviewSelectNotYourTurn"/>
            </State>

            <State name="PreviewSelectPicking">
                <When type="Property" SlotState="PreviewSelectPicking"/>
                <Action type="SetState" group="SlotVisualState" state="PreviewSelectPicking"/>
            </State>

            <State name="Selected">
                <When type="Property" SlotState="Selected"/>
                <Action type="SetState" group="SlotVisualState" state="Selected"/>
            </State>

            <State name="Countdown">
                <When type="Property" SlotState="Countdown"/>
                <Action type="SetState" group="SlotVisualState" state="Selected"/>
            </State>

            <State name="Swapping">
                <When type="Property" SlotState="Swapping"/>
                <Action type="SetState" group="SlotVisualState" state="Selected"/>
            </State>

            <State name="SelectedNotYourTurn">
                <When type="Property" SlotState="SelectedNotYourTurn"/>
                <Action type="SetState" group="SlotVisualState" state="SelectedNotYourTurn"/>
            </State>

            <State name="NotSelectedYourTurn">
                <When type="Property" SlotState="NotSelectedYourTurn"/>
                <Action type="SetState" group="SlotVisualState" state="NotSelectedYourTurn"/>
            </State>

            <State name="NotSelectedNotYourTurn">
                <When type="Property" SlotState="NotSelectedNotYourTurn"/>
                <Action type="SetState" group="SlotVisualState" state="NotSelectedNotYourTurn"/>
            </State>

            <State name="NotSelectedPicking">
                <When type="Property" SlotState="NotSelectedPicking"/>
                <Action type="SetState" group="SlotVisualState" state="NotSelectedPicking"/>
            </State>

            <State name="HiddenSelection">
                <When type="Property" SlotState="HiddenSelection"/>
                <Action type="SetState" group="SlotVisualState" state="HiddenSelection"/>
            </State>
        </StateGroup>

        <StateGroup name="PortraitOverlayState">
            <State name="None">
                <Action type="SetProperty" frame="PortraitContainer/PortraitOverlay" Visible="False"/>
                <Action type="SetProperty" frame="PortraitContainer/PortraitOverlayNotYourTurn" Visible="False"/>
            </State>

            <State name="Regular">
                <Action type="SetProperty" frame="PortraitContainer/PortraitOverlay" Visible="True"/>
                <Action type="SetProperty" frame="PortraitContainer/PortraitOverlayNotYourTurn" Visible="False"/>
            </State>

            <State name="NotYourTurn">
                <Action type="SetProperty" frame="PortraitContainer/PortraitOverlay" Visible="False"/>
                <Action type="SetProperty" frame="PortraitContainer/PortraitOverlayNotYourTurn" Visible="True"/>
            </State>
        </StateGroup>

        <StateGroup name="PortraitState">
            <State name="None">
                <Action type="SetProperty" frame="PortraitContainer/BackgroundImage" Alpha="0"/>
                <Action type="SetProperty" frame="PortraitContainer/Cutscene" Visible="False" undo="false"/>
            </State>

            <State name="HeroOnly">
                <Action type="SetProperty" frame="PortraitContainer/BackgroundImage" Alpha="0"/>
                <Action type="SetProperty" frame="PortraitContainer/Cutscene" Visible="True" undo="false"/>
            </State>

            <State name="BackgroundOnly">
                <Action type="SetProperty" frame="PortraitContainer/BackgroundImage" Alpha="255"/>
                <Action type="SetProperty" frame="PortraitContainer/Cutscene" Visible="False" undo="false"/>
            </State>

            <State name="HeroAndBackground">
                <Action type="SetProperty" frame="PortraitContainer/BackgroundImage" Alpha="255"/>
                <Action type="SetProperty" frame="PortraitContainer/Cutscene" Visible="True" undo="false"/>
            </State>
        </StateGroup>

        <StateGroup name="SlotVisualState">
            <DefaultState val="NotSelectedNotYourTurn"/>

            <State name="PreviewSelect">
                <Action type="SetState" group="PortraitState" state="HeroAndBackground"/>
                <Action type="SetState" group="HeroLabelState" state="Hidden"/>
                <Action type="SetState" group="NameLabelState" state="NotSelected"/>
                <Action type="SetState" group="PortraitOverlayState" state="Regular"/>
            </State>

            <State name="PreviewSelectNotYourTurn">
                <Action type="SetState" group="PortraitState" state="HeroOnly"/>
                <Action type="SetState" group="HeroLabelState" state="Hidden"/>
                <Action type="SetState" group="NameLabelState" state="Disabled"/>
                <Action type="SetState" group="PortraitOverlayState" state="Regular"/>
            </State>

            <State name="PreviewSelectPicking">
                <Action type="SetState" group="PortraitState" state="HeroAndBackground"/>
                <Action type="SetState" group="HeroLabelState" state="Picking"/>
                <Action type="SetState" group="NameLabelState" state="NotSelected"/>
                <Action type="SetState" group="PortraitOverlayState" state="Regular"/>
            </State>

            <State name="Selected">
                <Action type="SetState" group="PortraitState" state="HeroAndBackground"/>
                <Action type="SetState" group="HeroLabelState" state="Selected"/>
                <Action type="SetState" group="NameLabelState" state="Selected"/>
                <Action type="SetState" group="PortraitOverlayState" state="None"/>
            </State>

            <State name="SelectedNotYourTurn">
                <Action type="SetState" group="PortraitState" state="HeroOnly"/>
                <Action type="SetState" group="HeroLabelState" state="Disabled"/>
                <Action type="SetState" group="NameLabelState" state="Disabled"/>
                <Action type="SetState" group="PortraitOverlayState" state="NotYourTurn"/>
            </State>

            <State name="NotSelectedYourTurn">
                <Action type="SetState" group="PortraitState" state="BackgroundOnly"/>
                <Action type="SetState" group="HeroLabelState" state="Hidden"/>
                <Action type="SetState" group="NameLabelState" state="NotSelected"/>
                <Action type="SetState" group="PortraitOverlayState" state="None"/>
            </State>

            <State name="NotSelectedNotYourTurn">
                <Action type="SetState" group="PortraitState" state="None"/>
                <Action type="SetState" group="HeroLabelState" state="Hidden"/>
                <Action type="SetState" group="NameLabelState" state="Disabled"/>
                <Action type="SetState" group="PortraitOverlayState" state="None"/>
            </State>

            <State name="NotSelectedPicking">
                <Action type="SetState" group="PortraitState" state="BackgroundOnly"/>
                <Action type="SetState" group="HeroLabelState" state="Picking"/>
                <Action type="SetState" group="NameLabelState" state="Picking"/>
                <Action type="SetState" group="PortraitOverlayState" state="None"/>
                <Action type="SetProperty" frame="PortraitContainer/UnknownSelection" Visible="True"/>
            </State>

            <State name="HiddenSelection">
                <Action type="SetState" group="PortraitState" state="BackgroundOnly"/>
                <Action type="SetProperty" frame="PortraitContainer/UnknownSelection" Visible="True"/>
                <Action type="SetState" group="HeroLabelState" state="Picking"/>
                <Action type="SetState" group="NameLabelState" state="NotSelected"/>
                <Action type="SetState" group="PortraitOverlayState" state="None"/>
            </State>
        </StateGroup>

        <Frame type="Image" name="PortraitBorderSelectedGlowImage">
            <Anchor relative="$parent/BorderImage" />
            <Texture val="@UI/Storm_Draft_Portrait_Selected_Blue"/>
            <Alpha val="0"/>

            <Animation name="Ready">
                <Event event="MyTurn" action="DirectionForward,Play" frame="$parent"/>
                <Event event="EnemyTeamTurn" action="DirectionReverse,Play" frame="$parent"/>
                <Event event="MyTeamTurn" action="DirectionReverse,Play" frame="$parent"/>
                <Event event="ResetActiveState" action="DirectionReverse,Play" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                    <Key type="Curve" time=".9" value="255" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>

    <!--Player Frame Cutscenes-->
        <Frame type="Frame" name="PortraitContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="110"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="121"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-119"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-121"/>
            <AlphaMaskTexture val="@UI/storm_ui_draft_alphamask_portrait_left"/>
            <Visible val="false"/>

            <Frame type="BlurFrame" name="AlternateHeroTileListBlurFrame" template="StandardTemplates/FullscreenBlurFrame">
                <Anchor relative="$parent"/>
                <BlurAmount val="0.2"/>
            </Frame>

            <Frame type="Image" name="DisabledBackgroundImage">
                <Anchor side="Top" relative="$parent/$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent" pos="Mid" offset="0"/>
                <Texture val="@UI/storm_ui_glues_draft_portrait_disabled_bg"/>
                <Alpha val="0"/>
                <Width val="232"/>
                <Height val="256"/>
            </Frame>

            <Frame type="Image" name="BackgroundImage">
