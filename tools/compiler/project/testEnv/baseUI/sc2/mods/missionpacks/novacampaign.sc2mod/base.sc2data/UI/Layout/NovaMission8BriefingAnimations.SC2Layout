    <!-- 
    
    
    =============================
    
    TALDARIM DOT TEMPLATE
    
    =============================
    
    
    -->
    
    <Frame type="Image" name="TaldarimDotTemplate">
        <Alpha val="0"/>
        <Texture val="@UI/NovaStory/Briefing/General/RedTriangle"/>
        
        <Animation name="Display">
            <Event event="Display" action="Reset,Play"/>
            <Event event="MissionBriefing8_Phase1" action="Reset,Stop" frame="$MissionPreviewPanel"/>
            <Event event="ResetBriefing" action="Reset,Stop" frame="$MissionPreviewPanel"/>
            
            <Controller type="Fade" name="Display" end="Pause">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time=".3" value="255" out="Fast"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="$this">
                <Key type="Event" time="0" event=""/>
                <Key type="Event" time="0.3" event="Pulse"/>
            </Controller>
