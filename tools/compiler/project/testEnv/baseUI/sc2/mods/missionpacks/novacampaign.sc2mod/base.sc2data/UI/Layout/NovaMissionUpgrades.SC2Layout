        <StateGroup name="ItemStatusStateGroup">
        
            <!-- Disabled State -->
            <State name="DisabledState">
                <When type="Property" frame="$this" Enabled="False"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="MoviePointer" visible="False"/>
                <Action type="SetProperty" frame="DisabledImage" visible="True"/>
                <Action type="SetProperty" frame="NormalImage" alpha="0"/>
            </State>
            
            <!-- Available, Unselected, Unassigned State -->
            <State name="AvailableUnselectedUnassignedState">
                <When type="Property" frame="$this" Enabled="True"/>
                <When type="AnimationState" frame="$this" AvailableState="Available"/>
                <When type="AnimationState" frame="$this" SelectedState="Unselected"/>
                <When type="AnimationState" frame="$this" AssignedState="Unassigned"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="MoviePointer" visible="False"/>
                <Action type="SetProperty" frame="DisabledImage" visible="False"/>
            </State>
            
            <!-- Available, Unselected, SelfAssigned State -->
            <State name="AvailableUnselectedSelfAssignedState">
                <When type="Property" frame="$this" Enabled="True"/>
                <When type="AnimationState" frame="$this" AvailableState="Available"/>
                <When type="AnimationState" frame="$this" SelectedState="Unselected"/>
                <When type="AnimationState" frame="$this" AssignedState="SelfAssigned"/>
                <Action type="SetProperty" frame="SelfAssignedIcon" visible="True"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="MoviePointer" visible="False"/>
                <Action type="SetProperty" frame="DisabledImage" visible="False"/>
            </State>
            
            <!-- Available, Unselected, OtherAssigned State -->
            <State name="AvailableUnselectedOtherAssignedState">
                <When type="Property" frame="$this" Enabled="True"/>
                <When type="AnimationState" frame="$this" AvailableState="Available"/>
                <When type="AnimationState" frame="$this" SelectedState="Unselected"/>
                <When type="AnimationState" frame="$this" AssignedState="OtherAssigned"/>
                <Action type="SetProperty" frame="OtherAssignedIcon" visible="True"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="MoviePointer" visible="False"/>
                <Action type="SetProperty" frame="DisabledImage" visible="False"/>
            </State>
            
            <!-- Available, Selected, Unassigned State -->
            <State name="AvailableSelectedUnassignedState">
                <When type="Property" frame="$this" Enabled="True"/>
                <When type="AnimationState" frame="$this" AvailableState="Available"/>
                <When type="AnimationState" frame="$this" SelectedState="Selected"/>
                <When type="AnimationState" frame="$this" AssignedState="Unassigned"/>
                <Action type="SetProperty" frame="ToggleImage" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageBarTop" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageBarBottom" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageHighlightAnim" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageAnim" visible="True"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="None"/>
                <Action type="SetProperty" frame="MoviePointer" visible="True"/>
                <Action type="SetProperty" frame="DisabledImage" visible="False"/>
                <Action type="SetProperty" frame="NormalImage" alpha="0"/>
                <Action type="SetProperty" frame="HoverImage" alpha="0"/>
            </State>
            
            <!-- Available, Selected, SelfAssigned State -->
            <State name="AvailableSelectedSelfAssignedState">
                <When type="Property" frame="$this" Enabled="True"/>
                <When type="AnimationState" frame="$this" AvailableState="Available"/>
                <When type="AnimationState" frame="$this" SelectedState="Selected"/>
                <When type="AnimationState" frame="$this" AssignedState="SelfAssigned"/>
                <Action type="SetProperty" frame="ToggleImage" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageBarTop" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageBarBottom" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageHighlightAnim" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageAnim" visible="True"/>
                <Action type="SetProperty" frame="SelfAssignedIcon" visible="True"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="None"/>
                <Action type="SetProperty" frame="MoviePointer" visible="True"/>
                <Action type="SetProperty" frame="DisabledImage" visible="False"/>
                <Action type="SetProperty" frame="NormalImage" alpha="0"/>
                <Action type="SetProperty" frame="HoverImage" alpha="0"/>
            </State>
            
            <!-- Available, Selected, OtherAssigned State -->
            <State name="AvailableSelectedOtherAssignedState">
                <When type="Property" frame="$this" Enabled="True"/>
                <When type="AnimationState" frame="$this" AvailableState="Available"/>
                <When type="AnimationState" frame="$this" SelectedState="Selected"/>
                <When type="AnimationState" frame="$this" AssignedState="OtherAssigned"/>
                <Action type="SetProperty" frame="ToggleImage" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageBarTop" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageBarBottom" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageHighlightAnim" visible="True"/>
                <Action type="SetProperty" frame="ToggleImageAnim" visible="True"/>
                <Action type="SetProperty" frame="OtherAssignedIcon" visible="True"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="None"/>
                <Action type="SetProperty" frame="MoviePointer" visible="True"/>
                <Action type="SetProperty" frame="DisabledImage" visible="False"/>
                <Action type="SetProperty" frame="NormalImage" alpha="0"/>
                <Action type="SetProperty" frame="HoverImage" alpha="0"/>
            </State>
            
            <!-- Missed, Unselected, Unassigned State -->
            <State name="MissedUnselectedUnassignedState">
                <When type="Property" frame="$this" Enabled="True"/>
                <When type="AnimationState" frame="$this" AvailableState="Missed"/>
                <When type="AnimationState" frame="$this" SelectedState="Unselected"/>
                <When type="AnimationState" frame="$this" AssignedState="Unassigned"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="MoviePointer" visible="False"/>
                <Action type="SetProperty" frame="UnavailableIcon" visible="True"/>
            </State>
            
            <!-- Missed, Selected, Unassigned State -->
