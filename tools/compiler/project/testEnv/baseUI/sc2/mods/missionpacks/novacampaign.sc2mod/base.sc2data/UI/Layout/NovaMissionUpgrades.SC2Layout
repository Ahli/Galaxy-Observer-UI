                            <ImageMaskTexture val="Assets/Textures/ui_nova_armyupgrades_unitlinesmask.dds"/>
                        </Frame>

                        
                        <!--
                        ==================================================
                        ITEM STATUS STATE GROUP
                        ==================================================
                        -->
                        
                        <StateGroup name="ItemStatusStateGroup">
                        
                            <!-- Available State -->
                            <State name="Available">
                                <When type="AnimationState" frame="$this" ItemState="Available"/>
                                <Action type="SetProperty" frame="SelectedItemStatusIcon" texture="Assets/Textures/ui_nova_armyupgrades_itemstatusicon_available.dds"/>
                            </State>
                            
                            <!-- Equipped State -->
                            <State name="Equipped">
                                <When type="AnimationState" frame="$this" ItemState="Equipped"/>
                                <Action type="SetProperty" frame="SelectedItemStatusIcon" texture="Assets/Textures/ui_nova_armyupgrades_itemstatusicon_equipped.dds"/>
                            </State>
                            
                            <!-- In Use State -->
                            <State name="InUse">
                                <When type="AnimationState" frame="$this" ItemState="InUse"/>
                                <Action type="SetProperty" frame="SelectedItemStatusIcon" texture="Assets/Textures/ui_nova_armyupgrades_itemstatusicon_inuse.dds"/>
                                <Action type="SetProperty" frame="SelectedItemStatusIconBorder" texture="Assets/Textures/ui_nova_armyupgrades_itemstatusicon_container_orange.dds"/>
                                <Action type="SetProperty" frame="SelectedItemStatusBackground" color="240,108,56"/>
                                <Action type="SetProperty" frame="SelectedItemStatusLabel" style="@NovaArmyUpgradesSubselectionItemStatusTitleOrange"/>
                                <Action type="SetProperty" frame="SelectedItemStatusUnitLabel" style="@NovaArmyUpgradesSubselectionItemStatusDescriptionOrange"/>
                                <Action type="SetProperty" frame="SelectedItemStatusUnitIcon" visible="True"/>
                            </State>
                            
                            <!-- Locked State -->
                            <State name="Locked">
                                <When type="AnimationState" frame="$this" ItemState="Locked"/>
                                <Action type="SetProperty" frame="SelectedItemStatusIcon" texture="Assets/Textures/ui_nova_armyupgrades_itemstatusicon_locked.dds"/>
                            </State>
                        </StateGroup>
                    </Frame>
                    
                    <Frame type="Button" name="EquipItemButton" template="NovaMissionStandardTemplates/NovaStoryModeButtonTemplate">
                        <Anchor side="Bottom" relative="$parent" pos="Max" offset="-38"/>
                        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                        <Width val="506"/>

                        <ClickSound val="@UI_NovaUT_EquipClick"/>
                        <HoverSound val="@UI_NovaUT_MouseOverB"/>

                        <Frame type="Image" name="NormalImage">
                            <TextureType val="EndCap" layer="0"/>
                            <TextureType val="EndCap" layer="1"/>
                            <TextureType val="EndCap" layer="2"/>
                            <TextureType val="EndCap" layer="3"/>
                        </Frame>
                        
                        <Frame type="Image" name="HoverImage">
                            <TextureType val="EndCap" layer="0"/>
                            <TextureType val="EndCap" layer="1"/>
                            <TextureType val="EndCap" layer="2"/>
                            <TextureType val="EndCap" layer="3"/>
                        </Frame>
                        
                        <!--
                        ==================================================
                        ITEM STATUS STATE GROUP
                        ==================================================
                        -->
                        
                        <StateGroup name="ItemStatusStateGroup">
                        
                            <!-- Available State -->
                            <State name="Available">
                                <When type="AnimationState" frame="$parent/SelectedItemStatusContainer" ItemState="Available"/>
                                <Action type="SetProperty" frame="$this" enabled="True"/>
                                <Action type="SetProperty" frame="$this" clickSound="@UI_NovaUT_EquipClick"/>
                            </State>
                            
                            <!-- Equipped State -->
                            <State name="Equipped">
                                <When type="AnimationState" frame="$parent/SelectedItemStatusContainer" ItemState="Equipped"/>
                                <Action type="SetProperty" frame="$this" enabled="True"/>
                                <Action type="SetProperty" frame="$this" clickSound="@UI_NovaUT_UnequipClick"/>
                            </State>
                            
                            <!-- In Use State -->
                            <State name="InUse">
                                <When type="AnimationState" frame="$parent/SelectedItemStatusContainer" ItemState="InUse"/>
                                <Action type="SetProperty" frame="$this" enabled="True"/>
                                <Action type="SetProperty" frame="$this" clickSound="@UI_NovaUT_TransferClick"/>
                            </State>
                            
                            <!-- Locked State -->
                            <State name="Locked">
                                <When type="AnimationState" frame="$parent/SelectedItemStatusContainer" ItemState="Locked"/>
                                <Action type="SetProperty" frame="$this" enabled="False"/>
                            </State>
                        </StateGroup>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
        

        <Animation name="Show">
            <Event event="OnShown" action="Reset,Play" frame="$this"/>

            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0" sound="@UI_NovaUT_ScreenDown"/>
            </Controller>
        </Animation>
    </Frame>
</Desc>
