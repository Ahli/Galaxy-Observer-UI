<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
<!--
||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||

TOURNAMENT PORTAL FRAME SUPPORT TEMPLATES

||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||
-->

    <!--
        
    ====================================================================
    
    RACE SELECTION
    
    ====================================================================
    
    -->
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    RACE BUTTON CONTAINER TEMPLATE
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="TournamentRaceTileList" name="TournamentRaceTileListTemplate">
        <Width val="920"/>
        <Height val="80"/>

        <Frame type="RaceTileListItem" name="Item0" template="MatchmakingInvitePanel/MatchmakingRaceTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        </Frame>

        <Frame type="RaceTileListItem" name="Item1" template="MatchmakingInvitePanel/MatchmakingRaceTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="6"/>
        </Frame>

        <Frame type="RaceTileListItem" name="Item2" template="MatchmakingInvitePanel/MatchmakingRaceTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="6"/>
        </Frame>

        <Frame type="RaceTileListItem" name="Item3" template="MatchmakingInvitePanel/MatchmakingRaceTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item2" pos="Max" offset="6"/>
        </Frame>
    </Frame>
    
    <!--
        
    ====================================================================
    
    TOURNAMENT PORTAL BUTTONS
    
    ====================================================================
    
    -->
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    ENTER TOURNAMENT PORTAL BUTTON (SIGN UP FOR TOURNAMENT)
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="EnterTournamentPortalButton" name="EnterPortalButtonTemplate" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
        <!-- Events
        Empty - The portal does not exist
        NotLicensed - The player does not have the license to join the tournament
        NotCorrectExpansionLevel - The player has not selected the correct expansion level to join the tournament
        NotPartyLeader - The player is not the party leader
        NotCorrectPartySize- The player's party is not the correct size to join the tournament
        InTournament - The player is already playing in a tournament
        Current - The player is already signed up for the tournament
        InOtherPortal - The player is already signed up for a different tournament
        Inactive - The portal is expired
        NoRaceSelected - The player has not selected a race
        WaitingOnCallback - Waiting on the server to acknowledge entering the portal
        PendingHotPatch - A Hot Patch is pending
        Active - None of the other states are true
        -->

        <Text val="@UI/ScreenTournament/JoinButton"/>
        <!-- Hidden=Empty, Current, InTournament, InOtherPortal -->
        <Visible val="false"/>

        <StateGroup name="ButtonStateGroup">
            <State name="Placement">
                <When type="AnimationState" frame="$parent/MatchmakingLeaguePanel" LeagueState="Placement"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentPlacement"/>
            </State>
            
            <State name="SeasonPlacement">
                <When type="AnimationState" frame="$parent/MatchmakingLeaguePanel" LeagueState="SeasonPlacement"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentPlacement"/>
            </State>
            
            <State name="Active">
                <When type="AnimationState" DefaultState="Active"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="true"/>
                <Action type="SetProperty" Tooltip=""/>
            </State>
            
            <State name="Inactive">
                <When type="AnimationState" DefaultState="Inactive"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentExpired"/>
            </State>

            <State name="NotLicensed">
                <When type="AnimationState" DefaultState="NotLicensed"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentNotLicensed"/>
            </State>

            <State name="NotCorrectExpansionLevel">
                <When type="AnimationState" DefaultState="NotCorrectExpansionLevel"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentExpansionLevel"/>
            </State>

            <State name="NotPartyLeader">
                <When type="AnimationState" DefaultState="NotPartyLeader"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentNotPartyLeader"/>
            </State>

            <State name="NotCorrectPartySize">
                <When type="AnimationState" DefaultState="NotCorrectPartySize"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentNotCorrectPartySize"/>
            </State>

            <State name="NoRaceSelected">
                <When type="AnimationState" DefaultState="NoRaceSelected"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentNoRaceSelected"/>
            </State>

            <State name="WaitingOnCallback">
                <When type="AnimationState" DefaultState="WaitingOnCallback"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip=""/>
            </State>

            <State name="PendingHotPatch">
                <When type="AnimationState" DefaultState="PendingHotPatch"/>
                <Action type="SetProperty" Visible="true"/>
                <Action type="SetProperty" Enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenTournament/EnterTournamentPendingHotPatch"/>
            </State>
        </StateGroup>
    </Frame>
    
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    LEAVE TOURNAMENT PORTAL BUTTON (CANCEL READY)
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <Frame type="LeaveTournamentPortalButton" name="LeavePortalButtonTemplate" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
        <!-- Events
        Empty - The portal does not exist.
        NotInPortal - The player is not in the tournament portal
        Inactive - The portal is expired
        Active - None of the other states are true
        -->

        <Text val="@UI/ScreenTournament/CancelButton"/>

        <Animation name="Active">
            <Event event="Active" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>

            <Controller type="Enabled" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
        </Animation>

        <Animation name="Inactive">
            <Event event="Inactive" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>

            <Controller type="Enabled" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>

        <Animation name="Hidden">
            <Event event="Empty" action="Reset,Play"/>
            <Event event="NotInPortal" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>
    </Frame>
    
    <!--
        
    ====================================================================
    
    WARNINGS
    
    ====================================================================
    
    -->
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    LICENSE WARNING
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
    
    <Frame type="Frame" name="LicenseWarningTemplate">
        <Anchor side="Top" relative="$this/Button" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$this/ErrorText" pos="Max" offset="0"/>
        <Height val="120"/>
        <Visible val="False"/>
        
        <Frame type="CampaignBuyButton" name="Button" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <ShowPrice val="false"/>
            <AlwaysShow val="true"/>
            <Text val="@UI/ScreenTournament/LicenseWarningUpsellText"/>
        </Frame>
        
        <Frame type="Image" name="PurchaseWarningIcon">
            <Anchor side="Top" relative="$parent/Button" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Button" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/Button" pos="Max" offset="6"/>
            <Texture val="@UI/Glues_Icons_PurchaseWarning"/>
        </Frame>
        
        <Frame type="Label" name="ErrorText">
            <Anchor side="Top" relative="$parent/Button" pos="Min" offset="14"/>
            <Anchor side="Left" relative="$parent/PurchaseWarningIcon" pos="Max" offset="3"/>
            <Style val="@Tournament_LicenseWarningTitle"/>
            <Text val="@UI/ScreenTournament/LicenseWarningErrorText"/>
        </Frame>
        
        <Frame type="Label" name="ErrorDetails">
            <Anchor side="Top" relative="$parent/ErrorText" pos="Max" offset="1"/>
            <Anchor side="Left" relative="$parent/ErrorText" pos="Min" offset="0"/>
            <Style val="@Tournament_LicenseWarningDetails"/>
            <Text val="@UI/ScreenTournament/LicenseWarningErrorDetails"/>
        </Frame>
        
        <Animation name="Hidden">
            <Event event="Empty" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="Active" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="Inactive" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="Current" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="InOtherPortal" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="InTournament" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="NotCorrectExpansionLevel" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="NotPartyLeader" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="NotCorrectPartySize" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="NoRaceSelected" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="WaitingOnCallback" action="Reset,Play" frame="$parent/EnterButton"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>
        
        <Animation name="NotLicensed">
            <Event event="NotLicensed" action="Reset,Play" frame="$parent/EnterButton"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
        </Animation>
    </Frame>
    
    <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~
    EXPANSION LEVEL WARNING
    ~~~~~~~~~~~~~~~~~~~~~~~
    -->
    
    <Frame type="Frame" name="ExpansionLevelWarningTemplate">
        <Anchor side="Right" relative="$this/ErrorDetails" pos="Max" offset="50"/>
        <Height val="120"/>
        <Visible val="False"/>
        
        <Frame type="Image" name="PurchaseWarningIcon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
            <Texture val="@UI/Glues_Icons_PurchaseWarning"/>
        </Frame>
        
        <Frame type="Label" name="ErrorText">
            <Anchor side="Top" relative="$parent" pos="Min" offset="14"/>
            <Anchor side="Left" relative="$parent/PurchaseWarningIcon" pos="Max" offset="3"/>
            <Style val="@Tournament_LicenseWarningTitle"/>
            <Text val="@UI/ScreenTournament/ExpansionLevelWarningErrorText"/>
        </Frame>
        
        <Frame type="Label" name="ErrorDetails">
            <Anchor side="Top" relative="$parent/ErrorText" pos="Max" offset="1"/>
            <Anchor side="Left" relative="$parent/ErrorText" pos="Min" offset="0"/>
            <Style val="@Tournament_LicenseWarningDetails"/>
            <Text val="@UI/ScreenTournament/ExpansionLevelWarningErrorDetails"/>
        </Frame>
        
        <!--
        
        <Frame type="GlueOptionsButton" name="Button" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Top" relative="$parent/ErrorText" pos="Max" offset="5"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="25"/>
            <Anchor side="Left" relative="$parent/Icon" pos="Max" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="@UI/Options_Control"/>
            <Panel val="Gameplay"/>
        </Frame>
        
        -->
        
        <Animation name="Hidden">
            <Event event="Empty" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="Active" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="Inactive" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="Current" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="InOtherPortal" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="InTournament" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="NotLicensed" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="NotPartyLeader" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="NotCorrectPartySize" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="NoRaceSelected" action="Reset,Play" frame="$parent/EnterButton"/>
            <Event event="WaitingOnCallback" action="Reset,Play" frame="$parent/EnterButton"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>
        
        <Animation name="NotCorrectExpansionLevel">
            <Event event="NotCorrectExpansionLevel" action="Reset,Play" frame="$parent/EnterButton"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
        </Animation>
    </Frame>
    
    
<!--
||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||

TOURNAMENT PORTAL FRAME TEMPLATE AND ANIMATIONS

||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||
-->

    <!--
        
    ====================================================================
    
    ANIMATION STATES
    
    ====================================================================
    
    -->
    
    <!-- Default -->
    <Animation name="TournamentPortalFrameState_Default">
        
        <Controller type="Visibility" name="PortalFrameVisibility" end="Pause">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentInfoBGSmall">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentInfoBGLarge">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentNameLabel">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentTimeLabel">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="SignedUpLabel">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="ExpiredLabel">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="InfoIcon">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
    </Animation>
    
    
    <!-- Empty -->
    <Animation name="TournamentPortalFrameState_Empty" template="TournamentPortalFrame/TournamentPortalFrameState_Default">
        <Event event="Empty" action="Reset,Play"/>
        
        <Controller type="Visibility" name="PortalFrameVisibility" end="Pause">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
    </Animation>
    
    
    <!-- Active -->
    <Animation name="TournamentPortalFrameState_Active" template="TournamentPortalFrame/TournamentPortalFrameState_Default">
        <Event event="Active" action="Reset,Play"/>

        <Controller type="Visibility" end="Pause" frame="NextTournamentNameLabel">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentTimeLabel">
            <Key type="Bool" time="0" value="True"/>
        </Controller>

        <Controller type="Visibility" end="Pause" frame="InfoIcon">
            <Key type="Bool" time="0" value="True"/>
        </Controller>

        <Controller type="Visibility" end="Pause" frame="NextTournamentInfoBGSmall">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
    </Animation>
    
    
    <!-- Current (Signed Up) -->
    <Animation name="TournamentPortalFrameState_Current" template="TournamentPortalFrame/TournamentPortalFrameState_Default">
        <Event event="Current" action="Reset,Play"/>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentNameLabel">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentTimeLabel">
            <Key type="Bool" time="0" value="True"/>
        </Controller>

        <Controller type="Visibility" end="Pause" frame="InfoIcon">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="SignedUpLabel">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentInfoBGLarge">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
    </Animation>
    
    
    <!-- InTournament -->
    <Animation name="TournamentPortalFrameState_InTournament" template="TournamentPortalFrame/TournamentPortalFrameState_Default">
        <Event event="InTournament" action="Reset,Play"/>
        
        <Controller type="Visibility" name="PortalFrameVisibility" end="Pause">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
    </Animation>
    
    
    <!-- InOtherPortal -->
    <Animation name="TournamentPortalFrameState_InOtherPortal" template="TournamentPortalFrame/TournamentPortalFrameState_Default">
        <Event event="InOtherPortal" action="Reset,Play"/>
        
        <Controller type="Visibility" name="PortalFrameVisibility" end="Pause">
            <Key type="Bool" time="0" value="False"/>
        </Controller>
    </Animation>
    
    
    <!-- Inactive -->
    <Animation name="TournamentPortalFrameState_Inactive" template="TournamentPortalFrame/TournamentPortalFrameState_Default">
        <Event event="Inactive" action="Reset,Play"/>
        
        <Controller type="Visibility" end="Pause" frame="NextTournamentInfoBGLarge">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
        
        <Controller type="Visibility" end="Pause" frame="ExpiredLabel">
            <Key type="Bool" time="0" value="True"/>
        </Controller>
    </Animation>
    
    <!--
        
    ====================================================================
    
    ENTRANCE ANIMATION
    
    ====================================================================
    
    -->
    
    <Animation name="TournamentPortalFadeInTemplate">
        <Event event="OnShown" action="Reset,Play" frame="$parent"/>
        
        <Controller type="Fade" end="Pause">
            <Key type="Curve" time="0" value="255" out="Fast"/>
            <Key type="Curve" time="0.2" value="255" out="Fast"/>
            <Key type="Curve" time="0.5" value="255" in="Slow"/>
        </Controller>
    </Animation>
    
    
    <!--
        
    ====================================================================
    
    ANIMATION STATES
    
    ====================================================================
    
    -->
    
        


    <Frame type="TournamentPortalFrame" name="TournamentPortalFrameTemplate">
        <!-- Events
        Empty - The portal does not exist.
        InTournament - The player is already playing a tournament
        Current - The player is already signed up for this portal
        InOtherPortal - The player is already signed up for a different portal
        Inactive - The portal is expired
        Active - None of the other states are true
        -->
        
        <!--
        <Frame type="CutsceneFrame" name="TrophyCutscene">
            <Anchor relative="$parent"/>
            <RenderType val="HDR"/>
            <File val="Cutscenes/UI_Screens_Tournament_Start.SC2Cutscene"/>
            <Alpha val="255"/>
            
            <Animation name="Show">
                <Event event="OnShown" action="Reset,Play"/>
                
                <Controller type="Cutscene" end="Loop">
                    <Key type="Cutscene" action="Play" time="0.0"/>
                </Controller>
            </Animation>
        </Frame> -->
        
        <Frame type="Image" name="NextTournamentInfoBGLarge">
            <Anchor side="Top" relative="$parent/NextTournamentNameLabel" pos="Mid" offset="9"/>
            <Anchor side="Bottom" relative="$parent/NextTournamentNameLabel" pos="Mid" offset="9"/>
            <Anchor side="Left" relative="$parent/NextTournamentNameLabel" pos="Min" offset="-50"/>
            <Texture val="@UI/SC2_UI_Tournament_NextInfo_BG_Large"/>
            <Visible val="False"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>
        </Frame>

        <Frame type="Image" name="NextTournamentInfoBGSmall">
            <Anchor side="Top" relative="$parent/NextTournamentNameLabel" pos="Mid" offset="-1"/>
            <Anchor side="Bottom" relative="$parent/NextTournamentNameLabel" pos="Mid" offset="-1"/>
            <Anchor side="Left" relative="$parent/NextTournamentNameLabel" pos="Min" offset="-50"/>
            <Texture val="@UI/SC2_UI_Tournament_NextInfo_BG_Small"/>
            <Visible val="True"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>
        </Frame>

        <Frame type="Label" name="NextTournamentNameLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="328"/>
            <Anchor side="Left" relative="$Anchor_LeftNoBuffer" pos="Min" offset="118"/>
            <Style val="@Tournament_NextTournamentInfo"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>
        </Frame>
        
        <Frame type="Label" name="NextTournamentTimeLabel">
            <Anchor side="Top" relative="$parent/NextTournamentNameLabel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/NextTournamentNameLabel" pos="Max" offset="8"/>
            <Style val="@Tournament_NextTournamentInfoTime"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>
        </Frame>
        
        <Frame type="Label" name="SignedUpLabel">
            <Anchor side="Top" relative="$parent/NextTournamentNameLabel" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="118"/>
            <Style val="@Tournament_SignedUp"/>
            <Text val="@UI/ScreenTournament/YouAreSignedUp"/>
            <Visible val="False"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>
        </Frame>

        <Frame type="Image" name="InfoIcon">
            <Anchor side="Top" relative="$parent/NextTournamentNameLabel" pos="Mid" offset="-2"/>
            <Anchor side="Bottom" relative="$parent/NextTournamentNameLabel" pos="Mid" offset="-2"/>
            <Anchor side="Left" relative="$parent/NextTournamentNameLabel" pos="Max" offset="130"/>
            <Width val="36"/>
            <Height val="36"/>
            <Texture val="@UI/Glues_Icons_GenericInfo"/>
            <AcceptsMouse val="True"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>
        </Frame>

        <Frame type="Label" name="ExpiredLabel">
            <Anchor side="Top" relative="$parent/NextTournamentNameLabel" pos="Min" offset="-4"/>
            <Anchor side="Left" relative="$parent/NextTournamentNameLabel" pos="Min" offset="-4"/>
            <Text val="@UI/ScreenTournament/Expired"/>
            <Style val="@Tournament_NextTournamentInfoLowercase"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>
        </Frame>

        <Frame type="Label" name="RaceSelectionLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="421"/>
            <Anchor side="Left" relative="$parent/NextTournamentNameLabel" pos="Min" offset="-3"/>
            <Text val="@UI/ScreenTournament/SelectTitle"/>
            <Style val="@Tournament_DetailTitle"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>
        </Frame>

        <Frame type="TournamentRaceTileList" name="RaceTileList" template="TournamentPortalFrame/TournamentRaceTileListTemplate">
            <Anchor side="Top" relative="$parent/RaceSelectionLabel" pos="Max" offset="3"/>
            <Anchor side="Left" relative="$parent/RaceSelectionLabel" pos="Min" offset="-1"/>
            <Animation name="TournamentPortalFadeIn" template="TournamentPortalFrame/TournamentPortalFadeInTemplate"/>

            <StateGroup name="RaceSelectEnabledGroup">
                <DefaultState val="Hidden"/>
                
                <State name="Active">
                    <When type="AnimationState" frame="$parent" DefaultState="Active"/>
                    <Action type="SetProperty" visible="true"/>
                    <Action type="SetProperty" enabled="true"/>
                    <Action type="SetProperty" frame="$parent/RaceSelectionLabel" visible="true"/>
                </State>

                <State name="Current">
                    <When type="AnimationState" frame="$parent" DefaultState="Current"/>
                    <Action type="SetProperty" visible="true"/>
                    <Action type="SetProperty" enabled="false"/>
                    <Action type="SetProperty" frame="$parent/RaceSelectionLabel" visible="true"/>
                </State>

                <State name="Inactive">
                    <When type="AnimationState" frame="$parent" DefaultState="Inactive"/>
                    <Action type="SetProperty" visible="true"/>
                    <Action type="SetProperty" enabled="false"/>
