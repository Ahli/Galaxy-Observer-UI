            <Anchor side="Right" relative="$parent" pos="Max" offset="-14"/>
            <Width val="350"/>
            <Height val="50"/>
            <LoseFocusOnEnterOrEsc val="true"/>
            <Insets top="13" left="15" bottom="10" right="45"/>

