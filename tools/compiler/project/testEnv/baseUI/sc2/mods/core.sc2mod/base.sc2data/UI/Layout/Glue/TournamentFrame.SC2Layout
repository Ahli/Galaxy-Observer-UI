    <Frame type="TournamentByeIndicatorFrame" name="TournamentByeIndicatorFrameTemplate">
        <!--
        ~~~~~~~~~~~~~~~~~~~~~~~
        EVENTS
        ~~~~~~~~~~~~~~~~~~~~~~~
        The following events are being sent to this frame:

        Empty - The local player isn't in a tournament.
        Inactive - The local player doesn't currently have a bye.
        Active - The local player currently has a bye.
        -->

        <Width val="1200"/>
        <Height val="360"/>

        <Animation name="Active">
            <Event event="Active" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
        </Animation>

        <Animation name="Inactive">
            <Event event="Inactive" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>

        <Animation name="Empty">
            <Event event="Empty" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>

        <Frame type="Label" name="UpcomingTournamentMatchTitle">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-265"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="118"/>
            <Text val="@UI/ScreenTournament/YouHaveABye"/>
            <Style val="@Tournament_RoundStatusReady"/>
        </Frame>
    </Frame>

    <Frame type="TournamentEliminatedIndicatorFrame" name="TournamentEliminatedIndicatorFrameTemplate">
        <!--
        ~~~~~~~~~~~~~~~~~~~~~~~
        EVENTS
        ~~~~~~~~~~~~~~~~~~~~~~~
        The following events are being sent to this frame:

        Empty - The local player isn't in a tournament.
        Won - The local player has won the tournament.
        RunnerUp - The local player has placed second in the tournament.
        Active - The local player has been eliminated.
        Inactive - The local player hasn't been eliminated.
        -->

        <Width val="1200"/>
        <Height val="360"/>

        <Animation name="Won">
            <Event event="Won" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>

            <Controller type="Text" end="Pause" frame="StatusLabel">
                <Key type="Text" time="0" text="@UI/ScreenTournament/YouHaveWon"/>
            </Controller>

            <Controller type="Style" end="Pause" style1="Tournament_RoundStatusReady" style2="Tournament_RoundStatusReady" frame="StatusLabel">
                <Key type="Curve" time="0" value="0"/>
            </Controller>
        </Animation>

        <Animation name="RunnerUp">
            <Event event="RunnerUp" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>

            <Controller type="Text" end="Pause" frame="StatusLabel">
                <Key type="Text" time="0" text="@UI/ScreenTournament/YouPlacedSecond"/>
            </Controller>

            <Controller type="Style" end="Pause" style1="Tournament_RoundStatusReady" style2="Tournament_RoundStatusReady" frame="StatusLabel">
                <Key type="Curve" time="0" value="0"/>
            </Controller>
        </Animation>

        <Animation name="Active">
            <Event event="Active" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>

            <Controller type="Text" end="Pause" frame="StatusLabel">
                <Key type="Text" time="0" text="@UI/ScreenTournament/YouHaveBeenEliminated"/>
            </Controller>

            <Controller type="Style" end="Pause" style1="Tournament_RoundStatusEliminated" style2="Tournament_RoundStatusEliminated" frame="StatusLabel">
                <Key type="Curve" time="0" value="0"/>
            </Controller>
        </Animation>

        <Animation name="Inactive">
            <Event event="Inactive" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>

        <Animation name="Empty">
            <Event event="Empty" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>

        <Frame type="Label" name="StatusLabel">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-265"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="118"/>
            <Style val="@Tournament_RoundStatusEliminated"/>
        </Frame>
    </Frame>

