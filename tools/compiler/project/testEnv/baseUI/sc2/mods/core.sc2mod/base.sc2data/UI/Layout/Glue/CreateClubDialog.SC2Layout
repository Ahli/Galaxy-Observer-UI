            </Controller>

            <Controller type="Visibility" end="Pause" frame="$this/GroupCategoryLabel">
                <Key type="Bool" time="0" value="True"/>
                <Key type="Bool" time=".01" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="$this/GroupCategoryPulldown">
                <Key type="Bool" time="0" value="True"/>
                <Key type="Bool" time=".01" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="$this/GroupCategoryInfoTooltip">
