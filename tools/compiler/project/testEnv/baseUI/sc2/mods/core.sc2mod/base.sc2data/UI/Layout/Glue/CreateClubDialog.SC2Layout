            <Event event="ShowCreateGroup" action="DirectionReverse" frame="$this"/>
            <Event event="ShowCreateGroup" action="Play" frame="$this"/>

            <Controller type="Text" end="Pause" frame="$this/TitleLabel">
                <Key type="Text" time="0" text="@UI/Community/Group/DialogTitleCreate"/>
