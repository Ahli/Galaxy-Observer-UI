                    <When type="Property" frame="$parent" Allowed="false"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Locked"/>
                </State>

                <State name="NormalState">
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc"/>
                </State>
            </StateGroup>
             
            <StateGroup name="ListItemOwnershipState">
                <DefaultState val="NotOwnedState"/>
            
