                    <Action type="SetProperty" frame="$this" LayerColor="59,140,192" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerColor="179,225,255" index="3"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="3"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" LayerColor="5,20,38" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="230" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerColor="11,55,110" index="3"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="230" index="3"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" LayerColor="3,11,21" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="230" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerColor="12,43,82" index="3"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="230" index="3"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- Outer Border -->
        <Frame type="Image" name="BorderOuter">
            <Anchor relative="$parent/AnchorFrame"/>
            <DesaturateOnDisable val="False"/>
            <Color val="0,0,0"/>
            <LayerCount val="4"/>
            
            <Texture val="@UI/Glues_Battlenet_TabsStandard_Left_Mask" layer="0"/>
            <TextureType val="EndCap" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <AlphaChannel val="InverseAlpha" layer="0"/>
            
            <Texture val="@UI/Glues_Battlenet_TabsStandard_Left_Border_Outer" layer="1"/>
            <TextureType val="EndCap" layer="1"/>
            
            <Texture val="@UI/Glues_Battlenet_TabsStandard_Left_Mask" layer="2"/>
            <TextureType val="EndCap" layer="2"/>
            <AlphaLayer val="True" layer="2"/>
            <AlphaChannel val="InverseAlpha" layer="2"/>
            
            <Texture val="@UI/Glues_Battlenet_TabsStandard_Left_Border_Outer" layer="3"/>
            <TextureType val="EndCap" layer="3"/>
            
            <StateGroup name="CropState">
                <DefaultState val="Normal"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" ToggledState="Toggled"/>
                    <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_TabsStandard_Left_Mask" index="0"/>
                    <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_TabsStandard_Left_Mask" index="2"/>
                    <Action type="SetProperty" frame="$this" AlphaChannel="InverseAlpha" index="0"/>
                    <Action type="SetProperty" frame="$this" AlphaChannel="InverseAlpha" index="2"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Texture="Assets/Textures/White.dds" index="0"/>
                    <Action type="SetProperty" frame="$this" Texture="Assets/Textures/White.dds" index="2"/>
                    <Action type="SetProperty" frame="$this" AlphaChannel="Alpha" index="0"/>
                    <Action type="SetProperty" frame="$this" AlphaChannel="Alpha" index="2"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- Inner Border -->
        <Frame type="Image" name="BorderInner">
            <Anchor relative="$parent/AnchorFrame"/>
            <DesaturateOnDisable val="False"/>
            <Color val="36,107,169"/>
            <RenderPriority val="513"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Glues_Battlenet_TabsStandard_Left_Border_Inner" layer="0"/>
            <TextureType val="EndCap" layer="0"/>
            
            <Texture val="@UI/Glues_Battlenet_TabsStandard_Left_Border_Inner" layer="1"/>
            <TextureType val="EndCap" layer="1"/>
            
            <StateGroup name="ColorState">
                <DefaultState val="Normal"/>
                
                <State name="Disabled">
                    <When type="StateGroup" frame="$parent" DisabledState="Disabled"/>
                    <Action type="SetProperty" frame="$this" Color="29,57,86"/>
                </State>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" ToggledState="Toggled"/>
                    <Action type="SetProperty" frame="$this" Color="217,241,255"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Color="88,163,234"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Color="36,107,169"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- Border Highlight -->
        <Frame type="Image" name="BorderHighlight">
            <Anchor relative="$parent/AnchorFrame"/>
            <DesaturateOnDisable val="False"/>
            <RenderPriority val="525"/>
            <LayerCount val="2"/>
            <Color val="65,131,199"/>
            
            <Texture val="{$parent/BorderInner/@Texture[0]}" layer="0"/>
            <TextureType val="EndCap" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Highlight" layer="1"/>
            
            <StateGroup name="ColorState">
                <DefaultState val="Normal"/>
                
                <State name="Disabled">
                    <When type="StateGroup" frame="$parent" DisabledState="Disabled"/>
                    <Action type="SetProperty" frame="$this" Color="59,140,192"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="0" index="1"/>
                </State>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" ToggledState="Toggled"/>
                    <Action type="SetProperty" frame="$this" Color="255,255,255"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="1"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Color="141,208,255"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="1"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Color="65,131,199"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="1"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- Label Glow -->
        <Frame type="Image" name="LabelGlow">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <DesaturateOnDisable val="False"/>
            <RenderPriority val="511"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Glues_Battlenet_TabsStandard_Left_Mask" layer="0"/>
            <TextureType val="EndCap" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            
            <Texture val="@UI/Glues_Battlenet_StandardButtons_Small_TextGlow" layer="1"/>
            
            <StateGroup name="ColorState">
                <DefaultState val="Normal"/>
                
                <State name="Disabled">
                    <When type="StateGroup" frame="$parent" DisabledState="Disabled"/>
                    <Action type="SetProperty" frame="$this" LayerColor="255,255,255" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="0" index="1"/>
                </State>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" ToggledState="Toggled"/>
                    <Action type="SetProperty" frame="$this" LayerColor="255,255,255" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="0" index="1"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" LayerColor="19,141,255" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="1"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" LayerColor="0,138,255" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="179" index="1"/>
                </State>
            </StateGroup>
        </Frame>

        <!-- Label Shadow -->
        <Frame type="Label" name="LabelShadow">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Min" offset="1"/>
            <Anchor side="Bottom" relative="$parent/TitleLabel" pos="Max" offset="1"/>
            <Anchor side="Left" relative="$parent/TitleLabel" pos="Min" offset="1"/>
            <Anchor side="Right" relative="$parent/TitleLabel" pos="Max" offset="1"/>
            <Style val="StandardTabButton_Normal"/>
            <Text val="{$parent/Label/@Text}"/>
            <Color val="0,0,0"/>
            <Alpha val="191"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="Normal"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" ToggledState="Toggled"/>
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
            </StateGroup>
        </Frame>

        <!-- Label -->
        <Frame type="Label" name="Label">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-25"/>
            <Style val="StandardTabButton_Normal"/>
            <Alpha val="0"/>
            
            <StateGroup name="DisplayState">
                <DefaultState val="Not16x9"/>
                
                <State name="16x9">
                    <When type="StateGroup" frame="$GlueUI" AspectRatioState="16x9"/>
                    <Action type="SetAnchor" frame="$this" side="Top" relative="$parent/AnchorFrame" pos="Mid" offset="1"/>
                    <Action type="SetAnchor" frame="$this" side="Bottom" relative="$parent/AnchorFrame" pos="Mid" offset="1"/>
                </State>

                <State name="Not16x9">
                    <Action type="SetAnchor" frame="$this" side="Top" relative="$parent/AnchorFrame" pos="Mid" offset="0"/>
                    <Action type="SetAnchor" frame="$this" side="Bottom" relative="$parent/AnchorFrame" pos="Mid" offset="0"/>
                </State>
            </StateGroup>
        </Frame>

        <!-- Label -->
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/Label" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Label" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/Label" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/Label" pos="Max" offset="0"/>
            <Style val="StandardTabButton_Normal"/>
            <Text val="{$parent/Label/@Text}"/>
            
            <StateGroup name="StyleState">
                <DefaultState val="Normal"/>
                
                <State name="Disabled">
                    <When type="StateGroup" frame="$parent" DisabledState="Disabled"/>
                    <Action type="SetProperty" frame="$this" Style="StandardTabButton_Disabled"/>
                </State>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" ToggledState="Toggled"/>
                    <Action type="SetProperty" frame="$this" Style="StandardTabButton_Selected"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Style="StandardTabButton_Hovered"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Style="StandardTabButton_Normal"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>
        
        <!--
        ===============
        STATEGROUPS
        ===============
        -->
                
        <!-- ToggledState -->
        <StateGroup name="ToggledState">
            <DefaultState val="NotToggled"/>
            <State name="Toggled">
                <!-- <When type="Property" frame="$this" Toggled="True"/> -->
                <Action type="SetProperty" frame="$this" RenderPriority="515"/>
                <!-- <Action type="SetProperty" ClickSound=""/>
                <Action type="SetProperty" HoverSound=""/> -->
            </State>
            <State name="NotToggled">
                <Action type="SetProperty" frame="$this" RenderPriority="512"/>
            </State>
        </StateGroup>
                
        <!-- HoveredState -->
        <StateGroup name="HoveredState">
            <DefaultState val="NotHovered"/>
            <State name="Hovered">
                <When type="Property" frame="$this" Hovering="True"/>
            </State>
            <State name="NotHovered"/>
        </StateGroup>
                
        <!-- DisabledState -->
        <StateGroup name="DisabledState">
            <DefaultState val="NotDisabled"/>
            <State name="Disabled">
                <When type="Property" frame="$this" Enabled="False"/>
            </State>
            <State name="NotDisabled"/>
        </StateGroup>
    </Frame>
    
