    <Frame type="UserProfileSummaryCoopMasteryFrame" name="UserProfileSummaryCoopMasteryFrameTemplate">
        <PresenceId val="{$ScreenUserProfile/@PresenceId}"/>
        <BucketName val="COOP_PRESTIGE"/> <!-- This matches the value in seasons.xml -->
            
        <Frame type="Image" name="MasteryBase">
            <Anchor side="Top" relative="$parent/$parent/UserProfileSummaryCoopMutatorFrame/BountyIcon" pos="Max" offset="15"/>
            <Anchor side="Left" relative="$parent/$parent/UserProfileSummaryCoopMutatorFrame/BountyIcon" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/UserProfileSummaryCoopMutatorFrame/BountyIcon" pos="Mid" offset="0"/>
            <Texture val="@UI/SC2_UI_UserProfile_Summary_Coop_AscensionBase"/>
        </Frame>
        
        <Frame type="Frame" name="MasteryIconContainer" template="UserProfileSummaryCoopMasteryFrame/MasteryIconContainerTemplate"/>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Bottom" relative="$parent/MasteryBase" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/MasteryBase" pos="Max" offset="10"/>
            <Text val="@UI/ScreenUserProfile/SummaryCoop/MasteryLevel"/>
            <Style val="Profile_Summary_CoopMostPlayed"/>
        </Frame>

        <Frame type="Label" name="LevelLabel">
            <Anchor side="Top" relative="$parent/MasteryBase" pos="Mid" offset="-5"/>
            <Anchor side="Left" relative="$parent/MasteryBase" pos="Max" offset="10"/>
            <Text val="{$parent/@Level}"/>
            <Style val="Profile_Summary_CoopCommander"/>
            <Visible val="{$parent/TitleLabel/@Visible}"/>
        </Frame>

        <Frame type="Label" name="NoMasteryPromptLabel">
            <Anchor side="Top" relative="$parent/MasteryBase" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/MasteryBase" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/MasteryBase" pos="Max" offset="10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Text val="@UI/ScreenUserProfile/SummaryCoop/NoMasteryPrompt"/>
            <Style val="Profile_Summary_CoopMostPlayed"/>
        </Frame>
        
        <StateGroup name="MasteryState">
            <DefaultState val="NoMasteryTheirs"/>
            
            <State name="Ascension">
                <When type="Property" operator="GreaterThan" Level="90"/>
                <Action type="SetProperty" frame="TitleLabel" Text="@UI/ScreenUserProfile/SummaryCoop/AscensionLevel"/>
                <Action type="SetProperty" frame="TitleLabel" Visible="True"/>
                <Action type="SetProperty" frame="NoMasteryPromptLabel" Visible="False"/>
            </State>
            
            <State name="Mastery">
                <When type="Property" operator="GreaterThan" Level="0"/>
                <Action type="SetProperty" frame="TitleLabel" Text="@UI/ScreenUserProfile/SummaryCoop/MasteryLevel"/>
                <Action type="SetProperty" frame="TitleLabel" Visible="True"/>
                <Action type="SetProperty" frame="NoMasteryPromptLabel" Visible="False"/>
            </State>
            
            <State name="NoMasteryMine">
                <When type="Property" frame="$BattleUI" LocalToonPresenceId="{$ScreenUserProfile/@PresenceId}"/>
                <Action type="SetProperty" frame="TitleLabel" Visible="False"/>
                <Action type="SetProperty" frame="NoMasteryPromptLabel" Visible="True"/>
            </State>
            
            <State name="NoMasteryTheirs">
                <Action type="SetProperty" frame="TitleLabel" Text="@UI/ScreenUserProfile/SummaryCoop/MasteryLevel"/>
                <Action type="SetProperty" frame="TitleLabel" Visible="True"/>
                <Action type="SetProperty" frame="NoMasteryPromptLabel" Visible="False"/>
            </State>
        </StateGroup>
    </Frame>
</Desc>
