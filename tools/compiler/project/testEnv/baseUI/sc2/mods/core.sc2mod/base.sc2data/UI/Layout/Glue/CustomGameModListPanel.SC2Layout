        </Frame>
        
        <Frame type="Button" name="CloseButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Top" relative="$parent/CreateButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/CreateButton" pos="Max" offset="5"/>
            <Text val="@UI/Close"/>
            <Shortcut val="@UI/ScreenNavigationBack_Hotkey"/>
            <Width val="288"/>
        </Frame>

        <Frame type="ExpansionLevelFrame" name="ExpansionIconFrame">
            <Width val="82"/>
            <Height val="87"/>
            <Anchor side="Top" relative="$parent/CreateButton" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/CreateButton" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/CloseButton" pos="Max" offset="10"/>
            <AcceptsMouseTooltip val="true"/>

            <Frame type="Image" name="ExpansionIcon">
                <Anchor relative="$parent"/>
            </Frame>

            <StateGroup name="ExpansionStateGroup">
                <State name="StarterEditionState">
                    <When type="AnimationState" frame="$this" ExpansionLevel="ExpansionLevel0"/>
                    <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_SE"/>
                    <Action type="SetProperty" frame="$this" Tooltip=""/>
                </State>

                <State name="WoLState">
                    <When type="AnimationState" frame="$this" ExpansionLevel="ExpansionLevel1"/>
                    <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_WoL"/>
                    <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipWoL"/>
                </State>

                <State name="HotSState">
                    <When type="AnimationState" frame="$this" ExpansionLevel="ExpansionLevel2"/>
                    <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_HoTS"/>
                    <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipHotS"/>
                </State>

                <State name="LotVState">
                    <When type="AnimationState" frame="$this" ExpansionLevel="ExpansionLevel3"/>
                    <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_LotV"/>
                    <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipLotV"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="BattleMapContextMenu" name="ItemContextMenu" template="StandardTemplates/StandardContextMenuTemplate">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <ShowMapInfo val="true"/>
            <ModHandle val="{$parent/@SelectedMapHandle}"/>
        </Frame>

        <StateGroup name="FilterStateGroup">
            <DefaultState val="FilterDefaultState"/>

            <State name="SearchingState">
                <When type="AnimationState" frame="$this" SearchState="Searching"/>
                <Action type="SetProperty" frame="BookmarkCountFrame" visible="false"/>
            </State>
            
            <State name="FilterBlizzardModsState">
                <When type="AnimationState" frame="$this" FilterState="BlizzardMods"/>
                <Action type="SetProperty" frame="BookmarkCountFrame" visible="false"/>
            </State>

            <State name="FilterMyBookmarksState">
                <When type="AnimationState" frame="$this" FilterState="MyBookmarks"/>
                <Action type="SetProperty" frame="BookmarkCountFrame" visible="true"/>
            </State>

            <State name="FilterDefaultState">
                <Action type="SetProperty" frame="BookmarkCountFrame" visible="false"/>
            </State>
        </StateGroup>

        <StateGroup name="LoadingStateGroup">
            <DefaultState val="NotEmptyState"/>
            
            <State name="LoadingState">
                <When type="AnimationState" frame="$this" GameList="Loading"/>
                <Action type="SetProperty" frame="Loading" visible="true"/>
                <Action type="SetProperty" frame="ListEmpty" visible="false"/>
            </State>
            
            <State name="EmptyState">
                <When type="AnimationState" frame="$this" GameList="Empty"/>
                <Action type="SetProperty" frame="Loading" visible="false"/>
                <Action type="SetProperty" frame="ListEmpty" visible="true"/>
            </State>

            <State name="EmptyFilterState">
                <When type="AnimationState" frame="$this" GameList="EmptyFilter"/>
                <Action type="SetProperty" frame="Loading" visible="false"/>
                <Action type="SetProperty" frame="ListEmpty" visible="true"/>
            </State>

            <State name="NotEmptyState">
                <When type="AnimationState" frame="$this" GameList="NotEmpty"/>
                <Action type="SetProperty" frame="Loading" visible="false"/>
                <Action type="SetProperty" frame="ListEmpty" visible="false"/>
            </State>
        </StateGroup>
    </Frame>
</Desc>
