                    <Controller name="FadeIn" type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" in="Auto"/>
                        <Key type="Curve" time="0.3" value="0" in="Auto"/>
                        <Key type="Curve" time="0.52" value="255" out="Auto"/>
                    </Controller>
                    
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-10" out="Linear"/>
                        <Key type="Curve" time="0.3" value="-10" out="Linear"/>
                        <Key type="Curve" time="0.52" value="0" out="Linear"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Button" name="PortraitsButton" template="ScreenCollection/ScreenCollectionButtonTemplate">
                <Anchor side="Top" relative="$parent/EmojisButton" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/EmojisButton" pos="Max" offset="#ButtonOffsetLeft"/>
                <Text val="@UI/ScreenCollection/PortraitsButton"/>

                <Animation name="FadeNav">
                    <Event event="OnShown" action="Reset"/>
                    <Event event="OnShown" action="Play"/>

                    <Controller name="FadeIn" type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" in="Auto"/>
                        <Key type="Curve" time="0.3" value="0" in="Auto"/>
                        <Key type="Curve" time="0.55" value="255" out="Auto"/>
                    </Controller>
                    
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-10" out="Linear"/>
                        <Key type="Curve" time="0.3" value="-10" out="Linear"/>
                        <Key type="Curve" time="0.55" value="0" out="Linear"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Button" name="DecalsButton" template="ScreenCollection/ScreenCollectionButtonTemplate">
                <Anchor side="Top" relative="$parent/PortraitsButton" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/PortraitsButton" pos="Max" offset="#ButtonOffsetLeft"/>
