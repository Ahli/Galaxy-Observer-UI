            <Frame type="SkinPackUnitSelectionFrame" name="SkinFrame" template="SkinPackUnitSelectionFrame/SkinPackUnitSelectionFrameTemplate">
                <Anchor relative="$parent"/>
            </Frame>

            <Frame type="VoicePackSelectionFrame" name="VoicePackFrame" template="VoicePackSelectionFrame/VoicePackSelectionFrameTemplate">
                <Anchor relative="$parent"/>
            </Frame>

            <Frame type="EmoticonPackSelectionFrame" name="EmojiFrame" template="EmoticonPackSelectionFrame/EmoticonPackSelectionFrameTemplate">
                <Anchor relative="$parent"/>
            </Frame>

            <Frame type="UnlockablePanel" name="PortraitFrame" template="UnlockablePanel/PortraitSelectionPanelTemplate">
                <Anchor relative="$parent"/>
                
                <Frame type="Label" name="TitleLabel">
                    <Text val="@UI/ScreenCollection/Portraits/Title"/>
                </Frame>
            </Frame>

            <Frame type="UnlockablePanel" name="DecalFrame" template="UnlockablePanel/UnlockableSelectionPanelTemplate">
                <Anchor relative="$parent"/>
                
                <Frame type="Label" name="TitleLabel">
                    <Text val="@UI/ScreenCollection/Decals/Title"/>
                </Frame>
                
                <Frame type="Frame" name="DecalsActive">
                    <Visible val="True"/>
                </Frame>

                <Frame type="EditBox" name="FilterEditBox" template="StandardBattlenetTemplates/BattlenetEditBoxTemplate">
                    <Frame type="Label" name="SearchTextLabel">
                        <Text val="@UI/UnlockablePanel/SearchDecals"/>
                    </Frame>
                </Frame>

                <Frame type="RaceTabControl" name="RaceTabControl" template="CollectionTemplates/RaceTabControlTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="162"/>
                    <Anchor side="Left" relative="$parent/UnlockableTileList" pos="Min" offset="3"/>
                    <Anchor side="Bottom" relative="$this/RaceButton2" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$this/RaceButton2" pos="Max" offset="0"/>
                </Frame>
                
                <Frame type="Frame" name="InfoContainer">
                
                    <Frame type="Label" name="SprayHeader">
                        <Anchor side="Left" relative="$parent" pos="Min" offset="38"/>
                        <Anchor side="Bottom" relative="$parent/SprayInfo" pos="Min" offset="-2"/>
                        <Style val="@Collection_ItemFrame_AchievementInfo_Unearned"/>
                        <Text val="@UI/ScreenCollection/Decals/SprayHeader"/>
                    </Frame>
                    
                    <Frame type="Label" name="SprayInfo">
                        <Anchor side="Left" relative="$parent" pos="Min" offset="38"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="-35"/>
                        <Anchor side="Bottom" relative="$parent" pos="Max" offset="-35"/>
                        <Style val="@Collection_TooltipAchievementText"/>
                        <Text val="@UI/ScreenCollection/Decals/SprayInfo"/>
                    </Frame>
                </Frame>

                <StateGroup name="RaceStateGroup">
                    <State name="TerranState">
                        <When type="Property" frame="RaceTabControl" SelectedRace="Terr"/>
                        <Action type="SetProperty" frame="$this" UnlockableTag="TCAL"/>
                    </State>

                    <State name="ZergState">
                        <When type="Property" frame="RaceTabControl" SelectedRace="Zerg"/>
                        <Action type="SetProperty" frame="$this" UnlockableTag="ZCAL"/>
                    </State>

                    <State name="ProtossState">
                        <When type="Property" frame="RaceTabControl" SelectedRace="Prot"/>
                        <Action type="SetProperty" frame="$this" UnlockableTag="PCAL"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="UnlockablePanel" name="AnimationFrame" template="UnlockablePanel/UnlockableNoSelectionPanelTemplate">
                <Anchor relative="$parent"/>
                <UnlockableTag val="Anim"/>

                <Frame type="EditBox" name="FilterEditBox">
                    <Frame type="Label" name="SearchTextLabel">
                        <Text val="@UI/UnlockablePanel/SearchAnimations"/>
                    </Frame>
                </Frame>
                
                <Frame type="Label" name="TitleLabel">
                    <Text val="@UI/ScreenCollection/Animations/Title"/>
                </Frame>
                
                <Frame type="Frame" name="AnimationsActive">
                    <Visible val="True"/>
                </Frame>
            </Frame>

