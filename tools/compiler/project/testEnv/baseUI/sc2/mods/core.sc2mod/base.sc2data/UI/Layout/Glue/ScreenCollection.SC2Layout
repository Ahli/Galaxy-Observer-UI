                <Anchor side="Top" relative="$parent" pos="Min" offset="16"/>
                <Anchor side="Right" relative="$parent/PortraitsButton" pos="Max" offset="#ButtonOffset"/>
            </Frame>

            <Frame type="Image" name="ButtonDivider5" template="ScreenCollection/ButtonDividerTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="16"/>
                <Anchor side="Right" relative="$parent/DecalsButton" pos="Max" offset="#ButtonOffset"/>
            </Frame>

