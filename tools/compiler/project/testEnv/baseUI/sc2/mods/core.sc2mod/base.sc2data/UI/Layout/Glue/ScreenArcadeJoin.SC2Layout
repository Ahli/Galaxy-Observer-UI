            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="Info">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="GamesTileList">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="GameInfoPanel">
                <Key type="Bool" time="0" value="True"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="ListEmptyFilter">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="ListEmpty">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="Loading">
