        </Animation>

        <Animation name="SlideDown">
            <Event event="OnMouseExit" action="Reset"/>
            <Event event="OnMouseExit" action="Play"/>

            <Controller type="Event" end="Stop" frame="$parent">
                <Key type="Event" time="0.0" event="SlideDownBig"/>
            </Controller>
        </Animation>
    </Frame>

