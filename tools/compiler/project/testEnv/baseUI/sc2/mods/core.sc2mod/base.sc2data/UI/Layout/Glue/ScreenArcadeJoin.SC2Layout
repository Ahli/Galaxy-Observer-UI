            <MapHandle val="{$parent/@SelectedMapHandle}"/>
        </Frame>
        
        <!-- 

        JOIN SCREEN STATE ANIMATIONS
        
        -->
        
        <!-- Loading -->
        
        <Animation name="ListLoadingPrep">
            <Event event="Loading" action="Reset,Play"/>
            
            <Controller type="Visibility" end="Pause" frame="ModeHideFrame4x3/Mode">
                <Key type="Bool" time="0" value="False"/>
