            <Event event="OnMouseExit" action="DirectionReverse"/>
            <Event event="OnMouseExit" action="Play"/>

            <Controller type="Dimension" dimension="Height" relative="true" percentage="false" end="Pause">
                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                <Key type="Curve" time="#TileListItemAnimationTime" value="#TileListItemGrowHeight" out="Auto"/>
            </Controller>
        </Animation>

        <Animation name="SlideUp">
            <Event event="OnMouseEnter" action="Reset"/>
            <Event event="OnMouseEnter" action="Play"/>

            <Controller type="Event" end="Stop" frame="$parent">
                <Key type="Event" time="0.0" event="SlideUpBig"/>
