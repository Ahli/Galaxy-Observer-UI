        </Animation>

        <!-- Empty Filter -->

        <Animation name="ListEmptyFilterPrep">
            <Event event="EmptyFilter" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="ModeHideFrame4x3/Mode">
