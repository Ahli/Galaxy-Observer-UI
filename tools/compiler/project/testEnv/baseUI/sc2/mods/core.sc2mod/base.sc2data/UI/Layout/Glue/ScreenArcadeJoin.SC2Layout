    <Frame type="ArcadeGameTileListItem" name="ArcadeGameTileListItemTopTemplate" template="ScreenArcadeJoin/ArcadeGameTileListItemTemplate">
        <Animation name="Grow">
            <Event event="OnMouseEnter" action="DirectionForward"/>
            <Event event="OnMouseEnter" action="Play"/>
            <Event event="OnMouseExit" action="DirectionReverse"/>
            <Event event="OnMouseExit" action="Play"/>

            <Controller type="Dimension" dimension="Height" relative="true" percentage="false" end="Pause">
                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                <Key type="Curve" time="#TileListItemAnimationTime" value="#TileListItemGrowHeight" out="Auto"/>
            </Controller>
        </Animation>
        
        <Animation name="Slide">
            <Event event="SlideUp" action="DirectionForward" frame="$parent"/>
            <Event event="SlideUp" action="Play" frame="$parent"/>
            <Event event="SlideDown" action="DirectionReverse" frame="$parent"/>
            <Event event="SlideDown" action="Play" frame="$parent"/>

            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                <Key type="Curve" time="#TileListItemAnimationTime" value="#TileListItemMoveHeight" out="Auto"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="ArcadeGameTileListItem" name="ArcadeGameTileListItemMidTemplate" template="ScreenArcadeJoin/ArcadeGameTileListItemTemplate">
        <Animation name="Grow">
            <Event event="OnMouseEnter" action="DirectionForward"/>
            <Event event="OnMouseEnter" action="Play"/>
            <Event event="OnMouseExit" action="DirectionReverse"/>
            <Event event="OnMouseExit" action="Play"/>

            <Controller type="Dimension" dimension="Height" relative="true" percentage="false" end="Pause">
                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                <Key type="Curve" time="#TileListItemAnimationTime" value="#TileListItemGrowHeight" out="Auto"/>
            </Controller>
        </Animation>
        
        <Animation name="SlideUp">
            <Event event="OnMouseEnter" action="Reset"/>
            <Event event="OnMouseEnter" action="Play"/>

            <Controller type="Event" end="Stop" frame="$parent">
                <Key type="Event" time="0.0" event="SlideUp"/>
            </Controller>
        </Animation>

        <Animation name="SlideDown">
            <Event event="OnMouseExit" action="Reset"/>
            <Event event="OnMouseExit" action="Play"/>

            <Controller type="Event" end="Stop" frame="$parent">
                <Key type="Event" time="0.0" event="SlideDown"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="ArcadeGameTileListItem" name="ArcadeGameTileListItemBottomTemplate" template="ScreenArcadeJoin/ArcadeGameTileListItemTemplate">
        <Animation name="Grow">
            <Event event="OnMouseEnter" action="DirectionForward"/>
