            </Controller>
        </Animation>
        
        <!-- Not Empty -->
        
        <Animation name="ListNotEmptyPrep">
            <Event event="NotEmpty" action="Reset,Play"/>
            
            <Controller type="Visibility" end="Pause" frame="ModeHideFrame4x3/Mode">
