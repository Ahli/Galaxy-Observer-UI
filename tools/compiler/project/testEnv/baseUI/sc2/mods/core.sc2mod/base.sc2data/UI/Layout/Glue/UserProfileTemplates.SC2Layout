            <Anchor side="Left" relative="$parent" pos="Min" offset="11"/>
            <Texture val="@UI/SC2_UI_UserProfile_Achievements_Showcase_EditIcon"/>
        </Frame>
        
        <StateGroup name="HoveringState">
