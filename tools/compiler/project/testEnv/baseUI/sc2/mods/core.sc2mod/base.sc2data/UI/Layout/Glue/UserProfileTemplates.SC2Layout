    <!-- 
    =============================
    
    VERTICAL TAB TEXT TREE VIEW ITEM TEMPLATE
    
    =============================
    -->
    <Frame type="TextTreeViewItem" name="VerticalTabTextTreeViewTemplate" template="UserProfileTemplates/VerticalTabArrowControlTemplate">
        <HookupAlias original="ItemLabel" alias="Label"/>
        
        <!-- Styling set by the SelectionState was being reset as the TextTree was redrawn, visibility isn't reset so we're using a separate label with a different style -->
        <Frame type="Label" name="HoverLabel">
            <Anchor relative="$parent/Label"/>
            <Style val="@StandardBattlenetVerticalTabButton_Selected"/>
            <Options val="ScaleToPreventTruncation"/>
            <Text val="{$parent/Label/@Text}"/>
        </Frame>
        
        <StateGroup name="SelectionState">
            <DefaultState val="Normal"/>
            
            <State name="Selected">
                <When type="AnimationState" SelectedState="Selected"/>
                <Action type="SetProperty" frame="NormalImage" visible="False"/>
                <Action type="SetProperty" frame="HoverImage" visible="True"/>
                <Action type="SetProperty" frame="HoverImage" StateIndex="2"/>
                <Action type="SetProperty" frame="Label" visible="False"/>
                <Action type="SetProperty" frame="HoverLabel" visible="True"/>
                <!-- <Action type="SetProperty" frame="Label" style="StandardBattlenetVerticalTabButton_Selected"/> -->
            </State>
            
            <State name="Pressed">
                <When type="Property" Pushed="True"/>
                <Action type="SetProperty" frame="NormalImage" visible="False"/>
                <Action type="SetProperty" frame="HoverImage" visible="True"/>
                <Action type="SetProperty" frame="HoverImage" StateIndex="1"/>
                <Action type="SetProperty" frame="Label" visible="False"/>
                <Action type="SetProperty" frame="HoverLabel" visible="True"/>
                <!-- <Action type="SetProperty" frame="Label" style="StandardBattlenetVerticalTabButton_Selected"/> -->
            </State>
            
            <State name="Hover">
                <When type="Property" Hovering="True"/>
                <Action type="SetProperty" frame="NormalImage" visible="False"/>
                <Action type="SetProperty" frame="HoverImage" visible="True"/>
                <Action type="SetProperty" frame="HoverImage" StateIndex="0"/>
                <Action type="SetProperty" frame="Label" visible="False"/>
                <Action type="SetProperty" frame="HoverLabel" visible="True"/>
                <Action type="PlaySound" Sound="@UI_HoverTick"/>
                <!-- <Action type="SetProperty" frame="Label" style="StandardBattlenetVerticalTabButton_Selected"/> -->
            </State>
            
            <State name="Normal">
                <Action type="SetProperty" frame="NormalImage" visible="True"/>
                <Action type="SetProperty" frame="HoverImage" visible="False"/>
                <Action type="SetProperty" frame="NormalImage" StateIndex="0"/>
                <Action type="SetProperty" frame="Label" visible="True"/>
                <Action type="SetProperty" frame="HoverLabel" visible="False"/>
                <!-- <Action type="SetProperty" frame="Label" style="StandardBattlenetVerticalTabButton"/> -->
            </State>
        </StateGroup>
        
        <StateGroup name="ExpansionState">
            <DefaultState val="Collapsed"/>
            
            <State name="NoChildren">
                <When type="AnimationState" ChildrenState="NoChildren"/>
                <Action type="SetProperty" frame="Arrow" visible="False"/>
            </State>
            
            <State name="Expanded">
                <When type="AnimationState" ExpandedState="Expanded"/>
                <Action type="SetProperty" frame="Arrow" visible="True"/>
                <Action type="SetProperty" frame="Arrow" rotation="270"/>
            </State>
            
            <State name="Collapsed">
                <Action type="SetProperty" frame="Arrow" visible="True"/>
                <Action type="SetProperty" frame="Arrow" rotation="180"/>
            </State>
        </StateGroup>
        
        <StateGroup name="ScrollBarState">
            <DefaultState val="NoScrollBar"/>
            
            <State name="ScrollBar">
                <When type="Property" frame="$ancestor[@type=TreeView]/ScrollBar" Visible="True"/>
                <Action type="SetAnchor" frame="HitTestFrame" side="Right" relative="$parent" pos="Max" offset="-6"/>
            </State>
            
            <State name="NoScrollBar">
                <Action type="SetAnchor" frame="HitTestFrame" side="Right" relative="$parent" pos="Max" offset="0"/>
            </State>
        </StateGroup>
        
        <!--
        <StateGroup name="ClickSoundState">
            <DefaultState val="DoNotPlay"/>
            
            <State name="PlayExpand">
                <When type="AnimationState" SelectedState="Selected"/>
                <When type="AnimationState" ChildrenState="HasChildren"/>
                <When type="AnimationState" ExpandedState="Expanded"/>
                <Action type="PlaySound" Sound="@UI_FileSelectionSelect"/>
            </State>
            
            <State name="PlayCollapse">
                <When type="AnimationState" SelectedState="Selected"/>
                <When type="AnimationState" ChildrenState="HasChildren"/>
                <When type="AnimationState" ExpandedState="Collapsed"/>
                <Action type="PlaySound" Sound="@UI_FileSelectionSelect"/>
            </State>
            
            <State name="PlaySelected">
                <When type="AnimationState" SelectedState="Selected"/>
                <Action type="PlaySound" Sound="@UI_FileSelectionSelect"/>
            </State>
            
            <State name="DoNotPlay"/>
        </StateGroup>
        -->
    </Frame>
    
    <!-- 
    =============================
    
    SCROLLBAR BACKGROUND TEMPLATE
    
    =============================
    -->
    <Frame type="Image" name="ScrollBarBackgroundTemplate">
        <Anchor side="Top" relative="$parent/ScrollBar" pos="Min" offset="-4"/>
        <Anchor side="Bottom" relative="$parent/ScrollBar" pos="Max" offset="4"/>
        <Anchor side="Left" relative="$parent/ScrollBar" pos="Min" offset="-4"/>
        <Anchor side="Right" relative="$parent/ScrollBar" pos="Max" offset="4"/>
        <Texture val="@UI/SC2_UI_UserProfile_ScrollBar_Background"/>
        <TextureType val="NineSlice"/>
        <Alpha val="255"/>
            
        <StateGroup name="AlphaState">
            <DefaultState val="NotVisible"/>
        
            <State name="Visible">
                <When type="Property" frame="$parent/ScrollBar" Visible="True"/>
                <Action type="SetProperty" frame="$this" Alpha="255"/>
            </State>
        
            <State name="NotVisible">
                <Action type="SetProperty" frame="$this" Alpha="100"/>
            </State>
        </StateGroup>
    </Frame>
    
    <!-- 
    =============================
    
    SCROLLBAR HIDDEN BACKGROUND TEMPLATE
    
    =============================
    -->
    <Frame type="Image" name="ScrollBarHiddenBackgroundTemplate" template="UserProfileTemplates/ScrollBarBackgroundTemplate">
        <StateGroup name="AlphaState">
            <State name="NotVisible">
                <Action type="SetProperty" frame="$this" Alpha="0"/>
            </State>
        </StateGroup>
    </Frame>
    
    <!-- 
    =============================
    
    SCROLLBAR TEMPLATE
    
    =============================
    -->
    <Frame type="ScrollBar" name="ScrollBarTemplate">
        <Width val="12"/>
        <Orientation val="Vertical"/>
        <PreserveAnchorOffset val="True"/>

        <Frame type="Button" name="Button">
            <NormalImage val="NormalImage"/>
            <HoverImage val="HoverImage"/>

            <Frame type="Image" name="NormalImage">
                <Anchor relative="$parent"/>
                <LayerCount val="2"/>
                
                <Texture val="@UI/SC2_UI_UserProfile_ScrollBar_Normal" layer="0"/>
                <Texture val="@UI/SC2_UI_UserProfile_ScrollBar_Normal" layer="1"/>
                <TextureType val="NineSlice" layer="0"/>
                <TextureType val="NineSlice" layer="1"/>
            </Frame>

            <Frame type="Image" name="HoverImage">
                <Anchor relative="$parent"/>
                <Texture val="@UI/SC2_UI_UserProfile_ScrollBar_Hover"/>
                <TextureType val="NineSlice"/>
            </Frame>
        </Frame>
    </Frame>
    
    <!-- 
    =============================
    
    CONFIG BUTTON TEMPLATE
    
    =============================
    -->
    <Frame type="Button" name="ConfigButtonTemplate">
        <Width val="101"/>
        <Height val="75"/>
        
        <Frame type="Image" name="ButtonImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_UserProfile_Ladders_Showcase_CustomizeButton_Normal"/>
        </Frame>
        
        <StateGroup name="ButtonState">
            <DefaultState val="Normal"/>
            
            <State name="Disabled">
                <When type="Property" Enabled="False"/>
                <Action type="SetProperty" frame="ButtonImage" Texture="@UI/SC2_UI_UserProfile_Ladders_Showcase_CustomizeButton_Disabled"/>
            </State>
            
            <State name="Pressed">
                <When type="Property" Pushed="True"/>
                <Action type="SetProperty" frame="ButtonImage" Texture="@UI/SC2_UI_UserProfile_Ladders_Showcase_CustomizeButton_Down"/>
            </State>
            
            <State name="Hovered">
                <When type="Property" Hovering="True"/>
                <Action type="SetProperty" frame="ButtonImage" Texture="@UI/SC2_UI_UserProfile_Ladders_Showcase_CustomizeButton_Over"/>
            </State>
            
            <State name="Normal">
                <Action type="SetProperty" frame="ButtonImage" Texture="@UI/SC2_UI_UserProfile_Ladders_Showcase_CustomizeButton_Normal"/>
            </State>
        </StateGroup>
    </Frame>
</Desc>
