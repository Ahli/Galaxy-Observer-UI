        <Height val="35"/>
        
        <Frame type="Image" name="Border">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_UserProfile_Achievements_Showcase_EditFrame"/>
            <TextureType val="NineSlice"/>
            <Color val="180,180,180"/>
        </Frame>

        <Frame type="Image" name="Icon">
