				<Frame type="Image" name="BackgroundImage">
					<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
					<Anchor side="Left" relative="$parent" pos="Mid" offset="-300"/>
					<Anchor side="Right" relative="$parent" pos="Mid" offset="300"/>
					<Anchor side="Bottom" relative="$parent\LeftPlayerUnitsLostFrame" pos="Min" offset="-10"/>
					<Unclipped val="true"/>
					<Texture val="color_accents/swatch_black.dds"/>
					<TextureType val="Border"/>
					<LayerColor val="#BackGroundBorderColor"/>
					<Alpha val="191"/>
					<AcceptsMouse val="false"/>
					<IgnoreParentColor val="true"/>
					<RenderPriority val="100"/>
				</Frame>

				<!-- Player Slots -->
				<Frame type="PlayerOwnedUnitsFrame" name="PlayerStructuresLostFrame1" template="_unitsLost/PlayerStructuresLostFrameTemplate">
					<Anchor side="Top" relative="$parent" pos="Min" offset="50"/>
				</Frame>
				<Frame type="PlayerOwnedUnitsFrame" name="PlayerStructuresLostFrame2" template="_unitsLost/PlayerStructuresLostFrameTemplate">
					<Anchor side="Top" relative="$parent/PlayerStructuresLostFrame1" pos="Max" offset="0"/>
				</Frame>
				<Frame type="PlayerOwnedUnitsFrame" name="PlayerStructuresLostFrame3" template="_unitsLost/PlayerStructuresLostFrameTemplate">
					<Anchor side="Top" relative="$parent/PlayerStructuresLostFrame2" pos="Max" offset="0"/>
				</Frame>
				<Frame type="PlayerOwnedUnitsFrame" name="PlayerStructuresLostFrame4" template="_unitsLost/PlayerStructuresLostFrameTemplate">
					<Anchor side="Top" relative="$parent/PlayerStructuresLostFrame3" pos="Max" offset="0"/>
				</Frame>
				
			</Frame>
        
		</Frame>
		
		
    </Frame>

