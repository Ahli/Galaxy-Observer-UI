<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <Frame type="Frame" name="LeoricTraitFrame">
        <Height val="200"/>
        <Width val="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
        <Anchor side="Right" relative="LeoricTraitHeaderLabel" pos="Max" offset="0"/>
        <AcceptsMouse val="False"/>

        <Frame type="Image" name="LeoricTraitImage">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Texture val="@UI/Storm_UI_HUD_HeroMechanic_Leoric_CheatedIcon"/>
        </Frame>
        
        <Frame type="Image" name="LeoricTraitWhiteImage">
            <Anchor relative="$parent/LeoricTraitImage"/>
            <BlendMode val="Add"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Storm_UI_HUD_HeroMechanic_Leoric_CheatedIcon" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            
            <Color val="ffffff" layer="1"/>
            <Alpha val="0"/>
            <Animation name="Pulse">
                <Event event="TriggerEvent" action="Restart" frame="$parent"/>
                <Event event="TriggerEvent" action="Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" out="Fast"/>
                    <Key type="Curve" time=".4" value="0" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="LeoricTraitFlashImage">
            <Anchor side="Top" relative="$parent/LeoricTraitImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LeoricTraitImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/LeoricTraitImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/LeoricTraitImage" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_HUD_HeroMechanic_Leoric_CheatedIcon"/>
            <Unclipped val="True"/>
            <BlendMode val="Add"/>
            <Alpha val="0"/>
            <Animation name="Pulse">
                <Event event="TriggerEvent" action="Restart" frame="$parent"/>
                <Event event="TriggerEvent" action="Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" out="Fast"/>
                    <Key type="Curve" time=".4" value="0" in="Slow"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause" >
                    <Key type="Curve" time="0" value="100" out="Fast"/>
                    <Key type="Curve" time=".2" value="170" out="Slow"/>
                    <Key type="Curve" time=".4" value="220" in="Fast"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause" >
                    <Key type="Curve" time="0" value="100" out="Fast"/>
                    <Key type="Curve" time=".2" value="170" out="Slow"/>
                    <Key type="Curve" time=".4" value="220" in="Fast"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Label" name="LeoricTraitHeaderLabel">
            <Anchor side="Bottom" relative="$parent/LeoricTraitImage" pos="Mid" offset="7"/>
            <Anchor side="Left" relative="$parent/LeoricTraitImage" pos="Max" offset="0"/>
            <Width val="175"/>
            <Height val="0"/>
            <Style val="Storm_HeroSpecificTitleLabel"/>
            <Text val="@UI/Leoric/CheatedDeath"/>
        </Frame>

        <Frame type="Label" name="LeoricTraitLabel">
            <Anchor side="Top" relative="$parent/LeoricTraitImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LeoricTraitImage" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/LeoricTraitHeaderLabel" pos="Max" offset="0"/>
            <Width val="0"/>
            <Height val="100"/>
            <Style val="Storm_HeroSpecificValueLabel"/>
            <Text val=""/>
        </Frame>

        <Animation name="Show">
            <Event event="OnShown" action="Restart"/>
            <Event event="OnShown" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time="1" value="255" in="Slow"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/DeathStunOverrideIcon">
                <Key type="Bool" time="0.0" value="false"/>
                <Key type="Bool" time=".01" value="true"/>
            </Controller>
            <Controller type="Texture" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/DeathStunOverrideIcon">
                <Key type="Image" time="0" image="@UI/Storm_UI_HUD_HeroMechanic_Leoric_DeathStunIcon"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="False" percentage="False" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/DeathStunOverrideIcon">
                <Key type="Curve" time="0" value="100"/>
                <Key type="Curve" time=".3" value="132"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="False" percentage="False" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/DeathStunOverrideIcon">
                <Key type="Curve" time="0" value="111"/>
                <Key type="Curve" time=".3" value="184"/>
            </Controller>
            <Controller type="Texture" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/UnitStatusDurationBarRight">
                <Key type="Image" time=".01" image="@UI/Storm_UI_hud_bar_deathstun_ghost_right"/>
            </Controller>
            <Controller type="Texture" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/UnitStatusDurationBarLeft">
                <Key type="Image" time=".01" image="@UI/Storm_UI_hud_bar_deathstun_ghost"/>
            </Controller>
        </Animation>

        <Animation name="Hide">
            <Event event="Hide" action="Restart"/>
            <Event event="Hide" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="255" out="Slow"/>
                <Key type="Curve" time="1" value="0" in="Slow"/>
            </Controller>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0.0" value="true"/>
                <Key type="Bool" time="1.0" value="false"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/DeathStunOverrideIcon">
                <Key type="Bool" time="0.0" value="true"/>
                <Key type="Bool" time=".01" value="false"/>
            </Controller>
            <Controller type="Texture" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/UnitStatusDurationBarRight">
                <Key type="Image" time=".01" image="@UI/HUD_Bar_DeathStun_Right"/>
            </Controller>
            <Controller type="Texture" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/UnitStatusDurationBarLeft">
                <Key type="Image" time=".01" image="@UI/HUD_Bar_DeathStun"/>
            </Controller>
        </Animation>

        <Animation name="SwapHide">
            <Event event="SwapHide" action="Restart, Play"/>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" in="Slow"/>
            </Controller>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/DeathStunOverrideIcon">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
            <Controller type="Texture" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/UnitStatusDurationBarRight">
                <Key type="Image" time="0" image="@UI/HUD_Bar_DeathStun_Right"/>
            </Controller>
            <Controller type="Texture" end="Pause" frame="$parent/$parent/HeroDeathStunFrame/UnitStatusDurationBarLeft">
                <Key type="Image" time="0" image="@UI/HUD_Bar_DeathStun"/>
            </Controller>
        </Animation>
    </Frame>

</Desc>
