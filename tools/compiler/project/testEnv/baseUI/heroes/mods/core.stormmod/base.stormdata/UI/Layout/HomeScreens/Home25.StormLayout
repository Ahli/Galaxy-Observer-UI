        <Frame type="Image" name="GlowBaseImage">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Min" offset="960"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Min" offset="288"/>
            <Width val="1608"/>
            <Height val="240"/>
