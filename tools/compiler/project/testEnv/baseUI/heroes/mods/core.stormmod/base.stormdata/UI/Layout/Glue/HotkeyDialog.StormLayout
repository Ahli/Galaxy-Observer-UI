            <Anchor relative="$parent"/>

            <Animation name="DialogAnimation">
                <Event event="OnShown" action="Restart" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
                <Event event="OnHidden" action="DirectionReverse" frame="$parent"/>
                <Event event="OnHidden" action="Play" frame="$parent"/>
                <Controller type="Blur" end="Pause">
                    <Key type="Curve" time="0" value="0.0" out="Fast"/>
                    <Key type="Curve" time="0.2" value="1.0" in="Slow"/>
                </Controller>
                <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$parent/BackgroundFrameAnchor">
                    <Key type="Curve" time="0" value="600" out="Fast"/>
                    <Key type="Curve" time=".1" value="0" in="Slow"/>
                    <Key type="Curve" time=".2" value="-40" in="Slow" out="Slow"/>
                    <Key type="Curve" time=".3" value="0" in="Fast"/>
                </Controller>
            </Animation>
        </Frame>
        
