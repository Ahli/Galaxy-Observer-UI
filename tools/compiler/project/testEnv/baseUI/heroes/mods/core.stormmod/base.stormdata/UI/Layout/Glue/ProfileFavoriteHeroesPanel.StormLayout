            <Anchor side="Left" relative="$parent/Item28" pos="Max" offset="#FavoriteHeroesHorizontalSpace"/>
        </Frame>

        <Frame type="HeroCollectionTileListItem" name="Item30" template="ProfileFavoriteHeroesPanel/FavoriteTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item29" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item29" pos="Max" offset="#FavoriteHeroesHorizontalSpace"/>
        </Frame>

        <Frame type="HeroCollectionTileListItem" name="Item31" template="ProfileFavoriteHeroesPanel/FavoriteTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item30" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item30" pos="Max" offset="#FavoriteHeroesHorizontalSpace"/>
        </Frame>

        <!-- End Hero List Items -->

        <Frame type="PageSelectorButtonList" name="PageSelector" template="StandardTemplates/PageSelectorButtonListTemplate">
            <Anchor relative="$parent"/>
            
            <Frame type="Button" name="DecrementButton" template="StandardTemplates/StandardFloatingArrowButtonTemplate_Left">
                <Anchor side="Top" relative="$parent/$parent/Item8" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/$parent/Item8" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <ClickSound val="@UI_BNet_Decrement"/>
            </Frame>

            <Frame type="Button" name="IncrementButton" template="StandardTemplates/StandardFloatingArrowButtonTemplate_Right">
                <Anchor side="Top" relative="$parent/DecrementButton" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <ClickSound val="@UI_BNet_Increment"/>
            </Frame>
            
            <Frame type="Button" name="PageButton0" template="StandardTemplates/StandardPipButtonTemplate">
                <Anchor side="Top" relative="$this" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-30"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="-50"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="-50"/>
                <CollapseLayout val="true"/>
            </Frame>
        </Frame>
    </Frame>

    <!--=======================================================================================
    <! Panel
    <!======================================================================================-->
    <Frame type="ProfileFavoriteHeroesPanel" name="ProfileFavoriteHeroesPanelTemplate">

        <StateGroup name="LocalUserState">
            <DefaultState val="LocalUser"/>

            <State name="NonLocalUser">
                <When type="AnimationState" localUserState="NonLocalUser"/>
                
                <Action type="SetProperty" frame="FavoriteHeroTileList" visible="false" />
                <Action type="SetProperty" frame="HeaderUnderlineImage" visible="false"/>
                <Action type="SetProperty" frame="DropArea" visible="false"/>
                <Action type="SendEvent" frame="HeroTileList" event="Up"/>
                <Action type="SetProperty" frame="HeroTileList" maxItemPerPage="32"/>
            </State>

            <State name="LocalUser">
                <Action type="SetProperty" frame="FavoriteHeroTileList" visible="true" />
                <Action type="SetProperty" frame="HeaderUnderlineImage" visible="true"/>
                <Action type="SetProperty" frame="DropArea" visible="true"/>
                <Action type="SendEvent" frame="HeroTileList" event="Down"/>
                <Action type="SetProperty" frame="HeroTileList" maxItemPerPage="24"/>
            </State>
        </StateGroup>

        <Frame Type="HeroContextMenu" name="ContextMenu" template="StandardTemplates/StandardContextMenuTemplate" >
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>

        <Frame type="Label" name="HeaderLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Text val="@UI/ProfileDialog/FavoriteHeroesTitle"/>
            <Style val="PlayerProfileSectionLabel"/>
        </Frame>

        <Frame type="Frame" name="DropArea">
            <Anchor side="Top" relative="$parent/FavoriteHeroTileList" pos="Min" offset="52"/>
            <Anchor side="Left" relative="$parent/FavoriteHeroTileList" pos="Min" offset="81"/>
            <Anchor side="Right" relative="$parent/FavoriteHeroTileList" pos="Max" offset="-81"/>
            <Anchor side="Bottom" relative="$parent/HeaderUnderlineImage" pos="Min" offset="0"/>
        </Frame>

        <Frame type="TileList" name="FavoriteHeroTileList" template="ProfileFavoriteHeroesPanel/FavoriteTileListTemplate">
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="30"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Anchor side="Bottom" relative="Item0" pos="Max" offset="0"/>
        </Frame>

        <Frame type="Image" name="HeaderUnderlineImage">
            <Anchor side="Bottom" relative="$parent/FavoriteHeroTileList" pos="Max" offset="-25"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-53"/>
			<Texture val="@UI/Storm_UI_ProfileDialog_Seperator"/>
            <TextureType val="Normal"/>
        </Frame>

        <Frame type="TileList" name="HeroTileList" template="ProfileFavoriteHeroesPanel/HeroTileListTemplate">
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="250"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

            <Animation name="Anchor">
                <Event event="Up" action="Play"/>
                <Event event="Down" action="Stop"/>

                <Controller type="Anchor" end="Pause" side="Top" relative="true">
                    <Key type="Curve" time="0" value="-150"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="HeroFilterPanel" name="HeroFilterPanel" template="HeroSelectChangeHeroPanel/HeroFilterPanelTemplate">
            <Anchor side="Left" relative="$parent/HeroTileList" pos="Min" offset="88"/>
            <Anchor side="Top" relative="$parent/HeroTileList" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-78"/>
            <FilterTarget val="$parent"/>

            <!-- Hide Blur frame that creates an artifact -->
            <Frame type="HeroRoleButton" name="FilterWarriorButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="HeroRoleButton" name="FilterDamageButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="HeroRoleButton" name="FilterSupportButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="HeroRoleButton" name="FilterSpecialistButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="HeroRoleButton" name="FilterMulticlassButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterRetroButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterWarcraftButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterStarCraftButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterDiabloButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterOverwatchButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterRetroButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Frame" name="SortButtonGroups">
                <Visible val="false"/>
            </Frame>
        </Frame>

        <StateGroup name="DragState">
            <DefaultState val="None"/>

            <State name="Dragging">
                <When type="AnimationState" DragResult="Dragging"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Invalid"/>
            </State>

            <State name="DraggingNonFavorite">
                <When type="AnimationState" DragResult="DraggingNonFavorite"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Invalid"/>
            </State>

            <State name="DraggingAdd">
                <When type="AnimationState" DragResult="DraggingAdd"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="DropArea" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Add"/>
            </State>

            <State name="DraggingSwap">
                <When type="AnimationState" DragResult="DraggingSwap"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="DropArea" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Move"/>
            </State>

            <State name="DraggingRemove">
                <When type="AnimationState" DragResult="DraggingRemove"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Remove"/>
                <Action type="SetProperty" frame="DropArea" visible="true"/>
            </State>

            <State name="None">
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="false"/>
                <Action type="SetProperty" frame="DropArea" visible="false"/>
            </State>
        </StateGroup>

        <!-- This should be last so it always renders on top of all other inventory frames. -->
        <Frame type="Frame" name="CursorAnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="32"/>
            <Height  val="32"/>
            <RenderPriority val="1024"/>

            <Frame type="Image" name="DragCursorImage">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="-10"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-10"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Width val="84"/>
                <Height val="84"/>
                <LayerCount val="1"/>
                <TextureType val="Normal" layer="0"/>
                <Alpha val="200"/>
                <Unclipped val="true"/>
            </Frame>

            <Frame type="Image" name="DragBorderImage">
                <Anchor side="Top" relative="$parent/DragCursorImage" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/DragCursorImage" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/DragCursorImage" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/DragCursorImage" pos="Mid" offset="0"/>
                <TextureType val="Normal"/>
                <Texture val="@UI/Storm_UI_Play_HeroPortrait_Drag"/>
                <Alpha val="200"/>
                <Unclipped val="true"/>
            </Frame>

            <Frame type="Image" name="CursorIcon">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="20"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="20"/>
                <Width val="22"/>
                <Height val="22"/>
                <RenderPriority val="1024"/>
                <LayerCount val="1"/>
                <TextureType val="Normal" layer="0"/>
                <Unclipped val="true"/>
            </Frame>
        </Frame>        
    </Frame>
</Desc>
