            <Anchor side="Top" relative="$parent/Team1LevelLabel" pos="Min" offset="13"/>
            <Anchor side="Left" relative="$parent/Team1LevelLabel" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent/Team2LevelLabel" pos="Max" offset="10"/>
            <Anchor side="Bottom" relative="$parent/Team1LevelLabel" pos="Max" offset="3"/>
            <RenderPriority val="530"/>
            <AcceptsMouse val="True"/>
        </Frame>

        <Frame type="Frame" name="XPContributionAnchoredPopupAnchorFrame">
            <Anchor side="Top" relative="$parent/TeamLevelVSLabel" pos="Mid" offset="25"/>
            <Anchor side="Left" relative="$parent/TeamLevelVSLabel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/TeamLevelVSLabel" pos="Mid" offset="0"/>
