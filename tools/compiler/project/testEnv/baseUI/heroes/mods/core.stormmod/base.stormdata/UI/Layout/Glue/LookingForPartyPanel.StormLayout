<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Frame type="LookingForPartyListBoxItem" name="LookingForPartyListBoxItem">
    
        <Frame type="BlurFrame" name="BackgroundBlurFrame" template="StandardTemplates/FullscreenBlurFrame">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="5"/>
            <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="-5"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="5"/>
            <Anchor side="Right" relative="$parent/Background" pos="Max" offset="-5"/>
            <BlurAmount val="0.3"/>
        </Frame>
        
        <Frame type="Image" name="BackgroundColorImage">
            <Anchor side="Top" relative="$parent/BackgroundBlurFrame" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundBlurFrame" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundBlurFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundBlurFrame" pos="Max" offset="0"/>
            <Color val="24194f"/>
            <Alpha val="40"/>
        </Frame>
        
        <Frame type="Image" name="BackgroundGradientImage">
            <Anchor relative="$parent/BackgroundColorImage"/>
            <Texture val="@UI/storm_ui_glues_draft_hero_gradient"/>
            <TextureCoords top="-.5" left="0.0" bottom="1" right="1.0"/>
            <Alpha val="70"/>
        </Frame>

        
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Storm_Social_LfmPanel_Background"/>
            <TextureType val="Nineslice"/>
        </Frame>
        
        <Frame type="AvatarFrame" name="AvatarFrame" template="AvatarFrameTemplates/AvatarStateTemplate_Friend">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <AcceptsMouseTooltip val="True"/>
        </Frame>

        <Frame type="BattleUserStatusIcon" name="StatusIcon" template="StandardTemplates/BattleUserStatusIconTemplate">
            <Anchor side="Bottom" relative="$parent/AvatarFrame" pos="Max" offset="-7"/>
            <Anchor side="Right" relative="$parent/AvatarFrame" pos="Max" offset="-4"/>
            <Visible val="false" />
        </Frame>
        
        <Frame type="Label" name="DescriptionLabel">
            <Anchor side="Top" relative="$parent/AvatarFrame" pos="Min" offset="13"/>
            <Anchor side="Bottom" relative="$parent/JoinButton" pos="Min" offset="20"/>
            <Anchor side="Left" relative="$parent/AvatarFrame" pos="Max" offset="6"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-16"/>
            <Style val="HomeLFPText"/>
            <ApplyLanguageRules val="true"/>
            <AcceptsMouseTooltip val="True"/>
            <Options val="TooltipIfTruncated"/>
        </Frame>
        
        <Frame type="Button" name="JoinButton" template="StandardTemplates/StandardButtonMiniTemplate">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="6"/>
            <Anchor side="Left" relative="$parent/DescriptionLabel" pos="Min" offset="-21"/>
            <Text val="@UI/LookingForParty/JoinPartyButton"/>
            <Height val="75"/>
            <Width val="200"/>
        </Frame>
        
        <Frame type="Image" name="GoodMatchImage">
            <Anchor side="Top" relative="$parent/JoinButton" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/JoinButton" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/JoinButton" pos="Max" offset="-20"/>
            <Width val="56"/>
            <Height val="56"/>
            <Texture val="@UI/Storm_UI_MainNavigation_Recommended"/>
            <Tooltip val="@UI/LookingForParty/GoodMatchSolo"/>
            <TooltipAnchorPosition val="BottomCenter"/>
            <AcceptsMouseTooltip val="true"/>
        </Frame>

        <Frame Type="BattleUserContextMenu" name="TargetContextMenu" template="BattleUserContextMenu/BattleUserContextMenu" >
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>
        
        <Animation name="Show">
            <Event event="OnShown" action="Reset"/>
            <Event event="OnShown" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time=".2" value="255" in="Slow"/>
            </Controller>
         </Animation>
    </Frame>

    <Frame type="LookingForPartyPanel" name="LookingForPartyPanelTemplate">
        <Width val="464"/>
        <Height val="404"/>

        <Frame type="Image" name="BackgroundBorderImage">
            <Anchor relative="$parent" offset="10"/>
            <Texture val="@UI/Storm_Taskbar_SocialButton_Normal"/>
            <TextureType val="NineSlice"/>
            <Unclipped val="True"/>
            <Visible val="False"/>

            <Frame type="Image" name="BackgroundColorImage">
                <Anchor relative="$parent" offset="-10"/>
                <Color val="24194f"/>
                <Alpha val="75"/>
            </Frame>
            
            <Frame type="Image" name="BackgroundGradientImage">
                <Anchor relative="$parent/BackgroundColorImage"/>
                <Texture val="@UI/storm_ui_glues_draft_hero_gradient"/>
                <TextureCoords top="-.5" left="0.0" bottom="1" right="1.0"/>
                <Alpha val="45"/>
            </Frame>
        </Frame>


        <Frame type="CheckBox" name="LookingForPartyCheckBox" template="StandardTemplates/StandardCheckBoxTemplate">
            <Anchor side="Top" relative="$parent/LookingForPartyTitleLabel" pos="Mid" offset="4"/>
            <Anchor side="Bottom" relative="$parent/LookingForPartyTitleLabel" pos="Mid" offset="4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
        </Frame>
        
        <Frame type="Image" name="LookingForPartyIcon">
            <Anchor side="Top" relative="$parent/LookingForPartyCheckBox" pos="Mid" offset="-3"/>
            <Anchor side="Bottom" relative="$parent/LookingForPartyCheckBox" pos="Mid" offset="-3"/>
            <Anchor side="Right" relative="$parent/LookingForPartyCheckBox" pos="Min" offset="0"/>
            <Texture val="@UI/Storm_Social_LfmPanel_JoinPartyIcon"/>
        </Frame>
        
        <Frame type="Label" name="LookingForPartyLabel">
            <Anchor side="Bottom" relative="$parent/LookingForPartyTitleLabel" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="SocialPlayerStatus"/>
            <Visible val="false"/>
        </Frame>
        
        <Frame type="Label" name="LookingForPartyTitleLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent/LookingForPartyIcon" pos="Min" offset="-10"/>
            <Style val="HomeLFPTitle"/>
            <Text val="@UI/ScreenHome/LookingForParty"/>
        </Frame>
        
        <Frame type="Label" name="NoPartiesLabel">
            <Anchor side="Top" relative="$parent/LookingForPartyTitleLabel" pos="Max" offset="20"/>
            <Anchor side="Left" relative="$parent/LookingForPartyTitleLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Style val="HomeLFPEmptyText"/>
            <Text val="@UI/LookingForParty/NoParties"/>
            <Alpha val="255"/>
            <RenderType val="LDR"/>
        </Frame>

        <Frame type="ListBox" name="ListBox" template="StandardTemplates/StandardListBoxTemplate">
            <Anchor side="Top" relative="$parent/LookingForPartyTitleLabel" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <AcceptsMouse val="true"/>

            
            <ItemDesc val="LookingForPartyPanel/LookingForPartyListBoxItem"/>
            <ItemHeight val="110"/>
            <ItemGap val="5"/>
            <SaveScrollHeightOnClear val="true"/>
            <Insets top="0" left="8" bottom="0" right="7"/>
            <HighlightOnHover val="false"/>
            <SelectedImage val="SelectedImage"/>
            <ScrollBar val="$root/StandardTemplates/StandardScrollBarTemplate"/>
            <BackgroundImage val="BackgroundImage"/>

            <Frame type="Image" name="SelectedImage">
                <Alpha val="0"/>
                <DescFlags val="Internal"/>
            </Frame>

            <Frame type="Image" name="BackgroundImage">
                <Alpha val="0"/>
                <DescFlags val="Internal"/>
            </Frame>
        </Frame>
    </Frame>
</Desc>
