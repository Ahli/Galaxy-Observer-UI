<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Constant name="EarnedRewardTileSpacing" val="-5"/>
    <Constant name="EarnedRewardTooltipHorizontalLeftPadding" val="12"/>
    <Constant name="EarnedRewardTooltipHorizontalRightPadding" val="-5"/>

    <Constant name="RenderPriorityScoreScreenTooltip" val="600"/>

    <!--=======================================================================================
    <! Promotion / Demotion Explanation Tooltip
    <!======================================================================================-->
    <Frame type="AnchoredPopup" name="ScoreAnchoredPopupTemplate" template="StandardPopupTooltip/AnchoredPopupLeftArrowTemplate">
        <MinWidth val="450"/>

        <Frame type="Frame" name="Container">
            <Frame type="Label" name="TitleLabel">
                <Style val="RankedBadge_Promotion_Tooltip_Title_Left"/>
            </Frame>

            <Frame type="Label" name="BodyLabel">
                <Style val="RankedBadge_Tooltip_Text"/>
            </Frame>
        </Frame>

        <StateGroup name="ShowPopup">
            <DefaultState val="Hidden"/>
            
            <State name="Shown">
                <When type="Property" frame="$PlayerRewardPanel_RankScoreFrame" showPopup="true"/>
                <Action type="SetProperty" frame="$this" show="true"/>
            </State>
            <State name="Hidden">
                <Action type="SetProperty" frame="$this" show="false"/>
            </State>
        </StateGroup>

        <StateGroup name="TitleStyle">
            <DefaultState val="Normal"/>
            
            <State name="Demotion">
                <When type="Property" frame="$PlayerRewardPanel_RankScoreFrame" PromotionState="DemotionNextGame"/>
                <Action type="SetProperty" frame="Container/TitleLabel" style="RankedBadge_Demotion_Tooltip_Title_Left"/>
            </State>
            <State name="Normal">
                <Action type="SetProperty" frame="Container/TitleLabel" style="RankedBadge_Promotion_Tooltip_Title_Left"/>
            </State>
        </StateGroup>

        <Frame type="PropertyBind" name="PropertyBindTitle">
            <Anchor relative="$parent"/>
            <Source frame="$PlayerRewardPanel_RankScoreFrame" property="PopupTitle"/>
            <Target frame="$parent/Container/TitleLabel" property="Text"/>
        </Frame>

        <Frame type="PropertyBind" name="PropertyBindText">
            <Anchor relative="$parent"/>
            <Source frame="$PlayerRewardPanel_RankScoreFrame" property="PopupText"/>
            <Target frame="$parent/Container/BodyLabel" property="Text"/>
        </Frame>

        <Animation name="OnDismiss">
            <Event event="Dismiss" action="Play"/>
            <Controller type="Event" end="Stop" frame="$PlayerRewardPanel_RankScoreFrame">
                <Key type="Event" time="0" event="FirstTimeExplanationDismissed"/>
            </Controller>
        </Animation>
    </Frame>

    <!--===========================================================================================
    <! TOOLTIP TEMPLATES 
    <!==========================================================================================-->
    <Frame type="RewardBreakdownItem" name="ToolTipItemTemplate">
        <Height val="20"/>
        <CollapseLayout val="true"/>
        <HideWhenCleared val="true"/>
        <Visible val="false"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="#EarnedRewardTooltipHorizontalLeftPadding"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="#EarnedRewardTooltipHorizontalRightPadding"/>

        <Frame type="Label" name="NameLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AmountLabel" pos="Min" offset="-10"/>
            <Text val="999999999"/>
            <Style val="ScoreRewardAmountLabel"/>
            <Unclipped val="True"/>

            <Frame type="PropertyBind" name="BindText">
                <Anchor relative="$parent"/>
                <Source frame="$ancestor[@type=RewardBreakdownItem]" property="Text"/>
                <Target frame="$parent" property="Text"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="AmountLabel">
            <Anchor side="Top" relative="$parent/NameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="999999999"/>
            <Style val="ScoreRewardAmountLabel"/>

            <Frame type="PropertyBind" name="BindText">
                <Anchor relative="$parent"/>
                <Source frame="$ancestor[@type=RewardBreakdownItem]" property="Amount"/>
                <Target frame="$parent" property="Text"/>
            </Frame>
        </Frame>

        <Animation name="SpacerHeightAnimation">
            <Event event="NotSpacer" action="DirectionForward,Play"/>
            <Event event="IsSpacer" action="DirectionReverse,Play"/>

            <Controller type="Dimension" dimension="Height" relative="False" percentage="False" end="Pause">
                <Key type="Curve" time=".00" value="10"/>
                <Key type="Curve" time=".01" value="20"/>
            </Controller>
        </Animation>

        <StateGroup name="TooltipItemStyle">
            <DefaultState val="Normal"/>
            <State name="Spacer">
                <When type="Property" IsSpacer="true"/>
                <Action type="SetProperty" frame="NameLabel" visible="false"/>
                <Action type="SetProperty" frame="AmountLabel" visible="false"/>
                <Action type="SendEvent" event="IsSpacer"/>
            </State>
            <State name="Bold">
                <When type="Property" UseBold="true"/>
                <Action type="SetProperty" frame="NameLabel" style="ScoreRewardTotalLabel"/>
                <Action type="SetProperty" frame="AmountLabel" style="ScoreRewardTotalLabel"/>
                <Action type="SendEvent" event="NotSpacer"/>
            </State>
            <State name="Normal">
                <Action type="SetProperty" frame="NameLabel" style="ScoreRewardAmountLabel"/>
                <Action type="SetProperty" frame="AmountLabel" style="ScoreRewardAmountLabel"/>
                <Action type="SendEvent" event="NotSpacer"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="TileList" name="ToolTipFrameTemplate">
        <Unclipped val="True"/>
        
        <Frame type="Frame" name="EarnedToolTipBackground" template="StandardTooltip/TooltipBlurFrameTemplate">
            <Anchor relative="$parent" offset="13"/>
            <RenderPriority val="500"/>
            <Unclipped val="True"/>
        </Frame>

        <Frame type="Label" name="ToolTipHeader">
            <Anchor side="Left" relative="$parent" pos="Min" offset="#EarnedRewardTooltipHorizontalLeftPadding"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="#EarnedRewardTooltipHorizontalRightPadding"/>
            <Anchor side="Bottom" relative="$parent/Item0" pos="Min" offset="-10"/>
            <Style val="ScoreRewardPanelHeaderLabel"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item0" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item1" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item1" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item2" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item2" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item3" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item3" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item4" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item4" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item5" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item5" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item6" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item6" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item7" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item7" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item8" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item8" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item9" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item9" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item10" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item10" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item11" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item11" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item12" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item12" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item13" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item13" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item14" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item14" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item15" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item15" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent/Item16" pos="Min" offset="#EarnedRewardTileSpacing"/>
        </Frame>

        <Frame type="RewardBreakdownItem" name="Item16" template="PlayerRewardsPanel/ToolTipItemTemplate">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-15"/>
        </Frame>

    </Frame>

    <Frame type="Frame" name="EarnedToolTipHitTestFrameTemplate">
        <RenderPriority val="#RenderPriorityScoreScreenTooltip"/>
        <AcceptsMouse val="true"/>

        <Frame type="TileList" name="EarnedToolTipFrame" template="PlayerRewardsPanel/ToolTipFrameTemplate">
            <Anchor side="Top" relative="ToolTipHeader" pos="Min" offset="-10"/>
            <Anchor side="Bottom" relative="$parent" pos="Min" offset="-30"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="376"/>
            <Visible val="False"/>
        </Frame>

        <Animation name="Show">
            <Event event="OnMouseEnter" action="DirectionForward"/>
            <Event event="OnMouseEnter" action="Play"/>
            <Event event="OnMouseExit" action="DirectionReverse"/>
            <Event event="OnMouseExit" action="Play"/>
            <Controller type="Visibility" end="Pause" frame="EarnedToolTipFrame">
                <Key type="Bool" time="0" value="false"/>
                <Key type="Bool" time=".01" value="true"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="EarnedToolTipFrame">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time=".2" value="255" in="Auto"/>
            </Controller>
         </Animation>
    </Frame>
        
    <!--===========================================================================================
    <! SCOREREWARD TILELIST TEMPLATES 
    <!==========================================================================================-->
    <Frame type="RewardTileListItem" name="HeroRewardItemTemplate">
        <Height val="65"/>
        <Width val="65"/>
        
        <Frame type="Image" name="RewardIconImage">
            <Anchor relative="$parent"/>
        </Frame>

        <Animation name="RankLayout">
            <Event event="RankedGame" action="DirectionForward" frame="$parent/$parent"/>
            <Event event="RankedGame" action="Play" frame="$parent/$parent"/>
            <Event event="NonRankedGame" action="DirectionReverse" frame="$parent/$parent"/>
            <Event event="NonRankedGame" action="Play" frame="$parent/$parent"/>
            
            <Controller type="Dimension" dimension="Width" relative="False" percentage="False" end="Pause" >
                <Key type="Curve" time="0" value="90" out="Step"/>
                <Key type="Curve" time=".01" value="65" in="Step"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="False" percentage="False" end="Pause">
                <Key type="Curve" time="0" value="90" out="Step"/>
                <Key type="Curve" time=".01" value="65" in="Step"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="ScoreRewardItem" name="EarnedRewardItemTemplate">
        <Height val="30"/>
        <Width val="510"/>
        
        <Frame type="Image" name="IconImage">
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="BoostGlowImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/> 
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/Storm_Draft_Portrait_BackgroundTiles_Glow"/>
            <Color val="0,204,255"/>
            <Visible val="false"/>
            
            <Animation name="AmountStyleXPBoost">
                <Event event="ScoreRewardXPBoost" action="Reset" frame="$parent/AmountLabel"/>
                <Event event="ScoreRewardXPBoost" action="Play" frame="$parent/AmountLabel"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.00" value="true"/>
                </Controller>
                
                <Controller type="Color" end="Pause">
                    <Key type="Curve" time="0.00" value="0,204,255" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleGoldBoost">
                <Event event="ScoreRewardGoldBoost" action="Reset" frame="$parent/AmountLabel"/>
                <Event event="ScoreRewardGoldBoost" action="Play" frame="$parent/AmountLabel"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.00" value="true"/>
                </Controller>
                
                <Controller type="Color" end="Pause">
                    <Key type="Curve" time="0.00" value="255,198,0" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleGold">
                <Event event="ScoreRewardGoldAmount" action="Reset" frame="$parent/AmountLabel"/>
                <Event event="ScoreRewardGoldAmount" action="Play" frame="$parent/AmountLabel"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.00" value="false"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleXP">
                <Event event="ScoreRewardXPAmount" action="Reset" frame="$parent/AmountLabel"/>
                <Event event="ScoreRewardXPAmount" action="Play" frame="$parent/AmountLabel"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.00" value="false"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleUnlock">
                <Event event="ScoreRewardUnlockDescription" action="Reset" frame="$parent/AmountLabel"/>
                <Event event="ScoreRewardUnlockDescription" action="Play" frame="$parent/AmountLabel"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.00" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="CountdownLabel" name="AmountLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/> 
            <Anchor side="Right" relative="$parent" pos="Mid" offset="-120"/>
            <Style val="ScoreRewardItemXPAmount"/>
            <FormatNumber val="true"/>
            <Visible val="False"/>
            
            <Animation name="ShowLabel">
                <Event event="OnShown" action="Reset"/>
                <Event event="OnShown" action="Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0.0" value="0" out="Auto"/>
                    <Key type="Curve" time="0.2" value="255" in="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="InstantUp">
                <Controller type="Counter" end="Pause">
                    <Key type="Curve" time="0.0" value="1.0" out="Step"/>
                </Controller>
            </Animation>
            
            <Animation name="CountUp">
                <Controller type="Counter" end="Pause">
                    <Key type="Curve" time="0.0" value="0.0" out="Slow"/>
                    <Key type="Curve" time="1.0" value="1.0" in="Slow"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleXPBoost">
                <Event event="ScoreRewardXPBoost" action="Reset"/>
                <Event event="ScoreRewardXPBoost" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemXPAmount" style2="ScoreRewardItemXPAmount">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleGoldBoost">
                <Event event="ScoreRewardGoldBoost" action="Reset"/>
                <Event event="ScoreRewardGoldBoost" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemGoldAmount" style2="ScoreRewardItemGoldAmount">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleGold">
                <Event event="ScoreRewardGoldAmount" action="Reset"/>
                <Event event="ScoreRewardGoldAmount" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemGoldAmount" style2="ScoreRewardItemGoldAmount">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleXP">
                <Event event="ScoreRewardXPAmount" action="Reset"/>
                <Event event="ScoreRewardXPAmount" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemXPAmount" style2="ScoreRewardItemXPAmount">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleUnlock">
                <Event event="ScoreRewardUnlockDescription" action="Reset"/>
                <Event event="ScoreRewardUnlockDescription" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemXPAmount" style2="ScoreRewardItemXPAmount">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="SourceLabel">
            <Anchor relative="$parent"/>
            <Style val="ScoreRewardItemXPDescription"/>
            
            <Animation name="AmountStyleXPBoost">
                <Event event="ScoreRewardXPBoost" action="Reset"/>
                <Event event="ScoreRewardXPBoost" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemXPBoostDescription" style2="ScoreRewardItemXPBoostDescription">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleGoldBoost">
                <Event event="ScoreRewardGoldBoost" action="Reset"/>
                <Event event="ScoreRewardGoldBoost" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemGoldBoostDescription" style2="ScoreRewardItemGoldBoostDescription">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleGold">
                <Event event="ScoreRewardGoldSource" action="Reset"/>
                <Event event="ScoreRewardGoldSource" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemGoldDescription" style2="ScoreRewardItemGoldDescription">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="AmountStyleXP">
                <Event event="ScoreRewardXPSource" action="Reset"/>
                <Event event="ScoreRewardXPSource" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemXPDescription" style2="ScoreRewardItemXPDescription">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>

            <Animation name="AmountStyleUnlock">
                <Event event="ScoreRewardUnlockTitle" action="Reset"/>
                <Event event="ScoreRewardUnlockTitle" action="Play"/>
                <Controller type="Style" end="Pause" style1="ScoreRewardItemXPDescription" style2="ScoreRewardItemXPDescription">
                    <Key type="Curve" time="0.00" value="1.0" out="Auto"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
    
    <Frame type="ScoreRewardTileListItem" name="EarnedRewardTileListItemTemplate">
        <Height val="30"/>
        <Width val="510"/>
        <Alpha val="0"/>
        
        <Frame type="ScoreRewardItem" name="ScoreRewardItem" template="PlayerRewardsPanel/EarnedRewardItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            
            <Animation name="AmountEarned">
                <Event event="InstantUp" action="Reset"/>
                <Event event="InstantUp" action="Play"/>
                <Controller type="Animation" end="Stop" frame="AmountLabel" animation="InstantUp">
                    <Key type="Animation" time="0.0" action="Play"/>
                </Controller>
            </Animation>
            
            <Animation name="CountUp">
                <Event event="CountUp" action="Reset"/>
                <Event event="CountUp" action="Play"/>
                <Controller type="Animation" end="Stop" frame="AmountLabel" animation="CountUp">
                    <Key type="Animation" time="0.0" action="Reset"/>
                    <Key type="Animation" time="0.1" action="Play"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Animation name="SlideIn">
            <Event event="Intro" action="Reset"/>
            <Event event="Intro" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time=".4" value="255" in="Slow"/>
                <Key type="Curve" time=".8" value="255" out="Slow"/>
                <Key type="Curve" time="1" value="0" in="Slow"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0" value="200" out="Fast"/>
                <Key type="Curve" time=".2" value="0" in="Slow" out="Slow"/>
                <Key type="Curve" time=".25" value="8" in="Fast" />
                <Key type="Curve" time=".3" value="0" out="Auto"/>
                <Key type="Curve" time=".8" value="0" in="Slow" out="Slow"/>
                <Key type="Curve" time="1" value="-100" in="Fast"/>
            </Controller>
        </Animation>
    </Frame>
    
    <Frame type="Frame" name="LevelUpContainerTemplate">
        <Width val="240"/>
        <Height val="240"/>
        <Alpha val="0"/>
        
        <Animation name="Reset">
            <Event event="ShowIntro" action="Reset" frame="$parent/$parent"/>
            <Event event="ShowIntro" action="Play" frame="$parent/$parent"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Slow"/>
            </Controller>
        </Animation>

        <Animation name="Trigger">
            <Event event="LevelUp" action="Reset" frame="$parent"/>
            <Event event="LevelUp" action="Play" frame="$parent"/>
            <Event event="InitialDelay" action="Stop" frame="$parent/$parent"/>
            <Event event="ShowIntro" action="Stop" frame="$parent/$parent"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time=".4" value="255" in="Slow"/>
            </Controller>
            <Controller type="Event" end="Stop">
                <Key type="Event" time="0" event="Begin"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$parent/LevelDescriptionLabel">
                <Key type="Curve" time="0" value="255" out="Fast"/>
                <Key type="Curve" time=".3" value="0" in="Slow"/>
                <Key type="Curve" time="3" value="0" in="Slow"/>
                <Key type="Curve" time="3.4" value="255" in="Slow"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$parent/LevelLabel">
                <Key type="Curve" time="0" value="255" out="Fast"/>
                <Key type="Curve" time=".3" value="0" in="Slow"/>
                <Key type="Curve" time="3" value="0" in="Slow"/>
                <Key type="Curve" time="3.4" value="255" in="Slow"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="LevelUpLabel">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time=".4" value="255" in="Slow"/>
                <Key type="Curve" time="3" value="255" in="Slow"/>
                <Key type="Curve" time="3.15" value="0" in="Slow"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="LevelUpLabel">
                <Key type="Curve" time="0" value="200" out="Fast"/>
                <Key type="Curve" time=".3" value="0" in="Slow"/>
                <Key type="Curve" time=".4" value="-12" in="Slow" out="Slow"/>
                <Key type="Curve" time=".6" value="0" in="Fast"/>
                <Key type="Curve" time="3" value="0" in="Fast"/>
                <Key type="Curve" time="3.2" value="-155" in="Fast"/>
            </Controller>
        </Animation>
                        
        <Frame type="Image" name="BottomShadowImage">
            <Anchor relative="$parent" />
            <Texture val="@UI/Storm_UI_Scorescreen_LevelUp_BottomShadow"/>
        </Frame>
        
        <Frame type="Image" name="ArrowsImage">
            <Anchor relative="$parent" />
            <Width val="220"/>
            <Height val="220"/>
            <BlendMode val="Add"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Storm_UI_Scorescreen_LevelUp_Mask" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            
            <Texture val="@UI/Storm_UI_Scorescreen_LevelUp_Arrows" layer="1"/>
            <TextureCoords top=".23" left=".08" bottom=".87" right=".92" layer="1"/>
            <WrapUV val="True" layer="1"/>
            
            <Animation name="UVScroll">
                <Event event="Begin" action="Restart" frame="$parent"/>
