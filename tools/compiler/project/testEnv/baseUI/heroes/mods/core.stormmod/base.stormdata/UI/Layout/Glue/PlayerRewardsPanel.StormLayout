                    <Key type="Bool" time=".01" value="true"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$parent/RankCountingFrame">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".01" value="true"/>
                </Controller>
            </Animation>

            <StateGroup name="GrandMasterAnchor">
                <DefaultState val="Normal"/>

                <State name="GrandMaster">
                    <When type="Property" RankType="Grandmaster"/>
                    <Action type="SendEvent" event="RaiseRankBadgeAnchor"/>
                </State>
                <State name="Normal">
                    <Action type="SendEvent" event="ResetRankBadgeAnchor"/>
                </State>
            </StateGroup>

            <Animation name="RankBadgeAnchor">
                <Event event="RaiseRankBadgeAnchor" action="DirectionForward,Play"/>
                <Event event="ResetRankBadgeAnchor" action="DirectionReverse,Play"/>
                
                <Controller type="Anchor" end="Pause" side="Bottom" relative="False">
                    <Key type="Curve" time="0" value="0" out="Step"/>
