                    <DefaultState val="None"/>
                    
                    <State name="PromotionNextGame">
                        <When type="Property" frame="$ancestor[@type=PlayerRewardsPanel]" RankedPromotionState="PromotionNextGame"/>
                        <Action type="SetProperty" text="@UI/RankedPlay/PromotionNextGame"/>
