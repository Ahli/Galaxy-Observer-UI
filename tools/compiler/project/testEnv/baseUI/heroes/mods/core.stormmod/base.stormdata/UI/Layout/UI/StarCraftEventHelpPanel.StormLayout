<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <Frame type="Frame" name="StarCraftEventHelpPanelTemplate">
        <Anchor relative="$parent"/>
        <Visible val="false"/>
        <AcceptsMouse val="False"/>
        
        <Frame type="Image" name="DarkenScreenImage">
            <Anchor relative="$parent"/>
            <Color val="000000"/>
            <Alpha val="97"/>
        </Frame>

        <Animation name="TriggerShow">
            <Event event="OnShown" action="Restart,Play"/>
            <Event event="OnMouseDown" action="Restart,Play"/>
            <Controller type="Event" end="Stop">
                <Key type="Event" time="0" event="Show"/>
            </Controller>
        </Animation>

        <Animation name="Show">
            <Event event="Show" action="Restart,Play"/>

            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0" sound="@Event_SCEvent_Start_Countdown"/>
            </Controller>
            
            <Controller type="Fade" end="Pause" frame="$parent\UIContainer\FullscreenUpperContainer\ChatDisplay">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="2.6" value="0" in="Slow"/>
				<Key type="Curve" time="3.2" value="255" in="Slow"/>
            </Controller>
			
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time=".2" value="255" in="Slow"/>
                <Key type="Curve" time="2.6" value="255" out="Fast"/>
                <Key type="Curve" time="3.2" value="0" in="Slow"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="CountdownLabel">
                <Key type="Event" time=".6" event="Countdown"/>
            </Controller>

            <Controller type="Rotation" relative="False" end="Loop" frame="StarCraftEventBladesImage">
                <Key type="Curve" time="0" value="0" out="linear"/>
                <Key type="Curve" time="2.6" value="-90" in="linear"/>
                <Key type="Curve" time="3.2" value="-180" in="fast"/>
            </Controller>

            <Controller type="Dimension" dimension="Width" relative="False" percentage="true" end="stop" frame="StarCraftEventBladesImage">
                <Key type="Curve" time="0" value="75" out="slow"/>
                <Key type="Curve" time=".3" value="90" in="fast"/>
                <Key type="Curve" time="2.73" value="100" out="fast"/>
                <Key type="Curve" time="2.85" value="90" in="slow"/>
                <Key type="Curve" time="3.2" value="300"/>
            </Controller>
            
            <Controller type="Dimension" dimension="Height" relative="False" percentage="true" end="stop" frame="StarCraftEventBladesImage">
                <Key type="Curve" time="0" value="75" out="slow"/>
                <Key type="Curve" time=".3" value="90" in="fast"/>
                <Key type="Curve" time="2.73" value="100" out="fast"/>
                <Key type="Curve" time="2.85" value="90" in="slow"/>
                <Key type="Curve" time="3.2" value="300"/>
            </Controller>

            <Controller type="Dimension" dimension="Width" relative="False" percentage="true" end="stop" frame="StarCraftEventDiscImage">
                <Key type="Curve" time="0" value="500" out="slow"/>
                <Key type="Curve" time=".3" value="100" in="fast"/>
                <Key type="Curve" time="2.73" value="100" out="fast"/>
                <Key type="Curve" time="2.85" value="90" in="slow"/>
                <Key type="Curve" time="3.2" value="200"/>
            </Controller>
            
            <Controller type="Dimension" dimension="Height" relative="False" percentage="true" end="stop" frame="StarCraftEventDiscImage">
                <Key type="Curve" time="0" value="500" out="slow"/>
                <Key type="Curve" time=".3" value="100" in="fast"/>
                <Key type="Curve" time="2.73" value="100" out="fast"/>
                <Key type="Curve" time="2.85" value="90" in="slow"/>
                <Key type="Curve" time="3.2" value="200"/>
            </Controller>

            <Controller type="Fade" end="Pause" frame="StarCraftEventTitleAImage">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time=".2" value="0" out="Slow"/>
                <Key type="Curve" time=".5" value="255" in="Slow"/>
            </Controller>

            <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="StarCraftEventTitleAImage">
                <Key type="Curve" time="0" value="-300" out="Fast"/>
                <Key type="Curve" time=".2" value="-300" out="Fast"/>
                <Key type="Curve" time=".5" value="-10" in="linear"/>
                <Key type="Curve" time="2.8" value="10" in="linear"/>
                <Key type="Curve" time="3" value="300" out="Fast"/>
            </Controller>

            <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="StarCraftEventTitleAImage">
                <Key type="Curve" time="0" value="-300" out="Fast"/>
                <Key type="Curve" time=".2" value="-300" out="Fast"/>
                <Key type="Curve" time=".5" value="-10" in="linear"/>
                <Key type="Curve" time="2.8" value="10" in="linear"/>
                <Key type="Curve" time="3" value="300" out="Fast"/>
            </Controller>
            
            <Controller type="Fade" end="Pause" frame="StarCraftEventTitleCImage">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time=".2" value="0" out="Slow"/>
                <Key type="Curve" time=".5" value="255" in="Slow"/>
            </Controller>

            <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="StarCraftEventTitleCImage">
                <Key type="Curve" time="0" value="300" out="Fast"/>
                <Key type="Curve" time=".2" value="300" out="Fast"/>
                <Key type="Curve" time=".5" value="10" in="linear"/>
                <Key type="Curve" time="2.8" value="-10" in="linear"/>
                <Key type="Curve" time="3" value="-300" out="Fast"/>
            </Controller>

            <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="StarCraftEventTitleCImage">
                <Key type="Curve" time="0" value="300" out="Fast"/>
                <Key type="Curve" time=".2" value="300" out="Fast"/>
                <Key type="Curve" time=".5" value="10" in="linear"/>
                <Key type="Curve" time="2.8" value="-10" in="linear"/>
                <Key type="Curve" time="3" value="-300" out="Fast"/>
            </Controller>
            
            <Controller type="Fade" end="Pause" frame="InstructionLabel">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time=".4" value="0" out="Slow"/>
                <Key type="Curve" time=".7" value="255" in="Slow"/>
            </Controller>

            <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="InstructionLabel">
                <Key type="Curve" time="0" value="20" out="Fast"/>
                <Key type="Curve" time=".4" value="20" out="Fast"/>
                <Key type="Curve" time=".7" value="0" in="Slow"/>
            </Controller>
        </Animation>
        
        <Frame type="Image" name="BackgroundImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="1168"/>
            <Height val="736"/>
            <Texture val="@UI/Storm_UI_Splash_SC2Event_Background"/>
        </Frame>

        <Frame type="Image" name="StarCraftEventBladesImage">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Splash_SC2Event_Blades"/>
            <SubpixelRendering val="True"/>
       </Frame>  

       <Frame type="Image" name="StarCraftEventDiscImage">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Splash_SC2Event_Disc"/>
            <SubpixelRendering val="True"/>
       </Frame>

       <Frame type="Image" name="StarCraftEventTitleBImage">
            <Anchor side="Top" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="-31"/>
            <Anchor side="Bottom" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="-31"/>
            <Anchor side="Left" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Splash_SC2Event_TitleB"/>
            <SubpixelRendering val="True"/>
       </Frame>

       <Frame type="Image" name="StarCraftEventTitleAImage">
            <Anchor side="Top" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="-93"/>
            <Anchor side="Bottom" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="-93"/>
            <Anchor side="Left" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Splash_SC2Event_TitleA"/>
            <SubpixelRendering val="True"/>
       </Frame>

       <Frame type="Image" name="StarCraftEventTitleCImage">
            <Anchor side="Top" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="74"/>
            <Anchor side="Bottom" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="74"/>
            <Anchor side="Left" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/StarCraftEventDiscImage" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Splash_SC2Event_TitleC"/>
            <SubpixelRendering val="True"/>
       </Frame>
        
        <Frame type="Label" name="InstructionLabel">
            <Anchor side="Top" relative="$parent/StarCraftEventDiscImage" pos="Max" offset="225"/>
            <Anchor side="Left" relative="$parent/StarCraftEventDiscImage" pos="Min" offset="-350"/>
            <Anchor side="Right" relative="$parent/StarCraftEventDiscImage" pos="Max" offset="350"/>
            <Text val="@UI/Event/StarCraftEvent/MapUI/SplashDescription"/>
            <Style val="Storm_MapMechanic_StarCraftEvent_Splash_Description"/>
            <SubpixelRendering val="True"/>
        </Frame>

        <Frame type="Image" name="StarCraftEventTextPlateImage">
            <Anchor side="Top" relative="$parent/InstructionLabel" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/InstructionLabel" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/InstructionLabel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/InstructionLabel" pos="Mid" offset="0"/>
            <RenderPriority val="1"/>
            <Alpha val="180"/>
            <Color val="003061"/>
            <!-- <ColorAdjustMode val="Desaturate"/> -->
            <Height val="116"/>
            <Width val="1192"/>
            <Texture val="@UI/Storm_UI_GenFX_GradientReflected"/>
            <SubpixelRendering val="True"/>
       </Frame>  

        <Frame type="Label" name="CountdownLabel">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="530"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="530"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Width val="800"/>
            <Height val="300"/>
            <Text val="3"/>
            <ScaleText val="True"/>
            <Visible val="False"/>
            <SubpixelRendering val="True"/>
            <Style val="Storm_MapMechanic_StarCraftEvent_Splash_Countdown"/>
            <Animation name="Countdown">
                <Event event="Countdown" action="Restart,Play"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time=".01" value="True"/>
                    <Key type="Bool" time="3.2" value="False"/>
                </Controller>
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="255"/>
                    <Key type="Curve" time=".3" value="255" in="Slow"/>
                    <Key type="Curve" time=".5" value="0" in="Fast" out="Step"/>
                    <Key type="Curve" time="1" value="255" out="Slow"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Loop">
                    <Key type="Curve" time="0" value="100" out="Slow"/>
                    <Key type="Curve" time=".5" value="25" in="Fast"/>
                    <Key type="Curve" time="1" value="25" in="Fast"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Loop">
                    <Key type="Curve" time="0" value="100" out="Slow"/>
                    <Key type="Curve" time=".5" value="25" in="Fast"/>
                    <Key type="Curve" time="1" value="25" in="Fast"/>
                </Controller>
                <Controller type="Style" style1="Storm_MapMechanic_Event_Splash_Countdown_Start" style2="Storm_MapMechanic_StarCraftEvent_Splash_Countdown" end="Loop">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time=".2" value="1" in="Fast"/>
                    <Key type="Curve" time="1" value="1"/>
                </Controller>
                <Controller type="Text" end="Loop">
                    <Key type="Text" time="0" text="3"/>
                    <Key type="Text" time=".8" text="2"/>
                    <Key type="Text" time="1.6" text="1"/>
                    <Key type="Text" time="3.4" text="@UI/Event/LunarNewYear/Splash/GO"/>
                    <Key type="Text" time="3.6" text="@UI/Event/LunarNewYear/Splash/GO" out="Step"/>
                </Controller>
            </Animation>
        </Frame>

    </Frame>

</Desc>