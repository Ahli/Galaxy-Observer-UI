    <!-- Region: Rating Control -->
    <!--===========================================================================================
    <! RatingIcon
    <!=============================================================================================
    <! Events : Checked - Control is not hovered. Icon should be shown as selected.
    <!          Unchecked - Icon should be shown as unselected.
    <!          CheckedHovered - Control is hovered. Cursor is on higher rating icon than selected rating.
    <!          CheckedUnhovered - Control is hovered. Cursor is on lower rating icon than selected rating.
    <!==========================================================================================-->
    <Frame type="Frame" name="RatingIconTemplate">
        <Width val="59"/>
        <Height val="56"/>

        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Storm_RatingButton_Unchecked"/>
        </Frame>

        <Frame type="Image" name="FlashImage">
            <Anchor relative="$parent/NormalImage"/>
            <Alpha val="0"/>
            <LayerCount val="2"/>
            <BlendMode val="Add"/>
            
            <Texture val="@UI/Storm_RatingButton_Checked" layer="0"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <WrapUV val="True" layer="0"/>
            
            <Texture val="@UI/storm_ui_glues_draft_headerBar_AngleGlow_Glow" layer="1"/>
            <TextureCoords top="-1" left=".4" bottom="2" right=".6" layer="1"/>
            <LayerColor val="dfc482" layer="1"/>
            <WrapUV val="False" layer="1"/>
            
            <Animation name="UVScroll">
                <Event event="OnShown" action="Reset" frame="$parent"/>
                <Event event="RatingChanged" action="Reset,Play" frame="$parent/$parent"/>
                <Controller type="LayerUV" layer="1" side="Left" relative="False" sidelock="True" end="Stop" >
                    <Key type="Curve" time="0" value="2" out="fast"/>
                    <Key type="Curve" time="1" value="2" out="fast"/>
                    <Key type="Curve" time="3" value="-2" in="fast"/>
                    <Key type="Curve" time="7" value="-2" in="fast"/>
                </Controller>
                <Controller type="Fade" end="Stop">
                    <Key type="Curve" time="0" value="0" out="Auto"/>
                    <Key type="Curve" time="2.5" value="255" out="Auto"/>
                    <Key type="Curve" time="3" value="255" in="Auto"/>
                    <Key type="Curve" time="7" value="0" in="Auto"/>
                </Controller>
            </Animation>
        </Frame>

        <Animation name="CheckedHovered">
            <Event event="Unchecked" action="Stop"/>
            <Event event="Checked" action="DirectionForward,Play"/>
            <Event event="PartialChecked" action="DirectionReverse,Play"/>
            <Controller type="Texture" end="Pause" frame="NormalImage">
                <Key type="Image" time="0" image="@UI/Storm_RatingButton_Hovered"/>
                <Key type="Image" time="0.01" image="@UI/Storm_RatingButton_Checked"/>
            </Controller>
        </Animation>
    </Frame>

