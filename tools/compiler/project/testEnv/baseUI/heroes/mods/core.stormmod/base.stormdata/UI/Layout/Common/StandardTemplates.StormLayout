        <StateGroup name="TalentTierState">
            <DefaultState val="TierNotAvailable"/>

            <State name="TierHidden">
                <When type="AnimationState" frame="$this" TalentTierState="TierHidden"/>
                <Action type="SetProperty" frame="$this" visible="false"/>
            </State>

            <State name="TierNotAvailable">
                <When type="AnimationState" frame="$this" TalentTierState="TierNotAvailable"/>
                <Action type="SetProperty" frame="$this/IconImage" visible="false"/>
            </State>

            <State name="TierAvailable">
                <When type="AnimationState" frame="$this" TalentTierState="TierAvailable"/>
                <Action type="SetProperty" frame="$this/IconImage" texture="@UI/storm_ui_ingame_shop_shopbuttonicon_shadow"/>
            </State>

            <State name="TierSelected">
                <When type="AnimationState" frame="$this" TalentTierState="TierSelected"/>
                <!-- IconImage Texture taken care of by code -->
            </State>
        </StateGroup>

        <StateGroup name="TalentTypeState">
