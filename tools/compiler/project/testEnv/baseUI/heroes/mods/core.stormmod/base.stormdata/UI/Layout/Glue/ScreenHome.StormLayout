<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="Button" name="HomeDebugButtonTemplate" template="StandardTemplates/StandardButtonMiniTemplate">
        <CollapseLayout val="True"/>
        <Height val="70"/>
        <Animation name="Begin">
            <Event event="Begin" action="DirectionForward" frame="$parent"/>
            <Event event="Begin" action="Play" frame="$parent"/>
            <Event event="End" action="DirectionReverse" frame="$parent"/>
            <Event event="End" action="Play" frame="$parent"/>
            <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                <Key type="Curve" time="0" value="100" out="Fast"/>
                <Key type="Curve" time=".2" value="0" in="Slow"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="ScreenHome" name="ScreenHome" template="StandardTemplates/GlueScreen">
        <Handle val="ScreenHome_Main" />
        <Anchor relative="$parent"/>
        <AutoCycleContent val="true"/>
        <AutoShowContent val="true"/>
        <ContentCycleDelay val="30000"/>

        <Animation name="Transition">
            <Event event="Show" action="Reset,Play"/>
            <Event event="Hide" action="DirectionReverse,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0" out="Slow"/>
                <Key type="Curve" time="0.4" value="255" in="Slow" out="Slow"/>
            </Controller>
        </Animation>

        <Animation name="PreTransition">
            <Event event="Begin" action="Reset"/>
            <Event event="Begin" action="Play"/>

            <Controller type="Sound" end="Pause">
                <Key type="Sound" time="0.0" sound="@UI_Gluescreen_Transition_Home"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="GenericBackground">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <ScaleWidthToTexture val="true"/>
            <Texture val="@UI/Storm_UI_Null"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="HeroHomeInfoPanel" name="HeroHomeInfoPanel">
            <Anchor relative="$parent"/>
            <Debugging val="True"/>

            <Animation name="ShowContent">
                <Event event="ShowContent" action="DirectionForward, Play" frame="$parent"/>
                <Event event="HideContent" action="DirectionReverse, Play" frame="$parent"/>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.00" value="true"/>
                    <Key type="Bool" time="0.99" value="true"/>
                    <Key type="Bool" time="1" value="false"/>
                </Controller>
            </Animation>
            
