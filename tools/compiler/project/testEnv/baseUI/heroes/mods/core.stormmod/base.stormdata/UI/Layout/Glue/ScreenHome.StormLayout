            <Frame type="HeroGlueButton" name="HomeDebugButton" template="StandardTemplates/StandardButtonMiniTemplate" requiredtoload="IS_SCREEN_MAIN_MENU_ENABLED">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-112"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="-12"/>
                <!-- <Tooltip val="Homescreen Debug"/>
                <TooltipAnchorPosition val="TopLeft"/> -->
                <Visible val="True"/>
                <Width val="90"/>
                <Height val="87"/>
                <Frame type="Label" name="TextLabel">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent/ConfigImage" pos="Max" offset="0"/>
                    <Text val=""/>
                    <Style val="StandardGlueButtonSmallLeft"/>
                </Frame>
                <Frame type="Image" name="ConfigImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="1"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="1"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="34"/>
                    <Texture val="@UI/Storm_Lobby_Icon_Settings"/>
                </Frame>
                <Animation name="Click">
                    <Event event="OnMouseDown" action="ToggleDirection,Play"/>
                    <Controller type="Fade" end="Pause" frame="$parent/HomeDebugContainer">
                        <Key type="Curve" time="0" value="255" in="Slow"/>
                        <Key type="Curve" time=".2" value="0" out="Slow"/>
                    </Controller>
                    <Controller type="Event" end="Pause" frame="$parent/HomeDebugContainer">
                        <Key type="Event" time="0" event="Begin"/>
                        <Key type="Event" time=".01" event="End"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="ScrollableFrame" name="HomeDebugContainer">
                <Anchor side="Bottom" relative="$parent/HomeDebugButton" pos="Min" offset="20"/>
                <Anchor side="Left" relative="$parent/HomeDebugButton" pos="Min" offset="0"/>
                <Width val="210"/>
                <Height val="500"/>
                <Alpha val="0"/>

                <Animation name="HideTemplate">
                    <Event event="OnShown" action="DirectionForward,Play"/>
                    <Controller type="Event" end="Pause">
                        <Key type="Event" time="0" event="End"/>
                    </Controller>
                </Animation>

                <Frame type="Image" name="Background">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="17"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@UI/Editor/BorderImage"/>
                    <TextureType val="Nineslice"/>
                </Frame>

                <Frame type="ScrollBar" name="ScrollBar" template="StandardTemplates/ScrollableBarTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="-2"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="-2"/>
                    <MinValue val="0"/>
                </Frame>

                <Frame type="Frame" name="ContainerFrame">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="PTRButton" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>

                    <Frame type="Frame" name="PlaceHolder">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                        <Height val="22"/>
                    </Frame>

                    <!--  ADD NEW BUTTONS HERE  -->
                    <!--  Don't forget to update the current top button to be anchored to your new button,  -->
                    <!--  and to set your new button to be anchored to PlaceHolder.  -->

                    <Frame type="Button" name="LucioButton" template="ScreenHome/HomeDebugButtonTemplate">
                        <Anchor side="Top" relative="$parent/PlaceHolder" pos="Max" offset="-40"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Text val="Lucio"/>
                        <Animation name="Click">
                            <Event event="OnMouseDown" action="Restart,Play"/>
                            <Controller type="Event" end="Stop" frame="$parent/$parent/$parent">
                                <Key type="Event" time="0" event="HomeReset"/>
                                <Key type="Event" time="0.01" event="Lucio"/>
                            </Controller>
                        </Animation>
                    </Frame>
                    <Frame type="Button" name="SamuroMonkeyKingButton" template="ScreenHome/HomeDebugButtonTemplate">
                        <Anchor side="Top" relative="$parent/LucioButton" pos="Max" offset="-40"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Text val="Monkey King Samuro"/>
                        <Animation name="Click">
                            <Event event="OnMouseDown" action="Restart,Play"/>
                            <Controller type="Event" end="Stop" frame="$parent/$parent/$parent">
                                <Key type="Event" time="0" event="HomeReset"/>
                                <Key type="Event" time="0.01" event="SamuroMonkeyKing"/>
                            </Controller>
                        </Animation>
                    </Frame>
                    <Frame type="Button" name="ValeeraButton" template="ScreenHome/HomeDebugButtonTemplate">
                        <Anchor side="Top" relative="$parent/SamuroMonkeyKingButton" pos="Max" offset="-40"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Text val="Valeera"/>
                        <Animation name="Click">
                            <Event event="OnMouseDown" action="Restart,Play"/>
                            <Controller type="Event" end="Stop" frame="$parent/$parent/$parent">
                                <Key type="Event" time="0" event="HomeReset"/>
                                <Key type="Event" time="0.01" event="Valeera"/>
