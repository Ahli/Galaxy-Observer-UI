        <Frame type="ContentFeedPanel" name="TakeoverContentFeed" template="ContentFeedPanel/ContentFeedPanelTemplate">
            <Anchor relative="$parent"/>
            <UseCached val="True"/>
            <KeepDataOnHide val="True"/>
            <UseAdCampaign val="true"/>
            <NydusLink val="feed/ad"/>
            <!-- For Testing Purposes: -->
            <!-- <FeedURL val="https://cms-us.web.blizzard.net/system/cms/oauth/api/ad/list?locale=enUS&amp;community=Heroes_client&amp;mediaCategory=IN_GAME_AD"/> -->

            <Frame type="PageSelectorButtonList" name="PageSelector">
                <AllowAutoPageWrap val="True"/>
                <AutoIncrement val="False"/>
            </Frame>

            <Frame type="Button" name="BackToHeroInfoButton" template="StandardTemplates/StandardLongArrowButtonTemplate_Left">
                <Visible val="False"/>
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Shortcut val="@UI/ScreenNavigationLeft_Hotkey"/>
            </Frame>

            <Frame type="Frame" name="ContentFeedPage">
                <Frame type="Button" name="LinkButton" >
                    <ReportsStats val="true"/>
                    <Handle val="HomeTakeover_LinkButton"/>
                </Frame>

                <Frame type="BlurFrame" name="BlurBackground">
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Height val="80"/>
                    <BlurAmount val="0.8"/>

                    <Frame type="Image" name="LowImage">
                        <Anchor relative="$parent"/>
                        <Texture val="@UI/StormTempBlack"/>
                        <TextureType val="Nineslice"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <Animation name="ShowContent">
                <Event event="ShowContent" action="DirectionForward, Play" frame="$parent"/>
                <Event event="HideContent" action="DirectionReverse, Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0.0" value="0" out="Auto"/>
                    <Key type="Curve" time="0.5" value="255" out="Auto"/>
                </Controller>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.00" value="false"/>
                    <Key type="Bool" time="0.01" value="true"/>
                    <Key type="Bool" time="0.50" value="true"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Frame" name="IGRBenefitsPanel" template="IGRBenefitsPanel/IGRBenefitsPanelTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="230"/>
            <Anchor side="Left" relative="$parent/LookingForPartyPanel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/LookingForPartyPanel" pos="Max" offset="0"/>
            <CollapseLayout val="true"/>
            <PreserveAnchorOffset val="True"/>
        </Frame>

        <Frame type="LookingForPartyPanel" name="LookingForPartyPanel" template="LookingForPartyPanel/LookingForPartyPanelTemplate">
            <Handle val="ScreenHome_LookingForPartyPanel"/>
            <Anchor side="Top" relative="$parent/IGRBenefitsPanel" pos="Max" offset="300"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <PreserveAnchorOffset val="True"/>
        </Frame>
        
        <Frame type="Button" name="ShowTakeoverContentButton" template="StandardTemplates/StandardLongArrowButtonTemplate_Right">
            <Visible val="False"/>
            <Alpha val="0"/>
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Shortcut val="@UI/ScreenNavigationRight_Hotkey"/>

            <Animation name="ToggleButton">
                <Event event="HideContent" action="DirectionForward, Play" frame="$parent"/>
                <Event event="ShowContent" action="DirectionReverse, Play" frame="$parent"/>
                
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0.0" value="0" out="Auto"/>
                    <Key type="Curve" time="0.5" value="255" out="Auto"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
    
