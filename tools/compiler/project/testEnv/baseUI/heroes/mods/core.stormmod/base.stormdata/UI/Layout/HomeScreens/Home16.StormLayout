                    <Key type="Curve" time="0" value=".4" out="fast"/>
                    <Key type="Curve" time="2" value=".5" inOut="Slow,Slow"/>
                    <Key type="Curve" time="4" value=".4" inOut="Slow,Slow"/>
                </Controller>
            </Animation>
        </Frame>
        
