<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <!-- CleanLeaderPanel is a template to create a basic version of HeroLeaderPanel (tab screen), with extra frames hidden.
        Necessary to have player information display functionality.
    -->
    
    <Frame type="HeroLeaderPanel" name="CleanLeaderPanelTemplate" template="HeroLeaderPanel/HeroLeaderPanelTemplate">
        <Visible val="false"/>
        <Batch val="true"/>
        
        <Frame type="Frame" name="HeroLeaderPanelContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Width val="0"/>
            <Height val="0"/>
            <Visible val="true"/>
            
            <Frame type="Frame" name="HitTestFrame">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Image" name="BackgroundImage">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Frame" name="TopHexContainer">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Image" name="HexImage">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Image" name="HexOverlayImage">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Frame" name="BorderContainer">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Button" name="CloseButton">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Button" name="ClearSortButton">
                <Visible val="false"/>
            </Frame>

            <Frame type="Frame" name="BlueTeamStats">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Frame" name="RedTeamStats">
                <Visible val="false"/>
            </Frame>
            
            <Frame type="TabControl" name="TabControl">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-47"/>
                
                <Frame type="Button" name="ShowStatsButton">
                    <Visible val="false"/>
                </Frame>
                
                <Frame type="Button" name="ShowTalentsButton">
                    <Anchor relative="$parent" side="Bottom" pos="Min" offset="0"/>
                    <Alpha val="0"/>
                    <Visible val="false"/>
                    <AcceptsMouse val="false"/>
                </Frame>
                
                <Frame type="Button" name="ShowDeathRecapButton">
                    <Visible val="false"/>
                    <AcceptsMouse val="false"/>
                </Frame>
                
                <Frame type="Frame" name="StatsContainerFrame">
                    <Anchor relative="$parent" side="Bottom" pos="Min" offset="0"/>
                    <Alpha val="0"/>
                    <AcceptsMouse val="false"/>
                </Frame>
                
                <Frame type="Frame" name="TalentsContainerFrame">
                    <Anchor relative="$parent" side="Top" pos="Min" offset="0"/>
                    <Anchor relative="$parent" side="Bottom" pos="Min" offset="0"/>
                    <Visible val="false"/>
                    
                    <Frame type="Label" name="TalentTierLabel1">
                        <Height val="0"/>
                        <Visible val="false"/>
                    </Frame>
                    <Frame type="Label" name="TalentTierLabel2">
                        <Height val="0"/>
                        <Visible val="false"/>
                    </Frame>
                    <Frame type="Label" name="TalentTierLabel3">
                        <Height val="0"/>
                        <Visible val="false"/>
                    </Frame>
                    <Frame type="Label" name="TalentTierLabel4">
                        <Height val="0"/>
                        <Visible val="false"/>
                    </Frame>
                    <Frame type="Label" name="TalentTierLabel5">
                        <Height val="0"/>
                        <Visible val="false"/>
                    </Frame>
                    <Frame type="Label" name="TalentTierLabel6">
                        <Height val="0"/>
                        <Visible val="false"/>
                    </Frame>
                    <Frame type="Label" name="TalentTierLabel7">
                        <Height val="0"/>
                        <Visible val="false"/>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
    </Frame>

    <!-- Base for hero leader panel contents -->
    <Frame type="HeroLeaderPanelPlayerFrame" name="HeroLeaderPanelPlayerFrame_Base">
        <Frame type="Image" name="Background">
            <Visible val="false"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="9999"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="9999"/>
            <Anchor side="Bottom" relative="$parent" pos="Min" offset="9999"/>
            <Anchor side="Right" relative="$parent" pos="Min" offset="9999"/>
        </Frame>

        <Frame type="Image" name="SelfBackground">
            <Visible val="false"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="9999"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="9999"/>
            <Anchor side="Bottom" relative="$parent" pos="Min" offset="9999"/>
            <Anchor side="Right" relative="$parent" pos="Min" offset="9999"/>
        </Frame>
        
        <Frame type="Frame" name="StatsContainerFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Visible val="true"/>
            
            <Frame type="PlayerScoreValueFrame" name="SoloKillScoreValueFrame">
                <Visible val="false"/>
                
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Visible val="false"/>
                </Frame>
            </Frame>
            
            <Frame type="PlayerScoreValueFrame" name="AssistsScoreValueFrame">
                <Visible val="false"/>
                
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Visible val="false"/>
                </Frame>
            </Frame>
            
            <Frame type="PlayerScoreValueFrame" name="ExperienceScoreValueFrame">
                <Visible val="false"/>
                
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Visible val="false"/>
                </Frame>
            </Frame>
        </Frame>
        
        <Frame type="BattleUserContextMenu" name="PlayerContextMenu">
            <Visible val="false"/>
        </Frame>
        
        <Frame type="PlayerHeroIconFrame" name="PlayerHeroIconFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="9999"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="9999"/>
            <Anchor side="Bottom" relative="$this" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$this" pos="Min" offset="0"/>
            <Width val="115"/>
            <Height val="50"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="HeroIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Width val="0"/>
                <Height val="0"/>
                <TextureCoords top="0.1" left="0.0" bottom="0.9" right="1.0"/>
                <Visible val="false"/>
            </Frame>
        </Frame>
        
        <Frame type="PlayerDeathIndicatorFrame" name="PlayerDeathIndicatorFrame">
            <Visible val="false"/>

            <Frame type="Frame" name="ContainerFrame">
                <Frame type="Image" name="SkullImage">
                    <Visible val="false"/>
                </Frame>
            </Frame>
        </Frame>

        <Frame type="PlayerNameFrame" name="PlayerNameFrame">
            <Visible val="false"/>

            <Frame type="Label" name="PlayerNameLabel">
                <Visible val="false"/>
            </Frame>
        </Frame>

        <Frame type="PlayerHeroNameFrame" name="HeroNameFrame">
            <Visible val="false"/>

            <Frame type="Label" name="HeroNameLabel">
                <Visible val="false"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="TalentsContainerFrame">
            <Visible val="false"/>
        </Frame>

        <Frame type="PlayerBlockFrame" name="BlockFrame">
            <Visible val="false"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="9999"/>
        </Frame>

        <Frame type="PlayerScoreValueFrame" name="CarriedMechanicScoreValueFrame">
            <Visible val="false"/>

            <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                <Visible val="false"/>
                <Alpha val="0"/>
            </Frame>

            <Frame type="ScoreValueImageFrame" name="ScoreValueFrame1">
                <Visible val="false"/>
                <Alpha val="0"/>
            </Frame>
        </Frame>
    </Frame>    
</Desc>