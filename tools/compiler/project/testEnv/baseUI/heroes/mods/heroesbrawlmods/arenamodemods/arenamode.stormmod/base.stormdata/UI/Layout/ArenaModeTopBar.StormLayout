<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Frame type="Frame" name="ArenaModeTopBarPanelTemplate">
    
        <!-- Background override -->
        <Frame type="BlurFrame" name="TopBarBackingBlur">
            <AlphaMaskTexture val="@UI/Storm_UI_Brawl_HUD_Top_BG"/>
        </Frame>
        
        <Frame type="Image" name="TopBarBacking">
            <Texture val="@UI/Storm_UI_Brawl_HUD_Top_BG"/>
        </Frame>
        
        <!-- Tinted highlight -->
        <Frame type="Image" name="Team1XPBackgroundBlue">
            <Width val="192"/>
            <Height val="88"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TopBarBacking" pos="Min" offset="126"/>
            <Texture val="@UI/Storm_UI_Brawl_HUD_Top_BG_Blue"/>
        </Frame>
        
        <Frame type="Image" name="Team1XPBackgroundRed">
            <Width val="192"/>
            <Height val="88"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TopBarBacking" pos="Min" offset="126"/>
            <Texture val="@UI/Storm_UI_Brawl_HUD_Top_BG_Red"/>
