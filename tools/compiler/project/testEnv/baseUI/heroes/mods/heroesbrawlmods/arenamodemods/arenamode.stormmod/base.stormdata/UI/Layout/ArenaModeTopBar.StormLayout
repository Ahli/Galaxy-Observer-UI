        </Frame>
        
        <Frame type="Image" name="Team2XPBackgroundRed">
            <Width val="192"/>
            <Height val="88"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/TopBarBacking" pos="Max" offset="-124"/>
            <Texture val="@UI/Storm_UI_Brawl_HUD_Top_BG_Red"/>
        </Frame>
    
        <Frame type="Frame" name="NormalModeContainer">
            <Visible val="False"/>
        </Frame>
    </Frame>
    
    <Frame type="Frame" name="GameUI/UIContainer/ConsoleUIContainer/TopPanel" file="GameUI" template="ArenaModeTopBar/ArenaModeTopBarPanelTemplate"/>
</Desc>