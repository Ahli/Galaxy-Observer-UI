                </Frame>
            </Frame>
        </Frame>

        <!-- F -->
        <Frame type="CommandButton" name="CommandButton15" template="CommandButton/CommandButtonTemplate">
            <Anchor side="Top" relative="$parent/ButtonHolder" pos="Min" offset="-21"/>
            <Anchor side="Left" relative="$parent/ButtonHolder" pos="Min" offset="68"/>
            <ClickSound val="@UI_Game_ActionClick"/>
            <BatchGroup val="true"/>
            <RenderPriority val="521"/>

            <!-- Lock image covering the button icon when an ability is not yet picked up -->
            <Frame type="Image" name="DisabledLockOverlay">
                <Anchor relative="$parent/NormalImage"/>
                <Texture val="@UI/HUD_Btn_BG_Ability_Locked"/>
                <ImageMaskTexture val="@UI/HUD_BTN_BG_Ability_AlphaMask"/>

                <Animation name="Fade">
                    <Event event="FadeOut" action="DirectionForward,Play" frame="$this"/>
                    <Event event="FadeIn" action="DirectionReverse,Play" frame="$this"/>

                </Animation>
            </Frame>

            <Frame type="Image" name="GameButtonDefaultBorder">
                <StateGroup name="BorderStates">
                    <DefaultState val="Disabled"/>
                    <State name="Cleared">
                        <When type="Property" frame="$parent" Cleared="true" Index="0"/>
                        <Action type="SetProperty" frame="$parent" Visible="false"/>
                        <Action type="SetProperty" frame="$parent/$parent/ButtonHolder" Visible="false"/>
                        <Action type="SetProperty" frame="$parent/$parent/LedContainer" Visible="false"/>
                    </State>
                    <State name="Disabled">
                        <When type="AnimationState" frame="$parent/$parent/CommandUnitSelectionPanel/UnitStatusFrame0/ArmedValidator" UnitStatusValidatorState="MapMechanicDoesNotHaveMapMechanicAbilCombine"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Storm_UI_InGame_AbilityButtonBorder_Terran_Off"/>
                        <Action type="SetProperty" frame="$parent/GameButtonDefaultHoverBorder" Visible="false"/>
                        <Action type="SendEvent" frame="$parent/$parent/LedContainer" event="Stop"/>
                        <Action type="SendEvent" frame="$parent/DisabledLockOverlay" event="FadeIn"/>
                    </State>
                    <State name="Passive">
                        <When type="Property" frame="$parent" Passive="true" Index="0"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/ButtonBorderPassive"/>
                        <Action type="SetProperty" frame="$parent/GameButtonDefaultHoverBorder" Visible="false"/>
                    </State>
                    <State name="Executing">
                        <When type="Property" frame="$parent" Executing="true" Index="0"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Storm_UI_InGame_AbilityButtonBorder_Terran_Hover"/>
                        <Action type="SetProperty" frame="$parent/GameButtonDefaultHoverBorder" Visible="false"/>
                    </State>
                    <State name="Unbound">
                        <When type="Property" frame="$parent" Unbound="true" Index="0"/>
                    </State>
                    <State name="Highlighted">
                        <When type="Property" frame="$parent" Highlighted="true" Index="0"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Storm_UI_InGame_AbilityButtonBorder_Hover"/>
                    </State>
                    <State name="Normal">
                        <When type="Property" frame="$parent" Executing="false" Index="0"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Storm_UI_InGame_AbilityButtonBorder_Terran"/>
                        <Action type="SetProperty" frame="$parent/GameButtonDefaultHoverBorder" Visible="true"/>
                        <Action type="SendEvent" frame="$parent/$parent/LedContainer" event="Armed"/>
                        <Action type="SendEvent" frame="$parent/DisabledLockOverlay" event="FadeOut"/>
                    </State>
                    <State name="Arming">
                        <When type="Property" frame="$parent/CooldownLabel" Visible="true" Index="0"/>
                        <Action type="SetProperty" frame="$this" Texture="@UI/Storm_UI_InGame_AbilityButtonBorder_Terran"/>
                        <Action type="SetProperty" frame="$parent/GameButtonDefaultHoverBorder" Visible="true"/>
                        <Action type="SendEvent" frame="$parent/$parent/LedContainer" event="Arming"/>
                        <Action type="SendEvent" frame="$parent/DisabledLockOverlay" event="FadeOut"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="Image" name="GameButtonDefaultHoverBorder">
                <Texture val="@UI/Storm_UI_InGame_AbilityButtonBorder_Terran_Hover"/>
            </Frame>
        </Frame>

        <!-- Inherited button frames override -->
        <Frame type="Image" name="ButtonHolder">
            <Anchor side="Top" relative="$parent/CommandButton05" pos="Min" offset="21"/>
            <Anchor side="Left" relative="$parent/CommandButton05" pos="Min" offset="90"/>
            <BatchGroup val="true"/>
            <Texture val="@UI/Storm_UI_InGame_AbilityButtonBorder_Terran_Holder"/>
            <Constant name="RotationAngle" val="90"/>

            <!--RenderPriority val="510"/-->
            <!-- Set position relative to Command Panel -->
            <!-- Item Ability Button initial appearance animation -->
            <Animation name="RollIn">
                <Event event="OnShown" action="Play" frame="$this"/>

                <Controller name="Anchor_0" type="Anchor" end="Pause" side="Top" relative="true" unclip="false" frame="$this">
                    <Key type="CurveReal" time="0" inout="Linear,Step" value="80"/>
                    <Key type="CurveReal" time="1.3" inout="Step,Slow" value="80"/>
                    <Key type="CurveReal" time="1.9" inout="Linear,Smooth" value="-2"/>
                    <Key type="CurveReal" time="1.97" inout="Smooth" value="2"/>
                    <Key type="CurveReal" time="2.02" inout="Smooth" value="-1"/>
                    <Key type="CurveReal" time="2.1" inout="Smooth,Linear" value="0"/>
