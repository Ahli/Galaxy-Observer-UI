                    <Key type="CurveReal" time="2.6" inout="Linear,Smooth" value="1"/>
                    <Key type="CurveReal" time="2.67" inout="Smooth" value="-1"/>
                    <Key type="CurveReal" time="2.72" inout="Smooth" value="1"/>
                    <Key type="CurveReal" time="2.8" inout="Smooth,Linear" value="0"/>
                </Controller>
                <Controller name="Rotation_0" type="Rotation" end="Pause" relative="false" frame="$parent/CommandButton15/GameButtonDefaultBorder">
                    <Key type="CurveReal" time="0" inout="Linear,Step" value="90"/>
                    <Key type="CurveReal" time="2.9" inout="Step,Slow" value="90"/>
                    <Key type="CurveReal" time="3.3" inout="Linear,Smooth" value="-1"/>
                    <Key type="CurveReal" time="3.370" inout="Smooth" value="1"/>
                    <Key type="CurveReal" time="3.42" inout="Smooth" value="-1"/>
                    <Key type="CurveReal" time="3.5" inout="Smooth,Linear" value="0"/>
                </Controller>
                <Controller name="Rotation_1" type="Rotation" end="Pause" relative="false" frame="$parent/CommandButton15/DisabledLockOverlay">
                    <Key type="CurveReal" time="0" inout="Linear,Step" value="90"/>
                    <Key type="CurveReal" time="2.9" inout="Step,Slow" value="90"/>
                    <Key type="CurveReal" time="3.3" inout="Linear,Smooth" value="-1"/>
                    <Key type="CurveReal" time="3.370" inout="Smooth" value="1"/>
                    <Key type="CurveReal" time="3.42" inout="Smooth" value="-1"/>
                    <Key type="CurveReal" time="3.5" inout="Smooth,Linear" value="0"/>
                </Controller>
                <Controller name="Rotation_2" type="Rotation" end="Pause" relative="false" frame="$parent/CommandButton15/IconBackground">
                    <Key type="CurveReal" time="0" inout="Linear,Step" value="90"/>
