        </Frame>

        <!-- Cannonball travel animations-->
        <Frame type="Image" name="CannonballLeft"  template="BlackheartsRevengeTopBar/CannonballTemplate">
            <Anchor side="Top" relative="$parent/CannonLeft/CannonBaseFrameCold" pos="Min" offset="5"/>
            <Anchor side="Left" relative="$parent/CannonLeft/CannonBaseFrameCold" pos="Mid" offset="38"/>
            <Visible val="False"/>

            <Animation name="Soar">
                <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
                <Controller type="Anchor" side="Left" relative="true">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.44" value="310"/>
                    <Key type="Curve" time="1.00" value="620"/>
                    <Key type="Curve" time="4.00" value="620"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="CannonballRight"  template="BlackheartsRevengeTopBar/CannonballTemplate">
            <Anchor side="Top" relative="$parent/CannonRight/CannonBaseFrameCold" pos="Min" offset="5"/>
            <Anchor side="Right" relative="$parent/CannonRight/CannonBaseFrameCold" pos="Mid" offset="-38"/>

            <Animation name="Soar">
                <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
                <Controller type="Anchor" side="Right" relative="true">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.44" value="-310"/>
                    <Key type="Curve" time="1.00" value="-620"/>
                    <Key type="Curve" time="4.00" value="-620"/>
                </Controller>
            </Animation>
        </Frame>

        <!-- Cannon smoke animations -->
        <Frame type="Image" name="SmokeLeft" template="BlackheartsRevengeTopBar/SmokeTemplate">
            <Anchor side="Top" relative="$parent/CannonLeft/CannonBaseFrameCold" pos="Min" offset="-17"/>
            <Anchor side="Left" relative="$parent/CannonLeft/CannonBaseFrameCold" pos="Mid" offset="40"/>
            <TextureCoords top="0" left="1" bottom="1" right="0"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="Image" name="SmokeRight" template="BlackheartsRevengeTopBar/SmokeTemplate">
            <Anchor side="Top" relative="$parent/CannonRight/CannonBaseFrameCold" pos="Min" offset="-17"/>
            <Anchor side="Right" relative="$parent/CannonRight/CannonBaseFrameCold" pos="Mid" offset="-40"/>
        </Frame>

        <!-- Cannon fire animations -->
        <Frame type="Image" name="CannonballFireLeft" template="BlackheartsRevengeTopBar/CannonballFireTemplate">
            <Anchor side="Top" relative="$parent/CannonLeft/CannonBaseFrameCold" pos="Min" offset="-28"/>
            <Anchor side="Left" relative="$parent/CannonLeft/CannonBaseFrameCold" pos="Mid" offset="15"/>
            <Visible val="False"/>

            <Animation name="Fire">
                <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
                <Controller type="Anchor" side="Left" relative="true">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.24" value="-5"/>
                    <Key type="Curve" time="4.00" value="-5"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="CannonballFireRight" template="BlackheartsRevengeTopBar/CannonballFireTemplate">
            <Anchor side="Top" relative="$parent/CannonRight/CannonBaseFrameCold" pos="Min" offset="-28"/>
            <Anchor side="Right" relative="$parent/CannonRight/CannonBaseFrameCold" pos="Mid" offset="-15"/>

            <Animation name="Fire">
                <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
                <Controller type="Anchor" side="Right" relative="true">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.24" value="5"/>
                    <Key type="Curve" time="4.00" value="5"/>
                </Controller>
            </Animation>
        </Frame>

        <!-- Cannon animations -->
        <Frame type="Frame" name="CannonLeft" template="BlackheartsRevengeTopBar/CannonTemplate">
            <Anchor relative="$parent/Team1NotificationFrame/NotificationIconAnchor"/>
            <Visible val="False"/>

            <Frame type="Image" name="CannonBarrelFrameCold">
                <Anchor side="Left" relative="$parent/CannonBaseFrameCold" pos="Min" offset="16"/>
                <TextureCoords top="0" left="1" bottom="1" right="0"/>

                <Animation name="CannonShootCold">
                    <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
                    <Controller type="Anchor" side="Left" relative="true">
                        <Key type="Curve" time="0.00" value="0"/>
                        <Key type="Curve" time="0.12" value="-12"/>
                        <Key type="Curve" time="0.24" value="0" inout="slow,Linear"/>
                        <Key type="Curve" time="4.00" value="0"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Image" name="CannonBarrelFrameHot">
                <TextureCoords top="0" left="1" bottom="1" right="0"/>
            </Frame>

            <Frame type="Image" name="CannonBaseFrameCold">
                <Anchor side="Top" relative="$parent/$parent/Team1NotificationFrame/NotificationIconAnchor" pos="Mid" offset="-5"/>
                <Anchor side="Left" relative="$parent/$parent/Team1NotificationFrame/NotificationIconAnchor" pos="Mid" offset="-38"/>
                <Anchor side="Bottom" relative="$parent/$parent/Team1NotificationFrame/NotificationIconAnchor" pos="Mid" offset="-5"/>
                <TextureCoords top="0" left="1" bottom="1" right="0"/>
            </Frame>

            <Frame type="Image" name="CannonBaseFrameHot">
                <TextureCoords top="0" left="1" bottom="1" right="0"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="CannonRight" template="BlackheartsRevengeTopBar/CannonTemplate">
            <Anchor relative="$parent/Team2NotificationFrame/NotificationIconAnchor"/>
            
            <Frame type="Image" name="CannonBarrelFrameCold">
                <Anchor side="Left" relative="$parent/CannonBaseFrameCold" pos="Min" offset="-16"/>
                <Animation name="CannonShootCold">
                    <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
                    <Controller type="Anchor" side="Left" relative="true">
                        <Key type="Curve" time="0.00" value="0"/>
                        <Key type="Curve" time="0.12" value="12"/>
                        <Key type="Curve" time="0.24" value="0" inout="slow,Linear"/>
                        <Key type="Curve" time="4.00" value="0"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>

        <!-- Mangages the visibility of the Left/Right animations for each round -->
        <StateGroup name="Icons">
            <State name="Round1">
                <When type="AnimationState" Round="1"/>
                <Action type="SetProperty" frame="ShipLeft" Visible="True"/>
                <Action type="SetProperty" frame="ShipRight" Visible="False"/>
                <Action type="SetProperty" frame="ShipBackgroundExplosionLeft" Visible="True"/>
                <Action type="SetProperty" frame="ShipBackgroundExplosionRight" Visible="False"/>
                <Action type="SetProperty" frame="ShipForegroundExplosionLeft" Visible="True"/>
                <Action type="SetProperty" frame="ShipForegroundExplosionRight" Visible="False"/>
                <Action type="SetProperty" frame="Debris1Left" Visible="True"/>
                <Action type="SetProperty" frame="Debris1Right" Visible="False"/>
                <Action type="SetProperty" frame="Debris2Left" Visible="True"/>
                <Action type="SetProperty" frame="Debris2Right" Visible="False"/>
                <Action type="SetProperty" frame="Debris3Left" Visible="True"/>
                <Action type="SetProperty" frame="Debris3Right" Visible="False"/>
                <Action type="SetProperty" frame="CannonLeft" Visible="False"/>
                <Action type="SetProperty" frame="CannonRight" Visible="True"/>
                <Action type="SetProperty" frame="CannonballLeft" Visible="False"/>
                <Action type="SetProperty" frame="CannonballRight" Visible="True"/>
                <Action type="SetProperty" frame="CannonballFireLeft" Visible="False"/>
                <Action type="SetProperty" frame="CannonballFireRight" Visible="True"/>
                <Action type="SetProperty" frame="SmokeLeft" Visible="False"/>
                <Action type="SetProperty" frame="SmokeRight" Visible="True"/>
                <Action type="SetProperty" frame="Team1NotificationFrame/Lock" Visible="True"/>
                <Action type="SetProperty" frame="Team2NotificationFrame/Lock" Visible="False"/>
            </State>
            <State name="Round2">
                <When type="AnimationState" Round="2"/>
                <Action type="SetProperty" frame="ShipLeft" Visible="False"/>
                <Action type="SetProperty" frame="ShipRight" Visible="True"/>
                <Action type="SetProperty" frame="ShipBackgroundExplosionLeft" Visible="False"/>
                <Action type="SetProperty" frame="ShipBackgroundExplosionRight" Visible="True"/>
                <Action type="SetProperty" frame="ShipForegroundExplosionLeft" Visible="False"/>
                <Action type="SetProperty" frame="ShipForegroundExplosionRight" Visible="True"/>
                <Action type="SetProperty" frame="Debris1Left" Visible="False"/>
                <Action type="SetProperty" frame="Debris1Right" Visible="True"/>
                <Action type="SetProperty" frame="Debris2Left" Visible="False"/>
                <Action type="SetProperty" frame="Debris2Right" Visible="True"/>
                <Action type="SetProperty" frame="Debris3Left" Visible="False"/>
                <Action type="SetProperty" frame="Debris3Right" Visible="True"/>
                <Action type="SetProperty" frame="CannonLeft" Visible="True"/>
                <Action type="SetProperty" frame="CannonRight" Visible="False"/>
                <Action type="SetProperty" frame="CannonballLeft" Visible="True"/>
                <Action type="SetProperty" frame="CannonballRight" Visible="False"/>
                <Action type="SetProperty" frame="CannonballFireLeft" Visible="True"/>
                <Action type="SetProperty" frame="CannonballFireRight" Visible="False"/>
                <Action type="SetProperty" frame="SmokeLeft" Visible="True"/>
                <Action type="SetProperty" frame="SmokeRight" Visible="False"/>
                <Action type="SetProperty" frame="Team1NotificationFrame/Lock" Visible="False"/>
                <Action type="SetProperty" frame="Team2NotificationFrame/Lock" Visible="True"/>
            </State>
        </StateGroup>

        <!-- Color the cannon icon correctly -->
        <StateGroup name="ScoreLabelStyles">
            <State name="Team1IsBlue">
                <When type="AnimationState" frame="$ancestor[@type=GameUI]/UIContainer" TeamColors="Team1IsBlue"/>
                <Action type="SetProperty" frame="CannonLeft/CannonBaseFrameCold" Texture="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_Blue"/>
                <Action type="SetProperty" frame="CannonLeft/CannonBaseFrameHot" Texture="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_red"/>
                <Action type="SetProperty" frame="CannonRight/CannonBaseFrameCold" Texture="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_RedTeam"/>
                <Action type="SetProperty" frame="CannonRight/CannonBaseFrameHot" Texture="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_Red_RedTeam"/>
            </State>
            <State name="Team2IsBlue">
                <When type="AnimationState" frame="$ancestor[@type=GameUI]/UIContainer" TeamColors="Team2IsBlue"/>
                <Action type="SetProperty" frame="CannonLeft/CannonBaseFrameCold" Texture="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_RedTeam"/>
                <Action type="SetProperty" frame="CannonLeft/CannonBaseFrameHot" Texture="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_Red_RedTeam"/>
                <Action type="SetProperty" frame="CannonRight/CannonBaseFrameCold" Texture="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_Blue"/>
                <Action type="SetProperty" frame="CannonRight/CannonBaseFrameHot" Texture="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_red"/>
            </State>
        </StateGroup>

        <!-- HACK: Moving the AFK warning text. We can only do this because we are a sibling of this frame.  -->
        <StateGroup name="MoveAFKText">
            <DefaultState val="Default"/>
            <State name="Default">
                <Action type="SetAnchor" frame="$parent/DirectiveDisplay" side="Top" pos="Min" relative="$parent" offset="180"/>
            </State>
        </StateGroup>
    </Frame>

    <!-- This is partially taken from the Tutorial map. This displays the round instructions -->
    <Frame type="Frame" name="RoundInstructions">
        <Anchor relative="$parent"/>
        <Visible val="False"/>
        <Animation name="OnShown">
            <Event event="OnShown" action="DirectionForward,Play"/>
            <Event event="OnHidden" action="DirectionReverse,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".4" value="255" inout="Slow,Linear"/>
            </Controller>
        </Animation>
        <Animation name="Hide">
            <Event event="OnShown" action="Reset,Play"/>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0.00" value="True"/>
                <Key type="Bool" time="4.50" value="False"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/StormTempWhite"/>
            <Color val="170,170,170"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Frame" name="RoundTitleFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="40"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Height val="350"/>
            <Width val="1400"/>

            <Frame type="Frame" name="TitleLabelAnchor">
                <Anchor side="Top" relative="$parent" pos="Min" offset="60"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Height val="1"/>

                <Animation name="TitleFlyIn">
                    <Event event="OnShown" action="Reset, DirectionForward, Play"/>

                    <Controller type="Anchor" side="Left" end="Pause">
                        <Key type="Curve" time="0" value="500" inout="Linear"/>
                        <Key type="Curve" time=".5" value="15" inout="Slow,Linear"/>
                        <Key type="Curve" time="4" value="-15" inout="Linear,Slow"/>
                        <Key type="Curve" time="4.5" value="-500" inout="Linear"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Label" name="TitleLabel">
                <Anchor side="Top" relative="$parent/TitleLabelAnchor" pos="Mid" offset="80 "/>
                <Anchor side="Left" relative="$parent/TitleLabelAnchor" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/TitleLabelAnchor" pos="Mid" offset="0"/>
                <SubpixelRendering val="True"/>
                <Style val="Storm_VeteranTutorial_ChallengeTitle"/>
                <Text val=""/>
            </Frame>

            <Frame type="Image" name="LabelDividerImage">
                <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="-195"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Texture val="@UI/storm_ui_draft_intro_flare"/>
            </Frame>

            <Frame type="Frame" name="DescriptionAnchorFrame">
                <Anchor side="Top" relative="$parent/LabelDividerImage" pos="Max" offset="-260"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Height val="1"/>

                <Animation name="DescriptionFlyIn">
                    <Event event="OnShown" action="Reset, DirectionForward, Play"/>

                    <Controller type="Anchor" side="Left" end="Pause">
                        <Key type="Curve" time="0" value="-500" inout="Linear"/>
                        <Key type="Curve" time=".5" value="-15" inout="Slow,Linear"/>
                        <Key type="Curve" time="4" value="15" inout="Linear,Slow"/>
                        <Key type="Curve" time="4.5" value="500" inout="Linear"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Label" name="RoundDescriptionLabel">
                <Anchor side="Top" relative="$parent/DescriptionAnchorFrame" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/DescriptionAnchorFrame" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/DescriptionAnchorFrame" pos="Mid" offset="0"/>
                <SubpixelRendering val="True"/>
                <Style val="Storm_VeteranTutorial_ChallengeDescription"/>
                <Text val=""/>
            </Frame>

            <Animation name="Intro">
                <Event event="OnShown" action="Reset, DirectionForward, Play"/>

                <Controller type="Fade" end="Pause" frame="TitleLabel">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time=".3" value="255"/>
                    <Key type="Curve" time="4" value="255"/>
                    <Key type="Curve" time="4.5" value="0"/>
                </Controller>
                <Controller type="Fade" end="Pause" frame="LabelDividerImage">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time=".3" value="255"/>
                    <Key type="Curve" time="4" value="255"/>
                    <Key type="Curve" time="4.5" value="0"/>
                </Controller>
                <Controller type="Fade" end="Pause" frame="RoundDescriptionLabel">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time=".3" value="255"/>
                    <Key type="Curve" time="4" value="255"/>
                    <Key type="Curve" time="4.5" value="0"/>
                </Controller>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time=".3" value="255"/>
                    <Key type="Curve" time="4" value="255"/>
                    <Key type="Curve" time="4.5" value="0"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
</Desc>
