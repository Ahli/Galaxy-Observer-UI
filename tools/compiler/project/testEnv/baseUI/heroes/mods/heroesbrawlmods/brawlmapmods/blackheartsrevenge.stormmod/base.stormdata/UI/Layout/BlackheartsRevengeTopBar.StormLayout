        <Frame type="Image" name="Fill">
            <Anchor  side="Top" relative="$parent" pos="Min" offset="1"/>
            <Anchor  side="Left" relative="$parent" pos="Min" offset="1"/>
            <Anchor  side="Bottom" relative="$parent" pos="Max" offset="-1"/>
            <Anchor  side="Right" relative="$parent" pos="Max" offset="-1"/>
            <Texture val="{$parent/$parent/Damage1/Fill/@Texture}"/>
            <TextureCoords top="0" left="1" bottom="1" right="0"/>
            <ColorAdjustMode val="Desaturate"/>
            <AdjustmentColor val="{$GameUI/TeamOrderColors/@Color}"/>
            <Visible val="False"/>
            <!--<BlendMode val="Add"/>-->
            <!-- <Animation name="Flash">
                <Event event="OnShown" action="Restart,Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.7" value="255"/>
                </Controller>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.7" value="0" inout="Linear,Slow"/>
                    <Key type="Curve" time="1.5" value="255" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="1.5" value="True"/>
                </Controller>
            </Animation> -->
        </Frame>
    </Frame>

    <Frame type="Image" name="ShipDamageIndicatorTemplateRight" template="BlackheartsRevengeTopBar/ShipDamageIndicatorTemplateLeft">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Left" relative="" pos="Max" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-1"/>
        <TextureCoords top="0" left="0" bottom="1" right="1"/>

        <Frame type="Image" name="Fill">
            <AdjustmentColor val="{$GameUI/TeamChaosColors/@Color}"/>
            <TextureCoords top="0" left="0" bottom="1" right="1"/>
        </Frame>
    </Frame>

    <!-- Template for Debris coming from the ship when its hit -->
    <Frame type="Image" name="DebrisTemplate">
        <Height val="48"/>
        <Width val="48"/>
        <RenderPriority val="607"/>
        <Alpha val="0"/>
        <SubpixelRendering val="true"/>
        <Texture val="@UI/HUD_Blackhearts_Revenge_Debris"/>

        <Animation name="Rotate">
            <Event event="OnShown" action="Restart, Stop"/>
            <Controller type="Rotation" relative="False">
                <Key type="Curve" time="0" value="-90"/>
            </Controller>
        </Animation>
        <Animation name="ScatterShared">
            <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
            <Controller type="Fade">
                <Key type="Curve" time="0.00" value="0"/>
                <Key type="Curve" time="0.99" value="0"/>
                <Key type="Curve" time="1.00" value="255" inout="Linear,Slow"/>
                <Key type="Curve" time="1.52" value="0" inout="Slow,Linear"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="Image" name="DebrisLeftTemplate" template="BlackheartsRevengeTopBar/DebrisTemplate">
        <Anchor side="Top" relative="$parent/Team1NotificationFrame/NotificationIconAnchor" pos="Min" offset="15"/>
        <Anchor side="Right" relative="$parent/Team1NotificationFrame/NotificationIconAnchor" pos="Mid" offset="32"/>
    </Frame>

    <Frame type="Image" name="DebrisRightTemplate" template="BlackheartsRevengeTopBar/DebrisTemplate">
        <Anchor side="Top" relative="$parent/Team2NotificationFrame/NotificationIconAnchor" pos="Min" offset="15"/>
        <Anchor side="Left" relative="$parent/Team2NotificationFrame/NotificationIconAnchor" pos="Mid" offset="-32"/>
        <Visible val="False"/>
    </Frame>

    <!-- Template for Background explosion animation  -->
    <Frame type="Image" name="ShipBackgroundExplosionTemplate">
        <Height val="130"/>
        <Width  val="130"/>
        <Alpha val="0"/>
        <RenderPriority val="620"/>
        <SubpixelRendering val="true"/>
        <Texture val="@UI/HUD_Blackhearts_Revenge_Cannonball_Fire"/>

        <Animation name="ExplodeShared">
            <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
            <Controller type="Fade" end="Stop">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="0.99" value="0" inout="Linear"/>
                <Key type="Curve" time="1" value="255" />
                <Key type="Curve" time="1.6" value="0"/>
                <Key type="Curve" time="3.99" value="0" inout="Linear"/>
                <Key type="Curve" time="4.0" value="0" inout="Linear"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="true" percentage="false" end="Stop">
                <Key type="Curve" time="0.00" value="0"/>
                <Key type="Curve" time="0.99" value="0"/>
                <Key type="Curve" time="1.52" value="20"/>
                <Key type="Curve" time="4.00" value="20"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="true" percentage="false" end="Stop">
                <Key type="Curve" time="0.00" value="0"/>
                <Key type="Curve" time="0.99" value="0"/>
                <Key type="Curve" time="1.52" value="20"/>
                <Key type="Curve" time="4.00" value="20"/>
            </Controller>
            <Controller type="Anchor" side="Top" relative="true" end="Stop" >
                <Key type="Curve" time="0.00" value="0"/>
                <Key type="Curve" time="0.99" value="0"/>
                <Key type="Curve" time="1.52" value="-10"/>
                <Key type="Curve" time="4.00" value="-10"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="Image" name="ShipForegroundExplosionTemplate">
        <Height val="100"/>
        <Width  val="78"/>
        <RenderPriority val="620"/>
        <Alpha val="0"/>
        <SubpixelRendering val="true"/>
        <Texture val="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Fire"/>
        
        <Animation name="ExplodeShared">
            <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
            <Controller type="Fade">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="0.99" value="0" inout="Linear"/>
                <Key type="Curve" time="1" value="255" />
                <Key type="Curve" time="1.6" value="0"/>
                <Key type="Curve" time="3.99" value="0" inout="Linear"/>
                <Key type="Curve" time="4.0" value="0" inout="Linear"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="true" percentage="false">
                <Key type="Curve" time="0.00" value="0"/>
                <Key type="Curve" time="0.99" value="0"/>
                <Key type="Curve" time="1.52" value="20"/>
                <Key type="Curve" time="4.00" value="20"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="true" percentage="false">
                <Key type="Curve" time="0.00" value="0"/>
                <Key type="Curve" time="0.99" value="0"/>
                <Key type="Curve" time="1.52" value="20"/>
                <Key type="Curve" time="4.00" value="20"/>
            </Controller>
            <Controller type="Anchor" side="Top" relative="true">
                <Key type="Curve" time="0.00" value="0"/>
                <Key type="Curve" time="0.99" value="0"/>
                <Key type="Curve" time="1.52" value="-10"/>
                <Key type="Curve" time="4.00" value="-10"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="Image" name="CannonballTemplate">
        <Height val="20"/>
        <Width val="20"/>
        <RenderPriority val="670"/>
        <Alpha val="0"/>
        <SubpixelRendering val="true"/>
        <Texture val="@UI/HUD_Blackhearts_Revenge_Cannonball"/>

        <Animation name="SoarShared">
            <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
            <Controller type="Fade">
                <Key type="Curve" time="0.00" value="0" inout="Linear"/>
                <Key type="Curve" time="0.10" value="255" inout="Linear"/>
                <Key type="Curve" time="1.00" value="255" inout="Linear"/>
                <Key type="Curve" time="1.04" value="0" inout="Linear"/>
                <Key type="Curve" time="3.96" value="0" inout="Linear"/>
                <Key type="Curve" time="4.00" value="0" inout="Linear"/>
            </Controller>
            <Controller type="Anchor" side="Top" relative="true">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0.44" value="-25"/>
                <Key type="Curve" time="1.00" value="20"/>
                <Key type="Curve" time="4.00" value="20"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="Image" name="SmokeTemplate">
        <Height val="57"/>
        <Width val="170"/>
        <RenderPriority val="607"/>
        <Alpha val="0"/>
        <SubpixelRendering val="True"/>
        <Texture val="@UI/HUD_Blackhearts_Revenge_Cannon_Smoke"/>
        
        <Animation name="SmokeBurst">
            <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
            <Controller type="Dimension" dimension="Width" relative="false" percentage="True">
                <Key type="Curve" time="0" value="45"/>
                <Key type="Curve" time="0.04" value="60"/>
                <Key type="Curve" time="0.08" value="70"/>
                <Key type="Curve" time="0.12" value="75"/>
                <Key type="Curve" time="0.16" value="85"/>
                <Key type="Curve" time="0.20" value="90"/>
                <Key type="Curve" time="0.20" value="90"/>
                <Key type="Curve" time="0.72" value="100"/>
                <Key type="Curve" time="4.00" value="100"/>
            </Controller>
            <Controller type="Fade">
                <Key type="Curve" time="0" value="255" inout="Linear"/>
                <Key type="Curve" time=".72" value="0" inout="Slow,Linear"/>
                <Key type="Curve" time="4.0" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Rotation" relative="False">
                <Key type="Curve" time="0" value="6"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="Image" name="CannonballFireTemplate">
        <Height val="85"/>
        <Width val="85"/>
        <RenderPriority val="611"/>
        <Alpha val="0"/>
        <SubpixelRendering val="true"/>
        <Texture val="@UI/HUD_Blackhearts_Revenge_Cannonball_Fire"/>

        <Animation name="FireShared">
            <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
            <Controller type="Fade">
                <Key type="Curve" time="0.00" value="255" inout="Linear,slow"/>
                <Key type="Curve" time="0.24" value="0" inout="Slow,Linear"/>
                <Key type="Curve" time="4.00" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="true" percentage="false">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0.24" value="10"/>
                <Key type="Curve" time="4.00" value="10"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="true" percentage="false">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0.24" value="10"/>
                <Key type="Curve" time="4.00" value="10"/>
            </Controller>
            <Controller type="Anchor" side="Top" relative="true">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0.24" value="-5"/>
                <Key type="Curve" time="4.00" value="-5"/>
            </Controller>
            <Controller type="Anchor" side="Left" relative="true">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="0.24" value="-5"/>
                <Key type="Curve" time="4.00" value="-5"/>
            </Controller>
        </Animation>
    </Frame>
    <Frame type="Frame" name="CannonTemplate">
        <Frame type="Image" name="CannonBarrelFrameCold">
            <Anchor side="Top" relative="$parent/CannonBaseFrameCold" pos="Min" offset="0"/>
            <RenderPriority val="600"/>
            <SubpixelRendering val="true"/>
            <Texture val="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Head_Blue"/>
        </Frame>

        <Frame type="Image" name="CannonBarrelFrameHot">
            <Anchor side="Top" relative="$parent/CannonBarrelFrameCold" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/CannonBarrelFrameCold" pos="Min" offset="0"/>
            <Alpha val="0"/>
            <RenderPriority val="601"/>
            <SubpixelRendering val="true"/>
            <Texture val="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Head_Red"/>
            <Animation name="CannonShootHot">
                <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
                <Controller type="Fade">
                    <Key type="Curve" time="0.00" value="255" inout="Linear"/>
                    <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
                    <Key type="Curve" time="4" value="0" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="CannonBaseFrameCold">
            <Anchor side="Top" relative="$parent/$parent/Team2NotificationFrame/NotificationIconAnchor" pos="Mid" offset="-5"/>
            <Anchor side="Bottom" relative="$parent/$parent/Team2NotificationFrame/NotificationIconAnchor" pos="Mid" offset="-5"/>
            <Anchor side="Left" relative="$parent/$parent/Team2NotificationFrame/NotificationIconAnchor" pos="Mid" offset="-38"/>
            <RenderPriority val="604"/>
            <Texture val="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_Blue"/>
        </Frame>

        <Frame type="Image" name="CannonBaseFrameHot">
            <Anchor side="Top" relative="$parent/CannonBaseFrameCold" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/CannonBaseFrameCold" pos="Min" offset="0"/>
            <RenderPriority val="605"/>
            <Alpha val="0"/>
            <Texture val="@UI/Storm_UI_HUD_Minimap_Blackhearts_Revenge_Cannon_Body_red"/>
            <Animation name="CannonShootHot">
                <Event event="Fire" action="Restart, Play" frame="$BlackheartsRevenge_TopNotificationPanel"/>
                <Controller type="Fade" >
                    <Key type="Curve" time="0.00" value="255" inout="Linear"/>
                    <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
                    <Key type="Curve" time="4" value="0" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <!-- Team Score Panel Template - Party Frame Insert -->
    <Frame type="Frame" name="TeamScoreTemplate">
        <Anchor relative="$parent"/>
        
        <Frame type="Label" name="ScoreHint">
            <Anchor side="Bottom" relative="$parent" pos="Min" offset="75"/>
            <Style val="Storm_BlackheartsRevenge_MapMechanicScore_Hint"/>
            <Text val="Distance"/>
        </Frame>
        <Frame type="Label" name="Score">
            <Anchor side="Bottom" relative="$parent/ScoreHint" pos="Min" offset="10"/>
            <Style val="Storm_BlackheartsRevenge_MapMechanicScore"/>
            <Text val="0"/>
        </Frame>
        <Frame type="Label" name="ScoreSuffix">
            <Anchor side="Bottom" relative="$parent/Score" pos="Max" offset="-5"/>
            <Style val="Storm_BlackheartsRevenge_MapMechanicScore_Suffix"/>
            <Text val="%"/>
        </Frame>

        <!-- Progress Bar -->
        <Frame type="Frame" name="ShipProgress">
            <Anchor side="Top" relative="$parent/$parent/$parent/PartyFrameTopPanel/TopBarBacking" pos="Min" offset="3"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/PartyFrameTopPanel/TopBarBacking" pos="Mid" offset="-31"/>
            <Width val="106"/>
            <Height val="18"/>

            <Frame type="Image" name="ProgressBarBg">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Width val="{$parent/@Width}"/>
                <Height val="{$parent/@Height}"/>
                <Texture val="@UI/HUD_Blackhearts_Revenge_Ship_Bar"/>
            </Frame>

            <Frame type="ProgressBar" name="ShipProgressBar">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Width val="106"/>
                <Height val="18"/>
                <Value val="0"/>
                <UseFillContainer val="True"/>

                <Frame type="Frame" name="FillImageContainer">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Width val="106"/>
                    <Height val="18"/>

                    <Frame type="Image" name="FillImage">
                        <Anchor relative="$parent\$parent" offset="0"/>
                        <Texture val="@UI/HUD_Blackhearts_Revenge_Ship_Bar_Fill"/>
                        <ColorAdjustMode val="Desaturate"/>
                        <AdjustmentColor val="{$GameUI/TeamOrderColors/@Color}"/>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>

        <!-- Ship Damage -->
        <Frame type="Frame" name="ShipDamageIndicators">
            <Anchor side="Top" relative="$parent/$parent/$parent/PartyFrameTopPanel/TopBarBacking" pos="Min" offset="3"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/PartyFrameTopPanel/TopBarBacking" pos="Mid" offset="-30"/>
            <Width val="106"/>
            <Height val="18"/>
            <Frame type="Image" name="Damage1" template="BlackheartsRevengeTopBar/ShipDamageIndicatorTemplateLeft">
                <Anchor side="Left" relative="$parent" pos="Min" offset="-1"/>
            </Frame>
            <Frame type="Image" name="Damage2" template="BlackheartsRevengeTopBar/ShipDamageIndicatorTemplateLeft">
                <Anchor side="Left" relative="$parent/Damage1" pos="Max" offset="-9"/>
            </Frame>
            <Frame type="Image" name="Damage3" template="BlackheartsRevengeTopBar/ShipDamageIndicatorTemplateLeft">
                <Anchor side="Left" relative="$parent/Damage2" pos="Max" offset="-9"/>
            </Frame>
            <Frame type="Image" name="Damage4" template="BlackheartsRevengeTopBar/ShipDamageIndicatorTemplateLeft">
                <Anchor side="Left" relative="$parent/Damage3" pos="Max" offset="-9"/>
            </Frame>
            <Frame type="Image" name="Damage5" template="BlackheartsRevengeTopBar/ShipDamageIndicatorTemplateLeft">
                <Anchor side="Left" relative="$parent/Damage4" pos="Max" offset="-9"/>
            </Frame>
        </Frame>

        <!-- This manages the visibility of the damage indicators as well as how big the bar/fill images are -->
        <StateGroup name="ShipBlips">
            <State name="5ToWin">
                <When type="Property" frame="ScoreSuffix" Text="/5" />
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage2" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage3" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage4" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage5" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Texture="@UI/HUD_Blackhearts_Revenge_Cannon_Bar_5"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1/Fill" Texture="@UI/HUD_Blackhearts_Revenge_Cannon_Bar_5_Fill"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Width="28"/>
                <Action type="SetProperty" frame="ShipDamageIndicators" Visible="True"/>
                <Action type="SetProperty" frame="ShipProgress" Visible="False"/>
            </State>
            <State name="4ToWin">
                <When type="Property" frame="ScoreSuffix" Text="/4" />
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage2" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage3" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage4" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage5" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Texture="@UI/HUD_Blackhearts_Revenge_Cannon_Bar_4"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1/Fill" Texture="@UI/HUD_Blackhearts_Revenge_Cannon_Bar_4_Fill"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Width="33"/>
                <Action type="SetProperty" frame="ShipDamageIndicators" Visible="True"/>
                <Action type="SetProperty" frame="ShipProgress" Visible="False"/>
            </State>
            <State name="3ToWin">
                <When type="Property" frame="ScoreSuffix" Text="/3" />
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage2" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage3" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage4" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage5" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Texture="@UI/HUD_Blackhearts_Revenge_Cannon_Bar_3"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1/Fill" Texture="@UI/HUD_Blackhearts_Revenge_Cannon_Bar_3_Fill"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Width="41"/>
                <Action type="SetProperty" frame="ShipDamageIndicators" Visible="True"/>
                <Action type="SetProperty" frame="ShipProgress" Visible="False"/>
            </State>
            <State name="2ToWin">
                <When type="Property" frame="ScoreSuffix" Text="/2" />
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage2" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage3" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage4" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage5" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Texture="@UI/HUD_Blackhearts_Revenge_Cannon_Bar_2"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1/Fill" Texture="@UI/HUD_Blackhearts_Revenge_Cannon_Bar_2_Fill"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Width="57"/>
                <Action type="SetProperty" frame="ShipDamageIndicators" Visible="True"/>
                <Action type="SetProperty" frame="ShipProgress" Visible="False"/>
            </State>
            <State name="1ToWin">
                <When type="Property" frame="ScoreSuffix" Text="/1" />
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Visible="True"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage2" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage3" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage4" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage5" Visible="False"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Texture="@UI/HUD_Blackhearts_Revenge_Ship_Bar"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1/Fill" Texture="@UI/HUD_Blackhearts_Revenge_Ship_Bar_Fill"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" Width="106"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1/Fill" TextureCoords="0.000000,0.000000,1.000000,1.000000"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1" TextureCoords="0.000000,0.000000,1.000000,1.000000"/>
                <Action type="SetProperty" frame="ShipDamageIndicators" Visible="True"/>
                <Action type="SetProperty" frame="ShipProgress" Visible="False"/>
            </State>
            <State name="Hide">
                <When type="Property" frame="ScoreSuffix" Text="%" />
                <Action type="SetProperty" frame="ShipDamageIndicators" Visible="False"/>
                <Action type="SetProperty" frame="ShipProgress" Visible="True"/>
            </State>
        </StateGroup>
        
        <!-- Switching Ship damage indicators -->
        <StateGroup name="ShipDamageScore">
            <State name="Damage0">
                <When type="AnimationState" ShipDamage="0" frame="$parent"/>
                <Action type="SetProperty" frame="ShipDamageIndicators/Damage1/Fill" undo="false" Visible="False"/>
