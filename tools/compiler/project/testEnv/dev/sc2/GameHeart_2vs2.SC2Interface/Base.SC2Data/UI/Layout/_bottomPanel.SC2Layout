    <!-- Most of this has been created within the SC2 editor. Unfortunately, it does not handle comments very well. Thus, I didn't really comment the "_CONTROLS" and settings. :( -->
    <!-- created by Ahli (twitter: AhliSC2) -->
    <!-- Templates -->
    <!-- idea: always show ClanLogo, but when it has an image, the playerRaceIcon is pushed offscreen. This requires two different default images for visible one and offscreen pushing one -->
    <!-- due to a bug with the raceIcon in ClanLogoOrRaceIconTemplate ignoring the anchors of its parent frames while NOT being unclipped, this solution becamse quite messy as the raceIcon needs to be pushed offscreen when a clan logo is available -->
    <Frame type="Frame" name="ClanLogoOrRaceIconTemplate">
        <Height val="46"/>
        <Unclipped val="false"/>

        <!-- visible clan logo that shows a transparent image by default -->
        <Frame type="PlayerLogoImageFrame" name="ClanLogo">
            <Anchor relative="$parent"/>
            <Height val="42"/>
            <Width val="42"/>
            <FrameLogoAlias val="LogoSmall"/>
            <Unclipped val="false"/>

            <Frame type="Image" name="LogoImage">
                <Anchor relative="$parent"/>
                <IgnoreParentColor val="true"/>
                <Texture val="Assets/Textures/Blank.dds"/>
                <TextureType val="Normal"/>
                <Unclipped val="false"/>
            </Frame>
        </Frame>

        <Frame type="PlayerLogoImageFrame" name="PlayerRaceIcon_Pusher1">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <FrameLogoAlias val="LogoSmall"/>

            <Frame type="Image" name="LogoImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <IgnoreParentColor val="true"/>
                <ScaleWidthToTexture val="true"/>
                <TextureType val="Normal"/>
				<Alpha val="0"/>
