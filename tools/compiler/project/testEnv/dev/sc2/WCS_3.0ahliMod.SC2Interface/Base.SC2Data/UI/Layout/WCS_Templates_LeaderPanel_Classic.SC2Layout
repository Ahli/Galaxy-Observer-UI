        <!--
        ===================================
        Units Leaderpanel
        ===================================
        -->

        <Frame type="Frame" name="Template_Leaderpanel_Units" template="WCS_Templates_LeaderPanel_Classic/Template_StandardLeaderpanelContainerIcons">
            
            <StateGroup name="VisibleState">
            
                <State name="Visible">
                    <When type="Property" frame="$Leaderpanel_Units" visible="True"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <When type="Property" frame="$Leaderpanel_Units" visible="False"/>
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
            
            <Frame type="Image" name="PlayerIndicator" template="WCS_Templates_LeaderPanel_Classic/Template_PlayerIndicator"/>
            
            <Frame type="PlayerOwnedUnitsFrame" name="PlayerOwnedUnitsFrame" template="WCS_Templates_LeaderPanel_Classic/Template_PlayerOwnedUnitsFrame">
                <ShowUnits val="True"/>
                <ShowStructures val="False"/>
            </Frame>
        </Frame>
        
        
        <!--
        ===================================
        Structures Leaderpanel
        ===================================
        -->
        
        <Frame type="Frame" name="Template_Leaderpanel_Structures" template="WCS_Templates_LeaderPanel_Classic/Template_StandardLeaderpanelContainerIcons">
            
            <StateGroup name="VisibleState">
            
                <State name="Visible">
                    <When type="Property" frame="$Leaderpanel_Structures" visible="True"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <When type="Property" frame="$Leaderpanel_Structures" visible="False"/>
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
            
            <Frame type="Image" name="PlayerIndicator" template="WCS_Templates_LeaderPanel_Classic/Template_PlayerIndicator"/>
            
            <Frame type="PlayerOwnedUnitsFrame" name="PlayerOwnedUnitsFrame" template="WCS_Templates_LeaderPanel_Classic/Template_PlayerOwnedUnitsFrame">
                <ShowUnits val="False"/>
                <ShowStructures val="True"/>
            </Frame>
        </Frame>
        
    <!--

    =================================================

    LEADERPANEL INDIVIDUAL PLAYER TEMPLATE

    =================================================

    -->
        
        <Frame type="Frame" name="Template_Leaderpanel_PlayerContainer">
            <Anchor side="Bottom" relative="$this/PlayerData/StructuresTab" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <CollapseLayout val="True"/>
            
            <Frame type="PlayerFrame" name="PlayerData">
                <Anchor relative="$parent"/>
                <CustomTeamColor val="#Red" index="1"/>
                <CustomTeamColor val="#Blue" index="2"/>
                <CustomTeamColor val="#Teal" index="3"/>
                <CustomTeamColor val="#Purple" index="4"/>
                <CustomTeamColor val="#Yellow" index="5"/>
                <CustomTeamColor val="#Orange" index="6"/>
                <CustomTeamColor val="#Green" index="7"/>
                <CustomTeamColor val="#LightPink" index="8"/>
                <CustomTeamColor val="#Violet" index="9"/>
                <CustomTeamColor val="#LightGrey" index="10"/>
                <CustomTeamColor val="#DarkGreen" index="11"/>
                <CustomTeamColor val="#Brown" index="12"/>
                <CustomTeamColor val="#LightGreen" index="13"/>
                <CustomTeamColor val="#DarkGrey" index="14"/>
                <CustomTeamColor val="#Pink" index="15"/>
                
                <!-- APM Tab -->
                <Frame type="Frame" name="APMTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_APM">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                </Frame>
                
                <!-- Army Value Tab -->
                <Frame type="Frame" name="ArmyValueTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_ArmyValue">
                    <Anchor side="Top" relative="$parent/APMTab" pos="Max" offset="0"/>
                </Frame>
                
                <!-- Income Tab -->
                <Frame type="Frame" name="IncomeTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_Income">
                    <Anchor side="Top" relative="$parent/ArmyValueTab" pos="Max" offset="0"/>
                </Frame>
                
                <!-- Spending Tab -->
                <Frame type="Frame" name="SpendingTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_Spending">
                    <Anchor side="Top" relative="$parent/IncomeTab" pos="Max" offset="0"/>
                </Frame>
                
                <!-- Units Lost Tab -->
                <Frame type="Frame" name="UnitsLostTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_UnitsLost">
                    <Anchor side="Top" relative="$parent/SpendingTab" pos="Max" offset="0"/>
                </Frame>
                
                <!-- Production Tab -->
                <Frame type="Frame" name="ProductionTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_Production">
                    <Anchor side="Top" relative="$parent/UnitsLostTab" pos="Max" offset="0"/>
                </Frame>
                
                <!-- Upgrades Tab -->
                <Frame type="Frame" name="UpgradesTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_Upgrades">
                    <Anchor side="Top" relative="$parent/ProductionTab" pos="Max" offset="0"/>
                </Frame>
                
                <!-- Units Tab -->
                <Frame type="Frame" name="UnitsTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_Units">
                    <Anchor side="Top" relative="$parent/UpgradesTab" pos="Max" offset="0"/>
                </Frame>
                
                <!-- Structures Tab -->
                <Frame type="Frame" name="StructuresTab" template="WCS_Templates_LeaderPanel_Classic/Template_Leaderpanel_Structures">
                    <Anchor side="Top" relative="$parent/UnitsTab" pos="Max" offset="0"/>
                </Frame>
            </Frame>
        </Frame>
        
        <!--
        
        Classic Leaderpanel Template
        
        -->
        
        <Frame type="Frame" name="Template_Leaderpanel_Classic">
            <Width val="1600"/>
            <Height val="1000"/>
            
            <StateGroup name="GameTypeState">
                        
                <State name="1v1">
                    <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player2" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player3" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player4" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player2" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player3" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player4" visible="False"/>
                </State>
                
                <State name="2v2">
                    <When type="AnimationState" frame="$GameUI" versusMode="2v2"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player2" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player3" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player4" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player2" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player3" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player4" visible="False"/>
                </State>
                
                <State name="3v3">
                    <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player2" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player3" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player4" visible="False"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player2" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player3" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player4" visible="False"/>
                </State>
                
                <State name="4v4">
                    <When type="AnimationState" frame="$GameUI" versusMode="4v4"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player2" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player3" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamLeft/Player4" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player2" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player3" visible="True"/>
                    <Action type="SetProperty" frame="$this/TeamRight/Player4" visible="True"/>
                </State>
            </StateGroup>
            
            <Frame type="Label" name="Title">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
                <Height val="40"/>
                <Style val="@WCSPlayerName"/>
                <Text val="{$LeaderPanelLabel1/@Text}"/>
            </Frame>
            
            <!-- Categories Header -->
            <Frame type="Frame" name="CategoryHeader" template="WCS_Templates_LeaderPanel_Classic/Template_CategoryHeader">
                <Anchor side="Top" relative="$parent/Title" pos="Max" offset="5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            </Frame>
            
            <Frame type="Frame" name="TeamLeft">
                <Anchor side="Top" relative="$parent/CategoryHeader" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$this/Player4" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                
                <Frame type="Frame" name="Player1" template="WCS_TemplateS_LeaderPanel_Classic/Template_Leaderpanel_PlayerContainer">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Left"/>
                        <VersusModePlayerIndex val="0"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player2" template="WCS_TemplateS_LeaderPanel_Classic/Template_Leaderpanel_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player1" pos="Max" offset="#vOffset_LeaderPanel_Classic_Players"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Left"/>
                        <VersusModePlayerIndex val="1"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player3" template="WCS_TemplateS_LeaderPanel_Classic/Template_Leaderpanel_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player2" pos="Max" offset="#vOffset_LeaderPanel_Classic_Players"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Left"/>
                        <VersusModePlayerIndex val="2"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player4" template="WCS_TemplateS_LeaderPanel_Classic/Template_Leaderpanel_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player3" pos="Max" offset="#vOffset_LeaderPanel_Classic_Players"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Left"/>
                        <VersusModePlayerIndex val="3"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <Frame type="Image" name="Divider">
                <Anchor side="Top" relative="$parent/TeamLeft" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$LeaderPanel_Classic_CategoryHeader" pos="Max" offset="0"/>
                <Height val="6"/>
                <Color val="#Color_CenterBar_Background"/>
                <Alpha val="#Alpha_CenterBar_Background"/>
            </Frame>
            
            <Frame type="Frame" name="TeamRight">
                <Anchor side="Top" relative="$parent/Divider" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$this/Player4" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                
                <Frame type="Frame" name="Player1" template="WCS_TemplateS_LeaderPanel_Classic/Template_Leaderpanel_PlayerContainer">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Right"/>
                        <VersusModePlayerIndex val="0"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player2" template="WCS_TemplateS_LeaderPanel_Classic/Template_Leaderpanel_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player1" pos="Max" offset="#vOffset_LeaderPanel_Classic_Players"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Right"/>
                        <VersusModePlayerIndex val="1"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player3" template="WCS_TemplateS_LeaderPanel_Classic/Template_Leaderpanel_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player2" pos="Max" offset="#vOffset_LeaderPanel_Classic_Players"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Right"/>
                        <VersusModePlayerIndex val="2"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player4" template="WCS_TemplateS_LeaderPanel_Classic/Template_Leaderpanel_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player3" pos="Max" offset="#vOffset_LeaderPanel_Classic_Players"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Right"/>
                        <VersusModePlayerIndex val="3"/>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
</Desc>
