<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <!--

    =================================================

    PLAYER IDENTIFICATION TEMPLATES

    =================================================

    -->
        
        <!--
        ===================================
        Player Data Frame
        ===================================
        
        This is the frame that should contain all other frames specific to this player.  They will all point to this one as their ancestor
        to check to see what its "VersusModePlayerSide" and "VersusModePlayerIndex" properties are and assign those property
        values to themselves.
        
        In the final place where you are actually adding this frame to the GameUI you will need to make sure to set those two
        properties to the player you want, otherwise virtually nothing will work.
        -->
    
        <Frame type="PlayerFrame" name="Template_PlayerData">
            <Anchor relative="$parent"/>
            <CustomTeamColor val="#Red" index="1"/>
            <CustomTeamColor val="#Blue" index="2"/>
            <CustomTeamColor val="#Teal" index="3"/>
            <CustomTeamColor val="#Purple" index="4"/>
            <CustomTeamColor val="#Yellow" index="5"/>
            <CustomTeamColor val="#Orange" index="6"/>
            <CustomTeamColor val="#Green" index="7"/>
            <CustomTeamColor val="#LightPink" index="8"/>
            <CustomTeamColor val="#Violet" index="9"/>
            <CustomTeamColor val="#LightGrey" index="10"/>
            <CustomTeamColor val="#DarkGreen" index="11"/>
            <CustomTeamColor val="#Brown" index="12"/>
            <CustomTeamColor val="#LightGreen" index="13"/>
            <CustomTeamColor val="#DarkGrey" index="14"/>
            <CustomTeamColor val="#Pink" index="15"/>
        </Frame>

        
        <!-- 
        
        -->
        
        <Frame type="Label" name="Template_PlayerNameLabel">
            <Anchor relative="$parent"/>
            <Options val="NoWrapping"/>
            <Style val="WCSPlayerName"/>
            <IgnoreParentColor val="True"/>
            
            <StateGroup name="GameTypeState">
                
                <State name="1v1">
                    <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                    <Action type="SetProperty" frame="$this" style="WCSPlayerName"/>
                </State>
                
                <State name="2v2">
                    <When type="AnimationState" frame="$GameUI" versusMode="2v2"/>
                    <Action type="SetProperty" frame="$this" style="WCSPlayerName"/>
                </State>
                
                <State name="3v3">
                    <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                    <Action type="SetProperty" frame="$this" style="WCSPlayerName_Small"/>
                </State>
                
                <State name="4v4">
                    <When type="AnimationState" frame="$GameUI" versusMode="4v4"/>
                    <Action type="SetProperty" frame="$this" style="WCSPlayerName_Small"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!--
        ===================================
        Player name Labels
        ===================================
        
        This container handles displaying the player names, whether the default names, the names from the mod, or auto-detecting what to show.
        -->

        <Frame type="Frame" name="Template_PlayerNameLabelContainer">

            <!-- PlayerName Label (Standard) -->
            <Frame type="Frame" name="PlayerName_Label_Standard">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            
                <Frame type="PlayerNameFrame" name="PlayerNameFrame">
                    <Anchor relative="$parent"/>
                    <ShowClanTag val="false"/>
                    <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                    <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>
                    
                    <Frame type="Label" name="PlayerNameLabel" template="WCS_Templates_PlayerBar_Generic/Template_PlayerNameLabel">
                        <Anchor relative="$parent"/>
                        <Options val="NoWrapping"/>
                        <Style val="WCSPlayerName"/>
                        <IgnoreParentColor val="True"/>
                    </Frame>
                </Frame>
                
                <StateGroup name="VisibleState">

                    <State name="Visible">
                        <When type="Property" frame="$PlayerName_Standard" visible="True"/>
                        <Action type="SetProperty" frame="$this" visible="True"/>
                    </State>
                    
                    <State name="NotVisible">
                        <When type="Property" frame="$PlayerName_Standard" visible="False"/>
                        <Action type="SetProperty" frame="$this" visible="False"/>
                    </State>
                </StateGroup>
            </Frame>
            
            <!-- PlayerName Label (From Logos.SC2Mod) -->
            <Frame type="Frame" name="PlayerName_Label_FromMod">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                
                <Frame type="PlayerLogoTextFrame" name="PlayerLogoTextFrame">
                    <Anchor relative="$parent"/>
                    <FrameLogoAlias val="PlayerName"/>
                    
                    <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                    <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>

                    <Frame type="Label" name="LogoLabel" template="WCS_Templates_PlayerBar_Generic/Template_PlayerNameLabel">
                        <Anchor relative="$parent"/>
                        <Text val=" "/>
                        <IgnoreParentColor val="True"/>
                        <Style val="WCSPlayerName"/>
                    </Frame>
                </Frame>
                
                <StateGroup name="VisibleState">
            
                    <State name="Visible">
                        <When type="Property" frame="$PlayerName_FromMod" visible="True"/>
                        <Action type="SetProperty" frame="$this" visible="True"/>
                    </State>
                    
                    <State name="NotVisible">
                        <When type="Property" frame="$PlayerName_FromMod" visible="False"/>
                        <Action type="SetProperty" frame="$this" visible="False"/>
                    </State>
                </StateGroup>
            </Frame>
            
            <!-- PlayerName Label (Auto-detecting what to show between standard and Logos mod) (DEFAULT) -->
            <Frame type="Frame" name="PlayerName_Label_AutoDetect">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                
                <Frame type="Frame" name="Standard">
                    <Anchor relative="$parent"/>
                    
                    <Frame type="PlayerNameFrame" name="PlayerNameFrame">
                        <Anchor relative="$parent"/>
                        <ShowClanTag val="false"/>
                        <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                        <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>
                        <Visible val="False"/>
                        
                        <Frame type="Label" name="PlayerNameLabel" template="WCS_Templates_PlayerBar_Generic/Template_PlayerNameLabel">
                            <Anchor relative="$parent"/>
                            <Options val="NoWrapping"/>
                            <Style val="WCSPlayerName"/>
                            <IgnoreParentColor val="True"/>
                        </Frame>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="FromMod">
                    <Anchor relative="$parent"/>
                    <Visible val="False"/>
                    
                    <Frame type="PlayerLogoTextFrame" name="PlayerLogoTextFrame">
                        <Anchor relative="$parent"/>
                        <FrameLogoAlias val="PlayerName"/>
                        
                        <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                        <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>

                        <Frame type="Label" name="LogoLabel" template="WCS_Templates_PlayerBar_Generic/Template_PlayerNameLabel">
                            <Anchor relative="$parent"/>
                            <IgnoreParentColor val="True"/>
                            <Style val="WCSPlayerName"/>
                        </Frame>
                    </Frame>
                </Frame>
                
                <StateGroup name="TypeState">
                    <DefaultState val="FromMod"/>
            
                    <State name="Standard">
                        <When type="Property" frame="$this/FromMod/PlayerLogoTextFrame/LogoLabel" text=""/>
                        <Action type="SetProperty" frame="$this/Standard" visible="True"/>
                        <Action type="SetProperty" frame="$this/FromMod" visible="False"/>
                    </State>
                    
                    <State name="FromMod">
                        <Action type="SetProperty" frame="$this/Standard" visible="False"/>
                        <Action type="SetProperty" frame="$this/FromMod" visible="True"/>
                    </State>
                </StateGroup>
                
                <StateGroup name="VisibleState">
                
                    <State name="Visible">
                        <When type="Property" frame="$PlayerName_AutoDetect" visible="True"/>
                        <Action type="SetProperty" frame="$this" visible="True"/>
                    </State>
                    
                    <State name="NotVisible">
                        <When type="Property" frame="$PlayerName_AutoDetect" visible="False"/>
                        <Action type="SetProperty" frame="$this" visible="False"/>
                    </State>
                </StateGroup>
            </Frame>
        </Frame>

        <!--
        ===================================
        Team Logo
        ===================================
        
        This small frame displays the player's team logo when the player has one, the race logo when they don't.  If neither player
        has a team logo it will automatically hide itself.
        -->
        
        <Frame type="Frame" name="Template_TeamLogo">
            <Width val="#Height_PlayerBar"/>
            <Height val="#Height_PlayerBar"/>
            <CollapseLayout val="True"/>
            
            <Frame type="Image" name="Background">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Texture val="@WCS/RoundedRectangle_Small"/>
                <TextureType val="NineSlice"/>
                <Color val="#Color_RaceIcon_Background"/>
            </Frame>
            
            <Frame type="Frame" name="Logo">
                <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-2"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="2"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-2"/>
                <Visible val="False"/>
                
                <Frame type="PlayerLogoImageFrame" name="PlayerLogoImageFrame">
                    <Anchor relative="$parent"/>
                    <FrameLogoAlias val="LogoSmall"/>
                    <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                    <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>

                    <Frame type="Image" name="LogoImage">
                        <Anchor relative="$parent"/>
                        <TextureType val="Normal"/>
                        <BlendMode val="Normal"/>
                        <IgnoreParentColor val="True"/>
                        <Texture val="Assets/Textures/Blank.dds"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <Frame type="Frame" name="RaceIcon">
                <Anchor relative="$parent"/>
                <Visible val="False"/>
                
                <Frame type="PlayerRaceIconFrame" name="PlayerRaceIconFrame">
                    <Anchor relative="$parent"/>
                    <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                    <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>
                    <BaseTexture val="@WCS/Race"/>

                    <Frame type="Image" name="RaceImage">
                        <Anchor relative="$parent"/>
                        <TextureType val="Normal"/>
                        <IgnoreParentColor val="True"/>
                        <Color val="{$ancestor[@name=PlayerData]/@Color}"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <StateGroup name="TeamLogoState">
                <DefaultState val="Visible"/>
                
                <State name="NotVisible">
                    <When type="Property" frame="$this/Logo/PlayerLogoImageFrame/LogoImage" texture=""/>
                    <Action type="SetProperty" frame="$this/Logo" visible="False"/>
                    <Action type="SetProperty" frame="$this/RaceIcon" visible="True"/>
                </State>
                
                <State name="Visible">
                    <Action type="SetProperty" frame="$this/Logo" visible="True"/>
                    <Action type="SetProperty" frame="$this/RaceIcon" visible="False"/>
                </State>
            </StateGroup>
            
            <StateGroup name="VisibleState">
                <DefaultState val="Visible"/>
                
                <State name="NotVisible">
                    <When type="Property" frame="$TeamLogos_VisibleState" visible="False"/>
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
                
                <State name="Visible">
                    <When type="Property" frame="$TeamLogos_VisibleState" visible="True"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!--
        ===================================
        Complete player identification bar
        ===================================
        
        Dropping this into a PlayerData frame will automatically generate a display for the player's name, color, team logo and race icon.
        -->
        
        <Frame type="Frame" name="Template_PlayerIdentificationContainer">
            
            <Frame type="Image" name="Background">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Texture val="@WCS/RoundedRectangle_Small"/>
                <TextureType val="NineSlice"/>
                <Color val="{$ancestor[@name=PlayerData]/@Color}"/>
            </Frame>
            
            <Frame type="Frame" name="RaceIconContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-4"/>
                <Width val="41"/>
                <CollapseLayout val="True"/>
                
                <Frame type="Image" name="Background">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="-10"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@WCS/RoundedRectangle_Small"/>
                    <TextureType val="NineSlice"/>
                    <IgnoreParentColor val="True"/>
                    <Color val="70,70,70"/>
                    <BlendMode val="Subtract"/>
                </Frame>
                
                <Frame type="PlayerRaceIconFrame" name="RaceIconFrame">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="-20"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="1"/>
                    <Width val="40"/>
                    <Height val="40"/>
                    <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                    <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>
                    
                    <BaseTexture val="@WCS/Race"/>

                    <Frame type="Image" name="RaceImage">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                        <TextureType val="Normal"/>
                        <IgnoreParentColor val="True"/>
                        <Color val="255,255,255"/>
                    </Frame>
                </Frame>
                
                <StateGroup name="GameTypeState">
                
                    <State name="1v1">
                        <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                        <Action type="SetProperty" frame="$this" visible="True"/>
                    </State>
                    
                    <State name="2v2">
                        <When type="AnimationState" frame="$GameUI" versusMode="2v2"/>
                        <Action type="SetProperty" frame="$this" visible="True"/>
                    </State>
                    
                    <State name="3v3">
                        <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                        <Action type="SetProperty" frame="$this" visible="False"/>
                    </State>
                    
                    <State name="4v4">
                        <When type="AnimationState" frame="$GameUI" versusMode="4v4"/>
                        <Action type="SetProperty" frame="$this" visible="False"/>
                    </State>
                </StateGroup>
            </Frame>
            
            <Frame type="Image" name="PlayerName_RaceIconOverlap">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-42"/>
                <Texture val="@WCS/RoundedRectangle_Small"/>
                <TextureType val="NineSlice"/>
                <Color val="{$ancestor[@name=PlayerData]/@Color}"/>
                
                <StateGroup name="GameTypeState">
                
                    <State name="1v1">
                        <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                        <Action type="SetProperty" frame="$this" visible="True"/>
                    </State>
                    
                    <State name="2v2">
                        <When type="AnimationState" frame="$GameUI" versusMode="2v2"/>
                        <Action type="SetProperty" frame="$this" visible="True"/>
                    </State>
                    
                    <State name="3v3">
                        <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                        <Action type="SetProperty" frame="$this" visible="False"/>
                    </State>
                    
                    <State name="4v4">
                        <When type="AnimationState" frame="$GameUI" versusMode="4v4"/>
                        <Action type="SetProperty" frame="$this" visible="False"/>
                    </State>
                </StateGroup>
            </Frame>
            
            <!-- Team Logo -->
            <Frame type="Frame" name="TeamLogo" template="WCS_Templates_PlayerBar_Generic/Template_TeamLogo">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            </Frame>
            
            <Frame type="Frame" name="PlayerNameLabelContainer" template="WCS_Templates_PlayerBar_Generic/Template_PlayerNameLabelContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/TeamLogo" pos="Max" offset="8"/>
                <Anchor side="Right" relative="$parent/RaceIconContainer" pos="Min" offset="0"/>
                <PreserveAnchorOffset val="True"/>
            </Frame>
        </Frame>
        
        
        
    <!--

    =================================================

    PLAYER SCORE DATA TEMPLATES (Supply, Minerals etc)

    =================================================

    -->
    
        <!--
        ===================================
        Resource Label Template
        ===================================
        -->
        
        <Frame type="Label" name="Template_ResourceLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Style val="WCSResource_Standard"/>
            
            <StateGroup name="GameTypeState">
                
                <State name="1v1">
                    <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                    <Action type="SetProperty" frame="$this" style="WCSResource_Standard"/>
                </State>
                
                <State name="2v2">
                    <When type="AnimationState" frame="$GameUI" versusMode="2v2"/>
                    <Action type="SetProperty" frame="$this" style="WCSResource_Standard"/>
                </State>
                
                <State name="3v3">
                    <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                    <Action type="SetProperty" frame="$this" style="WCSResource_Small"/>
                </State>
                
                <State name="4v4">
                    <When type="AnimationState" frame="$GameUI" versusMode="4v4"/>
                    <Action type="SetProperty" frame="$this" style="WCSResource_Small"/>
                </State>
            </StateGroup>
        </Frame>

        <!--
        ===================================
        Player Supply Template
        ===================================
        -->
        
        <Frame type="PlayerSupplyFrame" name="PlayerSupply_Template">
            <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
            <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>
            
            <Frame type="Label" name="UsedLabel">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="14"/>
                <IgnoreParentColor val="true"/>
                <Style val="WCSSupplyUsed"/>
                
                <StateGroup name="GameTypeState">
                
                    <State name="1v1">
                        <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                        <Action type="SetProperty" frame="$this" style="WCSSupplyUsed"/>
                    </State>
                    
                    <State name="2v2">
                        <When type="AnimationState" frame="$GameUI" versusMode="2v2"/>
                        <Action type="SetProperty" frame="$this" style="WCSSupplyUsed_2v2"/>
                    </State>
                    
                    <State name="3v3">
                        <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                        <Action type="SetProperty" frame="$this" style="WCSSupplyUsed_3v3"/>
                    </State>
                    
                    <State name="4v4">
                        <When type="AnimationState" frame="$GameUI" versusMode="4v4"/>
                        <Action type="SetProperty" frame="$this" style="WCSSupplyUsed_4v4"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="Label" name="DividerLabel">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/UsedLabel" pos="Max" offset="0"/>
                <IgnoreParentColor val="true"/>
                <Style val="WCSSupplyCap"/>
                <Text val=" /"/>
            </Frame>

            <Frame type="Label" name="CapLabel">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/DividerLabel" pos="Max" offset="0"/>
                <IgnoreParentColor val="true"/>
                <Style val="WCSSupplyCap"/>
            </Frame>
        </Frame>
        
        <!--
        ===================================
        Player Minerals Template
        ===================================
        -->
        
        <Frame type="Frame" name="Template_Minerals">
            <Width val="#Width_Minerals"/>
            
            <Frame type="PlayerScoreValueFrame" name="PlayerScoreValueFrame">
                <Anchor relative="$parent"/>
                <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>
            
                <!-- Minerals -->
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$this/Label" pos="Max" offset="0"/>
                    <ScoreValue val="MineralsCurrent"/>
                    <AddCommas val="False"/>
                    
                    <Frame type="Label" name="Label" template="WCS_Templates_PlayerBar_Generic/Template_ResourceLabel">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Style val="WCSResource_Standard"/>
                        <Color val="#Color_Minerals"/>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
        
        <!--
        ===================================
        Player Minerals with Income Template
        ===================================
        -->
        
        <Frame type="Frame" name="Template_MineralsWithIncome">
            <Width val="#Width_Minerals"/>
            
            <Frame type="PlayerScoreValueFrame" name="PlayerScoreValueFrame">
                <Anchor relative="$parent"/>
                <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>
            
                <!-- Minerals -->
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$this/Label" pos="Max" offset="0"/>
                    <ScoreValue val="MineralsCurrent"/>
                    <AddCommas val="False"/>
                    
                    <Frame type="Label" name="Label" template="WCS_Templates_PlayerBar_Generic/Template_ResourceLabel">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Style val="WCSResource_Standard"/>
                        <Color val="#Color_Minerals"/>
                    </Frame>
                </Frame>
                
                <!-- Minerals Plus -->
                <Frame type="Label" name="MineralsPlus">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent/ScoreValueFrame0" pos="Max" offset="5"/>
                    <Style val="WCSResource_Small"/>
                    <Color val="#Color_MineralsIncome"/>
                    <Text val="+"/>
                </Frame>
                
                <!-- Minerals Income -->
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame1">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent/MineralsPlus" pos="Max" offset="-2"/>
                    <Anchor side="Right" relative="$this/Label" pos="Max" offset="0"/>
                    <ScoreValue val="MineralsCollectionRate"/>
                    <AddCommas val="False"/>
                    
                    <Frame type="Label" name="Label">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Style val="WCSResource_Small"/>
                        <Color val="#Color_MineralsIncome"/>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
        
        <!--
        ===================================
        Player Gas Template
        ===================================
        -->
        
        <Frame type="Frame" name="Template_Gas">
            <Width val="#Width_Gas"/>
            
            <Frame type="PlayerScoreValueFrame" name="PlayerScoreValueFrame">
                <Anchor relative="$parent"/>
                <VersusModePlayerSide val="{$ancestor[@name=PlayerData]/@VersusModePlayerSide}"/>
                <VersusModePlayerIndex val="{$ancestor[@name=PlayerData]/@VersusModePlayerIndex}"/>
            
                <!-- Gas -->
