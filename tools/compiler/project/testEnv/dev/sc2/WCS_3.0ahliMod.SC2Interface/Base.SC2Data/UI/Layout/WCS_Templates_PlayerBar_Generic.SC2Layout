                    <Action type="SetProperty" frame="MapScoreTop" visible="True"/>
                    <Action type="SetProperty" frame="MapScoreBottom" visible="False"/>
                </State>
                
                <State name="Bottom">
                    <When type="Property" frame="$ancestor[@name=PlayerData]" versusModePlayerSide="Right"/>
