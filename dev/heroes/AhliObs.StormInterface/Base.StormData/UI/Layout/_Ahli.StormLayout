	<!-- add life label to hero bars when on low life -->
	<Frame type="UnitStatusValidatorAnimElement" name="VitalityValueDisplayValidatorTemplate">
		<Anchor relative="$parent"/>
		<!-- potential validators:
			SourceLifeLT15Percent
			LifeLTOneThird
			TargetLifeLT35Percent
			CasterLifeLT25Percent
			TargetLifeLT30Percent
			TargetLifeLT25Percent
		-->
		<Validator val="SourceLifeLT15Percent"/>
		
		<Animation name="ShowValidators">
			<Event event="NoState" action="Reset"/>
			<Event event="SourceLifeLT15Percent" action="DirectionForward,Play"/>
			
			<Controller type="Visibility" end="Pause">
				<Key type="Bool" time="0" value="false"/>
				<Key type="Bool" time="1.0" value="false"/>
				<Key type="Bool" time="1.0" value="true"/>
				<Key type="Bool" time="2.0" value="true"/>
			</Controller>
			<Controller type="Fade" end="Pause" frame="VitalityLabel">
				<Key type="Curve" time="0" value="0"/>
				<Key type="Curve" time="1.0" value="0"/>
				<Key type="Curve" time="2.0" value="255"/>
			</Controller>
		</Animation>
		
		<Frame type="UnitStatusShieldedHealthLabel" name="VitalityLabel">
			<Anchor relative="$parent"/>
			<FractionDigits val="0"/>
			<HideMax val="true"/>
			<RoundMode val="up"/>
			<Frame type="Label" name="StatusLabel">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
				<Unclipped val="true"/>
				<Style val="AhliHeroLowVitalityLabel"/>
			</Frame>
		</Frame>
	</Frame>
