	<!-- FIX: KillLog victim border colors -->
	<Frame type="Frame" name="Item" file="KillLog">
		 <Frame type="Frame" name="TgtPortrait">
			<StateGroup name="Source">
				<!-- FIX: based on srccolor which works unless it is purple killing (hero dies to merc camp)-->
				<State name="Red">
					<When type="AnimationState" frame="$parent" SrcColor="Blue"/>
					<Action type="SetProperty" frame="PortraitFg" LightAdjustColor="{$ancestor[@name=KillLogPanel]/EnemyColors/@AdditionalColor[0]}"/>
					<Action type="SetProperty" frame="PortraitFg" DarkAdjustColor="{$ancestor[@name=KillLogPanel]/EnemyColors/@Color}"/>
					<Action type="SetProperty" frame="PortraitFgGlow" LightAdjustColor="{$ancestor[@name=KillLogPanel]/EnemyColors/@AdditionalColor[1]}"/>
					<Action type="SetProperty" frame="StrikethroughFrame/Image" DarkAdjustColor="{$ancestor[@name=KillLogPanel]/EnemyColors/@Color}"/>
					<Action type="SetProperty" frame="StrikethroughFrame/Glow" LightAdjustColor="{$ancestor[@name=KillLogPanel]/EnemyColors/@AdditionalColor[1]}"/>
					<Action type="SetProperty" frame="StrikethroughFrame/Glow" DarkAdjustColor="{$ancestor[@name=KillLogPanel]/EnemyColors/@AdditionalColor[1]}"/>
				</State>
				<State name="Blue">
					<When type="AnimationState" frame="$parent" SrcColor="Red"/>
					<Action type="SetProperty" frame="PortraitFg" LightAdjustColor="{$ancestor[@name=KillLogPanel]/AllyColors/@AdditionalColor[0]}"/>
					<Action type="SetProperty" frame="PortraitFg" DarkAdjustColor="{$ancestor[@name=KillLogPanel]/AllyColors/@Color}"/>
					<Action type="SetProperty" frame="PortraitFgGlow" LightAdjustColor="{$ancestor[@name=KillLogPanel]/AllyColors/@AdditionalColor[1]}"/>
					<Action type="SetProperty" frame="StrikethroughFrame/Image" DarkAdjustColor="{$ancestor[@name=KillLogPanel]/AllyColors/@Color}"/>
					<Action type="SetProperty" frame="StrikethroughFrame/Glow" LightAdjustColor="{$ancestor[@name=KillLogPanel]/AllyColors/@AdditionalColor[1]}"/>
					<Action type="SetProperty" frame="StrikethroughFrame/Glow" DarkAdjustColor="{$ancestor[@name=KillLogPanel]/AllyColors/@AdditionalColor[1]}"/>
				</State>
				<!-- Purple is buggy with player vision changes -->
				<State name="Red2">
