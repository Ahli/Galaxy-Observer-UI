							<Width val="62"/>
							<Height val="36"/>
							<AcceptsMouse val="false"/>
							<AcceptsMouseTooltip val="false"/>
							
							<Frame type="Image" name="IconImage">
								<Anchor side="Top" relative="$parent" pos="Min" offset="-8"/>
								<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
								<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
								<Anchor side="Bottom" relative="$parent" pos="Max" offset="8"/>
								<!-- for reference: 
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_talents"/> -->
							</Frame>
							
							<!-- extends StandardTemplates/UnitStatusTalentIconTemplate/TalentTierState -->
							<StateGroup name="TalentTierState_Ext">
								<DefaultState val="TierIrrelevant"/>

								<State name="TierHidden">
									<When type="AnimationState" frame="$this" TalentTierState="TierHidden"/>
									<When type="StateGroup" frame="$parent/$parent" StateControl="Varian"/>
									<Action type="SendEvent" frame="$ancestor[@type=HeroTalentsPanelPlayerFrame]" event="HideHeroic"/>
								</State>

								<State name="TierNotAvailable">
									<When type="AnimationState" frame="$this" TalentTierState="TierNotAvailable"/>
									<When type="StateGroup" frame="$parent/$parent" StateControl="Varian"/>
									<Action type="SendEvent" frame="$ancestor[@type=HeroTalentsPanelPlayerFrame]" event="HideHeroic"/>
								</State>

								<State name="TierAvailable">
									<When type="AnimationState" frame="$this" TalentTierState="TierAvailable"/>
									<When type="StateGroup" frame="$parent/$parent" StateControl="Varian"/>
									<Action type="SendEvent" frame="$ancestor[@type=HeroTalentsPanelPlayerFrame]" event="ShowHeroic"/>
