				</Frame>
				<Frame type="ProgressBar" name="ChargeFillProgress">
					<Anchor side="Top" relative="$parent/TracerPulseBombBackgroundImage" pos="Min" offset="0"/>
					<Anchor side="Left" relative="$parent/TracerPulseBombBackgroundImage" pos="Mid" offset="0"/>
					<Anchor side="Right" relative="$parent/TracerPulseBombBackgroundImage" pos="Mid" offset="0"/>
					<Width val="35"/>
					<Height val="38"/>
					<!-- @UI/Storm_UI_HUD_Tracer_Pulse_Bomb_Fill_Image 68x76 -->
