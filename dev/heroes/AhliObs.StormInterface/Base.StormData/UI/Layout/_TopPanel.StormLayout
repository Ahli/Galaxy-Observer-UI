	<Frame type="Frame" name="HeroOverviewTemplate_Base">
        <Height val="124"/>
		<RenderPriority val="513"/>
		
		<!-- Fade animation -->
		<Animation name="FadeAnim">
			<Event event="OnShown" action="DirectionForward,Play"/>
			<Event event="OnHidden" action="DirectionReverse,Play"/>
			
			<Controller type="Fade" end="Pause">
				<Key type="Curve" time="0" value="0"/>
				<Key type="Curve" time="1.0" value="255"/>
			</Controller>
			<Controller type="Visibility" end="Pause" frame="HeroStatus">
				<Key type="Bool" time="0.0" value="false"/>
				<Key type="Bool" time="1.0" value="true"/>
			</Controller>
		</Animation>
		
        <Frame type="Image" name="IconFrame">
            <Texture val="_Assets\Textures\storm_ui_ingame_obs_player_frame_blue.dds"/>
			<RenderType val="HDR"/>
        </Frame>
		
		<Frame type="Image" name="HeroPortraitImage">
			<RenderType val="HDR"/>
		</Frame>
		
		<Frame type="UnitStatusFrame" name="HeroStatus">
            <Anchor side="Top" relative="$parent" pos="Min" offset="64"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-36"/>
			<Height val="33"/>
            <Width val="134"/>			
			
			<Frame type="Frame" name="HeroicPlaceholder">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="-1"/>
				<Width val="1"/>
				<Visible val="false"/>
				
				<Animation name="Ahlimation">
					<Event event="HeroicShown" action="DirectionForward,Play"/>
					<Event event="HeroicHidden" action="DirectionReverse,Play"/>
					
					<Controller type="Dimension" dimension="Width" relative="False" percentage="False" end="Pause">
						<Key type="Curve" time="0" value="1" out="Fast"/>
						<Key type="Curve" time=".25" value="43" in="Slow"/>
					</Controller>
					
					<!-- HeroicShown/HeroicHidden events are not working 100% in replays with time jumps -> visibility is more persistent -->
					<Controller type="Visibility" end="Pause">
						<Key type="Bool" time="0" value="false"/>
						<Key type="Bool" time="0" value="true"/>
						<Key type="Bool" time="0.25" value="true"/>
					</Controller>
				</Animation>
				
			</Frame>
			
			<!-- Tracer Detection for Heroic -->
			<Frame type="UnitStatusBehaviorAnim" name="TracerDetector">
				<Anchor relative="$parent"/>
				<Behavior val="TracerReloadController"/>
				
				<!-- make placeholder visible, make cooldown visible -->
				<Animation name="TracerOn">
					<Event event="BehaviorCreate" action="Reset,Play"/>
					
					<Controller type="Event" end="Stop" frame="$parent/HeroicPlaceholder">
						<Key type="Event" time="0" event="HeroicShown"/>
					</Controller>
					<Controller type="Visibility" end="Pause" frame="$parent/TracerPulseBombChargeFrame">
						<Key type="Bool" time="0" value="True"/>
					</Controller>
				</Animation>
				<Animation name="TracerOff">
					<Event event="BehaviorDestroy" action="Reset,Play"/>
					
					<Controller type="Event" end="Stop" frame="$parent/HeroicPlaceholder">
						<Key type="Event" time="0" event="HeroicHidden"/>
					</Controller>
					<Controller type="Visibility" end="Pause" frame="$parent/TracerPulseBombChargeFrame">
						<Key type="Bool" time="0" value="False"/>
					</Controller>
				</Animation>
			</Frame>
			
			<!-- Tracer special heroic -->
			<Frame type="UnitStatusAbilCharge" name="TracerPulseBombChargeFrame" template="Tracer/TracerCommandCardFrame/UnitStatusFrame0/TracerPulseBombChargeFrame">
				<Visible val="false"/>
				<Frame type="Image" name="TracerPulseBombBackgroundImage">
					<Anchor side="Top" relative="$parent" pos="Min" offset="-23"/>
					<Anchor side="Left" relative="$parent" pos="Min" offset="3"/>
					<Width val="35"/>
