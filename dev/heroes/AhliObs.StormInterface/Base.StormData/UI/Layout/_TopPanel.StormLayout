						<!-- Tracer Detection for Heroic -->
						<Frame type="UnitStatusBehaviorAnim" name="TracerDetector">
							<Anchor relative="$parent"/>
							<Behavior val="TracerReloadController"/>
							
							<!-- make placeholder visible, make cooldown visible -->
							<Animation name="TracerOn">
								<Event event="BehaviorCreate" action="Reset,Play"/>
								
								<Controller type="Event" end="Stop" frame="$ancestor[@type=HeroTalentsPanelPlayerFrame]">
									<Key type="Event" time="0" event="ShowHeroic"/>
								</Controller>
								<Controller type="Visibility" end="Pause" frame="$parent">
									<Key type="Bool" time="0" value="True"/>
								</Controller>
							</Animation>
							<Animation name="TracerOff">
								<Event event="BehaviorDestroy" action="Reset,Play"/>
								
								<Controller type="Event" end="Stop" frame="$ancestor[@type=HeroTalentsPanelPlayerFrame]">
