									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="true"/>
								</State>

								<State name="TierAvailableTracer">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierAvailable"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="true"/>
