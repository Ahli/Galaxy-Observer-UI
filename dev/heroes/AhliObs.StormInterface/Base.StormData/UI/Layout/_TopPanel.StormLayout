<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
	<!-- created by Ahli
			twitter.com/AhliSC2 -->

	<!-- hero overview at the top -->
	<!-- original height: 104 -->

	<Frame type="HeroLeaderPanelPlayerFrame" name="HeroicTalentPlayerFrameBase">
		<Batch val="false"/>
		
		<Animation name="HeroicShownAnimation">
			<Event event="ShowHeroic" action="Play"/>
		</Animation>
		<Animation name="HeroicHiddenAnimation">
			<Event event="HideHeroic" action="Play"/>
		</Animation>
		
		<!-- icon + cooldown label -->
		<Frame type="Frame" name="TalentsContainerFrame">
			<Batch val="false"/>
			<BatchImages val="false"/>
			<BatchText val="false"/>
			<BatchGroup val="false"/>
			<RenderPriority val="600"/>
			
			<Frame type="SelectionUnitStatusPanel" name="SelectionUnitStatusPanel">
				<Frame type="UnitStatusFrame" name="UnitStatusFrame0">
					<Frame type="PlayerHeroSelectedTalentTreeFrame" name="HeroSelectedTalentTreeFrame">
						<RenderPriority val="512"/>
						<!-- hide other icons -->
						<Frame type="UnitStatusTalentIcon" name="TalentTreeIcon1">
							<Anchor side="Top" relative="$parent" pos="Min" offset="-999"/>
							<Visible val="false"/>
						</Frame>
						<Frame type="UnitStatusTalentIcon" name="TalentTreeIcon2">
							<Anchor side="Top" relative="$parent" pos="Min" offset="-999"/>
							<Visible val="false"/>
						</Frame>
						<Frame type="UnitStatusTalentIcon" name="TalentTreeIcon3">
							<Anchor side="Top" relative="$parent" pos="Min" offset="-999"/>
							<Visible val="false"/>
						</Frame>
						<Frame type="UnitStatusTalentIcon" name="TalentTreeIcon5">
							<Anchor side="Top" relative="$parent" pos="Min" offset="-999"/>
							<Visible val="false"/>
						</Frame>
						<Frame type="UnitStatusTalentIcon" name="TalentTreeIcon6">
							<Anchor side="Top" relative="$parent" pos="Min" offset="-999"/>
							<Visible val="false"/>
						</Frame>
						<Frame type="UnitStatusTalentIcon" name="TalentTreeIcon7">
							<Anchor side="Top" relative="$parent" pos="Min" offset="-999"/>
							<Visible val="false"/>
						</Frame>
						<!-- heroic icon -->
						<Frame type="UnitStatusTalentIcon" name="TalentTreeIcon4">
							<Anchor side="Top" relative="$parent" pos="Min" offset="44"/>
							<Anchor side="Left" relative="$parent" pos="Min" offset="-12"/>
							<Width val="62"/>
							<Height val="36"/>
							<Frame type="Image" name="IconImage">
								<Anchor side="Top" relative="$parent" pos="Min" offset="-8"/>
								<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
								<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
								<Anchor side="Bottom" relative="$parent" pos="Max" offset="8"/>
								<!-- for reference: 
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_talents"/> -->
							</Frame>
							
							<!-- extends StandardTemplates/UnitStatusTalentIconTemplate/TalentTierState -->
							<StateGroup name="TalentTierState2">
								<DefaultState val="TierNotAvailable"/>

								<State name="TierHidden">
									<When type="AnimationState" frame="$this" TalentTierState="TierHidden"/>
									<Action type="SendEvent" frame="$ancestor[@type=HeroLeaderPanelPlayerFrame]" event="HideHeroic"/>
								</State>

								<State name="TierNotAvailable">
									<When type="AnimationState" frame="$this" TalentTierState="TierNotAvailable"/>
									<Action type="SendEvent" frame="$ancestor[@type=HeroLeaderPanelPlayerFrame]" event="HideHeroic"/>
								</State>

								<State name="TierAvailable">
									<When type="AnimationState" frame="$this" TalentTierState="TierAvailable"/>
									<Action type="SendEvent" frame="$ancestor[@type=HeroLeaderPanelPlayerFrame]" event="ShowHeroic"/>
								</State>

								<State name="TierSelected">
									<When type="AnimationState" frame="$this" TalentTierState="TierSelected"/>
									<Action type="SendEvent" frame="$ancestor[@type=HeroLeaderPanelPlayerFrame]" event="ShowHeroic"/>
								</State>
							</StateGroup>
							
						</Frame>
						
						<!-- new border image -->
						<Frame type="Frame" name="TalentTreeIcon4Border">
							<Anchor side="Top" relative="$parent\TalentTreeIcon4" pos="Min" offset="-2"/>
							<Anchor side="Left" relative="$parent\TalentTreeIcon4" pos="Min" offset="-2"/>
							<Anchor side="Right" relative="$parent\TalentTreeIcon4" pos="Max" offset="2"/>
							<Anchor side="Bottom" relative="$parent\TalentTreeIcon4" pos="Max" offset="2"/>
							<Visible val="false"/>
							<RenderPriority val="1"/>
							
							<Frame type="Image" name="Image">
								<Anchor side="Top" relative="$parent" pos="Min" offset="-8"/>
								<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
								<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
								<Anchor side="Bottom" relative="$parent" pos="Max" offset="8"/>
								<Texture val="Assets/Textures/white32.dds"/>
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_talents"/>
								<IgnoreParentColor val="true"/>
								<RenderPriority val="1"/>
							</Frame>
							
							<StateGroup name="BackgroundState">
								<DefaultState val="TierNotAvailable"/>
								
								<State name="TierHidden">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierHidden"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="false"/>
									<Action type="SetProperty" frame="$this" visible="false"/>
									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="false"/>
								</State>

								<State name="TierNotAvailable">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierNotAvailable"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="false"/>
									<Action type="SetProperty" frame="$this" visible="false"/>
									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="false"/>
								</State>

								<State name="TierAvailable">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierAvailable"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="false"/>
									<Action type="SetProperty" frame="$this" visible="true"/>
									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="false"/>
								</State>

								<State name="TierSelected">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierSelected"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="false"/>
									<Action type="SetProperty" frame="$this" visible="true"/>
									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="false"/>
								</State>

								<State name="TierHiddenTracer">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierHidden"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="true"/>
									<Action type="SetProperty" frame="$this" visible="true"/>
									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="true"/>
								</State>

								<State name="TierNotAvailableTracer">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierNotAvailable"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="true"/>
									<Action type="SetProperty" frame="$this" visible="true"/>
									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="true"/>
								</State>

								<State name="TierAvailableTracer">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierAvailable"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="true"/>
									<Action type="SetProperty" frame="$this" visible="true"/>
									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="false"/>
								</State>

								<State name="TierSelectedTracer">
									<When type="AnimationState" frame="$parent/TalentTreeIcon4" TalentTierState="TierSelected"/>
									<When type="Property" frame="$parent/$parent/TracerChargeContainer" visible="true"/>
									<Action type="SetProperty" frame="$this" visible="true"/>
									<Action type="SetProperty" frame="$parent/$parent/TracerChargeContainer/TracerAbilityImage" visible="false"/>
								</State>
								
							</StateGroup>
						</Frame>
						
					</Frame>
					
					<Frame type="UnitStatusAbilIcon" name="UnitStatusAbil">
						<RenderPriority val="513"/>
						<Frame type="AbilIcon" name="AbilIcon">
							<Frame type="Label" name="CooldownLabel">
								<Style val="AhliHeroicCooldown"/>
								<Unclipped val="true"/>
								<RenderPriority val="650"/>
								
								<Animation name="DarkenIcon">
									<Event event="OnShown" action="DirectionForward, Play"/>
									<Event event="OnHidden" action="DirectionReverse, Play"/>
									
									<Controller type="Color" end="Pause" frame="$parent/$parent/$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage">
										<Key type="Curve" time="0.0" value="255;255;255" out="Step"/>
										<Key type="Curve" time="0.01" value="64;64;64" out="Step"/>
									</Controller>
								</Animation>
								
							</Frame>
							<Frame type="Image" name="CooldownImage">
								<Visible val="false"/>
								<Alpha val="0"/>
							</Frame>
							
							<Frame type="Image" name="CooldownImage2">
								<Visible val="false"/>
							</Frame>
							<StateGroup name="CDImageVisibility">
								<State name="Hidden">
									<!-- never -->
									<When type="Property" frame="$this" visible="True"/>
									<When type="Property" frame="$this" visible="False"/>
								</State>
								<State name="Showing">
									<!-- never -->
									<When type="Property" frame="$this" visible="True"/>
									<When type="Property" frame="$this" visible="False"/>
								</State>
							</StateGroup>
							
						</Frame>
					</Frame>
					
					<Frame type="Frame" name="TracerChargeContainer">
						<Anchor side="Top" relative="$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Min" offset="-8"/>
						<Anchor side="Left" relative="$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Min" offset="-2"/>
						<Anchor side="Right" relative="$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Max" offset="2"/>
						<Anchor side="Bottom" relative="$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Max" offset="8"/>
						<Visible val="false"/>
						<RenderPriority val="600"/>
						
						<!-- Abil Icon pre 10 (heroic bg image controls its visibility) -->
						<Frame type="Frame" name="TracerAbilityImage">
							<Anchor relative="$parent/$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4"/>
							<RenderPriority val="1"/>
							<Visible val="false"/>
							
							<Frame type="Image" name="Image">
								<Anchor side="Top" relative="$parent" pos="Min" offset="-8"/>
								<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
								<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
								<Anchor side="Bottom" relative="$parent" pos="Max" offset="8"/>
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_talents"/>
								<IgnoreParentColor val="True"/>
								<Texture val="Assets\Textures\storm_ui_icon_tracer_pulsebomb.dds"/>
								<RenderPriority val="1"/>
							</Frame>
							
							<Animation name="DarkenIcon">
								<Event event="DarkIcon" action="DirectionForward, Play"/>
								<Event event="BrightIcon" action="DirectionReverse, Play"/>
								
								<Controller type="Color" end="Pause" frame="Image">
									<Key type="Curve" time="0.0" value="255;255;255" out="Step"/>
									<Key type="Curve" time="0.01" value="64;64;64" out="Step"/>
								</Controller>
								<Controller type="Color" end="Pause" frame="$parent/$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage">
									<Key type="Curve" time="0.0" value="255;255;255" out="Step"/>
									<Key type="Curve" time="0.01" value="64;64;64" out="Step"/>
								</Controller>
							</Animation>
						</Frame>
						
						<!-- Blizzard does weird things with this frame's renderpriority... -->
						<Frame type="UnitStatusAbilCharge" name="TracerPulseBombChargeFrame" template="Tracer/TracerCommandCardFrame/UnitStatusFrame0/TracerPulseBombChargeFrame">
							<Anchor relative="$parent"/>
							<RenderPriority val="1000"/>
							<!-- this frame determines the position -->
							<Frame type="Image" name="TracerPulseBombBackgroundImage">
								<Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
								<Anchor side="Left" relative="$parent" pos="Min" offset="14"/>
								<Width val="35"/>
								<Height val="38"/>
								<!-- @UI/Storm_UI_HUD_Tracer_Pulse_Bomb_Background 68x76 -->
								<!-- <TextureCoords top="0.2097" left="0.0" bottom="0.7903" right="1.0"/>
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_heroic"/>
								<AlphaMaskTexture val=""/> -->
								<Unclipped val="true"/>
								<Alpha val="128"/>
								<RenderPriority val="1000"/>
								
								<Animation name="NotEnoughCharges">
									<Event event="OnShown" action="Play"/>
									<Controller type="Event" end="Stop" frame="$parent/$parent/TracerAbilityImage">
										<Key type="Event" time="0" event="DarkIcon"/>
									</Controller>
								</Animation>
								<Animation name="Usable">
									<Event event="OnHidden" action="Play"/>
									<Controller type="Event" end="Stop" frame="$parent/$parent/TracerAbilityImage">
										<Key type="Event" time="0" event="BrightIcon"/>
									</Controller>
								</Animation>
							</Frame>
							<Frame type="ProgressBar" name="ChargeFillProgress">
								<Anchor side="Top" relative="$parent/TracerPulseBombBackgroundImage" pos="Min" offset="0"/>
								<Anchor side="Left" relative="$parent/TracerPulseBombBackgroundImage" pos="Mid" offset="0"/>
								<Anchor side="Right" relative="$parent/TracerPulseBombBackgroundImage" pos="Mid" offset="0"/>
								<Width val="35"/>
								<Height val="38"/>
								<!-- @UI/Storm_UI_HUD_Tracer_Pulse_Bomb_Fill_Image 68x76 -->
								<Unclipped val="true"/>
								<Vertical val="False"/>
								<RenderPriority val="1000"/>
								<Frame type="Frame" name="FillImageContainer">
									<RenderPriority val="1000"/>
									<Frame type="Image" name="FillImage">
										<RenderPriority val="1000"/>
									</Frame>
								</Frame>
							</Frame>
							<Frame type="Label" name="ChargesLabel">
								<Anchor side="Top" relative="$parent/TracerPulseBombBackgroundImage" pos="Min" offset="6"/>
								<Anchor side="Right" relative="$parent/TracerPulseBombBackgroundImage" pos="Max" offset="2"/>
								<Anchor side="Left" relative="$parent/TracerPulseBombBackgroundImage" pos="Min" offset="2"/>
								<Width val="0"/>
								<Style val="Storm_HUD_Tracer_Pulse_Bomb_Charge_Ahli"/>
								<Unclipped val="true"/>
								<RenderPriority val="1000"/>
							</Frame>
							<!-- hidden -->
							<Frame type="Image" name="PulseBombReadyPingImage">
								<!-- @UI/Storm_UI_HeroUI_Greymane_PulseRing 120x120 animation alters height/width -->
								<Anchor side="Top" relative="$parent/TracerPulseBombBackgroundImage" pos="Mid" offset="9999"/>
								<Anchor side="Left" relative="$parent/TracerPulseBombBackgroundImage" pos="Mid" offset="9999"/>
								<Anchor side="Right" relative="$parent/TracerPulseBombBackgroundImage" pos="Mid" offset="9999"/>
								<Anchor side="Bottom" relative="$parent/TracerPulseBombBackgroundImage" pos="Mid" offset="9999"/>
							</Frame>
							<!-- hidden, no space for % -->
							<Frame type="Label" name="ChargePercentLabel">
								<Visible val="false"/>
							</Frame>

							<Animation name="Show/HideProgressBarAnims">
								<Event event="ChargesFull" action="Restart, DirectionForward, Play"/>
								<Event event="ChargesEmpty" action="Stop"/>
								<Event event="CurrentChanged_1" action="Stop"/>

								<Controller type="Visibility" end="Pause" frame="TracerPulseBombBackgroundImage">
									<Key type="Bool" time="0" value="False"/>
								</Controller>
								<Controller type="Visibility" end="Pause" frame="ChargeFillProgress/FillImageContainer">
									<Key type="Bool" time="0" value="False"/>
								</Controller>
								<Controller type="Visibility" end="Pause" frame="ChargesLabel">
									<Key type="Bool" time="0" value="False"/>
								</Controller>
								<!-- disabled actions -->
								<Controller type="Visibility" end="Pause" Frame="ChargePercentLabel">
								</Controller>
								<Controller type="Visibility" end="Pause" frame="PulseBombReadyPingImage">
								</Controller>
							</Animation>
						</Frame>
						
						<!-- Tracer Detection for Heroic -->
						<Frame type="UnitStatusBehaviorAnim" name="TracerDetector">
							<Anchor relative="$parent"/>
							<Behavior val="TracerReloadController"/>
							
							<!-- make placeholder visible, make cooldown visible -->
							<Animation name="TracerOn">
								<Event event="BehaviorCreate" action="Reset,Play"/>
								
								<Controller type="Event" end="Stop" frame="$ancestor[@type=HeroLeaderPanelPlayerFrame]">
									<Key type="Event" time="0" event="ShowHeroic"/>
								</Controller>
								<Controller type="Visibility" end="Pause" frame="$parent">
									<Key type="Bool" time="0" value="True"/>
								</Controller>
							</Animation>
							<Animation name="TracerOff">
								<Event event="BehaviorDestroy" action="Reset,Play"/>
								
								<Controller type="Event" end="Stop" frame="$ancestor[@type=HeroLeaderPanelPlayerFrame]">
									<Key type="Event" time="0" event="HideHeroic"/>
								</Controller>
								<Controller type="Visibility" end="Pause" frame="$parent">
									<Key type="Bool" time="0" value="False"/>
								</Controller>
							</Animation>
						</Frame>
						
					</Frame>
				</Frame>
			</Frame>
		</Frame>
	</Frame>
	<Frame type="HeroLeaderPanelPlayerFrame" name="HeroicTalentPlayerFrameBlue" template="_TopPanel/HeroicTalentPlayerFrameBase">
		<Frame type="Frame" name="TalentsContainerFrame">
			<Frame type="SelectionUnitStatusPanel" name="SelectionUnitStatusPanel">
				<Frame type="UnitStatusFrame" name="UnitStatusFrame0">
				
					<Frame type="PlayerHeroSelectedTalentTreeFrame" name="HeroSelectedTalentTreeFrame">
						<Frame type="Frame" name="TalentTreeIcon4Border">
							<Frame type="Image" name="Image">
								<Color val="4790C1"/>
							</Frame>
						</Frame>
						<Frame type="Frame" name="TalentTreeIcon4BorderTracer">
							<Frame type="Image" name="Image">
								<Color val="4790C1"/>
							</Frame>
						</Frame>
					</Frame>
				</Frame>
			</Frame>
		</Frame>
	</Frame>
	<Frame type="HeroLeaderPanelPlayerFrame" name="HeroicTalentPlayerFrameRed" template="_TopPanel/HeroicTalentPlayerFrameBase">
		<Frame type="Frame" name="TalentsContainerFrame">
			<Frame type="SelectionUnitStatusPanel" name="SelectionUnitStatusPanel">
				<Frame type="UnitStatusFrame" name="UnitStatusFrame0">
				
					<Frame type="PlayerHeroSelectedTalentTreeFrame" name="HeroSelectedTalentTreeFrame">
						<Frame type="UnitStatusTalentIcon" name="TalentTreeIcon4">
							<Anchor side="Left" relative="$parent" pos="Min" offset="16"/>
						</Frame>
						<Frame type="Frame" name="TalentTreeIcon4Border">
							<Frame type="Image" name="Image">
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_talents_mirrored"/>
								<Color val="FF3D3D"/>
							</Frame>
						</Frame>
						<Frame type="Frame" name="TalentTreeIcon4BorderTracer">
							<Frame type="Image" name="Image">
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_talents_mirrored"/>
								<Color val="FF3D3D"/>
							</Frame>
						</Frame>
					</Frame>
					<Frame type="Frame" name="TracerChargeContainer">
						<Frame type="UnitStatusAbilCharge" name="TracerPulseBombChargeFrame">
							<Frame type="Image" name="TracerPulseBombBackgroundImage">
								<Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
							</Frame>
						</Frame>
						<Frame type="Frame" name="TracerAbilityImage">
							<Frame type="Image" name="Image">
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_talents_mirrored"/>
							</Frame>
						</Frame>
					</Frame>
				</Frame>
			</Frame>
		</Frame>
	</Frame>



	<Frame type="Frame" name="HeroOverviewTemplate_Base">
		<Height val="124"/>
		<RenderPriority val="512"/>
		
		<!-- Fade animation -->
		<Animation name="FadeAnim">
			<Event event="OnShown" action="DirectionForward,Play"/>
			<Event event="OnHidden" action="DirectionReverse,Play"/>
			
			<Controller type="Fade" end="Pause">
				<Key type="Curve" time="0" value="0"/>
				<Key type="Curve" time="1.0" value="255"/>
			</Controller>
			<Controller type="Visibility" end="Pause" frame="HeroStatus">
				<Key type="Bool" time="0.0" value="false"/>
				<Key type="Bool" time="1.0" value="true"/>
			</Controller>
		</Animation>
		
		<Frame type="Image" name="IconFrame">
			<Texture val="_Assets\Textures\storm_ui_ingame_obs_player_frame_blue.dds"/>
			<RenderPriority val="512"/>
		</Frame>
		
		<Frame type="Image" name="HeroPortraitImage">
			<RenderPriority val="511"/>
		</Frame>
		
		<!-- custom vehicle portrait -->
		<Frame type="Frame" name="VehiclePortrait">
			<Anchor relative="$parent/HeroPortraitImage"/>
			<Visible val="false"/>
			<RenderPriority val="511"/>
			
			<Animation name="FadeLoop">
				<Event event="OnShown" action="Reset,DirectionForward,Play"/>
				<Event event="OnHidden" action="Stop"/>
				
				<Controller type="Fade" end="Loop">
					<Key type="Curve" time="0" value="255"/>
					<Key type="Curve" time="2.0" value="255"/>
					<Key type="Curve" time="2.5" value="0"/>
					<Key type="Curve" time="4.5" value="0"/>
					<Key type="Curve" time="5.0" value="255"/>
				</Controller>
			</Animation>
			
			<Frame type="Image" name="DragonKnightIcon">
				<Anchor relative="$parent"/>
				<Unclipped val="true"/>
				<!-- size: 184x164
				<Texture val="Assets/Textures/ui_ingame_mapmechanic_loadscreen_dragonshire_icon3.dds"/> -->
				<!-- size: 197x220
				<Texture val="Assets/Textures/hud_minimap_dragon_dragonhud.dds"/> -->
				<Texture val="Assets/Textures/ui_ingame_mapmechanic_loadscreen_dragonshire.dds"/>
				<TextureCoords top="0.1574" left="0.4328" bottom="0.3241" right="0.6229"/>
				<Visible val="false"/>
				<AlphaMaskTexture val="@UI/obs_alphaMask_portrait"/>
			</Frame>
			<Frame type="Image" name="GardenTerrorIcon">
				<!-- <Anchor side="Top" relative="$parent" pos="Min" offset="-50"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
				<Width val="100"/>
				<Height val="100"/> -->
				<Anchor relative="$parent"/>
				<Unclipped val="true"/>
				<!-- size: 300x300
				<Texture val="Assets/Textures/ui_ingame_mapmechanic_loadscreen_gardenofterror_icon3.dds"/> -->
				<Texture val="Assets/Textures/ui_ingame_mapmechanic_loadscreen_gardenofterror.dds"/>
				<TextureCoords top="0.1481" left="0.4047" bottom="0.3463" right="0.6521"/>
				<Visible val="false"/>
				<AlphaMaskTexture val="@UI/obs_alphaMask_portrait"/>
			</Frame>
			
			<StateGroup name="MapMechanicState">
				<DefaultState val="Nothing"/>
				<State name="DragonShire">
					<When type="Property" frame="$ancestor[@name=UIContainer]/ConsoleUIContainer/MapMechanicPanel/DragonShirePanel" visible="True"/>
					<Action type="SetProperty" frame="DragonKnightIcon" visible="True"/>
				</State>
				<State name="GardenOfTerror">
					<When type="Property" frame="$ancestor[@name=UIContainer]/ConsoleUIContainer/MapMechanicPanel/GardenOfTerrorPanel" visible="True"/>
					<Action type="SetProperty" frame="GardenTerrorIcon" visible="True"/>
				</State>
