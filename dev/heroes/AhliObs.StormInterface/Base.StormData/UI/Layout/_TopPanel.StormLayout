								<Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
								<Anchor side="Left" relative="$parent" pos="Min" offset="14"/>
								<Width val="35"/>
								<Height val="38"/>
								<!-- @UI/Storm_UI_HUD_Tracer_Pulse_Bomb_Background 68x76 -->
								<!-- <TextureCoords top="0.2097" left="0.0" bottom="0.7903" right="1.0"/>
								<ImageMaskTexture val="@UI/obs_alphaMask_mini_heroic"/>
								<AlphaMaskTexture val=""/> -->
								<Unclipped val="true"/>
