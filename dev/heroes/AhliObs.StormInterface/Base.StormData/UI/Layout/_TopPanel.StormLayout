				</Frame>
				<Frame type="Label" name="ChargesLabel">
					<Anchor side="Top" relative="$parent/TracerPulseBombBackgroundImage" pos="Min" offset="6"/>
					<Anchor side="Right" relative="$parent/TracerPulseBombBackgroundImage" pos="Max" offset="2"/>
