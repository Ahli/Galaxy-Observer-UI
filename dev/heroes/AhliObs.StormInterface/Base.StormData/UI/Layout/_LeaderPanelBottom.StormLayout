		<!-- adapted from GameStatsPanel/SelectedTalentTreeIconImageTemplate/TalentQuestProgress -->
		<StateGroup name="TalentQuestProgress">
			<DefaultState val="NoQuest"/>
			<State name="HasQuest">
				<When type="Property" frame="$OVERRIDE_ME" QuestProgress="QuestInProgress"/>
				<Action type="SetProperty" frame="QuestProgressBar" visible="True"/>
				<Action type="SetProperty" frame="UnfilledProgressImage" visible="True"/>
				<Action type="SetProperty" frame="QuestCompleteImage" visible="False"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel" visible="True"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel2" visible="True"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel3" visible="True"/>
				<Action type="SetAnchor" frame="VitalLabel0" side="Top" relative="$parent" pos="Min" offset="85"/>
				<Action type="SetAnchor" frame="QuestProgressStackLabel" side="Top" relative="$parent/QuestProgressBar" pos="Max" offset="-2"/>
				<Action type="SetAnchor" frame="QuestProgressStackLabel" side="Left" relative="$parent/QuestProgressBar" pos="Min" offset="0"/>
			</State>
			<State name="HasUntrackedQuest">
				<When type="Property" frame="$OVERRIDE_ME" QuestProgress="ProgressUntracked"/>
				<Action type="SetProperty" frame="QuestProgressBar" visible="False"/>
				<Action type="SetProperty" frame="UnfilledProgressImage" visible="True"/>
				<Action type="SetProperty" frame="QuestCompleteImage" visible="False"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel" visible="True"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel2" visible="False"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel3" visible="False"/>
				<Action type="SetAnchor" frame="VitalLabel0" side="Top" relative="$parent" pos="Min" offset="85"/>
				<Action type="SetAnchor" frame="QuestProgressStackLabel" side="Top" relative="$parent/QuestProgressBar" pos="Max" offset="-2"/>
				<Action type="SetAnchor" frame="QuestProgressStackLabel" side="Left" relative="$parent/QuestProgressBar" pos="Min" offset="0"/>
			</State>
			<State name="FinishedQuest">
				<When type="Property" frame="$OVERRIDE_ME" QuestProgress="FinishedQuest"/>
				<Action type="SetProperty" frame="QuestProgressBar" visible="False"/>
				<Action type="SetProperty" frame="UnfilledProgressImage" visible="True"/>
				<Action type="SetProperty" frame="QuestCompleteImage" visible="True"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel" visible="False"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel2" visible="False"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel3" visible="False"/>
				<Action type="SetAnchor" frame="VitalLabel0" side="Top" relative="$parent" pos="Min" offset="70"/>
				<Action type="SetAnchor" frame="QuestProgressStackLabel" side="Top" relative="$parent/QuestProgressBar" pos="Max" offset="-2"/>
				<Action type="SetAnchor" frame="QuestProgressStackLabel" side="Left" relative="$parent/QuestProgressBar" pos="Max" offset="-8"/>
			</State>
			<State name="FinishedUnlimitedQuest">
				<When type="Property" frame="$OVERRIDE_ME" QuestProgress="FinishedUnlimitedQuest"/>
				<Action type="SetProperty" frame="QuestProgressBar" visible="False"/>
				<Action type="SetProperty" frame="UnfilledProgressImage" visible="True"/>
				<Action type="SetProperty" frame="QuestCompleteImage" visible="True"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel" visible="True"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel2" visible="True"/>
				<Action type="SetProperty" frame="QuestProgressStackLabel3" visible="True"/>
				<Action type="SetAnchor" frame="VitalLabel0" side="Top" relative="$parent" pos="Min" offset="85"/>
				<Action type="SetAnchor" frame="QuestProgressStackLabel" side="Top" relative="$parent/QuestProgressBar" pos="Max" offset="-2"/>
				<Action type="SetAnchor" frame="QuestProgressStackLabel" side="Left" relative="$parent/QuestProgressBar" pos="Min" offset="0"/>
			</State>
			<State name="NoQuest">
