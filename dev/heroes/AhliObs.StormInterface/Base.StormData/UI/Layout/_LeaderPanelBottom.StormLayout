					<OptionalScoreValue val="true"/>
					<HideZero val="true"/>
					<Frame type="Label" name="Label">
						<Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
					</Frame>
				</Frame>
				<Frame type="Image" name="Icon">
					<Anchor side="Left" relative="$parent" pos="Min" offset="-15"/>
					<Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
					<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
					<Texture val="Assets\Textures\storm_ui_play_filter_support-on.dds"/>
					<Unclipped val="true"/>
					<!--<GlowColor val="{$GameUI/TeamOrderColors/@Color}"/>-->
					<ColorAdjustMode val="Colorize"/>
					<LightAdjustColor val="#ColorWhite"/>
					<DarkAdjustColor val="{$GameUI/TeamOrderColors/@Color}"/>
					<IgnoreParentColor val="true"/>
					<Color val="888888"/>
				</Frame>
			</Frame>
			<Frame type="PlayerScoreValueFrame" name="DamageTakenScoreValueFrame" template="_LeaderPanelBottom/ScoreValueFrameTemplate_L">
				<Anchor side="Left" relative="$parent/HeroDamageScoreValueFrame" pos="Max" offset="0"/>
				<Width val="105"/>
				<Visible val="true"/>
				<AcceptsMouse val="false"/>
				<AcceptsMouseTooltip val="false"/>
				<Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
					<Visible val="true"/>
					<AcceptsMouseTooltip val="false"/>
					<OptionalScoreValue val="true"/>
					<HideZero val="true"/>
					<ScoreValueLink val="DamageTaken"/>
					<Frame type="Label" name="Label">
						<Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
					</Frame>
				</Frame>
				<Frame type="Image" name="Icon">
					<Anchor side="Left" relative="$parent" pos="Min" offset="-15"/>
					<Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
					<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
					<Texture val="Assets\Textures\storm_ui_play_filter_warrior-on.dds"/>
					<Unclipped val="true"/>
					<!--<GlowColor val="{$GameUI/TeamOrderColors/@Color}"/>-->
					<ColorAdjustMode val="Colorize"/>
					<LightAdjustColor val="#ColorWhite"/>
					<DarkAdjustColor val="{$GameUI/TeamOrderColors/@Color}"/>
					<IgnoreParentColor val="true"/>
					<Color val="888888"/>
				</Frame>
			</Frame>
			
			<StateGroup name="RoleStatState">
				<DefaultState val="None"/>
				<State name="Healing">
					<When type="Property" frame="$this/HealingScoreValueFrame/ScoreValueFrame0/Label" operator="NotEqual" text=""/>
					<When type="Property" frame="$this/DamageTakenScoreValueFrame/ScoreValueFrame0/Label" text=""/>
					<Action type="SetProperty" frame="$this/HealingScoreValueFrame" alpha="255"/>
					<Action type="SetProperty" frame="$this/DamageTakenScoreValueFrame" alpha="0"/>
					<Action type="SendEvent" frame="$this" event="StopAlternate"/>
				</State>
				<State name="DamageTaken">
					<When type="Property" frame="$this/HealingScoreValueFrame/ScoreValueFrame0/Label" text=""/>
					<When type="Property" frame="$this/DamageTakenScoreValueFrame/ScoreValueFrame0/Label" operator="NotEqual" text=""/>
					<Action type="SetProperty" frame="$this/HealingScoreValueFrame" alpha="0"/>
					<Action type="SetProperty" frame="$this/DamageTakenScoreValueFrame" alpha="255"/>
					<Action type="SendEvent" frame="$this" event="StopAlternate"/>
				</State>
				<State name="Both">
					<When type="Property" frame="$this/HealingScoreValueFrame/ScoreValueFrame0/Label" operator="NotEqual" text=""/>
					<When type="Property" frame="$this/DamageTakenScoreValueFrame/ScoreValueFrame0/Label" operator="NotEqual" text=""/>
					<Action type="SendEvent" frame="$this" event="StartAlternate"/>
				</State>
				<State name="None">
					<When type="Property" frame="$this/HealingScoreValueFrame/ScoreValueFrame0/Label" text=""/>
					<When type="Property" frame="$this/DamageTakenScoreValueFrame/ScoreValueFrame0/Label" text=""/>
					<Action type="SetProperty" frame="$this/HealingScoreValueFrame" alpha="0"/>
					<Action type="SetProperty" frame="$this/DamageTakenScoreValueFrame" alpha="0"/>
					<Action type="SendEvent" frame="$this" event="StopAlternate"/>
				</State>
			</StateGroup>
			
			<Animation name="AlternateScoreVisibility">
				<Event event="StartAlternate" action="Play"/>
				<!--<Event event="StopAlternate" action="Stop"/> TODO BUG: stop breaks this -->
				<Controller type="Fade" end="PingPong" frame="$this/HealingScoreValueFrame">
					<Key type="Curve" time="0" value="0"/>
					<Key type="Curve" time="1.8" value="0"/>
					<Key type="Curve" time="2.2" value="255"/>
					<Key type="Curve" time="4" value="255"/>
				</Controller>
				<Controller type="Fade" end="PingPong" frame="$this/DamageTakenScoreValueFrame">
					<Key type="Curve" time="0" value="255"/>
					<Key type="Curve" time="1.8" value="255"/>
					<Key type="Curve" time="2.2" value="0"/>
					<Key type="Curve" time="4" value="0"/>
				</Controller>
			</Animation>
		</Frame>
	</Frame>
