	<Frame type="Frame" name="LeaderPanelBottomTemplate">
		
		<!-- heroLeaderPanel at the bottom - TALENT OVERVIEW -->
		<Frame type="ToggleControl" name="ToggleTalentOverview">
			<Anchor relative="$parent"/>
			<Shortcut val="#Hotkey - Toggle Talents"/>
			<AcceptsMouse val="false"/>
			<CollapseLayout val="true"/>
			<Visible val="false"/>
			<RenderPriority val="511"/>
			<LinkedModalFrame val="$parent/ToggleStatsDeathDmgHealingOverview"/>
			<LinkedModalFrame val="$parent/ToggleStatsAPMOverview"/>
			<LinkedModalFrame val="$parent/ToggleStatsXPOverview"/>
			<LinkedModalFrame val="$parent/ToggleStatsDeathTimeDeathsSelfHealOverview"/>
			<LinkedModalFrame val="$parent/ToggleStatsMechanicsOverview"/>
