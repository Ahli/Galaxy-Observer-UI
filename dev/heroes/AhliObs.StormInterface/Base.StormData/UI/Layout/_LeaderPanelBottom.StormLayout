	<Frame type="Frame" name="LeaderPanelBottomTemplate">
		
		<!-- TALENTS -->
		<Frame type="ToggleControl" name="ToggleTalentOverview">
			<Anchor relative="$parent"/>
			<Shortcut val="#Hotkey - Toggle Talents"/>
			<AcceptsMouse val="false"/>
			<CollapseLayout val="true"/>
			<Visible val="false"/>
			<LinkedModalFrame val="$parent/ToggleDeathsSiegeHeroDmgRole"/>
			<LinkedModalFrame val="$parent/ToggleCurApmAvgApm"/>
			<LinkedModalFrame val="$parent/ToggleExperience"/>
			<LinkedModalFrame val="$parent/ToggleTimeDeadDeathsSelfHealingHealOther"/>
			<LinkedModalFrame val="$parent/ToggleMechanic"/>
			<LinkedModalFrame val="$parent/ToggleKillsDeathsAssists"/>
			<LinkedModalFrame val="$parent/ToggleTimeCCdEnemyHeroes"/>
			
			<Animation name="Ahlimation">
				<Event event="OnShown" action="DirectionForward,Play"/>
				<Event event="OnHidden" action="DirectionReverse,Play"/>
				<Event event="Init" action="DirectionForward,Play"/>
				<Event event="InitEnd" action="DirectionReverse,Play"/>

				<Controller type="Fade" end="Pause" frame="$parent/TalentOverview">
					<Key type="Curve" time="0" value="0"/>
					<Key type="Curve" time="0.1" value="50"/>
					<Key type="Curve" time="0.25" value="255"/>
				</Controller>
				<Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$parent/TalentOverview">
					<Key type="Curve" time="0" value="4400" inout="Fast"/>
					<Key type="Curve" time="0.001" value="-550" inout="Fast"/>
					<Key type="Curve" time="0.25" value="-600" inout="Slow"/>
				</Controller>
				<Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="$parent/TalentOverview">
					<Key type="Curve" time="0" value="5000" inout="Fast"/>
					<Key type="Curve" time="0.001" value="50" inout="Fast"/>
					<Key type="Curve" time="0.25" value="0" inout="Slow"/>
				</Controller>
				<Controller type="Visibility" end="Pause" frame="$parent/TalentOverview/HeroLeaderPanelContainer/TabControl/BackgroundImage">
					<Key type="Bool" time="0" value="false"/>
					<Key type="Bool" time="0.001" value="true"/>
					<Key type="Bool" time="0.25" value="true"/>
				</Controller>
			</Animation>
		</Frame>
