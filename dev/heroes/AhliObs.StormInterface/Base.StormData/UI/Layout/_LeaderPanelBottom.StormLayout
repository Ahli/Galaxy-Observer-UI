	<!-- Talent Icons -->
	<Frame type="UnitStatusTalentIcon" name="TalentTreeIconTemplate">
		<Unclipped val="true"/>
		
		<Animation name="MouseHighlight">
			<Event event="OnMouseEnter" action="DirectionForward,Play"/>
			<Event event="OnMouseExit" action="DirectionReverse,Play"/>
			
			<Controller type="Fade" end="Pause" frame="IconBorderImageHighlight">
				<Key type="Curve" time="0" value="0"/>
				<Key type="Curve" time="0.25" value="0"/>
				<Key type="Curve" time="0.5" value="255"/>
				<Key type="Curve" time="1.0" value="255"/>
			</Controller>
		</Animation>
		
		<Frame type="Image" name="IconBorderImage">
			<Alpha val="#talentBorderAlpha2"/>
			<Unclipped val="true"/>
			
			<Animation name="TalentAvailable2">
				<Event event="NotAvailable" action="DirectionReverse,Play"/>
				<Event event="Available" action="Restart,Play"/>
				
				<Controller type="Visibility" end="Pause" frame="$parent/IconBorderImageHighlight">
					<Key type="Bool" time="0" value="false"/>
					<Key type="Bool" time="0" value="true"/>
				</Controller>
			</Animation>
		</Frame>
		
		<Frame type="Image" name="IconBorderImageHighlight">
			<Anchor relative="$parent/IconBorderImage"/>
			<Texture val="_Assets\Textures\talentAvailableHighlightBorder.dds"/>
			<Visible val="false"/>
			<Alpha val="0"/>
			<RenderPriority val="700"/>
			<IgnoreParentColor val="true"/>
			<Unclipped val="true"/>
		</Frame>
		
		<!-- extends StandardTemplates/UnitStatusTalentIconTemplate/TalentTierState -->
		<StateGroup name="TalentTierState2">
			<DefaultState val="TierNotAvailable"/>
			
			<State name="TierHidden">
				<When type="AnimationState" frame="$this" TalentTierState="TierHidden"/>
				<Action type="SendEvent" frame="IconBorderImage" event="NotAvailable"/>
				<Action type="SetProperty" frame="$this" visible="false"/>
			</State>
			
			<State name="TierNotAvailable">
				<When type="AnimationState" frame="$this" TalentTierState="TierNotAvailable"/>
				<Action type="SendEvent" frame="IconBorderImage" event="NotAvailable"/>
				<Action type="SetProperty" frame="$this" visible="false"/>
			</State>
			
			<State name="TierAvailable">
				<When type="AnimationState" frame="$this" TalentTierState="TierAvailable"/>
				<Action type="SendEvent" frame="IconBorderImage" event="Available"/>
				<Action type="SetProperty" frame="$this" visible="true"/>
			</State>
			
			<State name="TierSelected">
				<When type="AnimationState" frame="$this" TalentTierState="TierSelected"/>
				<Action type="SendEvent" frame="IconBorderImage" event="Available"/>
				<Action type="SetProperty" frame="$this" visible="true"/>
			</State>
		</StateGroup>
		
		<Frame type="Image" name="UnfilledProgressImage">
			<Unclipped val="true"/>
			<RenderPriority val="535"/>
		</Frame>
		<Frame type="ProgressBar" name="QuestProgressBar">
