	<Frame type="Frame" name="NotificationBaseTemplate">
		<Width val="260"/>
		<Height val="60"/>
		<Frame type="Image" name="Background">
			<Anchor relative="$parent"/>
			<Texture val="Assets\Textures\storm_ui_lootchest_notification_bg_triangle_grid.dds"/>
			<Alpha val="128"/>
			<RenderPriority val="480"/>
		</Frame>
		<Frame type="Image" name="Portrait">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
			<Width val="100"/>
			<Height val="40"/>
		</Frame>
		<Frame type="Image" name="TalentImage">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
			<Width val="40"/>
			<Height val="40"/>
			<AcceptsMouseTooltip val="False"/>
			<!-- To Override: <TooltipFrame val=""/> -->
		</Frame>
		<!-- cannot be retrieved
		<Frame type="Label" name="TalentName">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent/TalentImage" pos="Max" offset="0"/>
			<Anchor side="Right" relative="$parent/Portrait" pos="Min" offset="0"/>
			<Height val="40"/>
		</Frame>-->
		<Frame type="Label" name="PlayerName">
			<Anchor side="Top" relative="$parent/Portrait" pos="Max" offset="0"/>
			<Anchor side="Left" relative="$parent/Portrait" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent/Portrait" pos="Max" offset="0"/>
			<Style val="Ahli_Notifications_PlayerName"/>
			<Options val="NoWrapping | ScaleToPreventTruncation"/>
		</Frame>
		<Frame type="Image" name="TeamColor">
			<Anchor relative="$parent"/>
			<Texture val="_Assets\Textures\notificationBg.dds"/>
			<RenderPriority val="500"/>
			<Alpha val="128"/>
		</Frame>
		<Frame type="Label" name="Description">
			<Anchor side="Top" relative="$parent/Portrait" pos="Min" offset="-2"/>
			<Anchor side="Bottom" relative="$parent/Portrait" pos="Max" offset="2"/>
			<Anchor side="Left" relative="$parent/TalentImage" pos="Max" offset="2"/>
			<Anchor side="Right" relative="$parent/Portrait" pos="Min" offset="-2"/>
			<Style val="Ahli_Notifications_Description"/>
			<Options val="ScaleToPreventTruncation"/>
		</Frame>
		<Animation name="Visibility">
			<Event event="OnShown" action="Play"/>
			<Event event="OnHidden" action="Reset,Stop"/>
			<Controller type="Visibility" end="Pause">
				<Key type="Bool" time="5" value="false"/>
			</Controller>
			<Controller type="Fade" end="Pause">
				<Key type="Curve" time="0" value="0" out="Fast"/>
				<Key type="Curve" time=".03" value="255" out="Fast"/>
				<Key type="Curve" time="4.75" value="255" out="Fast"/>
				<Key type="Curve" time="5" value="0" out="Fast"/>
			</Controller>
			<Controller type="Anchor" end="Pause" side="Right" relative="true">
				<Key type="Curve" time="0.0" value="-260" in="Fast" out="Fast"/>
				<Key type="Curve" time="0.2" value="0" in="Slow" out="Slow"/>
				<Key type="Curve" time="4.75" value="0" in="Slow" out="Slow"/>
				<Key type="Curve" time="5.0" value="260" in="Fast" out="Fast"/>
			</Controller>
		</Animation>
	</Frame>
	
	<Frame type="Frame" name="NotificationInsert">
