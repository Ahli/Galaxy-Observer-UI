	<!-- add teamcolor to unitstatus unittracker (shows e.g. unit within Stitches' belly) -->
	<Frame type="Frame" name="UnitTrackerCbFixTemplate">
		<!-- @Override -->
		<StateGroup name="ReadRelation">
			<DefaultState val="Enemy"/>
			<State name="Owner">
				<When type="Property" frame="$parent" CurrentRelation="Owner"/>
				<Action type="SetProperty" frame="$this" Texture="@UI/Storm_UnitStatus_UnitContainer_Circle_Green" undo="false"/>
			</State>
			<State name="Ally">
				<When type="Property" frame="$parent" CurrentRelation="Ally"/>
				<When type="Option" colorblind="False"/>
				<Action type="SetProperty" frame="$this" Texture="@UI/Storm_UnitStatus_UnitContainer_Circle_Blue" undo="false"/>
			</State>
			<State name="AllyCB">
				<When type="Property" frame="$parent" CurrentRelation="Ally"/>
				<When type="Option" colorblind="True"/>
				<Action type="SetProperty" Texture="_Assets\Textures\storm_ui_hud_vehicle_generic_cyan.dds" undo="false"/>
			</State>
			<State name="TeamOrder">
				<When type="Property" frame="$parent" CurrentRelation="TeamOrder"/>
				<When type="Option" colorblind="False"/>
				<Action type="SetProperty" frame="$this" Texture="@UI/Storm_UnitStatus_UnitContainer_Circle_Blue" undo="false"/>
			</State>
			<State name="TeamOrderCB">
				<When type="Property" frame="$parent" CurrentRelation="TeamOrder"/>
				<When type="Option" colorblind="True"/>
				<Action type="SetProperty" frame="$this" Texture="_Assets\Textures\storm_ui_hud_vehicle_generic_cyan.dds" undo="false"/>
			</State>
			<State name="Enemy">
				<When type="Property" frame="$parent" CurrentRelation="Enemy"/>
				<When type="Option" colorblind="False"/>
