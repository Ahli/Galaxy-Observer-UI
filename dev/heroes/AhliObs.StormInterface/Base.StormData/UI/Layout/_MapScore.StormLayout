	<!-- Template containing controls,
			This Template is heavily based on Ryan Schutter (from Blizzard)'s code teaching me StateGroups, yay! -->
	<Frame type="Frame" name="ScoreControlsTemplate">	
		<!-- Increment/Decrement Button -->
		<!-- TODO: decrement does not receive mouse clicks because IncrementButton eats everything -->
		<Frame type="Button" name="DecrementButton">
			<Anchor relative="$parent/Score0"/>
			<Text val=""/>
            <AllowedButtons val="Right"/>
			
			<Animation name="Show">
				<Event event="OnClick" action="Reset,Play"/>
				 
				<Controller type="Visibility" end="Pause" frame="$parent/DecrementEventStateFrame">
					<Key type="Bool" time="0" value="True"/>
				</Controller>
			</Animation>
