	<Style name="AhliColumnHeaderL24" template="AhliColumnHeaderL" height="24"/>
	<Style name="AhliColumnHeaderR24" template="AhliColumnHeaderR" height="24"/>

	<Style name="AhliSelectedUnitName" template="HeaderExtendedTemplate" height="26" textcolor="#ColorLightGreyBlueGradient" vjustify="Middle" hjustify="Left" fontflags="Outline" styleflags="!Shadow|Glow|Uppercase" glowcolor="a3b6b8"/>
