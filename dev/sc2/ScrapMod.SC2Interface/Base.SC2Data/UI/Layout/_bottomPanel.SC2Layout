					<Action type="SetAnchor" frame="$parent/ResourceIcon1" side="Right" relative="$parent/ResourceIconBackground1" pos="Min" offset="5" undo="true"/>
					<!-- reduce alpha to 0 to make sure nothing is visible -->
					<Action type="SetProperty" Alpha="0" undo="true"/>
					<Action type="SetProperty" frame="$parent/ResourceIconBackground1" Alpha="0" undo="true"/>
					<Action type="SetProperty" frame="$parent/ResourceIcon1" Alpha="0" undo="true"/>
				</State>
				<State name="Dflt"/>
			</StateGroup>
		</Frame>
	</Frame>
	
