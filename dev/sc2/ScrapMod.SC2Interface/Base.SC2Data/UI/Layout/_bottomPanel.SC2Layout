		<Frame type="CountdownLabel" name="ResourceLabel1">
			<StateGroup name="Visibility">
				<DefaultState val="Dflt"/>
				<State name="Hidden">
					<When type="Property" CurrentValue="0"/>
					<!-- Setting Visibility does not work here -->
					<!-- => Hide icon, background and label via making the width 0 -->
					<Action type="SetAnchor" side="Left" relative="$parent/ResourceIcon2" pos="Min" offset="0" undo="true"/>
