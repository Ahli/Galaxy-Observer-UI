<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <!--

    =================================================

    TOGGLE TEMPLATES

    =================================================

    These frames and toggle controls are used to enable/disable elements in the UI either by the observer
    or automatically depending on the feature.  In most cases this works by having a toggle control with a
    specific handle sitting offscreen that other frames can check the visibility of using a stategroup or
    animation.
    
    -->
    
        <!--
        ===================================
        Aspect Ratio Toggles
        ===================================
        
        This container handles displaying the player names, whether the default names, the names from the mod, or auto-detecting what to show.
        -->
        
        <Frame type="Frame" name="Template_AspectRatioTogglesContainer">
            <Anchor relative="$parent"/>
        
            <Frame type="AspectRatioFrame" name="Pro2020_16x9">
                <Anchor relative="$parent"/>
                <Handle val="Pro2020_16x9"/>
                <MinVisibleAspectRatio val="1.75"/>
            </Frame>
            
            <Frame type="AspectRatioFrame" name="Pro2020_16x10">
                <Anchor relative="$parent"/>
                <Handle val="Pro2020_16x10"/>
                <MinVisibleAspectRatio val="1.6"/>
            </Frame>
        </Frame>

        <!--
        ===================================
        Player Name Type Toggle
        ===================================
        
        This container handles displaying the player names, whether the default names, the names from the mod, or auto-detecting what to show.
        -->
        
        <Frame type="Frame" name="Template_PlayerNameTogglesContainer">
            <Anchor relative="$parent"/>
        
            <Frame type="ToggleControl" name="PlayerName_Standard">
                <Anchor relative="$parent"/>
                <Handle val="PlayerName_Standard"/>
                <Shortcut val="#TogglePlayerNames"/>
                <Visible val="False"/>
                
                <LinkedModalFrame val="$parent/PlayerName_FromMod"/>
                <LinkedModalFrame val="$parent/PlayerName_AutoDetect"/>
            </Frame>
            
            <Frame type="ToggleControl" name="PlayerName_FromMod">
                <Anchor relative="$parent"/>
                <Handle val="PlayerName_FromMod"/>
                <Shortcut val="#TogglePlayerNamesFromMod"/>
                <Visible val="False"/>
                
                <LinkedModalFrame val="$parent/PlayerName_Standard"/>
                <LinkedModalFrame val="$parent/PlayerName_AutoDetect"/>
            </Frame>
            
            <Frame type="ToggleControl" name="PlayerName_AutoDetect">
                <Anchor relative="$parent"/>
                <Handle val="PlayerName_AutoDetect"/>
                <Shortcut val="#TogglePlayerNamesAutoDetect"/>
                <Visible val="True"/>
                
                <LinkedModalFrame val="$parent/PlayerName_Standard"/>
                <LinkedModalFrame val="$parent/PlayerName_FromMod"/>
            </Frame>
        </Frame>
        
        <!--
        ===================================
        Team Logos Listeners
        ===================================
        
        This container handles whether or not the UI should display team logos for players based on the data it is pulling from the Logos.SC2Mod
        -->
        
        <Frame type="Frame" name="Template_PlayerLogosListeningContainer">
            <Anchor relative="$parent"/>
            
