            <Frame type="Frame" name="TeamLogos_VisibleState">
                <Anchor relative="$parent"/>
                <Handle val="TeamLogos_VisibleState"/>
                <Visible val="False"/>
            </Frame>
            
            <StateGroup name="LogosVisibleState">
                <DefaultState val="Visible"/>
                
                <State name="3v3">
                    <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                    <Action type="SetProperty" frame="$this/TeamLogos_VisibleState" visible="False"/>
                </State>
                
                <State name="4v4">
                    <When type="AnimationState" frame="$GameUI" versusMode="4v4"/>
                    <Action type="SetProperty" frame="$this/TeamLogos_VisibleState" visible="False"/>
                </State>
            
                <State name="NoLogosPresent">
                    <When type="Property" frame="$TeamLogos_Listener_Top/Player0/LogoImage" texture=""/>
                    <When type="Property" frame="$TeamLogos_Listener_Top/Player1/LogoImage" texture=""/>
                    <When type="Property" frame="$TeamLogos_Listener_Top/Player2/LogoImage" texture=""/>
                    <When type="Property" frame="$TeamLogos_Listener_Top/Player3/LogoImage" texture=""/>
                    <When type="Property" frame="$TeamLogos_Listener_Bottom/Player0/LogoImage" texture=""/>
                    <When type="Property" frame="$TeamLogos_Listener_Bottom/Player1/LogoImage" texture=""/>
                    <When type="Property" frame="$TeamLogos_Listener_Bottom/Player2/LogoImage" texture=""/>
                    <When type="Property" frame="$TeamLogos_Listener_Bottom/Player3/LogoImage" texture=""/>
                    <Action type="SetProperty" frame="$this/TeamLogos_VisibleState" visible="False"/>
                </State>
                
                <State name="Visible">
                    <Action type="SetProperty" frame="$this/TeamLogos_VisibleState" visible="True"/>
                </State>
            </StateGroup>
        </Frame>
        
