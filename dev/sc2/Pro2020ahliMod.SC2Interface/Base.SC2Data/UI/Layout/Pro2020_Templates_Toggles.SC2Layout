                    <Anchor relative="$parent"/>
                    <FrameLogoAlias val="LogoSmallCropped"/>
                    <VersusModePlayerSide val="Left"/>
                    <VersusModePlayerIndex val="0"/>
                    <Visible val="False"/>

                    <Frame type="Image" name="LogoImage">
                        <Anchor relative="$parent"/>
                        <TextureType val="Normal"/>
                        <BlendMode val="Normal"/>
                        <IgnoreParentColor val="True"/>
                        <Texture val="Assets/Textures/Blank.dds"/>
                    </Frame>
                </Frame>
                
                <Frame type="PlayerLogoImageFrame" name="Player1">
                    <Anchor relative="$parent"/>
                    <FrameLogoAlias val="LogoSmallCropped"/>
                    <VersusModePlayerSide val="Left"/>
