            <Frame type="Frame" name="LabelContainer">
                <Anchor relative="$parent"/>
                
                <Frame type="Label" name="MatchTypeLabel">
                    <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Max" offset="0"/>
                    <Handle val="MatchTypeLabel"/>
                    
                    <StateGroup name="LogosVisibleState">
                        <DefaultState val="Empty"/>
                    
                        <State name="BestOf1">
                            <When type="Property" frame="$Bestof1" visible="True"/>
                            <Action type="SetProperty" frame="$this" text="@WCS/Bestof1"/>
                        </State>
                        
                        <State name="BestOf3">
                            <When type="Property" frame="$Bestof3" visible="True"/>
                            <Action type="SetProperty" frame="$this" text="@WCS/Bestof3"/>
                        </State>
                        
                        <State name="BestOf5">
                            <When type="Property" frame="$Bestof5" visible="True"/>
                            <Action type="SetProperty" frame="$this" text="@WCS/Bestof5"/>
                        </State>
                        
                        <State name="BestOf7">
                            <When type="Property" frame="$Bestof7" visible="True"/>
                            <Action type="SetProperty" frame="$this" text="@WCS/Bestof7"/>
                        </State>
                        
                        <State name="BestOf9">
                            <When type="Property" frame="$Bestof9" visible="True"/>
                            <Action type="SetProperty" frame="$this" text="@WCS/Bestof9"/>
                        </State>
                        
                        <State name="KingoftheHill">
                            <When type="Property" frame="$KingoftheHill" visible="True"/>
                            <Action type="SetProperty" frame="$this" text="@WCS/KingoftheHill"/>
                        </State>
                        
                        <State name="Empty">
                            <Action type="SetProperty" frame="$this" text=""/>
                        </State>
                    </StateGroup>
                </Frame>
            </Frame>
        </Frame>
        
