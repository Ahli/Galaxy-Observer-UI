<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Frame type="ToggleControl" name="Template_PlayerIntroPanelLeft">
        <Anchor relative="$parent"/>
        <Visible val="false"/>
        <AcceptsMouse val="false"/>

        <Frame type="PlayerFrame" name="PlayerIntroPlayerFrame">
            <Anchor relative="$parent"/>
            <VersusModePlayerSide val="Left"/>
            <CustomTeamColor val="#Red" index="1"/>
            <CustomTeamColor val="#Blue" index="2"/>
            <CustomTeamColor val="#Teal" index="3"/>
            <CustomTeamColor val="#Purple" index="4"/>
            <CustomTeamColor val="#Yellow" index="5"/>
            <CustomTeamColor val="#Orange" index="6"/>
            <CustomTeamColor val="#Green" index="7"/>
            <CustomTeamColor val="#LightPink" index="8"/>

            <!-- BACKGROUND BARS -->
            <Frame type="Image" name="FullBarBG">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="230"/>
                <Texture val="Assets/Textures/White.dds"/>
                <Color val="#Color_CenterBar_Background"/>
                <IgnoreParentColor val="True"/>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                        <Key type="Curve" time="0" value="200" inout="Linear"/>
                        <Key type="Curve" time="0.15" value="0" inout="Smooth"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="TopBarBG">
                <Anchor side="Bottom" relative="$parent/MiddleBarBG" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="116"/>
                <Texture val="Assets/Textures/White.dds"/>
                <Color val="#Color_PlayerBarSupply_Background"/>
                <IgnoreParentColor val="True"/>
            </Frame>

            <Frame type="Image" name="MiddleBarBG">
                <Anchor side="Bottom" relative="$parent/BottomBarBG" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="64"/>
                <Texture val="Assets/Textures/White.dds"/>
                <Color val="#Color_PlayerBar_Background"/>
                <IgnoreParentColor val="True"/>
            </Frame>

            <Frame type="Image" name="MiddleBarAngledBG">
                <Anchor side="Bottom" relative="$parent/FullBarBG" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent/BottomBarAngledBG" pos="Max" offset="-70"/>
                <Width val="500"/>
                <Height val="230"/>
                <Texture val="Base.SC2Data/Images/PlayerIntro_AngledBar.dds"/>
                <Color val="#Color_PlayerBar_Background"/>
                <IgnoreParentColor val="True"/>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Right" relative="true">
                        <Key type="Curve" time="0" value="-2100" inout="Linear"/>
                        <Key type="Curve" timeinc="0.55" value="0" inout="Smooth"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="MiddleBarAngledBGExtender">
                <Anchor side="Bottom" relative="$parent/MiddleBarAngledBG" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/MiddleBarAngledBG" pos="Max" offset="0"/>
                <Width val="5000"/>
                <Height val="230"/>
                <Color val="#Color_PlayerBar_Background"/>
                <IgnoreParentColor val="True"/>
            </Frame>

            <Frame type="Image" name="BottomBarBG">
                <Anchor side="Bottom" relative="$parent/FullBarBG" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="50"/>
                <Texture val="Assets/Textures/White.dds"/>
                <Color val="#Color_CenterBar_Background"/>
                <IgnoreParentColor val="True"/>
            </Frame>

            <Frame type="Image" name="BottomBarAngledBG">
                <Anchor side="Bottom" relative="$parent/FullBarBG" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="100"/>
                <Width val="500"/>
                <Height val="230"/>
                <Texture val="Base.SC2Data/Images/PlayerIntro_AngledBar.dds"/>
                <Color val="#Color_CenterBar_Background"/>
                <IgnoreParentColor val="True"/>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Right" relative="true">
                        <Key type="Curve" time="0" value="-2100" inout="Linear"/>
                        <Key type="Curve" timeinc="0.4" value="0" inout="Smooth"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="BottomBarAngledBGExtender">
                <Anchor side="Bottom" relative="$parent/BottomBarAngledBG" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/BottomBarAngledBG" pos="Max" offset="0"/>
                <Width val="5000"/>
                <Height val="230"/>
                <Color val="#Color_CenterBar_Background"/>
                <IgnoreParentColor val="True"/>
            </Frame>

            <!-- PORTRAIT AND PORTRAIT BACKGROUND -->
            <Frame type="Image" name="PortraitCutoutBG">
                <Anchor side="Bottom" relative="$parent/PortraitCutout" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/PortraitCutout" pos="Min" offset="-60"/>
                <Width val="#PortraitCutoutBGSize"/>
                <Height val="#PortraitCutoutBGSize"/>
                <Texture val="@WCS/PortraitCutoutBG"/>
                <TextureType val="Normal"/>
                <TextureCoords top="0" left="1" bottom="1" right="0"/>
                <Alpha val="{$parent/PortraitCutout/@Alpha}"/>
            </Frame>

            <Frame type="PlayerLogoImageFrame" name="PortraitCutout">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Width val="#PortraitCutoutSize"/>
                <Height val="#PortraitCutoutSize"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="PortraitCutoutLeft"/>

                <Frame type="Image" name="LogoImage">
                    <Anchor relative="$parent"/>
                    <TextureType val="Normal"/>
                    <IgnoreParentColor val="True"/>
                    <Texture val="Assets/Textures/Blank.dds"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-200" inout="Linear"/>
                        <Key type="Curve" timeinc="0.2" value="-200" inout="Linear"/>
                        <Key type="Curve" timeinc="0.15" value="10" inout="Smooth"/>
                        <Key type="Curve" timeinc="0.25" value="0" inout="Slow"/>
                        <Key type="Curve" timeinc="0.1" value="10" inout="Slow"/>
                    </Controller>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.2" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.4" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <!-- PLAYER NAME -->
            <Frame type="PlayerLogoTextFrame" name="PlayerName">
                <Anchor side="Top" relative="$parent/TopBarBG" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/TopBarBG" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PortraitCutout" pos="Max" offset="0"/>
                <Width val="500"/>
                <Height val="100"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="PlayerName"/>

                <Frame type="Label" name="LogoLabel">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Text val=" "/>
                    <Options val="NoWrapping|ScaleToPreventTruncation"/>
                    <IgnoreParentColor val="True"/>
                    <Style val="WCSPlayerIntroNameLeft"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.3" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.15" value="0" inout="Smooth"/>
                    </Controller>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.3" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <!-- FLAG AND REAL NAME -->
            <Frame type="PlayerLogoImageFrame" name="Flag">
                <Anchor side="Top" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayerName" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$this/LogoImage" pos="Max" offset="0"/>
                <Height val="34"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="Flag"/>

                <Frame type="Image" name="LogoImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Height val="34"/>
                    <ScaleWidthToTexture val="True"/>
                    <TextureType val="Normal"/>
                    <IgnoreParentColor val="True"/>
                    <Texture val="Assets/Textures/Blank.dds"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.4" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.15" value="0" inout="Smooth"/>
                    </Controller>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.4" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="PlayerLogoTextFrame" name="PlayerRealName">
                <Anchor side="Top" relative="$parent/MiddleBarBG" pos="Mid" offset="2"/>
                <Anchor side="Bottom" relative="$parent/MiddleBarBG" pos="Mid" offset="2"/>
                <Anchor side="Left" relative="$parent/Flag" pos="Max" offset="10"/>
                <Anchor side="Right" relative="$this/LogoLabel" pos="Max" offset="0"/>
                <Height val="64"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="PlayerRealName"/>

                <Frame type="Label" name="LogoLabel">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Text val=" "/>
                    <IgnoreParentColor val="True"/>
                    <Style val="WCSPlayerIntroRealNameLeft"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.4" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <!-- TEAM LOGO -->
            <Frame type="PlayerLogoImageFrame" name="TeamLogo">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-15"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
                <Width val="200"/>
                <Height val="200"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="Logo"/>

                <Frame type="Image" name="LogoImage">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <TextureType val="Normal"/>
                    <IgnoreParentColor val="True"/>
                    <Texture val="Assets/Textures/Blank.dds"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Right" relative="true">
                        <Key type="Curve" time="0" value="-30" inout="Linear"/>
                        <Key type="Curve" timeinc="0.6" value="-30" inout="Linear"/>
                        <Key type="Curve" timeinc="0.15" value="0" inout="Smooth"/>
                    </Controller>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.6" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <!-- FLAVOR TEXT -->
            <Frame type="PlayerLogoTextFrame" name="FlavorText">
                <Anchor side="Top" relative="$parent/BottomBarBG" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/BottomBarBG" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayerName" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$this/LogoLabel" pos="Max" offset="0"/>
                <Height val="50"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="PlayerFlavorText"/>

                <Frame type="Label" name="LogoLabel">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Text val=" "/>
                    <IgnoreParentColor val="True"/>
                    <Style val="WCSPlayerIntroFlavorLeft"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.5" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.15" value="0" inout="Smooth"/>
                    </Controller>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.5" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <!-- RANK AND SCORE VALUES -->
            <Frame type="Label" name="RankTitle">
                <Anchor side="Top" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/RankValue" pos="Min" offset="-16"/>
                <Height val="60"/>
                <Text val="EPT Rank:"/>
                <IgnoreParentColor val="True"/>
                <Style val="WCSPlayerIntroRankTitle"/>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.6" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="PlayerLogoTextFrame" name="RankValue">
                <Anchor side="Top" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$this/LogoLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScoreTitle" pos="Min" offset="-40"/>
                <Height val="60"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="PlayerRank"/>

                <Frame type="Label" name="LogoLabel">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Text val=" "/>
                    <IgnoreParentColor val="True"/>
                    <Style val="WCSPlayerIntroRankValue"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.6" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Label" name="ScoreTitle">
                <Anchor side="Top" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/ScoreValue" pos="Min" offset="-16"/>
                <Height val="60"/>
                <Text val="Points:"/>
                <IgnoreParentColor val="True"/>
                <Style val="WCSPlayerIntroRankTitle"/>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.6" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="PlayerLogoTextFrame" name="ScoreValue">
                <Anchor side="Top" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/MiddleBarBG" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$this/LogoLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-360"/>
                <Height val="60"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="PlayerScore"/>

                <Frame type="Label" name="LogoLabel">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Text val=" "/>
                    <IgnoreParentColor val="True"/>
                    <Style val="WCSPlayerIntroRankValue"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Right" relative="true">
                        <Key type="Curve" time="0" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.6" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.15" value="0" inout="Smooth"/>
                    </Controller>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.6" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <!-- TEAM NAME -->
            <Frame type="PlayerLogoTextFrame" name="TeamName">
                <Anchor side="Top" relative="$parent/TopBarBG" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/TopBarBG" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$this/LogoLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-360"/>
                <Height val="64"/>
                <VersusModePlayerSide val="Left"/>
                <FrameLogoAlias val="TeamName"/>

                <Frame type="Label" name="LogoLabel">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Width val="600"/>
                    <Text val=" "/>
                    <IgnoreParentColor val="True"/>
                    <Style val="WCSPlayerIntroTeamNameRight"/>
                    <Options val="NoWrapping|ScaleToPreventTruncation"/>
                </Frame>
                
                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play"/>
                     
                    <Controller type="Anchor" end="Pause" side="Right" relative="true">
                        <Key type="Curve" time="0" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.6" value="-20" inout="Linear"/>
                        <Key type="Curve" timeinc="0.15" value="0" inout="Smooth"/>
                    </Controller>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.6" value="0" out="Fast"/>
                        <Key type="Curve" timeinc="0.15" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>
    </Frame>
    
