            </Frame>

            <Frame type="Image" name="BottomBarBG">
                <Anchor side="Bottom" relative="$parent/FullBarBG" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="50"/>
