                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Texture val="ui_leaderpanel_buttonup.dds"/>
                <TextureType val="NineSlice"/>
                <Tiled val="true"/>
                <DescFlags val="Internal"/>
                <Unclipped val="true"/>
                <Alpha val="0"/>
            </Frame>

            <Frame type="Image" name="HoverImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Texture val="ui_leaderpanel_buttonhover.dds"/>
                <TextureType val="NineSlice"/>
                <Tiled val="true"/>
                <DescFlags val="Internal"/>
                <Unclipped val="true"/>
            </Frame>

            <Frame type="Frame" name="HitTestFrame">
                <Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-6"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-6"/>
                <DescFlags val="Internal"/>
                <Unclipped val="true"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="IconImage">
            <Anchor side="Top" relative="$parent/BackgroundButton" pos="Min" offset="7"/>
            <Anchor side="Bottom" relative="$parent/BackgroundButton" pos="Max" offset="-7"/>
            <Anchor side="Left" relative="$parent/BackgroundButton" pos="Min" offset="7"/>
