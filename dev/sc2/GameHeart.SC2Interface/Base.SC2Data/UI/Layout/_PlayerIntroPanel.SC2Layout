					<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
					<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
					<Text val=" "/>
					<IgnoreParentColor val="true"/>
					<Style val="_PlayerIntroRankValue"/>
					<Options val="ScaleToPreventTruncation"/>
				</Frame>
			</Frame>
