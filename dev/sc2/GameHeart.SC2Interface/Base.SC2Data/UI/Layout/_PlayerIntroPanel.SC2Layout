				<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

				<Frame type="Label" name="LogoLabel">
					<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
					<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
					<Text val="@DOCSTR_playerintro_rankTitle"/>
					<IgnoreParentColor val="true"/>
					<Style val="_PlayerIntroRankTitle"/>
					<Options val="ScaleToPreventTruncation"/>
				</Frame>
				
				<StateGroup name="FallbackDefault">
					<DefaultState val="UseMod"/>
					<State name="NoLogosModData">
						<When type="Property" frame="LogoLabel" operator="NotEquals" text=""/>
						<When type="Property" frame="LogoLabel" operator="NotEquals" text="@DOCSTR_playerintro_rankTitle"/>
					</State>
					<State name="UseMod">
						<Action type="SetProperty" frame="LogoLabel" text="@DOCSTR_playerintro_rankTitle"/>
					</State>
				</StateGroup>
			</Frame>

			<Frame type="PlayerLogoTextFrame" name="RankValue">
				<Anchor side="Bottom" relative="$parent/MainBarBG" pos="Max" offset="#IntroBottomTextOffset"/>
				<Height val="55"/>
				<FrameLogoAlias val="PlayerRank"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

				<Frame type="Label" name="LogoLabel">
					<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
					<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
					<Text val=" "/>
					<IgnoreParentColor val="true"/>
					<Style val="_PlayerIntroRankValue"/>
					<Options val="ScaleToPreventTruncation"/>
				</Frame>
			</Frame>

			<Frame type="PlayerLogoTextFrame" name="ScoreTitle">
				<Anchor side="Bottom" relative="$parent/MainBarBG" pos="Max" offset="#IntroBottomTextOffset"/>
				<Height val="55"/>
				<FrameLogoAlias val="PlayerScoreTitle"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
