	<!-- PlayerFrame Templates -->
	<Frame type="PlayerFrame" name="PlayerFrame_Left0">
		<VersusModePlayerSide val="Left"/>
		<VersusModePlayerIndex val="0"/>
	</Frame>
	<Frame type="PlayerFrame" name="PlayerFrame_Left1">
		<VersusModePlayerSide val="Left"/>
		<VersusModePlayerIndex val="1"/>
	</Frame>
	<Frame type="PlayerFrame" name="PlayerFrame_Right0">
		<VersusModePlayerSide val="Right"/>
		<VersusModePlayerIndex val="0"/>
	</Frame>
	<Frame type="PlayerFrame" name="PlayerFrame_Right1">
		<VersusModePlayerSide val="Right"/>
		<VersusModePlayerIndex val="1"/>
	</Frame>
	
	<!-- Changes depending on versus mode -->
	<Frame type="GameUI" name="GameUI" file="GameUI">
		<StateGroup name="VersusModeChanges">
			<DefaultState val="None"/>
			<State name="1vs1">
                <When type="AnimationState" VersusMode="1v1"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame2" visible="true"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame1" template="_bottomPanel/ResourceFrameTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame2" template="_bottomPanel/ResourceFrameTemplate_Right0"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" enabled="true"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" visible="true"/>
			</State>
			<State name="2vs2">
                <When type="AnimationState" VersusMode="2v2"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame2" visible="true"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame3" visible="true"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame4" visible="true"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame1" template="_bottomPanel/ResourceFrameTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame2" template="_bottomPanel/ResourceFrameTemplate_Left1"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame3" template="_bottomPanel/ResourceFrameTemplate_Right0"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame4" template="_bottomPanel/ResourceFrameTemplate_Right1"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" enabled="true"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" visible="true"/>
			</State>
			<State name="3vs3">
                <When type="AnimationState" VersusMode="3v3"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame1" template="_bottomPanel/ResourceFrameTemplate_Left0"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame2" visible="false"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" enabled="true"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" visible="true"/>
			</State>
			<State name="4vs4">
                <When type="AnimationState" VersusMode="4v4"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame1" template="_bottomPanel/ResourceFrameTemplate_Left0"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame2" visible="false"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" enabled="true"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" visible="true"/>
			</State>
			<State name="None">
                <When type="AnimationState" VersusMode="None"/>
				<Action type="ApplyTemplate" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame1" template="_bottomPanel/ResourceFrameTemplate_Left0"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/ResourcePanel/ResourceFrame2" visible="false"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" enabled="false"/>
				<Action type="SetProperty" frame="UIContainer/FullscreenUpperContainer/MapScorePanel" visible="false"/>
			</State>
		</StateGroup>
	</Frame>

