	<!-- Changes depending on versus mode -->
	<Frame type="GameUI" name="GameUI" file="GameUI">
		<Handle val="GameUI"/>
		<!-- Disabling PlayerIntro ToggleControl does not work here for some reason... -->
		<StateGroup name="VersusModeChanges">
			<DefaultState val="None"/>
			<State name="1vs1">
				<When type="AnimationState" VersusMode="1v1"/>
				<!-- Bottom Panel -->
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame2" visible="true"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame1" template="_bottomPanel/ResourceFrameTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame2" template="_bottomPanel/ResourceFrameTemplate_Right0"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame" visible="false"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame" template="_bottomPanel/ResourceFrameKeepHidden"/>
				<!-- Map Scores -->
				<Action type="SetProperty" frame="$MapScorePanel/ToggleControl" enabled="true"/>
				<Action type="SetProperty" frame="$MapScorePanel" visible="true"/>
				<!-- Background Stats -->
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame1" visible="true"/>
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame2" visible="true"/>
				<Action type="ApplyTemplate" frame="$BackgroundFrame/BackgroundStatsPlayerFrame1" template="_infoPanel/BackgroundStatsPlayerFrameTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="$BackgroundFrame/BackgroundStatsPlayerFrame2" template="_infoPanel/BackgroundStatsPlayerFrameTemplate_Right0"/>
				<!-- Units Lost -->
				<Action type="SetProperty" frame="$UnitsLostFrame/Container1" visible="true"/>
				<Action type="SetProperty" frame="$UnitsLostFrame/Container2" visible="true"/>
				<Action type="ApplyTemplate" frame="$UnitsLostFrame/Container1/PlayerUnitsLostFrame" template="_unitsLost/PlayerLostFrameTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="$UnitsLostFrame/Container2/PlayerUnitsLostFrame" template="_unitsLost/PlayerLostFrameTemplate_Right0"/>
				<!-- Player Intros -->
				<Action type="ApplyTemplate" frame="UIContainer/PlayerIntro1" template="_PlayerIntroPanel/PlayerIntroPanelTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="UIContainer/PlayerIntro2_R" template="_PlayerIntroPanel/PlayerIntroPanelTemplate_Right0"/>
				<!-- Scores -->
				<!-- SponsorImage -->
				<Action type="SetProperty" frame="$SponsorImage" visible="true"/>
			</State>
			<State name="2vs2">
				<When type="AnimationState" VersusMode="2v2"/>
				<!-- Bottom Panel -->
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame2" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame3" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame4" visible="true"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame1" template="_bottomPanel/ResourceFrameTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame2" template="_bottomPanel/ResourceFrameTemplate_Left1"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame3" template="_bottomPanel/ResourceFrameTemplate_Right0"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame4" template="_bottomPanel/ResourceFrameTemplate_Right1"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame" visible="false"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame" template="_bottomPanel/ResourceFrameKeepHidden"/>
				<!-- Map Scores -->
				<Action type="SetProperty" frame="$MapScorePanel/ToggleControl" enabled="true"/>
				<Action type="SetProperty" frame="$MapScorePanel" visible="true"/>
				<!-- Background Stats -->
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame1" visible="true"/>
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame2" visible="true"/>
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame3" visible="true"/>
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame4" visible="true"/>
				<Action type="ApplyTemplate" frame="$BackgroundFrame/BackgroundStatsPlayerFrame1" template="_infoPanel/BackgroundStatsPlayerFrameTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="$BackgroundFrame/BackgroundStatsPlayerFrame2" template="_infoPanel/BackgroundStatsPlayerFrameTemplate_Left1"/>
				<Action type="ApplyTemplate" frame="$BackgroundFrame/BackgroundStatsPlayerFrame3" template="_infoPanel/BackgroundStatsPlayerFrameTemplate_Right0"/>
				<Action type="ApplyTemplate" frame="$BackgroundFrame/BackgroundStatsPlayerFrame4" template="_infoPanel/BackgroundStatsPlayerFrameTemplate_Right1"/>
				<!-- Units Lost -->
				<Action type="SetProperty" frame="$UnitsLostFrame/Container1" visible="true"/>
				<Action type="SetProperty" frame="$UnitsLostFrame/Container2" visible="true"/>
				<Action type="SetProperty" frame="$UnitsLostFrame/Container3" visible="true"/>
				<Action type="SetProperty" frame="$UnitsLostFrame/Container4" visible="true"/>
				<Action type="ApplyTemplate" frame="$UnitsLostFrame/Container1/PlayerUnitsLostFrame" template="_unitsLost/PlayerLostFrameTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="$UnitsLostFrame/Container2/PlayerUnitsLostFrame" template="_unitsLost/PlayerLostFrameTemplate_Left1"/>
				<Action type="ApplyTemplate" frame="$UnitsLostFrame/Container3/PlayerUnitsLostFrame" template="_unitsLost/PlayerLostFrameTemplate_Right0"/>
				<Action type="ApplyTemplate" frame="$UnitsLostFrame/Container4/PlayerUnitsLostFrame" template="_unitsLost/PlayerLostFrameTemplate_Right1"/>
				<!-- Player Intros -->
				<Action type="ApplyTemplate" frame="UIContainer/PlayerIntro1" template="_PlayerIntroPanel/PlayerIntroPanelTemplate_Left0"/>
				<Action type="ApplyTemplate" frame="UIContainer/PlayerIntro2_L" template="_PlayerIntroPanel/PlayerIntroPanelTemplate_Left1"/>
				<Action type="ApplyTemplate" frame="UIContainer/PlayerIntro3" template="_PlayerIntroPanel/PlayerIntroPanelTemplate_Right0"/>
				<Action type="ApplyTemplate" frame="UIContainer/PlayerIntro4" template="_PlayerIntroPanel/PlayerIntroPanelTemplate_Right1"/>
				<!-- Scores -->
				<Action type="SetProperty" frame="$TogglePlayerScoresButton" enabled="false"/>
				<Action type="SetProperty" frame="$TogglePlayerScoresButton" visible="false"/>
				<!-- SponsorImage -->
				<Action type="SetProperty" frame="$SponsorImage" visible="false"/>
			</State>
			<State name="3vs3">
				<When type="AnimationState" VersusMode="3v3"/>
				<!-- Bottom Panel -->
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame1" visible="false"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame2" visible="false"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/TopBar" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/BottomBar" visible="true"/>
				<!-- Map Scores -->
				<Action type="SetProperty" frame="$MapScorePanel/ToggleControl" enabled="true"/>
				<Action type="SetProperty" frame="$MapScorePanel" visible="true"/>
				<!-- Background Stats -->
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame1" visible="false"/>
				<!-- Units Lost -->
				<Action type="SetProperty" frame="$UnitsLostFrame" visible="false"/>
				<Action type="SetProperty" frame="$UnitsLostToggleControl" enabled="false"/>
				<!-- Player Intros -->
				<!-- Scores -->
				<Action type="SetAnchor" frame="$MapScorePanel" side="Top" relative="$parent/ResourcePanel/TopBar" pos="Min" offset="0"/>
				<Action type="SetAnchor" frame="$MapScorePanel" side="Bottom" relative="$parent/ResourcePanel/BottomBar" pos="Max" offset="0"/>
				<!-- SponsorImage -->
				<Action type="SetProperty" frame="$SponsorImage" visible="false"/>
			</State>
			<State name="4vs4">
				<When type="AnimationState" VersusMode="4v4"/>
				<!-- Bottom Panel -->
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame1" visible="false"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame2" visible="false"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/TopBar" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/BottomBar" visible="true"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel" template="_bottomPanel/VersusFrame_Enable4vs4"/>
				<!-- Map Scores -->
				<Action type="SetProperty" frame="$MapScorePanel/ToggleControl" enabled="true"/>
				<Action type="SetProperty" frame="$MapScorePanel" visible="true"/>
				<!-- Background Stats -->
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame1" visible="false"/>
				<!-- Units Lost -->
				<Action type="SetProperty" frame="$UnitsLostFrame" visible="false"/>
				<Action type="SetProperty" frame="$UnitsLostToggleControl" enabled="false"/>
				<!-- Player Intros -->
				<!-- Scores -->
				<Action type="SetAnchor" frame="$MapScorePanel" side="Top" relative="$parent/ResourcePanel/TopBar" pos="Min" offset="0"/>
				<Action type="SetAnchor" frame="$MapScorePanel" side="Bottom" relative="$parent/ResourcePanel/BottomBar" pos="Max" offset="0"/>
				<!-- SponsorImage -->
				<Action type="SetProperty" frame="$SponsorImage" visible="false"/>
			</State>
			<State name="None"> <!-- FFA, other -->
				<When type="AnimationState" VersusMode="None"/>
				<!-- Bottom Panel -->
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame1" visible="false"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame2" visible="false"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/TopBar" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/BottomBar" visible="true"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel" template="_bottomPanel/VersusFrame_EnableFFA"/>
				<!-- Map Scores -->
				<Action type="SetProperty" frame="$MapScorePanel/ToggleControl" enabled="false"/>
				<Action type="SetProperty" frame="$MapScorePanel" visible="false"/>
				<!-- Background Stats -->
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame1" visible="false"/>
				<!-- Units Lost -->
				<Action type="SetProperty" frame="$UnitsLostFrame" visible="false"/>
				<Action type="SetProperty" frame="$UnitsLostToggleControl" enabled="false"/>
				<!-- Player Intros -->
				<!-- Scores -->
				<Action type="SetProperty" frame="$TogglePlayerScoresButton" enabled="false"/>
				<Action type="SetProperty" frame="$TogglePlayerScoresButton" visible="false"/>
				<!-- SponsorImage -->
				<Action type="SetProperty" frame="$SponsorImage" visible="false"/>
			</State>
		</StateGroup>

	</Frame>

