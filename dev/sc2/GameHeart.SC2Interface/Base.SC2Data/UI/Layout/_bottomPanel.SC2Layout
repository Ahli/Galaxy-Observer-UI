		<!-- Custom Player Race Image button to alternat between 3 settings -->
		<Frame type="Frame" name="RaceIconSettingIncrement">
			<Anchor relative="$parent"/>
			<Visible val="False"/>
			
			<Animation name="Hide">
				<Event event="Hide" action="Reset,Play"/>
				 
