	<Frame type="UnitButton" name="UnitButtonTemplate">
		<TooltipFrame val="ForcedTooltipUpdate"/>
		
		<!-- detect additional selections via buttons becoming visible -->
		<StateGroup name="Vis">
			<DefaultState val="No"/>
			<State name="Yes">
				<!-- unit button becomes visible -->
				<When type="Property" visible="true"/>
				<Action type="SendEvent" frame="$parent/$parent" event="FixBug"/>
			</State>
			<State name="No"/>
		</StateGroup>
		
		<Frame type="UnitStatusFrame" name="UnitStatusFrame0">
			<Anchor relative="$parent"/>
			<UnitTag val="{$parent/@UnitTag}"/>
			
			<Frame type="DataRefFrame" name="PlayerColor">
				<Anchor relative="$parent"/>
				<Argument name="GameValue" val="PlayerColor"/>
				<PlayerId val="{$parent/@ControlPlayerId}"/>
			</Frame>
			<!-- debug labels
			<Frame type="Label" name="Label">
				<Anchor relative="$parent"/>
				<Text val="{$parent/$parent/@UnitTag}"/>
				<Options val="ScaleToPreventTruncation"/>
			</Frame>
			<Frame type="Label" name="Label2">
				<Anchor side="Top" relative="$parent" pos="33%" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
				<Text val="{$parent/@UnitTag}"/>
				<Options val="ScaleToPreventTruncation"/>
			</Frame>
			<Frame type="Label" name="Label3">
				<Anchor side="Top" relative="$parent" pos="66%" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
				<Text val="{$parent/@ControlPlayerId}"/>
				<Options val="ScaleToPreventTruncation"/>
			</Frame>
			-->
			<Frame type="Frame" name="ForceUpdateCommand">
				<Anchor relative="$parent"/>
				<Visible val="false"/>
				<Enabled val="false"/>
			</Frame>
			<Animation name="Ahlimation">
				<Event event="FixBug" action="Reset,Play" frame="$parent/$parent/$parent"/>
				
				<Controller type="Enabled" frame="ForceUpdateCommand" end="Pause">
					<!-- time:
						0 = freeze
						0.001 = fine
					-->
					<Key type="Bool" time="0.001" value="true"/>
					<Key type="Bool" time="0.001" value="false"/>
				</Controller>
			</Animation>
			
			<StateGroup name="SetUnitTagToForceUpdate">
				<DefaultState val="other"/>
				<State name="ForcingUpdate">
					<When type="Property" frame="ForceUpdateCommand" enabled="true"/>
					 <!-- forces update of UnitButton which updates bindings which updates ControlPlayerId, wtf?!
						 This interacts with the game's hardcoded blackbox logic of the parent GameButton which sets the unitTag to the correct unit again.
						 For some reason, ControlPlayerId is updated then.
					 -->
					<Action type="SetProperty" frame="$parent" unitTag="0#0"/>
				</State>
				<State name="other"/>
			</StateGroup>
		</Frame>
		
		<Frame type="Image" name="CustomBorder">
			<Anchor relative="$parent"/>
			<RenderPriority val="402"/>
			<Texture val="_Assets\Textures\bottompanel\btn-border-queue-trainingslot01.dds"/>
			
			<StateGroup name="Color">
				<DefaultState val="other"/>
				<State name="Red">
					<When type="Property" frame="$parent/UnitStatusFrame0/PlayerColor" stringValue="255,180,20,30"/>
					<Action type="SetProperty" undo="false" color="#Red"/>
				</State>
				<State name="Blue">
