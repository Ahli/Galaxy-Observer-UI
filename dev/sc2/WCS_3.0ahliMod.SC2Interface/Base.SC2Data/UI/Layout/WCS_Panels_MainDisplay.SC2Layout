<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <Frame type="Frame" name="GameUI/UIContainer/ConsoleUIContainer" file="GameUI">
        
        <Frame type="Frame" name="MainDisplay">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Handle val="MainDisplay"/>
            
            <!--
            ======================================================
            
            Game Type StateGroup
            
            ======================================================
            -->
            
            <StateGroup name="GameTypeState">
                
                <State name="1v1">
                    <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                    <Action type="SetProperty" frame="$1v1" visible="True"/>
                </State>
                
                <State name="2v2">
                    <When type="AnimationState" frame="$GameUI" versusMode="2v2"/>
                    <Action type="SetProperty" frame="$2v2" visible="True"/>
                </State>
                
                <State name="3v3">
                    <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                    <Action type="SetProperty" frame="$3v3" visible="True"/>
                </State>
                
                <State name="4v4">
                    <When type="AnimationState" frame="$GameUI" versusMode="4v4"/>
                    <Action type="SetProperty" frame="$4v4" visible="True"/>
                </State>
                
                <State name="None">
                    <When type="AnimationState" frame="$GameUI" versusMode="None"/>
                    <Action type="SetProperty" frame="$UnsupportedGametype" visible="True"/>
                </State>
            </StateGroup>
            
            <!--
            ======================================================
            
            1V1
            
            ======================================================
            -->
            
