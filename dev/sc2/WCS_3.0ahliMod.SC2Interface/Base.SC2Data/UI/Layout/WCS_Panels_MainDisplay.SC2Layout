                <Frame type="Frame" name="LeaderpanelShimmer">
                    <Anchor side="Top" relative="$parent/PlayerBar_Top_Player0" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$LeaderPanelSection1" pos="Min" offset="-100"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Handle val="Leaderpanel_Shimmer"/>
                    
                    <Frame type="Image" name="Shimmer">
                        <Anchor relative="$parent"/>
                     
                        <LayerCount val="2"/>
                     
                        <Texture val="@WCS/LeaderpanelMask" layer="0"/>
                        <AlphaLayer val="True" layer="0"/>
                     
                        <Texture val="@WCS/LeaderpanelShimmer" layer="1"/>
                        <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
                        <LayerAlpha val="65" layer="1"/>
                        <LayerColor val="108,123,141" layer="1"/>
                     
                        <Animation name="Shimmer">
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_APM"/>
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_ArmyValue"/>
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_Income"/>
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_Production"/>
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_Spending"/>
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_Structures"/>
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_Units"/>
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_UnitsLost"/>
                            <Event event="OnShown" action="Reset,Play" frame="$Leaderpanel_Upgrades"/>
                     
                            <Controller type="LayerUV" layer="1" side="Left" relative="False" sidelock="True" end="Pause" >
                                <Key type="Curve" time="0" value=".7" out="fast"/>
                                <Key type="Curve" time=".45" value="-.7" in="fast"/>
                            </Controller>
