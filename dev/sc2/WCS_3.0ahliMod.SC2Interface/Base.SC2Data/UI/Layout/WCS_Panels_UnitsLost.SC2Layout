        <ShowKilled val="true"/>
        <ShowStructures val="true"/>
        <ShowComplete val="false"/>
    </Frame>
    
    <Frame type="Frame" name="Template_PlayerContainer">
        <Anchor side="Bottom" relative="$this/PlayerData/ResourcesContainer" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Width val="1334"/>
        <CollapseLayout val="True"/>
        
        <Frame type="PlayerFrame" name="PlayerData" template="WCS_Templates_PlayerBar_Generic/Template_PlayerData">
            
            <Frame type="Image" name="Background">
                <Anchor relative="$parent"/>
                <IgnoreParentColor val="True"/>
                <Color val="{$ancestor[@name=PlayerData]/@Color}"/>
                <Alpha val="0"/>
            </Frame>
            
            <Frame type="Image" name="PlayerIndicator">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
                <Width val="10"/>
                <Texture val="@WCS/RoundedRectangle_Small"/>
                <TextureType val="NineSlice"/>
                <Color val="{$ancestor[@name=PlayerData]/@Color}"/>
            </Frame>
            
            <Frame type="PlayerOwnedUnitsFrame" name="UnitsLostList" template="WCS_Panels_UnitsLost/Template_PlayerUnitsLostFrame">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent/PlayerIndicator" pos="Max" offset="10"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <CollapseLayout val="True"/>
            </Frame>
            
            <Frame type="Frame" name="StructuresLostContainer">
                <Anchor side="Top" relative="$parent/UnitsLostList" pos="Max" offset="5"/>
                <Anchor side="Left" relative="$parent/UnitsLostList" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="60"/>
                <CollapseLayout val="True"/>
                
                <Frame type="PlayerOwnedUnitsFrame" name="StructuresLostList" template="WCS_Panels_UnitsLost/Template_PlayerStructuresLostFrame">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                </Frame>
            </Frame>
            
            <Frame type="Frame" name="ResourcesContainer">
                <Anchor side="Top" relative="$parent/StructuresLostContainer" pos="Max" offset="5"/>
                <Anchor side="Left" relative="$parent/StructuresLostContainer" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="40"/>
                <CollapseLayout val="True"/>
                
                <Frame type="PlayerRaceIconFrame" name="MineralsIcon" template="WCS_Templates_PlayerBar_Generic/Template_DataIcon">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <BaseTexture val="@WCS/Minerals"/>
                </Frame>
                
                <Frame type="Frame" name="MineralsLost" template="WCS_Templates_PlayerBar_Generic/Template_MineralsLost">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent/MineralsIcon" pos="Max" offset="0"/>
                </Frame>
                
                <Frame type="PlayerRaceIconFrame" name="GasIcon" template="WCS_Templates_PlayerBar_Generic/Template_DataIcon">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent/MineralsLost" pos="Max" offset="40"/>
                    <BaseTexture val="@WCS/Gas"/>
                </Frame>
                
                <Frame type="Frame" name="GasLost" template="WCS_Templates_PlayerBar_Generic/Template_GasLost">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent/GasIcon" pos="Max" offset="0"/>
                </Frame>
            </Frame>
        </Frame>
        
        <StateGroup name="GameTypeState">
            <DefaultState val="Not1v1"/>
                
            <State name="1v1">
                <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                <Action type="SetProperty" frame="$this/PlayerData/StructuresLostContainer" visible="True"/>
                <Action type="SetProperty" frame="$this/PlayerData/ResourcesContainer" visible="True"/>
            </State>
            
            <State name="Not1v1">
                <Action type="SetProperty" frame="$this/PlayerData/StructuresLostContainer" visible="False"/>
                <Action type="SetProperty" frame="$this/PlayerData/ResourcesContainer" visible="False"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="Frame" name="GameUI/UIContainer/ConsoleUIContainer" file="GameUI">
        
        <Frame type="ToggleControl" name="UnitsLostPopup">
            <Anchor side="Top" relative="$parent" pos="Min" offset="200"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$this/BottomTeam" pos="Max" offset="25"/>
            <Width val="#Width_UnitsLost"/>
            <Shortcut val="#ToggleUnitsLostPopup"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="Background">
                <Anchor relative="$parent"/>           
                <Color val="#Color_PlayerBar_Background"/>
				<Texture val="@WCS/RoundedRectangle_Small"/>
                <TextureType val="NineSlice"/>
                <Alpha val="#Alpha_PlayerBar_Background"/>
            </Frame>
			
			<Frame type="Image" name="CenterBG">
                <Anchor side="Top" relative="$parent/TopTeam" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent/BottomTeam" pos="Max" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>    
				<Texture val="Assets/Textures/White.dds"/>
                <Color val="#Color_CenterBar_Background"/>
            </Frame>

            <Frame type="Label" name="Title">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Bottom" relative="$parent/CenterBG" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Text val="@WCS/UnitsLostCountTitle"/>
                <Style val="WCSPopupTitle"/>
            </Frame>
            
            <Frame type="Frame" name="TopTeam">
                <Anchor side="Top" relative="$parent" pos="Min" offset="40"/>
                <Anchor side="Bottom" relative="$this/Player3" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                
                <Frame type="Frame" name="Player0" template="WCS_Panels_UnitsLost/Template_PlayerContainer">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Left"/>
                        <VersusModePlayerIndex val="0"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player1" template="WCS_Panels_UnitsLost/Template_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player0" pos="Max" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Left"/>
                        <VersusModePlayerIndex val="1"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player2" template="WCS_Panels_UnitsLost/Template_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player1" pos="Max" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Left"/>
                        <VersusModePlayerIndex val="2"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player3" template="WCS_Panels_UnitsLost/Template_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player2" pos="Max" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Left"/>
                        <VersusModePlayerIndex val="3"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <Frame type="Frame" name="Divider">
                <Anchor side="Top" relative="$parent/TopTeam" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="10"/>
            </Frame>
            
            <Frame type="Frame" name="BottomTeam">
                <Anchor side="Top" relative="$parent/Divider" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$this/Player3" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                
                <Frame type="Frame" name="Player0" template="WCS_Panels_UnitsLost/Template_PlayerContainer">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Right"/>
                        <VersusModePlayerIndex val="0"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player1" template="WCS_Panels_UnitsLost/Template_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player0" pos="Max" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Right"/>
                        <VersusModePlayerIndex val="1"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player2" template="WCS_Panels_UnitsLost/Template_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player1" pos="Max" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Right"/>
                        <VersusModePlayerIndex val="2"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="Player3" template="WCS_Panels_UnitsLost/Template_PlayerContainer">
                    <Anchor side="Top" relative="$parent/Player2" pos="Max" offset="0"/>
                    
                    <Frame type="PlayerFrame" name="PlayerData">
                        <VersusModePlayerSide val="Right"/>
                        <VersusModePlayerIndex val="3"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <!--
            ======================================================
            
            Game Type StateGroup
            
            ======================================================
            -->
            
            <StateGroup name="GameTypeState">
                
                <State name="1v1">
                    <When type="AnimationState" frame="$GameUI" versusMode="1v1"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player0" visible="True"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player1" visible="False"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player2" visible="False"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player3" visible="False"/>
                    <Action type="SetProperty" frame="$this/BottomTeam/Player0" visible="True"/>
                    <Action type="SetProperty" frame="$this/BottomTeam/Player1" visible="False"/>
                    <Action type="SetProperty" frame="$this/BottomTeam/Player2" visible="False"/>
                    <Action type="SetProperty" frame="$this/BottomTeam/Player3" visible="False"/>
                </State>
                
                <State name="2v2">
                    <When type="AnimationState" frame="$GameUI" versusMode="2v2"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player0" visible="True"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player2" visible="False"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player3" visible="False"/>
                    <Action type="SetProperty" frame="$this/BottomTeam/Player0" visible="True"/>
                    <Action type="SetProperty" frame="$this/BottomTeam/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/BottomTeam/Player2" visible="False"/>
                    <Action type="SetProperty" frame="$this/BottomTeam/Player3" visible="False"/>
                </State>
                
                <State name="3v3">
                    <When type="AnimationState" frame="$GameUI" versusMode="3v3"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player0" visible="True"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player1" visible="True"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player2" visible="True"/>
                    <Action type="SetProperty" frame="$this/TopTeam/Player3" visible="False"/>
