	<!-- Changes depending on versus mode -->
	<Frame type="GameUI" name="GameUI" file="GameUI">
		<Handle val="GameUI"/>
		<!-- Disabling PlayerIntro ToggleControl does not work here for some reason... -->
		<StateGroup name="VersusModeChanges">
			<DefaultState val="None"/>
			<State name="1vs1">
				<When type="AnimationState" VersusMode="1v1"/>
				<!-- Bottom Panel -->
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame1" visible="false"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame1vs1top" template="_bottomPanel/ResourceFrame1vs1Template_Left0"/>
				<Action type="ApplyTemplate" frame="$ResourcePanel/ResourceFrame1vs1bot" template="_bottomPanel/ResourceFrame1vs1Template_Right0"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame" visible="false"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame" alpha="0"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame1vs1bot" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame1vs1divider" visible="true"/>
				<Action type="SetProperty" frame="$ResourcePanel/ResourceFrame1vs1top" visible="true"/>
				<!-- Map Scores -->
				<Action type="SetProperty" frame="$MapScorePanel/ToggleControl" enabled="true"/>
				<Action type="SetProperty" frame="$MapScorePanel" visible="true"/>
				<Action type="SetAnchor" frame="$MapScorePanel" side="Top" relative="$ResourcePanel/ResourceFrame1vs1top" pos="Min" offset="0"/>
				<Action type="SetAnchor" frame="$MapScorePanel" side="Bottom" relative="$ResourcePanel/ResourceFrame1vs1bot" pos="Max" offset="0"/>
				<!-- Background Stats -->
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStatsPlayerFrame1" visible="false"/>
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStats1vs1bot" visible="true"/>
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStats1vs1divider" visible="true"/>
				<Action type="SetProperty" frame="$BackgroundFrame/BackgroundStats1vs1top" visible="true"/>
				<Action type="ApplyTemplate" frame="$BackgroundFrame/BackgroundStats1vs1top" template="_infoPanel/BackgroundStats1vs1Template_Left0"/>
				<Action type="ApplyTemplate" frame="$BackgroundFrame/BackgroundStats1vs1bot" template="_infoPanel/BackgroundStats1vs1Template_Right0"/>
