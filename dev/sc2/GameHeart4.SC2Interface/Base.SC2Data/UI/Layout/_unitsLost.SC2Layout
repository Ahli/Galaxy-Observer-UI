	<Frame type="Frame" name="ScoreDifferenceTemplate">
		<Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
		<Height val="34"/>
		<Width val="209"/>
		
		<Frame type="Label" name="Header">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
			<Width val="102"/>
			<Style val="_unitsLost_headings"/>
			<Text val="@GH_minerals"/>
			<Options val="NoWrapping | ScaleToPreventTruncation"/>
		</Frame>
		
		<Frame type="Label" name="ResultText">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent/Header" pos="Max" offset="0"/>
			<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
			<Width val="102"/>
			<Text val="{$parent/TextFormatAminusB/@Text}"/>
			<Style val="_unitsLost_Minerals"/>
			<Options val="NoWrapping | ScaleToPreventTruncation"/>
			<Visible val="false"/>
		</Frame>
		
		<Frame type="PlayerFrame" name="PlayerA">
			<Anchor relative="$parent"/>
			<VersusModePlayerSide val="Left"/>
			
			<Frame type="Image" name="Border">
				<Anchor relative="$parent"/>
				<Texture val="_Assets\Textures\bottompanel\border_2px.dds"/>
				<TextureType val="Border"/>
				<Alpha val="153"/>
				<Visible val="false"/>
			</Frame>
		</Frame>
		<Frame type="PlayerFrame" name="PlayerB">
			<Anchor relative="$parent"/>
			<VersusModePlayerSide val="Right"/>
			
			<Frame type="Image" name="Border">
				<Anchor relative="$parent"/>
				<Texture val="_Assets\Textures\bottompanel\border_2px.dds"/>
				<TextureType val="Border"/>
				<Alpha val="153"/>
				<Visible val="false"/>
			</Frame>
		</Frame>
		
		<StateGroup name="Higher">
			<State name="A">
				<When type="Property" frame="TextFormatAminusB/AminusB" operator="GreaterThan" result="0"/>
				<Action type="SetProperty" frame="ResultText" visible="true"/>
				<Action type="SetProperty" frame="PlayerB/Border" visible="true"/>
			</State>
			<State name="B">
				<When type="Property" frame="TextFormatAminusB/AminusB" operator="LessThan" result="0"/>
				<Action type="SetProperty" frame="ResultText" visible="true"/>
				<Action type="SetProperty" frame="PlayerA/Border" visible="true"/>
			</State>
		</StateGroup>
		
		<Frame type="TextFormatFrame" name="TextFormatAminusB">
			<Anchor relative="$parent"/>
			<FormatText val="+%0%"/>
			<Replacement index="0" val="{AbsoluteDifference/@Result}"/>
			
			<Frame type="MathFrame" name="AbsoluteDifference">
				<A val="{$parent/AminusB/@Result}"/>
				<Operator val="Absolute"/>
			</Frame>
			<Frame type="MathFrame" name="AminusB">
				<A val="{$parent/$parent/Value_A/@RealValue}"/>
				<B val="{$parent/$parent/Value_B/@RealValue}"/>
				<Operator val="Subtract"/>
			</Frame>
		</Frame>
		
		<Frame type="DataRefFrame" name="Value_A">
			<Argument name="Score" val="MineralsLostUnits"/>
			<PlayerId val="{$PlayerLeft0/@PlayerId}"/>
		</Frame>
		<Frame type="DataRefFrame" name="Value_B">
			<Argument name="Score" val="MineralsLostUnits"/>
			<PlayerId val="{$PlayerRight0/@PlayerId}"/>
		</Frame>
	</Frame>

