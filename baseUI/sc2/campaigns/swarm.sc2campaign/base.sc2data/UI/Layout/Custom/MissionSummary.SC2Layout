                    <BlendMode val="Add"/>
                    <LayerColor val="ffae00"/>
                </Frame>
                
                <Frame type="Image" name="DisciplineAbilityImage1">
                    <Anchor side="Left" relative="$parent/DisciplineAbilitiesLabel" pos="Max" offset="10"/>
                    <Anchor side="Top" relative="$parent/DisciplineAbilitiesLabel" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent/DisciplineAbilitiesLabel" pos="Mid" offset="0"/>
                    <Height val="64"/>
                    <Width val="64"/>
                    <AcceptsMouse val="true"/>
                </Frame>

                <Frame type="Image" name="DisciplineAbility2IconBorderImage">
                    <Anchor side="Top" relative="$parent/DisciplineAbilityImage2" pos="Min" offset="-8"/>
                    <Anchor side="Left" relative="$parent/DisciplineAbilityImage2" pos="Min" offset="-8"/>
                    <Anchor side="Bottom" relative="$parent/DisciplineAbilityImage2" pos="Max" offset="8"/>
                    <Anchor side="Right" relative="$parent/DisciplineAbilityImage2" pos="Max" offset="8"/>
                    <Texture val="@@UI/ButtonBorderCargo_Prot"/>
                    <TextureType val="Border"/>
                    <BlendMode val="Add"/>
                    <LayerColor val="ffae00"/>
                </Frame>

                <Frame type="Image" name="DisciplineAbilityImage2">
                    <Anchor side="Left" relative="$parent/DisciplineAbilityImage1" pos="Max" offset="20"/>
                    <Anchor side="Top" relative="$parent/DisciplineAbilitiesLabel" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent/DisciplineAbilitiesLabel" pos="Mid" offset="0"/>
                    <Height val="64"/>
                    <Width val="64"/>
                    <AcceptsMouse val="true"/>
                </Frame>
            </Frame>           

            <Frame type="Label" name="EnhancementHeaderLabel">
                <Anchor side="Top" relative="$parent/DisciplineFrame" pos="Max" offset="15"/>
                <Anchor side="Left" relative="$parent/InfestationHeaderLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/InfestationHeaderLabel" pos="Max" offset="0"/>
                <Style val="MissionSummaryRewardHeader"/>
                <Text val="EnhancementHeaderLabel"/>
                <CollapseLayout val="true"/>
                <Height val="25"/>
            </Frame>

            <Frame type="Label" name="EnhancementDescriptionLabel">
                <Anchor side="Top" relative="$parent/EnhancementHeaderLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/InfestationDescriptionLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/InfestationDescriptionLabel" pos="Max" offset="0"/>
                <Style val="MissionSummaryReward"/>
                <Text val="EnhancementDescriptionLabel"/>
                <CollapseLayout val="true"/>
            </Frame>
        </Frame>       
    </Frame>
</Desc>

