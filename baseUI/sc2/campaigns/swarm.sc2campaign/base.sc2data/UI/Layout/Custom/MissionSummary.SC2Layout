<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    
    <Frame type="Frame" name="MissionSummaryPanelTemplate">
        <Frame type="Frame" name="RewardFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="600"/>            
            <Anchor side="Right" relative="$parent" pos="Mid" offset="-50"/>
            <Height val="500"/>
            <Width val="650"/>
            
            <Frame type="Label" name="InfestationHeaderLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="80"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="40"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
                <Style val="MissionSummaryRewardHeader"/>
                <Text val="InfestationHeaderLabel"/>
                <CollapseLayout val="true"/>
                <Height val="25"/>
            </Frame>

            <Frame type="Label" name="InfestationDescriptionLabel">
                <Anchor side="Top" relative="$parent/InfestationHeaderLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/InfestationHeaderLabel" pos="Min" offset="50"/>
                <Anchor side="Right" relative="$parent/InfestationHeaderLabel" pos="Max" offset="0"/>
                <Style val="MissionSummaryReward"/>
                <Text val="InfestationDescriptionLabel"/>
                <CollapseLayout val="true"/>
            </Frame>

            <Frame type="Image" name="AbilityIconBorderImage">
                <Anchor side="Top" relative="$parent/AbilityIconImage" pos="Min" offset="-8"/>
                <Anchor side="Left" relative="$parent/AbilityIconImage" pos="Min" offset="-8"/>
                <Anchor side="Bottom" relative="$parent/AbilityIconImage" pos="Max" offset="8"/>
                <Anchor side="Right" relative="$parent/AbilityIconImage" pos="Max" offset="8"/>
                <Texture val="@@UI/ButtonBorderCargo_Prot"/>
                <TextureType val="Border"/>
                <BlendMode val="Add"/>
                <LayerColor val="ffae00"/>
            </Frame>
            
            <Frame type="Image" name="AbilityIconImage">
                <Anchor side="Left" relative="$parent/InfestationDescriptionLabel" pos="Min" offset="0"/>
                <Anchor side="Top" relative="$parent/InfestationDescriptionLabel" pos="Max" offset="10"/>
                <Height val="64"/>
                <Width val="64"/>
                <CollapseLayout val="true"/>
                <AcceptsMouse val="true"/>
            </Frame>

            <Frame type="Label" name="NewAbilityLabel">
                <Anchor side="Top" relative="$parent/AbilityIconImage" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent/AbilityIconImage" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/AbilityIconImage" pos="Max" offset="10"/>
                <Style val="MissionSummaryReward"/>
