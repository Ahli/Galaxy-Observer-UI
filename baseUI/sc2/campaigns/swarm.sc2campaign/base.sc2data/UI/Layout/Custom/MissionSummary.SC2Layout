            </Frame>
            
            <Frame type="Label" name="AbilityDescriptionLabel">
                <Anchor side="Top" relative="$parent/NewAbilityLabel" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/NewAbilityLabel" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent/NewAbilityLabel" pos="Max" offset="0"/>
                <Style val="MissionSummaryBody"/>
                <Text val="AbilityDescriptionLabel"/>
            </Frame>
            
            <Frame type="Frame" name="DisciplineFrame">
                <Anchor side="Left" relative="$parent/InfestationDescriptionLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-40"/>
