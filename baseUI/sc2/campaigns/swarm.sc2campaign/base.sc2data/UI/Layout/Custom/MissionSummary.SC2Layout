                <Frame type="Image" name="DisciplineBorderImage">
                    <Anchor side="Top" relative="$parent/DisciplineHeaderLabel" pos="Max" offset="5"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@@UI/ButtonBorderCargo_Prot"/>
                    <TextureType val="Border"/>
                    <Tiled val="true"/>
                    <LayerColor val="2a5a6b"/>
                </Frame>

                <Frame type="Label" name="TraitBonusLabel">
                    <Anchor side="Top" relative="$parent/DisciplineBorderImage" pos="Min" offset="20"/>
