                <Frame type="Label" name="DisciplineBonusLabel">
                    <Anchor side="Top" relative="$parent/TraitBonusLabel" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/TraitBonusLabel" pos="Max" offset="0"/>
