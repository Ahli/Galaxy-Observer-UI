        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="50"/>

        <Frame type="OffscreenUnitFrame" name="TutorialOffscreenUnitFrame">
            <Height val="200"/>
            <Width val="200"/>

            <Frame type="Image" name="ArrowImage">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Height val="192"/>
                <Width val="192"/>
                <TextureCoords top="1" left="1" bottom="0" right="0" layer="0"/>
                <!--Texture val="Assets\Textures\tutorialarrow00.dds"/-->
                <BlendMode val="Add"/>

                <Frame type="Animation" name="Show">
                    <Event event="OnShown" action="Reset"/>
                    <Event event="OnShown" action="Play"/>
                    <Controller type="Texture" end="Loop">
                        <Key type="Image" time="0" image="Assets\Textures\tutorialarrow00.dds"/>
                        <Key type="Image" time=".15" image="Assets\Textures\tutorialarrow04.dds"/>
                        <Key type="Image" time=".3" image="Assets\Textures\tutorialarrow03.dds"/>
                        <Key type="Image" time=".45" image="Assets\Textures\tutorialarrow02.dds"/>
                        <Key type="Image" time=".6" image="Assets\Textures\tutorialarrow01.dds"/>
                        <Key type="Image" time=".9" image="Assets\Textures\tutorialarrow00.dds"/>
                    </Controller>
                </Frame>
            </Frame>
        </Frame>
    </Frame>
