    <!--
    ==================
    Hero Select Button Template
    ==================
    -->
    
    <Frame type="Button" name="HeroSelectButtonTemplate" template="StandardGreyboxTemplates/StandardIconButtonLargeTemplate">
        <Anchor side="Top" relative="$sibling-1" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$sibling-1" pos="Max" offset="8"/>
        <Width val="80"/>
        <Height val="96"/>
         
        <Frame type="Image" name="Icon">
            <Texture val="Assets/Textures/white.dds"/>
        </Frame>
        
        <Frame type="Frame" name="Border">
        
            <Frame type="Image" name="BorderTop">
                <Height val="3"/>
            </Frame>
        
            <Frame type="Image" name="BorderBottom">
                <Height val="3"/>
            </Frame>
        
            <Frame type="Image" name="BorderLeft">
                <Width val="3"/>
            </Frame>
        
            <Frame type="Image" name="BorderRight">
                <Width val="3"/>
            </Frame>
        
            <StateGroup name="BorderState">
                
                <State name="Normal">
                    <When type="AnimationState" frame="$parent" operator="NotEqual" SelectionState="Selected"/>
                    <When type="Property" frame="$parent" Hovering="False"/>
                    <When type="Property" frame="$parent" Enabled="True"/>
                </State>
                
                <State name="Hover">
                    <When type="AnimationState" frame="$parent" operator="NotEqual" SelectionState="Selected"/>
                    <When type="Property" frame="$parent" Hovering="True"/>
                    <When type="Property" frame="$parent" Enabled="True"/>
                </State>
                
                <State name="Toggled">
                    <When type="AnimationState" frame="$parent" SelectionState="Selected"/>
                    <When type="Property" frame="$parent" Enabled="True"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Image" name="FriendIcon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
            <Texture val="Assets\Textures\ui_glues_icons_friendslist.dds"/>
            <Width val="15"/>
            <Height val="19"/>
        </Frame>
        
        <Frame type="Label" name="FriendLabel">
            <Anchor side="Top" relative="$parent/FriendIcon" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/FriendIcon" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/FriendIcon" pos="Min" offset="2"/>
            <Style val="W3C_SelectedHeroFriend"/>
            <Text val="2"/>
        </Frame>
        
        <Frame type="Label" name="LevelLabel">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-1"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="3"/>
            <Style val="W3C_LevelLabel"/>
            <Text val="15"/>
        </Frame>
        
        <!-- State Groups -->
        <StateGroup name="HeroSelectState">
            <DefaultState val="Empty"/>
            
            <State name="NotEmpty">
                <When type="AnimationState" EmptyState="NotEmpty"/>
                <Action type="SetProperty" Visible="True"/>
            </State>
            
            <State name="Empty">
                <Action type="SetProperty" Visible="False"/>
            </State>
        </StateGroup>
        
        <StateGroup name="SelectedState">
            <DefaultState val="NotSelected"/>
            
            <State name="Selected">
                <When type="AnimationState" SelectionState="Selected"/>
                <Action type="SetProperty" frame="LevelLabel" Color="255,255,255"/>
            </State>
            
            <State name="NotSelected">
                <Action type="SetProperty" frame="LevelLabel" Color="150,150,150"/>
            </State>
        </StateGroup>
        
        <StateGroup name="FriendState">
            <DefaultState val="NotSelected"/>
            
            <State name="Selected">
                <When type="AnimationState" FriendState="Selected"/>
                <Action type="SetProperty" frame="FriendLabel" Visible="True"/>
                <Action type="SetProperty" frame="FriendIcon" Visible="True"/>
            </State>
            
            <State name="NotSelected">
                <Action type="SetProperty" frame="FriendLabel" Visible="False"/>
                <Action type="SetProperty" frame="FriendIcon" Visible="False"/>
            </State>
        </StateGroup>
        
        <StateGroup name="EnabledState">
            <DefaultState val="Disabled"/>
            
            <State name="Enabled">
                <When type="AnimationState" EnabledState="Enabled"/>
                <Action type="SetProperty" frame="Icon" Desaturated="False"/>
                <Action type="SetProperty" frame="Icon" Alpha="255"/>
                <Action type="SetProperty" frame="LevelLabel" Text="15"/>
            </State>
            
            <State name="Disabled">
                <Action type="SetProperty" frame="Icon" Desaturated="True"/>
                <Action type="SetProperty" frame="Icon" Alpha="127"/>
                <Action type="SetProperty" frame="LevelLabel" Text="1"/>
            </State>
        </StateGroup>
    </Frame>
    

