    <!--
    ==========================
    Hero Select Background Image Template
    ==========================
    -->
    
    <Frame type="Image" name="HeroSelectBackgroundImageTemplate">
        <Anchor relative="AnchorFrame"/>
        <LayerCount val="2"/>
            
        <Texture val="Assets/Textures/war3coop_heroselectbackgroundmask.dds" layer="0"/>
        <TextureCoords top="0" bottom="1" left="0" right="4" layer="0"/>
        <AlphaLayer val="True" layer="0"/>
        
        <Texture val="{$this/AnchorFrame/@Texture}" layer="1"/>
        
        <Frame type="Image" name="AnchorFrame">
            <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="0"/>
            <ScaleWidthToTexture val="True"/>
            <Texture val="Assets/Textures/war3coop_heroselectbackground_grom.dds"/>
            <Visible val="False"/>
        </Frame>
    </Frame>

    <!--
    ==========================
    Hero Select Background Image Template
    ==========================
    -->
    
    <Frame type="Label" name="HeroAvailabilityLabelTemplate">
        <Anchor side="Top" relative="$parent/HeroSelectBackgroundImage" pos="Mid" offset="0"/>
        <Anchor side="Bottom" relative="$parent/HeroSelectBackgroundImage" pos="Mid" offset="0"/>
        <Anchor side="Left" relative="$parent/HeroSelectBackgroundImage" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent/HeroSelectBackgroundImage" pos="Mid" offset="0"/>
        <Style val="W3C_HeroAvailabilityLabel"/>
        <Text val="@UI/HeroSelectionScreen/Unavailable"/>
        
        <StateGroup name="VisibilityState">
            <DefaultState val="NotVisible"/>
            
            <State name="Visible">
                <When type="AnimationState" frame="$ancestor[@name=HeroSelectionScreen]" EnabledState="Disabled"/>
                <Action type="SetProperty" Visible="True"/>
            </State>
            
            <State name="NotVisible">
                <Action type="SetProperty" Visible="False"/>
            </State>
        </StateGroup>
    </Frame>

    <!--
    ===================
    Selection Title Label Template
    ===================
    -->
        
