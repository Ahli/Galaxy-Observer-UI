    <!--
    ================
    Countdown Panel Template
    ================
    -->
    
    <Frame type="Frame" name="CountdownPanelTemplate">
        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="160"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Height val="400"/>
        <Visible val="false"/>
        
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@White"/>
            <Color val="0,0,0"/>
            <Alpha val="220"/>
        </Frame>
        
        <Frame type="Image" name="Icon">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/W3C_HeroSelect_CountdownMasks" layer="0"/>
            <AlphaLayer val="true" layer="0"/>
            <AlphaChannel val="Blue" layer="0"/>
            
            <Texture val="@White" layer="1"/>
        </Frame>
            
        <Animation name="LootBoxAnim3OddTemplate">
            <Event event="Ready" action="Reset,Play" frame="$parent"/>
            <Event event="NotReady" action="Reset,Stop" frame="$parent"/>
            
            <Controller type="Property" property="Visible" end="Pause">
                <Key type="Property" time="0" value="false"/>
                <Key type="Property" time="0" value="true"/>
            </Controller>
            
            <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                <Key type="Curve" time="0" value="200" inout="Smooth"/>
                <Key type="Curve" time=".25" value="0" inout="Smooth"/>
            </Controller>
            
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".25" value="255" inout="Linear"/>
            </Controller>
            
            <Controller type="Property" frame="Icon" property="AlphaChannel" index="0" end="Pause">
                <Key type="Property" time="0" value="Blue"/>
                <Key type="Property" time="1" value="Green"/>
                <Key type="Property" time="2" value="Red"/>
            </Controller>
 
            <Controller type="LayerUV" frame="Icon" layer="0" side="Top" relative="False" sidelock="False" end="Pause" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="1" value="0" inout="Linear"/>
                <Key type="Curve" time="1" value="-2" inout="Linear"/>
                <Key type="Curve" time="1.15" value="0" inout="Linear"/>
                <Key type="Curve" time="2" value="0" inout="Linear"/>
                <Key type="Curve" time="2" value="-2" inout="Linear"/>
                <Key type="Curve" time="2.15" value="0" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="Icon" layer="0" side="Bottom" relative="False" sidelock="False" end="Pause" >
                <Key type="Curve" time="0" value="1" inout="Linear"/>
                <Key type="Curve" time="1" value="1" inout="Linear"/>
                <Key type="Curve" time="1" value="3" inout="Linear"/>
                <Key type="Curve" time="1.15" value="1" inout="Linear"/>
                <Key type="Curve" time="2" value="1" inout="Linear"/>
                <Key type="Curve" time="2" value="3" inout="Linear"/>
                <Key type="Curve" time="2.15" value="1" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="Icon" layer="0" side="Left" relative="False" sidelock="False" end="Pause" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="1" value="0" inout="Linear"/>
                <Key type="Curve" time="1" value="-2" inout="Linear"/>
                <Key type="Curve" time="1.15" value="0" inout="Linear"/>
                <Key type="Curve" time="2" value="0" inout="Linear"/>
                <Key type="Curve" time="2" value="-2" inout="Linear"/>
                <Key type="Curve" time="2.15" value="0" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="Icon" layer="0" side="Right" relative="False" sidelock="False" end="Pause" >
                <Key type="Curve" time="0" value="1" inout="Linear"/>
                <Key type="Curve" time="1" value="1" inout="Linear"/>
                <Key type="Curve" time="1" value="3" inout="Linear"/>
                <Key type="Curve" time="1.15" value="1" inout="Linear"/>
                <Key type="Curve" time="2" value="1" inout="Linear"/>
                <Key type="Curve" time="2" value="3" inout="Linear"/>
                <Key type="Curve" time="2.15" value="1" inout="Linear"/>
            </Controller>
            
            <!-- Force a redraw after changing alpha channel -->
            <Controller type="Property" frame="Icon" property="Visible" end="Pause">
                <Key type="Property" time="0" value="false"/>
                <Key type="Property" time="0" value="true"/>
                <Key type="Property" time="1" value="false"/>
                <Key type="Property" time="1" value="true"/>
                <Key type="Property" time="2" value="false"/>
                <Key type="Property" time="2" value="true"/>
            </Controller>
        </Animation>
    </Frame>
    

