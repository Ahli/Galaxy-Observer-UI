                <Key type="Property" timeinc="0.300" value="127" inout="Linear"/>
                <Key type="Property" timeinc="0.000" value="0" inout="Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="Static">
            <Event event="OnShown" action="Reset,Play"/>
            <Event event="OnHeroDeath" frame="$ancestor[@type=UnitStatusFrame]" action="Reset,Play"/>
 
            <!-- Noise -->
            <Controller type="LayerUV" frame="NoiseOverlay" layer="1" side="Top" relative="True" sidelock="False" end="Loop" >
