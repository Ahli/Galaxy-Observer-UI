            </Controller>
 
            <Controller type="LayerUV" frame="NoiseOverlay" layer="1" side="Bottom" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".25" value="1" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="NoiseOverlay" layer="1" side="Left" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".25" value="1" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="NoiseOverlay" layer="1" side="Right" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".25" value="1" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="NoiseOverlay" layer="2" side="Top" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".25" value="-1" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="NoiseOverlay" layer="2" side="Bottom" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".25" value="-1" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="NoiseOverlay" layer="2" side="Left" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".25" value="-1" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="NoiseOverlay" layer="2" side="Right" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".25" value="-1" inout="Linear"/>
            </Controller>
 
            <!-- Wipe Overlay -->
            <Controller type="LayerUV" frame="WipeOverlay" layer="1" side="Top" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="3.5" value=".6" inout="Slow"/>
                <Key type="Curve" time="5.5" value=".5" inout="Slow"/>
                <Key type="Curve" time="8.0" value="1.1" inout="Linear"/>
            </Controller>
 
            <Controller type="LayerUV" frame="WipeOverlay" layer="1" side="Bottom" relative="True" sidelock="False" end="Loop" >
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="3.5" value=".6" inout="Slow"/>
                <Key type="Curve" time="5.5" value=".5" inout="Slow"/>
                <Key type="Curve" time="8.0" value="1.1" inout="Linear"/>
            </Controller>
        </Animation>
