        <Frame type="Image" name="2DPortrait">
            <Anchor relative="$parent"/>
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Image" name="ImageDistortionLeft" template="Coop_TopBar_Tychus/ImageDistortionTemplate">
            <RenderType val="HDR"/>
            <AlphaChannel val="Blue" layer="1"/>
            <Texture val="{$parent/2DPortrait/@Texture}" layer="2"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="#Coop_Tychus_HeroPortrait_Dead_AdjustmentColor"/>
        </Frame>
        
        <Frame type="Image" name="ImageDistortionRight" template="Coop_TopBar_Tychus/ImageDistortionTemplate">
            <RenderType val="HDR"/>
            <AlphaChannel val="Alpha" layer="1"/>
            <Texture val="{$parent/2DPortrait/@Texture}" layer="2"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="#Coop_Tychus_HeroPortrait_Dead_AdjustmentColor"/>
        </Frame>
  
        <Animation name="Jitter">
            <Event event="OnShown" action="Reset,Play"/>
            
            <Controller type="Anchor" frame="ImageDistortionLeft" end="Loop" side="Left" relative="true">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.100" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="-1" inout="Linear"/>
                <Key type="Curve" timeinc="0.100" value="-1" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.500" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="-1" inout="Linear"/>
                <Key type="Curve" timeinc="0.100" value="-1" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.500" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="-2" inout="Linear"/>
                <Key type="Curve" timeinc="0.100" value="-2" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="0" inout="Linear"/>
            </Controller>
            
            <Controller type="Anchor" frame="ImageDistortionRight" end="Loop" side="Left" relative="true">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.100" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="1" inout="Linear"/>
                <Key type="Curve" timeinc="0.100" value="1" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.500" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="1" inout="Linear"/>
                <Key type="Curve" timeinc="0.100" value="1" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.500" value="0" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="2" inout="Linear"/>
                <Key type="Curve" timeinc="0.100" value="2" inout="Linear"/>
                <Key type="Curve" timeinc="0.000" value="0" inout="Linear"/>
            </Controller>
        </Animation>
    </Frame>
    
