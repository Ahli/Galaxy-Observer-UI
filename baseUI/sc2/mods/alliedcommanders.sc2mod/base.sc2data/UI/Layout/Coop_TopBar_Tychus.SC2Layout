        <Animation name="Death">
            <Event event="OnHeroDeath" frame="$ancestor[@type=UnitStatusFrame]" action="Reset,Play"/>
            <Event event="OnHeroRevive" frame="$ancestor[@type=UnitStatusFrame]" action="Stop"/>
            
            <!-- Fade In -->
            <Controller type="Property" property="LayerAlpha" frame="NoiseBackground" curve="True" index="1" end="Pause">
                <Key type="Property" time="0" value="255" inout="Linear"/>
                <Key type="Property" timeinc="0.300" value="255" inout="Linear"/>
                <Key type="Property" timeinc="0.000" value="0" inout="Linear"/>
                <Key type="Property" timeinc="0.100" value="0" inout="Linear"/>
                <Key type="Property" timeinc="0.000" value="255" inout="Linear"/>
                <Key type="Property" timeinc="2.000" value="63" inout="Linear"/>
            </Controller>
            
            <Controller type="Property" property="LayerAlpha" frame="NoiseOverlay" curve="True" index="3" end="Pause">
                <Key type="Property" time="0" value="255" inout="Linear"/>
                <Key type="Property" timeinc="0.300" value="255" inout="Linear"/>
                <Key type="Property" timeinc="0.000" value="0" inout="Linear"/>
                <Key type="Property" timeinc="0.100" value="0" inout="Linear"/>
                <Key type="Property" timeinc="0.000" value="255" inout="Linear"/>
                <Key type="Property" timeinc="2.000" value="63" inout="Linear"/>
