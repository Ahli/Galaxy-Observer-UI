                <Action type="SetProperty" frame="InnerGlow" Color="#Coop_Tychus_HeroPortrait_Default_InnerGlow_Color"/>
                <Action type="SetProperty" frame="ScanLines" Color="#Coop_Tychus_HeroPortrait_Default_ScanLines_Color"/>
                <Action type="SetProperty" frame="ScanLinesGlow" Color="#Coop_Tychus_HeroPortrait_Default_ScanLinesGlow_Color"/>
                <Action type="SetProperty" frame="Emissive" Color="#Coop_Tychus_HeroPortrait_Default_Emissive_Color"/>
                <Action type="SetProperty" frame="ColorIndicator" Color="#Coop_Tychus_HeroPortrait_Default_Indicator_Color"/>
                <Action type="SetProperty" frame="EquipBlipMaster" Color="#Coop_Tychus_HeroPortrait_Default_EquipBlip_Color"/>
            </State>
        </StateGroup>
        
        <!-- Controls how the unit portrait looks and responds to clicks -->
        <StateGroup name="UnitPortraitState">
            <DefaultState val="Alive"/>
            
            <State name="Available">
                <When type="AnimationState" AvailabilityState="Available"/>
                <When type="AnimationState" EmptyState="Empty"/>
                <Action type="SendEvent" event="OnAvailable"/>
                <Action type="SetProperty" frame="PortraitContainer" Visible="False"/>
                <Action type="SetProperty" frame="2DPortraitContainer" Visible="False"/>
                <Action type="SetProperty" frame="ReadySlotContainer" Visible="True"/>
                <Action type="SetProperty" frame="SquadDeathIndicator" Visible="False"/>
                <!-- <Action type="SetProperty" frame="SquadAvailableIndicator" Visible="True"/> -->
                <Action type="SetProperty" frame="SpotlightEffect" Color="#Coop_Tychus_Spotlight_Effect_Color"/>
                <Action type="SetProperty" frame="SpotlightEffect" Visible="True"/>
                <Action type="SetProperty" frame="UnitTargetFrame" Visible="False"/>
                <Action type="SetProperty" frame="BarTargetFrame" Visible="True"/>
                <Action type="SetProperty" frame="EquipmentStructureTargetFrame" Visible="False"/>
            </State>
            
            <State name="Unlocking">
                <When type="AnimationState" AvailabilityState="Unlocking"/>
                <When type="AnimationState" EmptyState="Empty"/>
                <Action type="SendEvent" event="OnUnlocking"/>
                <Action type="SetProperty" frame="PortraitContainer" Visible="False"/>
                <Action type="SetProperty" frame="2DPortraitContainer" Visible="False"/>
                <Action type="SetProperty" frame="ReadySlotContainer" Visible="False"/>
                <Action type="SetProperty" frame="SquadDeathIndicator" Visible="False"/>
                <!-- <Action type="SetProperty" frame="SquadAvailableIndicator" Visible="False"/> -->
                <Action type="SetProperty" frame="SpotlightEffect" Visible="False"/>
                <Action type="SetProperty" frame="UnitTargetFrame" Visible="False"/>
                <Action type="SetProperty" frame="BarTargetFrame" Visible="False"/>
                <Action type="SetProperty" frame="EquipmentStructureTargetFrame" Visible="False"/>
            </State>
            
            <State name="Unavailable">
                <When type="AnimationState" EmptyState="Empty"/>
                <Action type="SetProperty" frame="PortraitContainer" Visible="False"/>
                <Action type="SetProperty" frame="2DPortraitContainer" Visible="False"/>
                <Action type="SetProperty" frame="ReadySlotContainer" Visible="False"/>
                <Action type="SetProperty" frame="SquadDeathIndicator" Visible="False"/>
                <!-- <Action type="SetProperty" frame="SquadAvailableIndicator" Visible="False"/> -->
                <Action type="SetProperty" frame="SpotlightEffect" Visible="False"/>
                <Action type="SetProperty" frame="UnitTargetFrame" Visible="False"/>
                <Action type="SetProperty" frame="BarTargetFrame" Visible="False"/>
                <Action type="SetProperty" frame="EquipmentStructureTargetFrame" Visible="False"/>
            </State>
            
            <State name="Dead">
                <When type="AnimationState" DeathState="Dead"/>
                <Action type="SendEvent" event="OnHeroDeath"/>
                <Action type="SetProperty" frame="PortraitContainer" Visible="False"/>
                <Action type="SetProperty" frame="2DPortraitContainer" Visible="True"/>
                <Action type="SetProperty" frame="ReadySlotContainer" Visible="False"/>
                <Action type="SetProperty" frame="SquadDeathIndicator" Visible="True"/>
                <!-- <Action type="SetProperty" frame="SquadAvailableIndicator" Visible="False"/> -->
                <Action type="SetProperty" frame="SpotlightEffect" Color="#Coop_Tychus_Spotlight_Effect_Dead_Color"/>
                <Action type="SetProperty" frame="SpotlightEffect" Visible="True"/>
                <Action type="SetProperty" frame="UnitTargetFrame" Visible="False"/>
                <Action type="SetProperty" frame="BarTargetFrame" Visible="True"/>
                <Action type="SetProperty" frame="EquipmentStructureTargetFrame" Visible="False"/>
            </State>
            
            <State name="Alive">
                <Action type="SendEvent" event="OnHeroRevive"/>
                <Action type="SetProperty" frame="PortraitContainer" Visible="True"/>
                <Action type="SetProperty" frame="2DPortraitContainer" Visible="False"/>
                <Action type="SetProperty" frame="ReadySlotContainer" Visible="False"/>
                <Action type="SetProperty" frame="SquadDeathIndicator" Visible="False"/>
                <!-- <Action type="SetProperty" frame="SquadAvailableIndicator" Visible="False"/> -->
                <Action type="SetProperty" frame="SpotlightEffect" Visible="False"/>
                <Action type="SetProperty" frame="UnitTargetFrame" Visible="True"/>
                <Action type="SetProperty" frame="BarTargetFrame" Visible="False"/>
                <Action type="SetProperty" frame="EquipmentStructureTargetFrame" Visible="True"/>
            </State>
        </StateGroup>
    </Frame>
    
    <!--
    ==========================
    Charge Indicator Template
    ==========================
    -->
    
    <Frame type="Image" name="ChargeIndicatorTemplate">
        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <Width val="8"/>
        <Height val="22"/>
        <Texture val="Assets/Textures/UI_InGame_Coop_TopBar_Tychus_MainBar_ChargeIndicator.dds"/>
        <Color val="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
    </Frame>
    
    <Frame type="Image" name="ChargeIndicatorSlaveTemplate" template="Coop_TopBar_Tychus/ChargeIndicatorTemplate">
        <Rotation val="{$this/AngleOffset/@Result}"/>
    
        <Frame type="MathFrame" name="AngleOffset">
            <Anchor relative="$parent"/>
            <A val="{$parent/$sibling-1/@Rotation}"/>
            <B val="0"/>
            <Operator val="Add"/>
        </Frame>
            
        <StateGroup name="MaxChargesState">
            <DefaultState val="Two"/>
            
            <State name="Five">
                <When type="AnimationState" frame="$parent/$parent" AbilityMaxChargesState="5"/>
                <Action type="SetProperty" frame="AngleOffset" B="72"/>
            </State>
            
            <State name="Four">
                <When type="AnimationState" frame="$parent/$parent" AbilityMaxChargesState="4"/>
                <Action type="SetProperty" frame="AngleOffset" B="90"/>
            </State>
            
            <State name="Three">
                <When type="AnimationState" frame="$parent/$parent" AbilityMaxChargesState="3"/>
                <Action type="SetProperty" frame="AngleOffset" B="120"/>
            </State>
            
            <State name="Two">
                <When type="AnimationState" frame="$parent/$parent" AbilityMaxChargesState="2"/>
                <Action type="SetProperty" frame="AngleOffset" B="180"/>
            </State>
        </StateGroup>
    </Frame>
    
    <!--
    ==========================
    Charge Indicators Container Template
    ==========================
    -->
    
    <Frame type="Frame" name="ChargeIndicatorsContainerTemplate">
        <Anchor side="Top" relative="$parent/PlateBackdrop" pos="Min" offset="23"/>
        <Anchor side="Left" relative="$parent/PlateBackdrop" pos="Min" offset="67"/>
        <Width val="22"/>
        <Height val="22"/>
        
        <Frame type="Image" name="ChargeIndicator1" template="Coop_TopBar_Tychus/ChargeIndicatorTemplate"/>
        <Frame type="Image" name="ChargeIndicator2" template="Coop_TopBar_Tychus/ChargeIndicatorSlaveTemplate"/>
        <Frame type="Image" name="ChargeIndicator3" template="Coop_TopBar_Tychus/ChargeIndicatorSlaveTemplate"/>
        <Frame type="Image" name="ChargeIndicator4" template="Coop_TopBar_Tychus/ChargeIndicatorSlaveTemplate"/>
        <Frame type="Image" name="ChargeIndicator5" template="Coop_TopBar_Tychus/ChargeIndicatorSlaveTemplate"/>
        
        <Animation name="Rotate">
            <Event event="OnShown" action="Reset,Play"/>
            
            <Controller type="Rotation" frame="ChargeIndicator1" relative="False" end="Loop">
                <Key type="Curve" time="0" value="0" inout="linear"/>
                <Key type="Curve" time="5" value="360" inout="linear"/>
            </Controller>
        </Animation>
            
        <StateGroup name="CurrentChargesState">
            <DefaultState val="Zero"/>
            
            <State name="Five">
                <When type="AnimationState" frame="$parent" AbilityCurrentChargesState="5"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
            </State>
            
            <State name="Four">
                <When type="AnimationState" frame="$parent" AbilityCurrentChargesState="4"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
            </State>
            
            <State name="Three">
                <When type="AnimationState" frame="$parent" AbilityCurrentChargesState="3"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
            </State>
            
            <State name="Two">
                <When type="AnimationState" frame="$parent" AbilityCurrentChargesState="2"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
            </State>
            
            <State name="One">
                <When type="AnimationState" frame="$parent" AbilityCurrentChargesState="1"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Color="#Coop_Tychus_ChargeIndicator_Ready_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
            </State>
            
            <State name="Zero">
                <Action type="SetProperty" frame="ChargeIndicator1" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Color="#Coop_Tychus_ChargeIndicator_NotReady_Color"/>
            </State>
        </StateGroup>
            
        <StateGroup name="MaxChargesState">
            <DefaultState val="One"/>
            
            <State name="Five">
                <When type="AnimationState" frame="$parent" AbilityMaxChargesState="5"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Visible="True"/>
            </State>
            
            <State name="Four">
                <When type="AnimationState" frame="$parent" AbilityMaxChargesState="4"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Visible="False"/>
            </State>
            
            <State name="Three">
                <When type="AnimationState" frame="$parent" AbilityMaxChargesState="3"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Visible="False"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Visible="False"/>
            </State>
            
            <State name="Two">
                <When type="AnimationState" frame="$parent" AbilityMaxChargesState="2"/>
                <Action type="SetProperty" frame="ChargeIndicator1" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Visible="False"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Visible="False"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Visible="False"/>
            </State>
            
            <State name="One">
                <Action type="SetProperty" frame="ChargeIndicator1" Visible="True"/>
                <Action type="SetProperty" frame="ChargeIndicator2" Visible="False"/>
                <Action type="SetProperty" frame="ChargeIndicator3" Visible="False"/>
                <Action type="SetProperty" frame="ChargeIndicator4" Visible="False"/>
                <Action type="SetProperty" frame="ChargeIndicator5" Visible="False"/>
            </State>
        </StateGroup>
    </Frame>
    
    <!--
    ==========================
    Tychus Top Bar Template
    ==========================
    -->
    
    <Frame type="Frame" name="TychusTopBarTemplate">
        <Anchor relative="$parent"/>
        <RenderPriority val="500"/>
            
        <Frame type="ProgressBar" name="BarTrainingProgress">
            <Anchor relative="$parent"/>
            <MaxValue val="100"/>
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Image" name="PlateBackdrop">
