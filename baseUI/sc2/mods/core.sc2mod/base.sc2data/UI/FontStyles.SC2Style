    </FontGroup>
    <Constant name="FontStandardExtended" val="ExtStandardFontGroup"/>

    <!-- FontMonospace is used for any situations that call for fixed width text, usually   -->
    <!-- for purposes of lining up columns of characters vertically                         -->
    <FontGroup name="MonospaceFontGroup">
