<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Frame type="ScreenScore" name="ScreenScore">
        <Anchor relative="$parent"/>
        <!-- This must render above ScreenBattlenet so it's on top of Flash score screen -->
        <RenderPriority val="800"/>

        <Frame type="Frame" name="BackgroundFrame">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-50"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-50"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="1559"/>
            <Height val="1005"/>
        </Frame>

        <Frame type="GlueButton" name="PlayAgainButton" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
            <Anchor side="Bottom" relative="$parent/BackgroundFrame" pos="Max" offset="-33"/>
            <Anchor side="Left" relative="$parent/BackgroundFrame" pos="Min" offset="40"/>
            <Width val="306"/>
            <Text val="@UI/ScreenScore/PlayAgain"/>
        </Frame>

        <Frame type="GlueButton" name="WatchReplayButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Bottom" relative="$parent/PlayAgainButton" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayAgainButton" pos="Max" offset="0"/>
            <Width val="314"/>
            <Text val="@UI/ScreenScore/WatchReplay"/>
        </Frame>

        <Frame type="Button" name="RenameReplayButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Bottom" relative="$parent/WatchReplayButton" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/WatchReplayButton" pos="Max" offset="0"/>
            <Width val="314"/>
            <Text val="@UI/ScreenScore/RenameReplay"/>
        </Frame>
    </Frame>
</Desc>
