                <Action type="SetProperty" frame="MasteryLevelLabel" Visible="false"/>
                <Action type="SetProperty" frame="MasteryMaxIconAnchor" Visible="true"/>
                <Action type="SetProperty" frame="MasteryMaxIcon" Visible="true"/>
