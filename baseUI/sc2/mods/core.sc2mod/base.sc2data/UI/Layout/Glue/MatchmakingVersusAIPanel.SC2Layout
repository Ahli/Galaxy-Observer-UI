<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="MatchmakingVersusAIPanel" name="MatchmakingVersusAIPanelTemplate">
        <Width val="800"/>
        <Height val="300"/>
        
        <Frame type="BlurFrame" name="ContainerBGBlur">
            <Anchor side="Top" relative="$parent/VersusAIPanelContainmentBG" pos="Min" offset="5"/>
            <Anchor side="Left" relative="$parent/VersusAIPanelContainmentBG" pos="Min" offset="6"/>
            <Anchor side="Bottom" relative="$parent/VersusAIPanelContainmentBG" pos="Max" offset="-5"/>
            <Anchor side="Right" relative="$parent/VersusAIPanelContainmentBG" pos="Max" offset="-6"/>
            <AlphaMaskTexture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ContainerBlurMask"/>

            <Animation name="Blur">
                <Event event="OnShown" action="Reset,Play" frame="$parent" />

                <Controller type="Blur" end="Pause">
                    <Key type="Curve" time="0" value=".35" out="Fast"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="VersusAIPanelContainmentBG">
            <Anchor side="Top" relative="$parent" pos="Min" offset="18"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="42"/>
            <Texture val="@UI/SC2_UI_Tournament_LeaguePanelBG"/>
            <TextureType val="NineSlice"/>
            <Width val="712"/>
            <Height val="264"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/VersusAIPanelContainmentBG" pos="Min" offset="35"/>
            <Anchor side="Left" relative="$parent/VersusAIPanelContainmentBG" pos="Min" offset="193"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
            <Style val="@MatchmakingLeaguePanel_Title"/>
        </Frame>

        <Frame type="Image" name="InfoIcon">
            <Anchor side="Top" relative="$parent/VersusAIPanelContainmentBG" pos="Min" offset="13"/>
            <Anchor side="Right" relative="$parent/VersusAIPanelContainmentBG" pos="Max" offset="-15"/>
            <Width val="43"/>
            <Height val="43"/>
            <Texture val="@UI/Glues_Icons_GenericInfo"/>
            <AcceptsMouseTooltip val="true"/>
            <Tooltip val="@UI/MatchmakingVersusAIPanel/Tooltip"/>
        </Frame>
        
        <!-- Placement Container -->
        
        <Frame type="Frame" name="PlacementContainer">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="DifficultyIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                <Width val="212"/>
                <Height val="216"/>
                <Texture val="@UI/Glues_Icons_Ladder_Placement"/>
            </Frame>
            
            <Frame type="Label" name="RemainingMatchesLabel">
                <Anchor side="Top" relative="$parent/$parent/TitleLabel" pos="Max" offset="1"/>
                <Anchor side="Left" relative="$parent/$parent/TitleLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Subtitle"/>
                <Text val="@UI/MatchmakingVersusAIPanel/RemainingMatches"/>
            </Frame>
            
            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/RemainingMatchesLabel" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/RemainingMatchesLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingVersusAIPanel/PlacementDescription"/>
            </Frame>
        </Frame>
        
        <!-- Difficulty Container -->
        
        <Frame type="Frame" name="DifficultyContainer">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="DifficultyIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Width val="232"/>
                <Height val="252"/>
                <Texture val="@UI/Glues_Icons_GameMode_Difficulty_VeryEasy"/>
            </Frame>

            <Frame type="Label" name="DifficultyLabel">
                <Anchor side="Top" relative="$parent/$parent/TitleLabel" pos="Max" offset="1"/>
                <Anchor side="Left" relative="$parent/$parent/TitleLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Subtitle"/>
                <Text val="@UI/MatchmakingVersusAIPanel/DifficultyName"/>
            </Frame>

            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/DifficultyLabel" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/DifficultyLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingVersusAIPanel/DifficultyDescription"/>
            </Frame>

            <Frame type="ProgressBar" name="DifficultyProgressBar">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-30"/>
                <Anchor side="Left" relative="$parent/DescriptionLabel" pos="Min" offset="-65"/>
                <Width val="566"/>
                <Height val="70"/>
                <UseFillContainer val="true"/>
                <FillInset val="0.0406"/>

                <Frame type="Image" name="BorderImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Texture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarBg"/>
                    <Width val="520"/>
                    <Height val="22"/>
                </Frame>

                <Frame type="Frame" name="FillImageContainer">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

                    <Frame type="Image" name="FillImage">
                        <Anchor relative="$parent/$parent"/>
                        <Texture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarFill"/>
                    </Frame>

                    <Frame type="Frame" name="FillImageAccentMask">
                        <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="0"/>
                        <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/$parent" pos="Max" offset="0"/>
                        <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="0"/>
                        <Width val="566"/>
                        <Height val="70"/>
                        <AlphaMaskTexture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarMask"/>

                        <Frame type="Image" name="FillImageAccent">
                            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                            <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="8"/>
                            <Texture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarHighlight"/>
                        </Frame>
                    </Frame>
                </Frame>
                
                <Frame type="Image" name="DividersImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Texture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarDividers"/>
                    <Width val="520"/>
                    <Height val="22"/>
                </Frame>
            </Frame>
        </Frame>
        
        <!--
        
        
        VERSUS AI STATE ANIMATIONS
        
        
        -->
        
        <!-- Empty -->
        
        <Animation name="VersusAIStateEmpty">
            <Event event="Empty" action="Reset,Play"/>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="DifficultyContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- Placement -->
        
        <Animation name="VersusAIStatePlacement">
            <Event event="Placement" action="Reset,Play"/>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="DifficultyContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- Difficulty -->
        
        <Animation name="VersusAIStateDifficulty">
            <Event event="Difficulty" action="Reset,Play"/>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="DifficultyContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
        </Animation>
        
        <!--
        
        
        DIFFICULTY ICON ANIMATIONS
        
        
        -->
        
        <!-- Very Easy -->
        
        <Animation name="VeryEasy">
            <Event event="VyEy" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="DifficultyContainer/DifficultyIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_GameMode_Difficulty_VeryEasy"/>
            </Controller>
        </Animation>
        
        <!-- Easy -->
        
        <Animation name="Easy">
            <Event event="Easy" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="DifficultyContainer/DifficultyIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_GameMode_Difficulty_Easy"/>
            </Controller>
        </Animation>
        
        <!-- Medium -->
        
        <Animation name="Medium">
            <Event event="Medi" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="DifficultyContainer/DifficultyIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_GameMode_Difficulty_Medium"/>
            </Controller>
        </Animation>
        
        <!-- Hard -->
        
        <Animation name="Hard">
            <Event event="MdHd" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="DifficultyContainer/DifficultyIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_GameMode_Difficulty_Hard"/>
            </Controller>
        </Animation>
        
        <!-- Harder -->
        
        <Animation name="Harder">
            <Event event="Hard" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="DifficultyContainer/DifficultyIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_GameMode_Difficulty_Harder"/>
            </Controller>
        </Animation>
        
        <!-- Very Hard -->
        
        <Animation name="VeryHard">
            <Event event="HdVH" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="DifficultyContainer/DifficultyIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_GameMode_Difficulty_VeryHard"/>
            </Controller>
        </Animation>
        
        <!-- Elite -->
        
        <Animation name="Elite">
            <Event event="VyHd" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="DifficultyContainer/DifficultyIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_GameMode_Difficulty_Elite"/>
            </Controller>
        </Animation>

        <!--
        
        
        DIFFICULTY DESCRIPTION ANIMATIONS
        
        
        -->

        <!-- Very Easy -->
        <!-- Easy -->
        <!-- Medium -->
        <!-- Hard -->
        <!-- Harder -->
        <!-- Very Hard -->
        <!-- Elite -->

        <Animation name="DescriptionMaxDifficulty">
            <Event event="VyEy" action="Reset,Stop"/>
            <Event event="Easy" action="Reset,Stop"/>
            <Event event="Medi" action="Reset,Stop"/>
            <Event event="MdHd" action="Reset,Stop"/>
            <Event event="Hard" action="Reset,Stop"/>
            <Event event="HdVH" action="Reset,Stop"/>
            <Event event="VyHd" action="Reset,Play"/>

            <Controller type="Text" end="Pause" frame="DifficultyContainer/DescriptionLabel">
                <Key type="Text" time="0" text="@UI/MatchmakingVersusAIPanel/DifficultyMaxDescription"/>
            </Controller>
        </Animation>
    </Frame>
</Desc>
