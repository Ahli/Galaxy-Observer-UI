                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name_Selected"/>
                </State>

                <State name="NormalState">
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="CostLabel">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-22"/>
            <Anchor side="Left" relative="$parent/NameLabel" pos="Min" offset="0"/>
            <Style val="@Collection_ListItem_Cost"/>
            <Text val="{$parent/GameContentFrame/@CostText}"/>
        </Frame>
        
        <Frame type="Frame" name="StateLabelContainer">
            <Anchor side="Top" relative="$parent/NameLabel" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$this/StateLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/NameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/NameLabel" pos="Max" offset="0"/>
            
            <Frame type="Label" name="StateLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Options val="Ellipsis | NoWrapping | TooltipIfTruncated"/>
                <Style val="@Collection_ListItem_Desc"/>
                
                <StateGroup name="TextStates">
                    <DefaultState val="NormalState"/>
                
                    <State name="ToggledState">
                        <When type="Property" frame="$parent/$parent/SelectedImage" visible="True"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Selected"/>
                    </State>
                    
                    <State name="HoverState">
                        <When type="Property" frame="$parent/$parent/HoverImage" visible="True"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Selected"/>
                    </State>
                    
                    <State name="LockedState">
                        <When type="StateGroup" frame="$parent/$parent" GameContentOwnedStateGroup="GameContentLocked"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Locked"/>
                    </State>

                    <State name="NormalState">
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc"/>
                    </State>
                </StateGroup>
            </Frame>
            
            <Frame type="Label" name="StateLabelEquipped">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Options val="Ellipsis | NoWrapping | TooltipIfTruncated"/>
                <Style val="@Collection_ListItem_DescEquipped"/>
                <Text val="{$parent/StateLabel/@Text}"/>
                
                <StateGroup name="TextStates">
                    <DefaultState val="NormalState"/>
                
                    <State name="ToggledState">
                        <When type="Property" frame="$parent/$parent/SelectedImage" visible="True"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_DescEquipped_Selected"/>
                    </State>
                    
                    <State name="HoverState">
                        <When type="Property" frame="$parent/$parent/HoverImage" visible="True"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_DescEquipped_Selected"/>
                    </State>
                    
                    <State name="LockedState">
                        <When type="StateGroup" frame="$parent/$parent" GameContentOwnedStateGroup="GameContentLocked"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_DescEquipped_Locked"/>
                    </State>

                    <State name="NormalState">
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_DescEquipped"/>
                    </State>
                </StateGroup>
            </Frame>
        </Frame>
        
        <Frame type="Image" name="EquippedBorder">
            <Anchor side="Top" relative="$parent" pos="Min" offset="11"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-11"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="11"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-11"/>
            <Texture val="@UI/SC2_UI_Collection_Equipped_Border"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="CheckMarkImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="11"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-11"/>
            <Texture val="@UI/SC2_UI_Collection_Equipped_Checkmark"/>
        </Frame>
        
        <StateGroup name="ToggledStateGroup">
            <DefaultState val="UntoggledState"/>
        
            <State name="ToggledState">
                <When type="Property" frame="$this/SelectedImage" visible="True"/>
                <Action type="SetProperty" frame="$this/NormalImage" alpha="0"/>
                <Action type="SetProperty" frame="$this/HoverImage" alpha="0"/>
                <Action type="SetProperty" frame="$this/HoverGlow" alpha="0"/>
            </State>
            
            <State name="UntoggledState">
                <When type="Property" frame="$this/SelectedImage" visible="False"/>
                <Action type="SetProperty" frame="$this/NormalImage" alpha="255"/>
                <Action type="SetProperty" frame="$this/HoverImage" alpha="255"/>
                <Action type="SetProperty" frame="$this/HoverGlow" alpha="255"/>
            </State>
        </StateGroup>

        <StateGroup name="RewardSelectedStateGroup">
            <DefaultState val="RewardNotSelected"/>

            <State name="RewardSelected">
                <When type="AnimationState" frame="RewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="CheckMarkImage" visible="true"/>
                <Action type="SetProperty" frame="EquippedBorder" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabelEquipped" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" visible="false"/>
            </State>

            <State name="RewardNotSelected">
                <Action type="SetProperty" frame="CheckMarkImage" visible="false"/>
                <Action type="SetProperty" frame="EquippedBorder" visible="false"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabelEquipped" visible="false"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" visible="true"/>
            </State>
        </StateGroup>

        <StateGroup name="GameContentOwnedStateGroup">
            <DefaultState val="GameContentNotOwned"/>

            <State name="GameContentPurchased">
                <When type="AnimationState" frame="GameContentFrame" PurchasedState="Purchased"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Owned"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockIcon" visible="false"/>
            </State>

            <State name="GameContentDefault">
                <When type="Property" frame="RewardCacheFrame" Default="true"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Default"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockIcon" visible="false"/>
            </State>

            <State name="GameContentAllowed">
                <When type="AnimationState" frame="RewardCacheFrame" AllowedState="Allowed"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Owned"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockIcon" visible="false"/>
            </State>

            <State name="GameContentLocked">
                <When type="Property" frame="GameContentFrame" CanPurchase="false"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Locked"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockIcon" visible="True"/>
            </State>

            <State name="GameContentNotOwned">
                <Action type="SetProperty" frame="StateLabelContainer" visible="false"/>
                <Action type="SetProperty" frame="CostLabel" visible="true"/>
                <Action type="SetProperty" frame="LockIcon" visible="True"/>
            </State>
        </StateGroup>

        <StateGroup name="HasDataStateGroup">
            <State name="NoDataState">
                <When type="Property" frame="$this" HasData="False"/>
                <Action type="SetProperty" frame="EmptyImage" Visible="true"/>
                <Action type="SetProperty" frame="CheckMarkImage" Alpha="0"/>
                <Action type="SetProperty" frame="EquippedBorder" Alpha="0"/>
                <Action type="SetProperty" frame="UnitModelFrame" Alpha="0"/>
                <Action type="SetProperty" frame="TextBackground" Alpha="0"/>
                <Action type="SetProperty" frame="NameLabel" Alpha="0"/>
                <Action type="SetProperty" frame="CostLabel" Alpha="0"/>
                <Action type="SetProperty" frame="StateLabelContainer" Alpha="0"/>
                <Action type="SetProperty" frame="LockedItemBackgroundGlow" Alpha="0"/>
                <Action type="SetProperty" frame="LockIcon" Alpha="0"/>
                <Action type="SetProperty" frame="BackgroundDarken" Alpha="0"/>
                <Action type="SetProperty" frame="BackgroundBlur" BlurAmount=".1"/>
            </State>

            <State name="HasDataState">
                <When type="Property" frame="$this" HasData="True"/>
                <Action type="SetProperty" frame="EmptyImage" Visible="false"/>
                <Action type="SetProperty" frame="CheckMarkImage" Alpha="255"/>
                <Action type="SetProperty" frame="EquippedBorder" Alpha="255"/>
                <Action type="SetProperty" frame="UnitModelFrame" Alpha="255"/>
                <Action type="SetProperty" frame="TextBackground" Alpha="255"/>
                <Action type="SetProperty" frame="NameLabel" Alpha="255"/>
                <Action type="SetProperty" frame="CostLabel" Alpha="255"/>
                <Action type="SetProperty" frame="StateLabelContainer" Alpha="255"/>
                <Action type="SetProperty" frame="LockedItemBackgroundGlow" Alpha="120"/>
                <Action type="SetProperty" frame="LockIcon" Alpha="191"/>
                <Action type="SetProperty" frame="BackgroundDarken" Alpha="80"/>
                <Action type="SetProperty" frame="BackgroundBlur" BlurAmount=".35"/>
            </State>
        </StateGroup>
        
        <StateGroup name="UnitStateGroup">
        
            <!-- TERRAN -->
            <State name="Ghost">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Ghost"/>
                <Action type="SendEvent" frame="$this" event="Ghost"/>
            </State>
            
            <State name="Marauder">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Marauder"/>
                <Action type="SendEvent" frame="$this" event="Marauder"/>
            </State>
            
            <State name="Marine">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Marine"/>
                <Action type="SendEvent" frame="$this" event="Marine"/>
            </State>
            
            <State name="SupplyDepot">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="SupplyDepot"/>
                <Action type="SendEvent" frame="$this" event="SupplyDepot"/>
            </State>
            
            <State name="Thor">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Thor"/>
                <Action type="SendEvent" frame="$this" event="Thor"/>
            </State>
            
            <!-- ZERG -->
            <State name="Overlord">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Overlord"/>
                <Action type="SendEvent" frame="$this" event="Overlord"/>
            </State>
            
            <State name="Roach">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Roach"/>
                <Action type="SendEvent" frame="$this" event="Roach"/>
            </State>
            
            <State name="Ultralisk">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Ultralisk"/>
                <Action type="SendEvent" frame="$this" event="Ultralisk"/>
            </State>
            
            <State name="Zergling">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Zergling"/>
                <Action type="SendEvent" frame="$this" event="Zergling"/>
            </State>
            
            <!-- PROTOSS -->
            <State name="Adept">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Adept"/>
                <Action type="SendEvent" frame="$this" event="Adept"/>
            </State>
            
            <State name="Colossus">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Colossus"/>
                <Action type="SendEvent" frame="$this" event="Colossus"/>
            </State>
            
            <State name="Pylon">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Pylon"/>
                <Action type="SendEvent" frame="$this" event="Pylon"/>
            </State>
            
            <State name="Stalker">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Stalker"/>
                <Action type="SendEvent" frame="$this" event="Stalker"/>
            </State>
            
            <State name="Zealot">
                <When type="Property" frame="$UnitSkinSelectionFrame" Unit="Zealot"/>
                <Action type="SendEvent" frame="$this" event="Zealot"/>
            </State>
        </StateGroup>
        
        <!-- UNIT ANIMATIONS -->
        
        <!-- Terran -->
        <Animation name="Ghost" template="SkinPackUnitAnimationTemplates/Ghost"/>
        <Animation name="Marauder" template="SkinPackUnitAnimationTemplates/Marauder"/>
        <Animation name="Marine" template="SkinPackUnitAnimationTemplates/Marine"/>
        <Animation name="SupplyDepot" template="SkinPackUnitAnimationTemplates/SupplyDepot"/>
        <Animation name="Thor" template="SkinPackUnitAnimationTemplates/Thor"/>
        
        <!-- Zerg -->
        <Animation name="Overlord" template="SkinPackUnitAnimationTemplates/Overlord"/>
        <Animation name="Roach" template="SkinPackUnitAnimationTemplates/Roach"/>
        <Animation name="Ultralisk" template="SkinPackUnitAnimationTemplates/Ultralisk"/>
        <Animation name="Zergling" template="SkinPackUnitAnimationTemplates/Zergling"/>
        
        <!-- Protoss -->
        <Animation name="Adept" template="SkinPackUnitAnimationTemplates/Adept"/>
        <Animation name="Colossus" template="SkinPackUnitAnimationTemplates/Colossus"/>
        <Animation name="Pylon" template="SkinPackUnitAnimationTemplates/Pylon"/>
        <Animation name="Stalker" template="SkinPackUnitAnimationTemplates/Stalker"/>
        <Animation name="Zealot" template="SkinPackUnitAnimationTemplates/Zealot"/>
    </Frame>

    <Frame type="TileList" name="SkinTileListTemplate">
        <Width val="1062"/>
        <Height val="683"/>
        <AcceptsMouse val="true"/>
        <RequireSelection val="true"/>

        <Frame type="UnitSkinSelectionTileListItem" name="Item0" template="SkinPackUnitSelectionFrame/UnitSkinSelectionTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        </Frame>

        <Frame type="UnitSkinSelectionTileListItem" name="Item1" template="SkinPackUnitSelectionFrame/UnitSkinSelectionTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="18"/>
        </Frame>

        <Frame type="UnitSkinSelectionTileListItem" name="Item2" template="SkinPackUnitSelectionFrame/UnitSkinSelectionTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item1" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="18"/>
        </Frame>

        <Frame type="UnitSkinSelectionTileListItem" name="Item3" template="SkinPackUnitSelectionFrame/UnitSkinSelectionTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Max" offset="18"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>
        
        <Frame type="UnitSkinSelectionTileListItem" name="Item4" template="SkinPackUnitSelectionFrame/UnitSkinSelectionTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item3" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item3" pos="Max" offset="18"/>
        </Frame>
        
        <Frame type="UnitSkinSelectionTileListItem" name="Item5" template="SkinPackUnitSelectionFrame/UnitSkinSelectionTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item4" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item4" pos="Max" offset="18"/>
        </Frame>

        <Frame type="ScrollBar" name="ScrollBar" template="CollectionTemplates/ScrollBarTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="-4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-12"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-1"/>
        </Frame>
    </Frame>

    <Frame type="UnitSkinSelectionFrame" name="UnitSkinSelectionFrameTemplate">
        <Height val="1200"/>
        <PresenceId val="{$BattleUI/@LocalToonPresenceId}"/>

        <Frame type="GameContentSortPulldown" name="SortPulldown" template="StandardBattlenetTemplates/BattlenetLargePulldownTemplate">
            <Anchor side="Bottom" relative="$Collection_DropdownAnchor" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/SkinTileList/Item2" pos="Max" offset="0"/>
            <Animation name="HoverSound">
                <Event event="OnMouseEnter" action="Reset,Play"/>
                <Event event="OnMouseExit" action="Reset, Stop"/>
                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0" sound="@UI_HoverTick"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="TileList" name="SkinTileList" template="SkinPackUnitSelectionFrame/SkinTileListTemplate">
            <Anchor side="Top" relative="$parent/$parent/UnitTileList" pos="Min" offset="4"/>
            <Anchor side="Left" relative="$parent/$parent/UnitTileList" pos="Max" offset="18"/>
        </Frame>

        <Frame type="SelectUnlockableButton" name="SelectUnlockableButton" template="CollectionTemplates/SelectUnlockableButtonTemplate">
            <Anchor side="Bottom" relative="$Anchor_BottomActionButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Reward val="{$parent/@SelectedReward}"/>
            <PresenceId val="{$parent/@PresenceId}"/>

            <Animation name="HoverSound">
                <Event event="OnMouseEnter" action="Reset,Play"/>
                <Event event="OnMouseExit" action="Reset, Stop"/>
                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0" sound="@UI_HoverTick"/>
                </Controller>
            </Animation>

            <Frame type="GameContentFrame" name="GameContentFrame">
                <GameContent val="{$parent/$parent/@SelectedGameContent}"/>
            </Frame>
            
            <StateGroup name="PurchaseableState">

                <State name="NotPurchaseable">
                    <When type="Property" frame="$parent/BuyButton" visible="False"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>

                <State name="Purchaseable">
                    <When type="Property" frame="$parent/BuyButton" visible="True"/>
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="BuyButton" name="BuyButton" template="CollectionTemplates/BuyButtonTemplate">
            <Anchor side="Bottom" relative="$Anchor_BottomActionButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/SelectUnlockableButton" pos="Min" offset="0"/>
            <GameContent val="{$parent/@SelectedGameContent}"/>
        </Frame>
        
        <Frame type="Frame" name="EquippedWarningContainer" template="CollectionTemplates/EquippedWarningContainerTemplate">
            <Anchor side="Top" relative="$parent/SelectUnlockableButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/SelectUnlockableButton" pos="Max" offset="20"/>
            
            <StateGroup name="EquippedStateGroup">
            
                <State name="EquippedState">
                    <When type="AnimationState" frame="$parent/SelectUnlockableButton/RewardCacheFrame" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>
                
                <State name="UnequippedState">
                    <When type="AnimationState" frame="$parent/SelectUnlockableButton/RewardCacheFrame" SelectedState="NotSelected"/>
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Frame" name="AchievementLockedWarningContainer" template="CollectionTemplates/AchievementLockedWarningContainerTemplate">
            <Anchor side="Bottom" relative="$Anchor_BottomActionButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/SelectUnlockableButton" pos="Max" offset="20"/>
            
            <StateGroup name="AchievementLockedStateGroup">
                <DefaultState val="NotAchievementLockedState"/>
            
                <State name="AchievementLockedState">
                    <When type="AnimationState" frame="$parent/SelectUnlockableButton/GameContentFrame" PurchasedState="NotPurchased"/>
                    <When type="Property" frame="$parent/SelectUnlockableButton/RewardCacheFrame" Default="False"/>
                    <When type="Property" frame="$parent/SelectUnlockableButton/GameContentFrame" CanPurchase="False"/>
                    <When type="AnimationState" frame="$parent/SelectUnlockableButton/RewardCacheFrame" AllowedState="NotAllowed"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>
                
                <State name="NotAchievementLockedState">
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>

    <Frame type="SkinPackUnitSelectionFrame" name="SkinPackUnitSelectionFrameTemplate">
        <SelectedRace val="{$this/RaceTabControl/@SelectedRace}"/>

        <Frame type="SelectedSkinPropertyTooltip" name="UnitSkinSelectionTileListItemTooltip" template="SkinPackUnitSelectionFrame/UnitSkinSelectionTileListItemTooltipTemplate">

        </Frame>

        <Frame type="RaceTabControl" name="RaceTabControl" template="CollectionTemplates/RaceTabControlTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="162"/>
            <Anchor side="Left" relative="$parent/UnitTileList" pos="Min" offset="3"/>
            <Anchor side="Bottom" relative="$this/RaceButton2" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$this/RaceButton2" pos="Max" offset="0"/>
        </Frame>
        
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="98"/>
            <Anchor side="Left" relative="$Anchor_LeftHeader" pos="Max" offset="0"/>
            <Style val="@Arcade_Title"/>
            <Text val="@UI/ScreenCollection/Skins/Title"/>
        </Frame>

        <Frame type="TileList" name="UnitTileList" template="SkinPackUnitSelectionFrame/UnitTileListTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="212"/>
            <Anchor side="Left" relative="$Anchor_LeftStandard" pos="Max" offset="0"/>
        </Frame>

        <Frame type="UnitSkinSelectionFrame" name="UnitSkinSelectionFrame" template="SkinPackUnitSelectionFrame/UnitSkinSelectionFrameTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-200"/>
            <Anchor side="Left" relative="$parent/UnitTileList" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Handle val="UnitSkinSelectionFrame"/>
            <Unit val="{$parent/@SelectedUnit}"/>
            <SortMethod val="{$this/SortPulldown/@SelectedSort}"/>
        </Frame>

        <Frame type="SkinPackLargeFormatUnitSelectionFrame" name="LargeFormatUnitSelectionFrame" template="SkinPackLargeFormatUnitSelectionFrame/SkinPackLargeFormatUnitSelectionFrameTemplate">
            <Anchor side="Top" relative="$parent/UnitTileList" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/UnitTileList" pos="Max" offset="80"/>
            <Anchor side="Left" relative="$parent/UnitTileList" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/UnitTileList" pos="Max" offset="0"/>
            <PresenceId val="{$BattleUI/@LocalToonPresenceId}"/>
            <Race val="{$parent/RaceTabControl/@SelectedRace}"/>
            <!-- Remove once we have large format selection -->
            <Visible val="false"/>
            <!-- Also add this to RewardData.xml -->
            <!-- <Flags index="ExcludeInLargeFormatGame" value="1"/> -->
        </Frame>
    </Frame>
</Desc>
