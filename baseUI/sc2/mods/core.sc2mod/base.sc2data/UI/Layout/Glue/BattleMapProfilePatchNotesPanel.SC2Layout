<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
 
    <Frame type="BattleMapProfileSectionListBoxItem" name="ListBoxItemTemplate">
        <!-- This frame determines the size of the list box item -->
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/DescriptionLabel" pos="Max" offset="10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>
        
        <Frame type="Image" name="ScreenshotsBackground">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
        </Frame>
        
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-10"/>
            <Style val="@Arcade_Title"/>
        </Frame>

        <Frame type="Label" name="SubtitleLabel">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/TitleLabel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
            <Style val="@Arcade_GameInfo_Description"/>
        </Frame>

        <Frame type="Label" name="DescriptionLabel">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-10"/>
            <Style val="@Arcade_GameInfo_Description"/>
            <AcceptsMouse val="true"/>
        </Frame>
    </Frame>

    <Frame type="BattleMapProfilePatchNotesPanel" name="BattleMapProfilePatchNotesPanelTemplate">
        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
        </Frame>
        
        <Frame type="BattleMapProfilePatchNotesListBox" name="ListBox">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="10"/>
            <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="-10"/>
            <Anchor side="Right" relative="$parent/Background" pos="Max" offset="-10"/>
            <Insets top="0" left="0" bottom="0" right="0"/>
            <ItemDesc val="BattleMapProfilePatchNotesPanel/ListBoxItemTemplate"/>
            <ItemHeight val="0"/>
            <ItemGap val="2"/>
            <HighlightOnHover val="false"/>
            <SaveScrollHeightOnClear val="false"/>
            <ScrollBar val="ScrollBar"/>

            <Frame type="ScrollBar" name="ScrollBar" template="StandardBattlenetTemplates/GluesSocialScrollBarTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="-4"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="3"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="4"/>
            </Frame>
        </Frame>
    </Frame>
</Desc>
