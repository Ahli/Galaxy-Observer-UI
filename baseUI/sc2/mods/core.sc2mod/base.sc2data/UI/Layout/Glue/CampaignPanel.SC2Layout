<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Constant name="CampaignButtonPrimaryStyle" val="StandardButtonAlternate"/>
    <Constant name="CampaignButtonSecondaryStyle" val="StandardButton"/>

    <Frame type="Button" name="CampaignButtonTemplate" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
        <Frame type="Image" name="NormalImage">
            <LayerCount val="2"/>
            <Texture val="@@UI/StandardButtonAlternateNormal" layer="1"/>
            <TextureType val="EndCap" layer="1"/>
            <Tiled val="false" layer="1"/>
            <StateCount val="2" layer="1"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <LayerCount val="2"/>
            <Texture val="@@UI/StandardButtonAlternateHover" layer="1"/>
            <TextureType val="EndCap" layer="1"/>
            <Tiled val="false" layer="1"/>
            <StateCount val="2" layer="1"/>
        </Frame>

        <Frame type="Label" name="Label">
            <Options val="StripHotkeys"/>
        </Frame>
    </Frame>
    
    <Frame type="FlashFrame" name="CampaignHelpTipTemplate">
        <Width val="496"/>
        <Height val="244"/>
        <UseAlternateTime val="false"/> <!-- MSS: Used to be true -->
        <AcceptsMouse val="true"/>
    </Frame>
    
    <Frame type="Image" name="CampaignSubSelectionButtonBackgroundTemplate">
        <Width val="360"/>
        <Height val="448"/>
        <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionButton_Background"/>
        <Visible val="False"/>
        <Alpha val="0"/>
        
        <Frame type="Image" name="HighlightImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionButton_Background"/>
            <BlendMode val="Add"/>
            
            <Animation name="Pulse">
                <Event event="OnShown" action="Reset"/>
                <Event event="OnShown" action="Play"/>
                 
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time="1" value="255" in="Slow"/>
                    <Key type="Curve" time="2" value="0" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <Frame type="Button" name="CampaignSubSelectionButtonTemplate">
        <Style val="StandardButton"/>
        <NormalImage val="NormalImage"/>
        <HoverImage val="HoverImage"/>
        <ClickSound val="@UI_ActionButtonSelect"/>
        <Width val="288"/>
        <Height val="364"/>
        
        <Frame type="Image" name="CampaignIcon">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionIcon_Void"/>
            <AlphaMaskTexture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionButton_Mask"/>
            <Alpha val="255"/>
            <DesaturateOnDisable val="False"/>
            
            <Animation name="FadeIn">
                <Event event="OnMouseEnter" action="Reset" frame="$parent"/>
                <Event event="OnMouseEnter" action="Play" frame="$parent"/>
                <Event event="OnMouseExit" action="Reset" frame="$parent"/>
                 
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" out="Slow"/>
                </Controller>
            </Animation>
            
            <Animation name="Hide">
                <Event event="OnEnabled" action="Reset" frame="$parent"/>
                <Event event="OnDisabled" action="Play" frame="$parent"/>
                 
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="False"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="CampaignIconLocked">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionIcon_Void"/>
            <AlphaMaskTexture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionButton_Mask"/>
            <Alpha val="255"/>
            <Visible val="False"/>
            <DesaturateOnDisable val="False"/>
            
            <Animation name="Show">
                <Event event="OnEnabled" action="Reset" frame="$parent"/>
                <Event event="OnDisabled" action="Play" frame="$parent"/>
                 
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="True"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="Darken">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionIcon_Darken"/>
            <AlphaMaskTexture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionButton_Mask"/>
        </Frame>

        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionButton_Normal"/>
            <TextureType val="Normal"/>
            <Tiled val="false"/>
            <DesaturateOnDisable val="False"/>
            
            <Animation name="Locked">
                <Event event="OnEnabled" action="Reset" frame="$parent"/>
                <Event event="OnDisabled" action="Play" frame="$parent"/>
                 
                <Controller type="Texture" end="Pause">
                    <Key type="Image" time="0" image="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionButton_Locked"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor relative="$parent"/>
            <TextureType val="Normal"/>
            <Tiled val="false"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionButton_Selected"/>
        </Frame>

        <Frame type="Image" name="ButtonLockedIcon">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_SubselectionIcon_Lock"/>
            <Width val="88"/>
            <Height val="88"/>
            <Visible val="False"/>
            <DesaturateOnDisable val="False"/>
        </Frame>
        
        <Frame type="Label" name="Subtitle">
            <Anchor side="Bottom" relative="$parent/Title" pos="Min" offset="1"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="26"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-26"/>
            <Style val="@Campaign_SubviewButtonSubtitle"/>
        </Frame>

        <Frame type="Label" name="Title">
            <Anchor side="Bottom" relative="$parent/ModdableText" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="26"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-26"/>
            <Style val="@Campaign_SubviewButtonTitle"/>
            
            <Animation name="MouseOverStyle">
                <Event event="OnMouseEnter" action="Reset,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="Reset" frame="$parent"/>

                <Controller type="Style" end="Pause" style1="Campaign_SubviewButtonTitleHighlight" style2="Campaign_SubviewButtonTitleHighlight">
                    <Key type="Curve" time="0" value="0"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Label" name="ProgressText">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-22"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="24"/>
            <Style val="@Campaign_SubviewButtonProgress"/>
			<CollapseLayout val="True"/>
        </Frame>
        
        <Frame type="Label" name="ModdableText">
            <Anchor side="Bottom" relative="$parent/ProgressText" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="26"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="-26"/>
            <Style val="@Campaign_SubviewButtonProgress"/>
			<CollapseLayout val="True"/>
        </Frame>

        <!--
        
        
        SUBSELECTION BUTTON STATES
        
        
        -->
        
        <!-- Default -->
        
        <Animation name="Default">
            
            <Controller type="Visibility" name="ProgressText" end="Pause" frame="ProgressText">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" name="ModdableText" end="Pause" frame="ModdableText">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Tooltip" name="Tooltip" end="Pause">
                <Key type="Text" time="0" text=""/>
            </Controller>
            
            <Controller type="Visibility" name="ButtonLockedIcon" end="Pause" frame="ButtonLockedIcon">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Enabled" name="Enabled" end="Pause">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
        </Animation>
        
        <!-- Not Purchased -->
        <!-- Store Offline - treat as not purchased, as Purchase Panel handles the Store Offline state-->
        <Animation name="NotPurchased" template="CampaignPanel/CampaignSubSelectionButtonTemplate/Default">
            <Event event="NotPurchased" action="Reset,Play"/>
            <Event event="NotPurchasedStoreOffline" action="Reset,Play"/>
            
            <Controller type="Text" end="Pause" frame="ModdableText">
                <Key type="Text" time="0" text="@UI/CampaignPanel/PurchaseToPlay"/>
            </Controller>
        </Animation>
        
        <!-- Not Purchased Offline -->
        
        <Animation name="NotPurchasedOffline" template="CampaignPanel/CampaignSubSelectionButtonTemplate/Default">
            <Event event="NotPurchasedOffline" action="Reset,Play"/>
            
            <Controller type="Text" end="Pause" frame="ModdableText">
                <Key type="Text" time="0" text="@UI/CampaignPanel/NotPurchasedOffline"/>
            </Controller>
        </Animation>

        <!-- Not Pre-purchased -->
        
        <Animation name="NotPrePurchased" template="CampaignPanel/CampaignSubSelectionButtonTemplate/Default">
            <Event event="NotPrePurchased" action="Reset,Play"/>
            
            <Controller type="Text" end="Pause" frame="ModdableText">
                <Key type="Text" time="0" text="@UI/CampaignPanel/PrepurchaseNow"/>
            </Controller>
        </Animation>
        
        <!-- Pre Purchased -->
        
        <Animation name="PrePurchased" template="CampaignPanel/CampaignSubSelectionButtonTemplate/Default">
            <Event event="PrePurchased" action="Reset,Play"/>
            
            <Controller type="Text" end="Pause" frame="ModdableText">
                <Key type="Text" time="0" text="@UI/CampaignPanel/ComingSoon"/>
            </Controller>
        </Animation>
        
        <!-- Recommended -->
        
        <Animation name="Recommended" template="CampaignPanel/CampaignSubSelectionButtonTemplate/Default">
            <Event event="Recommended" action="Reset,Play"/>
            
            <Controller type="Text" end="Pause" frame="ModdableText">
                <Key type="Text" time="0" text="@UI/CampaignPanel/Recommended"/>
            </Controller>
        </Animation>
        
        <!-- Unlocked -->
        
        <Animation name="Unlocked" template="CampaignPanel/CampaignSubSelectionButtonTemplate/Default">
            <Event event="Unlocked" action="Reset,Play"/>
            
            <Controller type="Visibility" name="ProgressText" end="Pause" frame="ProgressText">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" name="ModdableText" end="Pause" frame="ModdableText">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- Locked Content -->
        
        <Animation name="LockedContent" template="CampaignPanel/CampaignSubSelectionButtonTemplate/Default">
            <Event event="LockedContent" action="Reset,Play"/>
            
            <Controller type="Visibility" name="ButtonLockedIcon" end="Pause" frame="ButtonLockedIcon">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Text" end="Pause" frame="ModdableText">
                <Key type="Text" time="0" text="@UI/CampaignPanel/Locked"/>
            </Controller>
            
            <Controller type="Tooltip" name="Tooltip" end="Pause">
                <Key type="Text" time="0" text="@UI/LegacyOfTheVoidEpilogueNotAvailableTooltip"/>
            </Controller>
            
            <Controller type="Enabled" name="Enabled" end="Pause">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
    </Frame>
    


    <Frame type="CampaignPanel" name="CampaignPanel">
        <Handle val="CampaignPanel"/>

        <Animation name="CampaignPanelVisibility">
            <Event event="ShowCampaignPanel" action="DirectionReverse,Play"/>
            <Event event="HideCampaignPanel" action="DirectionForward,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="True"/>
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
        </Animation>
        
        
        
        <!--
        
        
        
        SHARED FRAME
        
        This frame is shared between the CampaginView and the SubPageView
        
        
        
        -->

        <Frame type="Frame" name="SharedFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <RenderType val="HDR"/>

            <Frame type="BlurFrame" name="ContentBGBlur">
                <Anchor relative="$parent/ContentBG"/>

                <Animation name="Blur">
                    <Event event="OnShown" action="Reset,Play" frame="$parent" />

                    <Controller type="Blur" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" time="1" value=".35" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Image" name="ContentBG">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="620"/>
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_PanelBg"/>
                
                <Animation name="Entrance">
                    <Event event="OnShown" action="Reset" frame="$parent"/>
                    <Event event="OnShown" action="Play" frame="$parent"/>
             
                    <Controller type="Anchor" end="Pause" side="Right" relative="true">
                        <Key type="Curve" time="0" value="-2000" out="Fast"/>
                        <Key type="Curve" time=".4" value="0" in="Slow"/>
                    </Controller>

                    <Controller type="Sound" end="Stop">
                        <Key type="Sound" time="0" sound="*@C_UI_CampaignPanel_EntranceSound"/>
                    </Controller>

                </Animation>
            </Frame>
            
            <Frame type="Image" name="ContentBGShimmer">
                <Anchor side="Top" relative="$parent/ContentBG" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent/ContentBG" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/ContentBG" pos="Min" offset="0"/>
                <LayerCount val="2"/>
                <Alpha val="255"/>
                <ScaleWidthToTexture val="True"/>
     
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_PanelBg_Mask_Shimmer" layer="0"/>
                <AlphaLayer val="True" layer="0"/>
             
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_PanelBg_Mask" layer="1"/>
                <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
                
             
                <Animation name="ShimmerAnimation">
                    <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                    <Event event="OnShown" action="Play" frame="$parent"/>
             
                    <Controller type="LayerUV" layer="0" side="Left" relative="False" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="0.5" out="linear"/>
                        <Key type="Curve" time="0" value="0.5" out="linear"/>
                        <Key type="Curve" time="9" value="-1" in="linear"/>
                    </Controller>
             
                </Animation>
            </Frame>

            <Frame type="Image" name="BorderTop">
                <Anchor side="Top" relative="$parent/ContentBG" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/ContentBG" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ContentBG" pos="Max" offset="0"/>
                <Height val="2"/>
                <Color val="86,159,200"/>
                <Alpha val="77"/>
            </Frame>

            <Frame type="Image" name="BorderTopShimmer">
                <Anchor side="Top" relative="$parent/BorderTop" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/BorderTop" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_PanelBorderShimmer"/>

                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play" frame="$parent" />

                    <Controller type="Anchor" end="Loop" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-2000" out="Fast"/>
                        <Key type="Curve" time="5" value="2000" in="Slow"/>
                        <Key type="Curve" time="5.05" value="2000" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Image" name="BorderBottom">
                <Anchor side="Bottom" relative="$parent/ContentBG" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/ContentBG" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ContentBG" pos="Max" offset="0"/>
                <Height val="2"/>
                <Color val="86,159,200"/>
                <Alpha val="77"/>
            </Frame>

            <Frame type="Image" name="BorderBottomShimmer">
                <Anchor side="Top" relative="$parent/BorderBottom" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/BorderBottom" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Campaign_PanelBorderShimmer"/>

                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play" frame="$parent" />

                    <Controller type="Anchor" end="Loop" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-2000" out="Fast"/>
                        <Key type="Curve" time="0.05" value="-2000" in="Fast"/>
                        <Key type="Curve" time="5.05" value="2000" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Button" name="BackButton">
                <Handle val="CampaignPanel_BackButton"/>
                <Anchor side="Bottom" relative="$parent/ContentBG" pos="Min" offset="-15"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="100"/>
                <Anchor side="Right" relative="$this/Label" pos="Max" offset="0"/>
                <Height val="50"/>
                <CollapseLayout val="true"/>
                <ClickSound val="@UI_ActionButtonSelect"/>
                <!-- <HoverSound val="@AC_UI_MouseOver01"/> -->
                <RenderType val="LDR"/>
                <Shortcut val="@UI/ScreenNavigationBack_Hotkey" priority="50"/>
                <DisplayType val="ImageLayer"/>
                
                <Frame type="Image" name="NormalImage">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <StateCount val="1"/>
                    <LayerCount val="4"/>
                    <Texture val="@UI/Glues_Battlenet_SquareIconButton_Normal" layer="0"/>
                    <Texture val="@UI/Glues_Battlenet_SquareIconButton_Down" layer="1"/>
                    <Texture val="@UI/Glues_Battlenet_SquareIconButton_Down" layer="2"/>
                    <Texture val="@UI/Glues_Battlenet_SquareIconButton_Disabled" layer="3"/>
                    <TextureType val="Normal"/>
                    <Tiled val="false"/>
                    <DesaturateOnDisable val="false"/>
                </Frame>

                <Frame type="Image" name="HoverImage">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <StateCount val="1"/>
                    <LayerCount val="4"/>
                    <Texture val="@UI/Glues_Battlenet_SquareIconButton_Hover" layer="0"/>
                    <Texture val="@UI/Glues_Battlenet_SquareIconButton_Down" layer="1"/>
                    <Texture val="@UI/Glues_Battlenet_SquareIconButton_Down" layer="2"/>
                    <Texture val="@UI/Glues_Battlenet_SquareIconButton_Disabled" layer="3"/>
                    <TextureType val="Normal"/>
                    <Tiled val="false"/>
                    <DesaturateOnDisable val="false"/>
                </Frame>
                
                <!--
                        
                <Frame type="Image" name="NormalImage">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Texture val="Assets/Textures/UI_Glues_BlueButtons_SmallSquareButtonUp.dds"/>
                    <AcceptsMouse val="False"/>
                    <RenderType val="LDR"/>
                </Frame>
                
                -->

                <Frame type="Image" name="ArrowIcon">
                    <Anchor side="Top" relative="$parent/NormalImage" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent/NormalImage" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent/NormalImage" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent/NormalImage" pos="Mid" offset="0"/>
                    <Texture val="Assets/Textures/UI_Glues_BlueButtons_PageArrow.dds"/>
                    <AcceptsMouse val="False"/>
                    <RenderType val="LDR"/>
                </Frame>

                <Frame type="Label" name="Label">
                    <Anchor side="Top" relative="$parent/NormalImage" pos="Mid" offset="1"/>
                    <Anchor side="Bottom" relative="$parent/NormalImage" pos="Mid" offset="1"/>
                    <Anchor side="Left" relative="$parent/NormalImage" pos="Max" offset="10"/>
                    <Style val="@Campaign_BackButton"/>
                    <Text val="@UI/CampaignPanel/BacktoCampaignSelection"/>
                    <AcceptsMouse val="False"/>
                    <RenderType val="LDR"/>
                </Frame>
                
                <Frame type="Frame" name="MouseDetectionFrame">
                    <Anchor side="Top" relative="$parent/NormalImage" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent/NormalImage" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent/NormalImage" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/Label" pos="Max" offset="0"/>
                    <RenderType val="LDR"/>
                    <!--
                    <Animation name="MouseOver">
                        <Event event="OnMouseEnter" action="Reset"/>
                        <Event event="OnMouseEnter" action="Play"/>
                         
                        <Controller type="Texture" end="Pause" frame="$parent/NormalImage">
                            <Key type="Image" time="0" image="Assets/Textures/UI_Glues_BlueButtons_SmallSquareButtonOver.dds"/>
                        </Controller>
                    </Animation> -->
                </Frame>

                <Animation name="ContextSensitiveBackToVoid">
                    <Event event="ShowContextBackToCampaign" action="Reset" frame="$parent/$parent" />
                    <Event event="ShowContextBackToVoid" action="Reset,Play" frame="$parent/$parent" />

                    <Controller type="Text" end="Pause" frame="$this/Label">
                        <Key type="Text" time="0" text="@UI/CampaignPanel/BacktoVoid"/>
                    </Controller>    
                </Animation>
            </Frame>
        </Frame>
        
        <!--
        
        
        
        CAMPAIGN VIEW PANEL
        
        This panel shows the actual individual campaign screens and purchase flows for those campaigns.
        
        
        
        -->

        <Frame type="Frame" name="CampaignView">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Label" name="CampaignTitleLabel">
                <Anchor side="Top" relative="$parent/$parent/SharedFrame/ContentBG" pos="Min" offset="70"/>
                <Anchor side="Left" relative="$parent/$parent/SharedFrame/ContentBG" pos="Min" offset="119"/>
                <Style val="@Campaign_Title"/>

                <Animation name="Hide">
                    <Event event="OnHidden" action="Reset,Play" frame="$parent/$parent/CampaignView" />

                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" out="Fast"/>
                    </Controller>
                </Animation>

                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play" frame="$parent/$parent/CampaignView" />

                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" out="Fast"/>
                        <Key type="Curve" time="0.3" value="0" out="Fast"/>
                        <Key type="Curve" time="0.7" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Frame" name="MapPreviewIcon" template="CampaignPurchasePanel/CampaignPreviewIconTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="411"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="70"/>
                <Visible val="False"/>

                <Animation name="MapPreviewVisibility">
                    <Event event="HasCampaignProgress" action="Reset,Play" frame="$parent/$parent"/>
                    <Event event="NoCampaignProgress" action="Reset" frame="$parent/$parent"/>

                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="True"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Frame" name="StorySoFarPreviewIcon" template="CampaignPurchasePanel/CampaignPreviewIconTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="411"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="70"/>
                <Visible val="False"/>

                <Animation name="StorySoFarVisibility">
                    <Event event="HasCampaignProgress" action="Reset" frame="$parent/$parent"/>
                    <Event event="NoCampaignProgress" action="Reset,Play" frame="$parent/$parent"/>

                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="True"/>
                    </Controller>
                </Animation>

                <Frame type="Frame" name="StorySoFarContainer">
                    <Visible val="True"/>
                </Frame>

                <Frame type="Button" name="PlayButton">
                    <Visible val="True"/>
                </Frame>
            </Frame>

