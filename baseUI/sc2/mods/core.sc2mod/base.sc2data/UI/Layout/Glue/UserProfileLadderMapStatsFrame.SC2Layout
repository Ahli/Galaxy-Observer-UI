            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/WinsLabel" pos="Max" offset="0"/>
            <Width val="200"/>
            <Text val="@UI/ScreenUserProfile/LadderStats/Losses"/>
