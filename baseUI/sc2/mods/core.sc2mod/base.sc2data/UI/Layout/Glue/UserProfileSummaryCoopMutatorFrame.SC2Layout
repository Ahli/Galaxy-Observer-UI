<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="UserProfileSummaryCoopMutatorFrame" name="UserProfileSummaryCoopMutatorFrame">
        <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-12"/>
        <Height val="30"/>
        <PresenceId val="{$ScreenUserProfile/@PresenceId}"/>

        <Frame type="Label" name="WinsTitleLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Text val="{$parent/FormatFrame/@Text}"/>
        </Frame>

        <Frame type="Label" name="WinsLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$parent/@Wins}"/>
        </Frame>
    </Frame>

    <Frame type="Frame" name="UserProfileSummaryCoopMutatorContainerFrame">
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_TableLargeHeaderBg"/>
            <TextureType val="NineSlice"/>
            <Tiled val="True"/>
            <Visible val="true"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Text val="@UI/ScreenUserProfile/SummaryCoop/MutationWins"/>
            <Style val="Tournament_TrophyCaseTitle"/>
        </Frame>
        
        <Frame type="UserProfileSummaryCoopMutatorFrame" name="CasualFrame" template="UserProfileSummaryCoopMutatorFrame/UserProfileSummaryCoopMutatorFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="80"/>
            <Difficulty val="1"/>

            <Frame type="Label" name="WinsTitleLabel">
                <Text val="@CommanderDifficultyLevel/Name/Casual"/>
            </Frame>
        </Frame>

        <Frame type="UserProfileSummaryCoopMutatorFrame" name="NormalFrame" template="UserProfileSummaryCoopMutatorFrame/UserProfileSummaryCoopMutatorFrame">
            <Anchor side="Top" relative="$parent/CasualFrame" pos="Max" offset="0"/>
            <Difficulty val="2"/>

            <Frame type="Label" name="WinsTitleLabel">
                <Text val="@CommanderDifficultyLevel/Name/Normal"/>
            </Frame>
        </Frame>

        <Frame type="UserProfileSummaryCoopMutatorFrame" name="HardFrame" template="UserProfileSummaryCoopMutatorFrame/UserProfileSummaryCoopMutatorFrame">
            <Anchor side="Top" relative="$parent/NormalFrame" pos="Max" offset="0"/>
            <Difficulty val="3"/>

            <Frame type="Label" name="WinsTitleLabel">
                <Text val="@CommanderDifficultyLevel/Name/Hard"/>
            </Frame>
        </Frame>

        <Frame type="UserProfileSummaryCoopMutatorFrame" name="BrutalFrame" template="UserProfileSummaryCoopMutatorFrame/UserProfileSummaryCoopMutatorFrame">
            <Anchor side="Top" relative="$parent/HardFrame" pos="Max" offset="0"/>
            <Difficulty val="4"/>

            <Frame type="Label" name="WinsTitleLabel">
                <Text val="@CommanderDifficultyLevel/Name/Brutal"/>
            </Frame>
        </Frame>

        <Frame type="UserProfileSummaryCoopMutatorFrame" name="AllFrame" template="UserProfileSummaryCoopMutatorFrame/UserProfileSummaryCoopMutatorFrame">
            <Anchor side="Top" relative="$parent/BrutalFrame" pos="Max" offset="0"/>

            <Frame type="Label" name="WinsTitleLabel">
                <Text val="@UI/ScreenUserProfile/SummaryCoop/MutatorAll"/>
            </Frame>
        </Frame>
    </Frame>
</Desc>
