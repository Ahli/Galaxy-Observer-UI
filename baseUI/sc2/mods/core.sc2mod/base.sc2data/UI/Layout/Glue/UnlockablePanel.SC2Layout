        </Frame>

        <Frame type="Label" name="AchievementTitleLabel">
            <Anchor side="Top" relative="$parent/UnlockTitleLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/UnlockableNameLabel" pos="Min" offset="0"/>
