                </State>

                <State name="UnequippedState">
                    <When type="AnimationState" frame="$parent/RewardCacheFrame" SelectedState="NotSelected"/>
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
