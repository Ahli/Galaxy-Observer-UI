                <State name="HasParentBundle">
                    <When type="Property" frame="$parent/RewardCacheFrame" HasParentBundle="true"/>
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>

                <State name="AchievementLockedState">
                    <When type="AnimationState" frame="$parent/AchievementInfoFrame" AchievementInfoFrame="Active"/>
                    <When type="Property" frame="$parent/AchievementInfoFrame" Awarded="False"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>

                <State name="NotAchievementLockedState">
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
