        <Frame type="Frame" name="StateLabelContainer">
            <Anchor side="Top" relative="$parent/IconImage" pos="Mid" offset="2"/>
            <Anchor side="Bottom" relative="$this/StateLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/NameLabel" pos="Min" offset="1"/>
            <Anchor side="Right" relative="$parent/NameLabel" pos="Max" offset="0"/>
            
            <Frame type="Label" name="StateLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Options val="Ellipsis | NoWrapping | TooltipIfTruncated"/>
                <Style val="@Collection_ListItem_Desc"/>
                
                <StateGroup name="TextStates">
                    <DefaultState val="NormalState"/>
                
                    <State name="ToggledState">
                        <When type="Property" frame="$parent/$parent/SelectedImage" visible="True"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Selected"/>
                    </State>
                    
                    <State name="HoverState">
                        <When type="Property" frame="$parent/$parent/HoverImage" visible="True"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Selected"/>
                    </State>
                    
                    <State name="LockedState">
                        <When type="StateGroup" frame="$parent/$parent" GameContentOwnedStateGroup="GameContentLocked"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Locked"/>
                    </State>

                    <State name="NormalState">
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc"/>
                    </State>
                </StateGroup>
            </Frame>
            
            <Frame type="Label" name="StateLabelEquipped">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Options val="Ellipsis | NoWrapping | TooltipIfTruncated"/>
                <Style val="@Collection_ListItem_DescEquipped"/>
                <Text val="{$parent/StateLabel/@Text}"/>
                
                <StateGroup name="TextStates">
                    <DefaultState val="NormalState"/>
                
                    <State name="ToggledState">
                        <When type="Property" frame="$parent/$parent/SelectedImage" visible="True"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_DescEquipped_Selected"/>
                    </State>
                    
                    <State name="HoverState">
                        <When type="Property" frame="$parent/$parent/HoverImage" visible="True"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_DescEquipped_Selected"/>
                    </State>
                    
                    <State name="LockedState">
                        <When type="StateGroup" frame="$parent/$parent" GameContentOwnedStateGroup="GameContentLocked"/>
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_DescEquipped_Locked"/>
                    </State>

                    <State name="NormalState">
                        <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_DescEquipped"/>
                    </State>
                </StateGroup>
            </Frame>
        </Frame>

        <Frame type="Image" name="CheckMarkImage">
