        <StateGroup name="GameContentOwnedStateGroup">
            <DefaultState val="GameContentNotOwned"/>

            <State name="FirstRewardSelected">
                <When type="AnimationState" frame="FirstUnlockedRewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Equipped"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="SecondRewardSelected">
                <When type="AnimationState" frame="SecondUnlockedRewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Equipped"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="ThirdRewardSelected">
                <When type="AnimationState" frame="ThirdUnlockedRewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Equipped"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="GameContentPurchased">
                <When type="AnimationState" frame="$this/GameContentFrame" PurchasedState="Purchased"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Owned"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="GameContentDefault">
                <When type="Property" frame="$this/VoicePackFrame" Default="true"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Default"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="GameContentAllowed">
                <When type="Property" frame="$this/GameContentFrame" IsAllowed="true"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Earned"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="GameContentLocked">
                <When type="Property" frame="$this/GameContentFrame" CanPurchase="false"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Locked"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="True"/>
            </State>

            <State name="GameContentNotOwnedPurchaseHidden">
                <When type="Property" frame="$this/VoicePackFrame" IsPurchaseHidden="true"/>
                <Action type="SetProperty" frame="StateLabelContainer" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" text="@UI/ScreenCollection/Locked"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="True"/>
            </State>

            <State name="GameContentNotOwned">
                <Action type="SetProperty" frame="StateLabelContainer" visible="false"/>
                <Action type="SetProperty" frame="CostLabel" visible="true"/>
                <Action type="SetProperty" frame="LockedIcon" visible="True"/>
            </State>
        </StateGroup>

        <StateGroup name="VoicePackRewardSelectedStateGroup">
            <DefaultState val="VoicePackRewardNotSelected"/>

            <State name="FirstRewardSelected">
                <When type="AnimationState" frame="FirstUnlockedRewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="CheckMarkImage" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabelEquipped" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" visible="false"/>
            </State>

            <State name="SecondRewardSelected">
                <When type="AnimationState" frame="SecondUnlockedRewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="CheckMarkImage" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabelEquipped" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" visible="false"/>
            </State>

            <State name="ThirdRewardSelected">
                <When type="AnimationState" frame="ThirdUnlockedRewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="CheckMarkImage" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabelEquipped" visible="true"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" visible="false"/>
            </State>

            <State name="VoicePackRewardNotSelected">
                <Action type="SetProperty" frame="CheckMarkImage" visible="false"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabelEquipped" visible="false"/>
                <Action type="SetProperty" frame="StateLabelContainer/StateLabel" visible="true"/>
            </State>
        </StateGroup>
    </Frame>

