<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Frame type="Frame" name="LoadingPlayerCardTemplate">
        <Width val="456"/>
        <Height val="178"/>
        <CollapseLayout val="true"/>
        
        <Frame type="Image" name="Banner">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Width val="448"/>
            <Height val="116"/>
        </Frame>
        
        <Frame type="Image" name="DetailsBackground">
            <Anchor side="Top" relative="$parent" pos="Min" offset="108"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Width val="448"/>
            <Height val="68"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_DetailsBackground"/>
        </Frame>
        
        <Frame type="Image" name="ColorIndicator">
            <Anchor side="Bottom" relative="$parent/DetailsBackground" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/DetailsBackground" pos="Max" offset="0"/>
            <Width val="28"/>
            <Height val="68"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_ColorIndicator"/>
        </Frame>
        
        <Frame type="Image" name="ColorBorder">
            <Anchor side="Bottom" relative="$parent/DetailsBackground" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/DetailsBackground" pos="Max" offset="0"/>
            <Width val="28"/>
            <Height val="68"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_ColorBorder"/>
        </Frame>
        
        <Frame type="Image" name="RaceIndicator">
            <Anchor side="Bottom" relative="$parent/DetailsBackground" pos="Max" offset="-6"/>
            <Anchor side="Right" relative="$parent/DetailsBackground" pos="Max" offset="-17"/>
            <Width val="48"/>
            <Height val="52"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_RaceIndicator_Zerg"/>
            <Color val="134,151,191"/>
        </Frame>
        
        <Frame type="Label" name="PlayerName">
            <Anchor side="Bottom" relative="$parent/DetailsBackground" pos="Max" offset="-26"/>
            <Anchor side="Left" relative="$parent/DetailsBackground" pos="Min" offset="91"/>
            <Style val="LoadingPlayerCardName"/>
            <Text val="{$parent/FormatPlayerName/@Text}"/>
        </Frame>
        
        <Frame type="TextFormatFrame" name="FormatPlayerName">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <FormatText val="%1%"/>
        </Frame>
        
        <Frame type="Label" name="TeamName">
            <Anchor side="Top" relative="$parent/PlayerName" pos="Max" offset="-2"/>
            <Anchor side="Left" relative="$parent/PlayerName" pos="Min" offset="0"/>
            <Style val="LoadingPlayerCardTeam"/>
            <Text val="{$parent/TeamMathFrame/FormatTeam/@Text}"/>
        </Frame>
        
        <Frame type="MathFrame" name="TeamMathFrame">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <A val="0"/>
            <B val="1"/>
            <Operator val="Add"/>
        
            <Frame type="TextFormatFrame" name="FormatTeam">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
                <FormatText val="@UI/LoadingPlayerCard/TeamName"/>
                <Replacement index="0" val="{$parent/@ResultInt}"/>
            </Frame>
        </Frame>
        
        <Frame type="ProgressBar" name="LoadingProgressBar">
            <Anchor side="Top" relative="$parent/DetailsBackground" pos="Min" offset="2"/>
            <Anchor side="Right" relative="$parent/DetailsBackground" pos="Max" offset="0"/>
            <Width val="368"/>
            <Height val="6"/>
            <MaxValue val="99"/>
            <UseFillContainer val="True"/>
            
            <Frame type="Frame" name="FillImageContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                
                <Frame type="Image" name="FillImage">
                    <Anchor relative="$parent/$parent"/>
                    <Texture val="Assets/Textures/white.dds"/>
                    <Color val="105,132,189"/>
                </Frame>
                
                <Frame type="Image" name="LeadingGradient">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Width val="112"/>
                    <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_LoadingBar_TipGradient"/>
                </Frame>
            </Frame>
        </Frame>
                
        <Frame type="Image" name="LoadingProgressBarLeadingGlow">
            <Anchor side="Top" relative="$parent/LoadingProgressBar/FillImageContainer" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/LoadingProgressBar/FillImageContainer" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LoadingProgressBar/FillImageContainer" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/LoadingProgressBar/FillImageContainer" pos="Max" offset="0"/>
            <Width val="54"/>
            <Height val="16"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_LoadingBar_TipGlow"/>
            <Alpha val="63"/>
        </Frame>
        
        <Frame type="BattleClubFrame" name="ClanIconContainer">
            <Anchor side="Bottom" relative="$parent/DetailsBackground" pos="Max" offset="-7"/>
            <Anchor side="Right" relative="$parent/DetailsBackground" pos="Max" offset="-73"/>
            <Width val="48"/>
            <Height val="48"/>
            
            <Frame type="Image" name="ClanIcon">
                <Anchor relative="$parent"/>
                <LayerCount val="2"/>
                
                <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_ClanIconMask" layer="0"/>
                <AlphaLayer val="true" layer="0"/>
                <AlphaChannel val="Alpha" layer="0"/>
                
                <Texture val="{$parent/@Decal}" layer="1"/>
            </Frame>
        </Frame>
            
        <!-- BORDER OUTER -->
        <Frame type="Image" name="BorderOuter">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Outer"/>
            <TextureType val="NineSlice"/>
            <Color val="0,0,0"/>
            <RenderPriority val="513"/>
        </Frame>
        
        <!-- BORDER INNER -->
        <Frame type="Image" name="BorderInner">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Inner"/>
            <TextureType val="NineSlice"/>
            <Color val="72,91,130"/>
            <RenderPriority val="513"/>
        </Frame>
        
        <Frame type="Label" name="RaceAILabel">
            <Anchor relative="$parent"/>
            <Enabled val="false"/>
            <Visible val="false"/>
        </Frame>
        
        <Frame type="Image" name="ComputerAvatarImage">
            <Anchor relative="$parent/AvatarFrame/PortraitFrame/UnlockableImage"/>
            <Texture val="Assets/Textures/ai_avatar_loading.dds"/>
            <Visible val="false"/>
            <RenderPriority val="513"/>
        </Frame>

        <Frame type="AvatarFrame" name="AvatarFrame" template="StandardTemplates/AvatarFrameMediumTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="84"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="-18"/>
            <Unclipped val="true"/>
            <RenderPriority val="515"/>
        </Frame>
        
        <StateGroup name="VisibilityState">
            <DefaultState val="Visible"/>
            
            <State name="NotVisible">
                <When type="Property" frame="RaceAILabel" Enabled="false"/>
                <When type="Property" frame="PlayerName" Text=""/>
                <Action type="SetProperty" Visible="false"/>
            </State>
            
            <State name="Visible">
                <Action type="SetProperty" Visible="true"/>
            </State>
        </StateGroup>
        
        <StateGroup name="ClanState">
            <DefaultState val="NoClan"/>
            
            <State name="HasClan">
                <When type="Property" frame="ClanIconContainer" IsClan="true"/>
                <Action type="SetProperty" frame="FormatPlayerName" FormatText="&lt;%0%&gt; %1%"/>
            </State>
            
            <State name="NoClan"/>
        </StateGroup>
        
        <StateGroup name="LoadingState">
            <DefaultState val="Finished"/>
            
            <State name="NotStarted">
                <When type="Property" frame="LoadingProgressBar" ProgressPercent="0"/>
                <Action type="SetProperty" frame="LoadingProgressBar/FillImageContainer/LeadingGradient" Visible="false"/>
                <Action type="SetProperty" frame="LoadingProgressBarLeadingGlow" Visible="false"/>
                <Action type="SetProperty" frame="LoadingProgressBar/FillImageContainer/FillImage" Color="105,132,189"/>
            </State>
            
            <State name="InProgress">
                <When type="Property" frame="LoadingProgressBar" operator="LessThan" ProgressPercent="1"/>
                <When type="Property" frame="LoadingProgressBar" Visible="true"/>
                <Action type="SetProperty" frame="LoadingProgressBar/FillImageContainer/LeadingGradient" Visible="true"/>
                <Action type="SetProperty" frame="LoadingProgressBarLeadingGlow" Visible="true"/>
                <Action type="SetProperty" frame="LoadingProgressBar/FillImageContainer/FillImage" Color="105,132,189"/>
            </State>
            
            <State name="Finished">
                <Action type="SetProperty" frame="LoadingProgressBar/FillImageContainer/LeadingGradient" Visible="false"/>
                <Action type="SetProperty" frame="LoadingProgressBarLeadingGlow" Visible="false"/>
                <Action type="SetProperty" frame="LoadingProgressBar/FillImageContainer/FillImage" Color="72,91,130"/>
            </State>
        </StateGroup>
        
        <StateGroup name="AIRaceState">
            <DefaultState val="NotAI"/>
            
            <State name="TerranAI">
                <When type="Property" frame="RaceAILabel" Enabled="true"/>
                <When type="Property" frame="RaceAILabel" Text="Terr"/>
                <Action type="SetProperty" frame="ComputerAvatarImage" Visible="true"/>
                <Action type="SetProperty" frame="Banner" Texture="Assets\Textures\RaceBanner_Terran_Default_1v1.dds"/>
                <Action type="SetProperty" frame="PlayerName" Text="@UI/LoadingPlayerCard/ComputerPlayerName"/>
                <Action type="SetProperty" frame="LoadingProgressBar" Value="{$this/@MaxValue}"/>
            </State>
            
            <State name="ZergAI">
                <When type="Property" frame="RaceAILabel" Enabled="true"/>
                <When type="Property" frame="RaceAILabel" Text="Zerg"/>
                <Action type="SetProperty" frame="ComputerAvatarImage" Visible="true"/>
                <Action type="SetProperty" frame="Banner" Texture="Assets\Textures\RaceBanner_Zerg_Default_1v1.dds"/>
                <Action type="SetProperty" frame="PlayerName" Text="@UI/LoadingPlayerCard/ComputerPlayerName"/>
                <Action type="SetProperty" frame="LoadingProgressBar" Value="{$this/@MaxValue}"/>
            </State>
            
            <State name="ProtossAI">
                <When type="Property" frame="RaceAILabel" Enabled="true"/>
                <When type="Property" frame="RaceAILabel" Text="Prot"/>
                <Action type="SetProperty" frame="ComputerAvatarImage" Visible="true"/>
                <Action type="SetProperty" frame="Banner" Texture="Assets\Textures\RaceBanner_Protoss_Default_1v1.dds"/>
                <Action type="SetProperty" frame="PlayerName" Text="@UI/LoadingPlayerCard/ComputerPlayerName"/>
                <Action type="SetProperty" frame="LoadingProgressBar" Value="{$this/@MaxValue}"/>
            </State>
            
            <State name="RandomAI">
                <When type="Property" frame="RaceAILabel" Enabled="true"/>
                <Action type="SetProperty" frame="ComputerAvatarImage" Visible="true"/>
                <Action type="SetProperty" frame="Banner" Texture="Assets\Textures\RaceBanner_Random_Default_1v1.dds"/>
                <Action type="SetProperty" frame="PlayerName" Text="@UI/LoadingPlayerCard/ComputerPlayerName"/>
                <Action type="SetProperty" frame="LoadingProgressBar" Value="{$this/@MaxValue}"/>
            </State>
            
            <State name="NotAI"/>
        </StateGroup>
        
        <StateGroup name="RaceState">
            <DefaultState val="Random"/>
            
            <State name="Terran">
                <When type="Property" frame="RaceAILabel" Text="Terr"/>
                <Action type="SetProperty" frame="RaceIndicator" Texture="@UI/SC2_UI_LoadingScreen_PlayerCard_RaceIndicator_Terran"/>
            </State>
            
            <State name="Zerg">
                <When type="Property" frame="RaceAILabel" Text="Zerg"/>
                <Action type="SetProperty" frame="RaceIndicator" Texture="@UI/SC2_UI_LoadingScreen_PlayerCard_RaceIndicator_Zerg"/>
            </State>
            
            <State name="Protoss">
                <When type="Property" frame="RaceAILabel" Text="Prot"/>
                <Action type="SetProperty" frame="RaceIndicator" Texture="@UI/SC2_UI_LoadingScreen_PlayerCard_RaceIndicator_Protoss"/>
            </State>
            
            <State name="Random">
                <Action type="SetProperty" frame="RaceIndicator" Texture="@UI/SC2_UI_LoadingScreen_PlayerCard_RaceIndicator_Random"/>
            </State>
        </StateGroup>
        
        <StateGroup name="ClanDecalState">
            <DefaultState val="NoClanDecal"/>
            
            <State name="HasClanDecal">
                <When type="Property" frame="ClanIconContainer" operator="NotEqual" Decal=""/>
                <Action type="SetProperty" frame="ClanIconContainer" Visible="true"/>
            </State>
            
            <State name="NoClanDecal">
                <Action type="SetProperty" frame="ClanIconContainer" Visible="false"/>
            </State>
        </StateGroup>
    </Frame>
    
    <!-- Reverse -->
    <Frame type="Frame" name="LoadingPlayerCardReverseTemplate" template="LoadingPlayerCardTemplates/LoadingPlayerCardTemplate">
        
        <Frame type="Image" name="Banner">
            <TextureCoords top="0" bottom="1" left="1" right="0"/>
        </Frame>
        
        <Frame type="Image" name="DetailsBackground">
            <TextureCoords top="0" bottom="1" left="1" right="0"/>
        </Frame>
        
        <Frame type="Image" name="ColorIndicator">
            <Anchor side="Left" relative="$parent/DetailsBackground" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$this" pos="Min" offset="0"/>
            <TextureCoords top="0" bottom="1" left="1" right="0"/>
        </Frame>
        
        <Frame type="Image" name="ColorBorder">
            <Anchor side="Left" relative="$parent/DetailsBackground" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$this" pos="Min" offset="0"/>
            <TextureCoords top="0" bottom="1" left="1" right="0"/>
        </Frame>
        
        <Frame type="Image" name="RaceIndicator">
            <Anchor side="Left" relative="$parent/DetailsBackground" pos="Min" offset="17"/>
            <Anchor side="Right" relative="$this" pos="Min" offset="0"/>
        </Frame>
        
        <Frame type="Label" name="PlayerName">
            <Anchor side="Left" relative="$this" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/DetailsBackground" pos="Max" offset="-91"/>
        </Frame>
        
        <Frame type="Label" name="TeamName">
            <Anchor side="Left" relative="$this" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/PlayerName" pos="Max" offset="0"/>
        </Frame>
        
        <Frame type="ProgressBar" name="LoadingProgressBar">
            <Anchor side="Left" relative="$parent/DetailsBackground" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$this" pos="Min" offset="0"/>
        </Frame>
        
        <Frame type="BattleClubFrame" name="ClanIconContainer">
            <Anchor side="Left" relative="$parent/DetailsBackground" pos="Min" offset="73"/>
            <Anchor side="Right" relative="$this" pos="Min" offset="0"/>
            
            <Frame type="Image" name="ClanIcon">
                <TextureCoords top="0" bottom="1" left="1" right="0" layer="0"/>
            </Frame>
        </Frame>

        <Frame type="AvatarFrame" name="AvatarFrame" template="StandardTemplates/AvatarFrameMediumTemplate">
            <Anchor side="Left" relative="$this" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="18"/>
        </Frame>
    </Frame>
    
    <!--
    ==========================
    Loading Tandem Player Card Templates
    ==========================
    -->
    
    <Frame type="Frame" name="LoadingTandemPlayerCardTemplate" template="LoadingPlayerCardTemplates/LoadingPlayerCardTemplate">
        <Height val="350"/>

        <Frame type="AvatarFrame" name="AvatarFrame" template="StandardTemplates/AvatarFrameMediumTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="68"/>
        </Frame>
        
        <Frame type="Label" name="PlayerName">
            <Anchor side="Bottom" relative="$parent/DetailsBackground" pos="Max" offset="-24"/>
        </Frame>
        
        <Frame type="Label" name="TeamName">
            <Anchor side="Top" relative="$parent/TandemPlate" pos="Mid" offset="1"/>
            <Anchor side="Bottom" relative="$parent/TandemPlate" pos="Mid" offset="1"/>
            <Style val="LoadingPlayerCardTeamTandem"/>
            <RenderPriority val="514"/>
        </Frame>
        
        <Frame type="Image" name="TandemBanner">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Width val="448"/>
            <Height val="116"/>
        </Frame>
        
        <Frame type="Image" name="TandemDetailsBackground">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-108"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Width val="448"/>
            <Height val="68"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_DetailsBackground"/>
            <TextureCoords top="1" bottom="0" left="0" right="1"/>
        </Frame>
        
        <Frame type="Image" name="TandemColorIndicator">
            <Anchor side="Top" relative="$parent/TandemDetailsBackground" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/TandemDetailsBackground" pos="Max" offset="0"/>
            <Width val="28"/>
            <Height val="68"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_ColorIndicator"/>
            <TextureCoords top="1" bottom="0" left="0" right="1"/>
            <Color val="{$parent/ColorIndicator/@Color}"/>
        </Frame>
        
        <Frame type="Image" name="TandemColorBorder">
            <Anchor side="Top" relative="$parent/TandemDetailsBackground" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/TandemDetailsBackground" pos="Max" offset="0"/>
            <Width val="28"/>
            <Height val="68"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_ColorBorder"/>
            <TextureCoords top="1" bottom="0" left="0" right="1"/>
        </Frame>
        
        <Frame type="Label" name="TandemPlayerName">
            <Anchor side="Top" relative="$parent/TandemDetailsBackground" pos="Min" offset="25"/>
            <Anchor side="Left" relative="$parent/TandemDetailsBackground" pos="Min" offset="91"/>
            <Style val="LoadingPlayerCardName"/>
            <Text val="{$parent/TandemFormatPlayerName/@Text}"/>
        </Frame>
        
        <Frame type="TextFormatFrame" name="TandemFormatPlayerName">
            <Anchor relative="$parent"/>
            <Visible val="false"/> 
            <FormatText val="%1%"/>
        </Frame>
        
        <Frame type="ProgressBar" name="TandemLoadingProgressBar">
            <Anchor side="Bottom" relative="$parent/TandemDetailsBackground" pos="Max" offset="-1"/>
            <Anchor side="Right" relative="$parent/TandemDetailsBackground" pos="Max" offset="0"/>
            <Width val="368"/>
            <Height val="8"/>
            <MaxValue val="99"/>
            <UseFillContainer val="True"/>
            
            <Frame type="Frame" name="FillImageContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                
                <Frame type="Image" name="FillImage">
                    <Anchor relative="$parent/$parent"/>
                    <Texture val="Assets/Textures/white.dds"/>
                    <Color val="105,132,189"/>
                </Frame>
                
                <Frame type="Image" name="LeadingGradient">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Width val="112"/>
                    <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_LoadingBar_TipGradient"/>
                </Frame>
            </Frame>
        </Frame>
                
        <Frame type="Image" name="TandemLoadingProgressBarLeadingGlow">
            <Anchor side="Top" relative="$parent/TandemLoadingProgressBar/FillImageContainer" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/TandemLoadingProgressBar/FillImageContainer" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/TandemLoadingProgressBar/FillImageContainer" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/TandemLoadingProgressBar/FillImageContainer" pos="Max" offset="0"/>
            <Width val="54"/>
            <Height val="16"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_LoadingBar_TipGlow"/>
        </Frame>
        
        <Frame type="BattleClubFrame" name="TandemClanIconContainer">
            <Anchor side="Top" relative="$parent/TandemDetailsBackground" pos="Min" offset="7"/>
            <Anchor side="Right" relative="$parent/TandemDetailsBackground" pos="Max" offset="-73"/>
            <Width val="48"/>
            <Height val="48"/>
            
            <Frame type="Image" name="ClanIcon">
                <Anchor relative="$parent"/>
                <LayerCount val="2"/>
                
                <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_ClanIconMask" layer="0"/>
                <AlphaLayer val="true" layer="0"/>
                <AlphaChannel val="Alpha" layer="0"/>
                <TextureCoords top="1" bottom="0" left="0" right="1" layer="0"/>
                
                <Texture val="{$parent/@Decal}" layer="1"/>
            </Frame>
        </Frame>
        
        <Frame type="Image" name="TandemPlate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="158"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="34"/>
            <Texture val="@UI/SC2_UI_LoadingScreen_PlayerCard_TandemPlate"/>
            <RenderPriority val="513"/>
        </Frame>
        
        <Frame type="Image" name="TandemComputerAvatarImage">
            <Anchor relative="$parent/TandemAvatarFrame/PortraitFrame/UnlockableImage"/>
            <Texture val="Assets/Textures/ai_avatar_loading.dds"/>
            <Visible val="false"/>
            <RenderPriority val="513"/>
        </Frame>

        <Frame type="AvatarFrame" name="TandemAvatarFrame" template="StandardTemplates/AvatarFrameMediumTemplate">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-64"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="-18"/>
            <Unclipped val="true"/>
            <RenderPriority val="515"/>
        </Frame>
        
        <Frame type="Label" name="TandemRaceAILabel">
            <Anchor relative="$parent"/>
            <Enabled val="false"/>
            <Visible val="false"/>
        </Frame>
        
        <StateGroup name="TandemAIRaceState">
            <DefaultState val="NotAI"/>
            
            <State name="TerranAI">
                <When type="Property" frame="TandemRaceAILabel" Enabled="true"/>
                <When type="Property" frame="TandemRaceAILabel" Text="Terr"/>
                <Action type="SetProperty" frame="TandemComputerAvatarImage" Visible="true"/>
                <Action type="SetProperty" frame="TandemBanner" Texture="Assets\Textures\RaceBanner_Terran_Default_1v1.dds"/>
                <Action type="SetProperty" frame="TandemPlayerName" Text="@UI/LoadingPlayerCard/ComputerPlayerName"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar" Value="{$this/@MaxValue}"/>
            </State>
            
            <State name="ZergAI">
                <When type="Property" frame="TandemRaceAILabel" Enabled="true"/>
                <When type="Property" frame="TandemRaceAILabel" Text="Zerg"/>
                <Action type="SetProperty" frame="TandemComputerAvatarImage" Visible="true"/>
                <Action type="SetProperty" frame="TandemBanner" Texture="Assets\Textures\RaceBanner_Zerg_Default_1v1.dds"/>
                <Action type="SetProperty" frame="TandemPlayerName" Text="@UI/LoadingPlayerCard/ComputerPlayerName"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar" Value="{$this/@MaxValue}"/>
            </State>
            
            <State name="ProtossAI">
                <When type="Property" frame="TandemRaceAILabel" Enabled="true"/>
                <When type="Property" frame="TandemRaceAILabel" Text="Prot"/>
                <Action type="SetProperty" frame="TandemComputerAvatarImage" Visible="true"/>
                <Action type="SetProperty" frame="TandemBanner" Texture="Assets\Textures\RaceBanner_Protoss_Default_1v1.dds"/>
                <Action type="SetProperty" frame="TandemPlayerName" Text="@UI/LoadingPlayerCard/ComputerPlayerName"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar" Value="{$this/@MaxValue}"/>
            </State>
            
            <State name="RandomAI">
                <When type="Property" frame="TandemRaceAILabel" Enabled="true"/>
                <Action type="SetProperty" frame="TandemComputerAvatarImage" Visible="true"/>
                <Action type="SetProperty" frame="TandemBanner" Texture="Assets\Textures\RaceBanner_Random_Default_1v1.dds"/>
                <Action type="SetProperty" frame="TandemPlayerName" Text="@UI/LoadingPlayerCard/ComputerPlayerName"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar" Value="{$this/@MaxValue}"/>
            </State>
            
            <State name="NotAI"/>
        </StateGroup>
        
        <StateGroup name="TandemClanState">
            <DefaultState val="NoClan"/>
            
            <State name="HasClan">
                <When type="Property" frame="TandemClanIconContainer" IsClan="true"/>
                <Action type="SetProperty" frame="TandemFormatPlayerName" FormatText="&lt;%0%&gt; %1%"/>
            </State>
            
            <State name="NoClan"/>
        </StateGroup>
        
        <StateGroup name="TandemClanDecalState">
            <DefaultState val="NoClanDecal"/>
            
            <State name="HasClanDecal">
                <When type="Property" frame="TandemClanIconContainer" operator="NotEqual" Decal=""/>
                <Action type="SetProperty" frame="TandemClanIconContainer" Visible="true"/>
            </State>
            
            <State name="NoClanDecal">
                <Action type="SetProperty" frame="TandemClanIconContainer" Visible="false"/>
            </State>
        </StateGroup>
        
        <StateGroup name="TandemLoadingState">
            <DefaultState val="Finished"/>
            
            <State name="NotStarted">
                <When type="Property" frame="TandemLoadingProgressBar" ProgressPercent="0"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar/FillImageContainer/LeadingGradient" Visible="false"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBarLeadingGlow" Visible="false"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar/FillImageContainer/FillImage" Color="105,132,189"/>
            </State>
            
            <State name="InProgress">
                <When type="Property" frame="TandemLoadingProgressBar" operator="LessThan" ProgressPercent="1"/>
                <When type="Property" frame="TandemLoadingProgressBar" Visible="true"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar/FillImageContainer/LeadingGradient" Visible="true"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBarLeadingGlow" Visible="true"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar/FillImageContainer/FillImage" Color="105,132,189"/>
            </State>
            
            <State name="Finished">
                <Action type="SetProperty" frame="TandemLoadingProgressBar/FillImageContainer/LeadingGradient" Visible="false"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBarLeadingGlow" Visible="false"/>
                <Action type="SetProperty" frame="TandemLoadingProgressBar/FillImageContainer/FillImage" Color="72,91,130"/>
            </State>
        </StateGroup>
    </Frame>
    
