<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="TournamentTrophyTallyFrame" name="TournamentTrophyTallyFrameTemplate">
        <Height val="300"/>
        <Width val="800"/>

        <Animation name="Default">
            <Event event="Active" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
        </Animation>

        <Animation name="Empty">
            <Event event="Empty" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>
        
        <Frame type="Image" name="LeaguePanelContainmentBG">
            <Anchor side="Top" relative="$parent" pos="Min" offset="16"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="29"/>
            <Texture val="@UI/SC2_UI_Tournament_LeaguePanelBG"/>
            <TextureType val="NineSlice"/>
            <Width val="712"/>
            <Height val="264"/>
        </Frame>
        
        <Frame type="Image" name="LeagueIconImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="212"/>
            <Height val="212"/>
            <Texture val="@UI/Glues_Icons_League_Matchmaking_Placement"/>
        </Frame>
        
        <Frame type="Label" name="LeagueLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="55"/>
            <Anchor side="Left" relative="$parent/LeagueIconImage" pos="Max" offset="9"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
            <Style val="@MatchmakingLeaguePanel_Title"/>
        </Frame>
        
        <Frame type="Label" name="HighestLeagueCountLabel">
            <Anchor side="Top" relative="$parent/LeagueLabel" pos="Max" offset="1"/>
            <Anchor side="Left" relative="$parent/LeagueLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
            <Style val="@MatchmakingLeaguePanel_Subtitle"/>
        </Frame>

        <Frame type="Label" name="TotalCountLabel">
            <Anchor side="Top" relative="$parent/HighestLeagueCountLabel" pos="Max" offset="1"/>
            <Anchor side="Left" relative="$parent/HighestLeagueCountLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
            <Style val="@MatchmakingLeaguePanel_Details"/>
        </Frame>

        <Animation name="TrophyCountSame">
            <Event event="TrophyCountSame" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="TotalCountLabel">
                <Key type="Bool" time="0" value="false"/>
            </Controller>
        </Animation>

        <Animation name="TrophyCountNotSame">
            <Event event="TrophyCountNotSame" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="TotalCountLabel">
                <Key type="Bool" time="0" value="true"/>
            </Controller>
        </Animation>
        
        <!--
        =======================
        
        LEAGUE EVENT FRAMES
        
        =======================
        -->
        
        <!-- Bronze -->
        <Frame type="Frame" name="EventFrameBronze">
            <Anchor relative="$parent"/>
            <RenderPriority val="400"/>
            <Visible val="False"/>
        
            <Animation name="Bronze">
                <Event event="Bronze" action="Reset,Play" frame="$parent"/>
                <Event event="Silver" action="Reset" frame="$parent"/>
                <Event event="Gold" action="Reset" frame="$parent"/>
                <Event event="Platinum" action="Reset" frame="$parent"/>
                <Event event="Diamond" action="Reset" frame="$parent"/>
                <Event event="Master" action="Reset" frame="$parent"/>
                <Event event="Grandmaster" action="Reset" frame="$parent"/>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="True"/>
                </Controller>
            </Animation>
            
            <Animation name="UpdateLeague">
                <Event event="OnShown" action="Reset,Play"/>

                <Controller type="Texture" end="Pause" frame="$parent/LeagueIconImage">
                    <Key type="Image" time="0" image="@UI/Glues_Icons_League_Matchmaking_Bronze"/>
                </Controller>
                
                <Controller type="Cutscene" end="Stop" frame="$parent/$parent/$parent/TrophyCutscene">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Bronze"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Silver"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Gold"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Platinum"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Diamond"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Master"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Grandmaster"/>
                </Controller>
            </Animation>
        </Frame>
        
        
