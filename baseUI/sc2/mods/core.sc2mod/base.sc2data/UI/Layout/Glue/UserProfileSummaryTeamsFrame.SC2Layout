<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="ProgressBar" name="ProgressBar">
        <Width val="566"/>
        <Height val="70"/>
        <UseFillContainer val="true"/>
        <FillInset val="0.0406"/>

        <Frame type="Image" name="BorderImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarBg"/>
            <Width val="520"/>
            <Height val="22"/>
        </Frame>

        <Frame type="Frame" name="FillImageContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Image" name="FillImage">
                <Anchor relative="$parent/$parent"/>
                <Texture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarFill"/>
            </Frame>

            <Frame type="Frame" name="FillImageAccentMask">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent/$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="0"/>
                <Width val="566"/>
                <Height val="70"/>
                <AlphaMaskTexture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarMask"/>

                <Frame type="Image" name="FillImageAccent">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="8"/>
                    <Texture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ProgressBarHighlight"/>
                </Frame>
            </Frame>
        </Frame>
    </Frame>

    <Frame type="LadderTeamTileListItem" name="LadderTeamTileListItem">
        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
            <Alpha val="255"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Over"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Selected"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="MatchmakingLeagueIcon" name="LeagueIcon" template="MatchmakingLeagueIcon/MatchmakingLeagueIconTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="-100"/>
            <League val="{$parent/@League}"/>
            <LeagueTier val="{$parent/@Tier}"/>
            <LeagueRank val="{$parent/@Rank}"/>
        </Frame>

        <Frame type="Button" name="ViewLadderTeamButton">
            <Anchor relative="$parent/LeagueIcon"/>
        </Frame>

        <Frame type="Label" name="QueueLabel">
            <Anchor side="Top" relative="$parent/LeagueIcon" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/LeagueIcon" pos="Max" offset="-70"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$this/QueueFrame/@ShortName}"/>
            <Style val="@Arcade_Title"/>

            <Frame type="BattleQueueFrame" name="QueueFrame">
                <Anchor relative="$parent"/>
                <QueueId val="{$parent/$parent/@QueueId}"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="RatingLabel">
            <Anchor side="Top" relative="$parent/QueueLabel" pos="Max" offset="20"/>
            <Anchor side="Left" relative="$parent/QueueLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$this/FormatFrame/@Text}"/>
            <Style val="@Arcade_Title"/>

            <Frame type="TextFormatFrame" name="FormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/UserProfileSummaryTeamsFrame/MMR"/>
                <Replacement index="0" val="{$parent/$parent/@Rating}"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="LeagueLabel">
            <Anchor side="Top" relative="$parent/RatingLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/RatingLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$this/FormatFrame/@Text}"/>
            <Style val="@Arcade_Title"/>
            
            <Frame type="MatchmakingLeagueIcon" name="LeagueIcon">
                <Anchor relative="$parent"/>
                <League val="{$parent/$parent/@League}"/>
            </Frame>

            <Frame type="TextFormatFrame" name="FormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="%0% %1%"/>
                <Replacement index="0" val="{$parent/LeagueIcon/@LeagueName}"/>
                <Replacement index="1" val="{$parent/$parent/@Tier}"/>
            </Frame>
        </Frame>

        <Frame type="ProgressBar" name="RatingProgressBar" template="UserProfileSummaryTeamsFrame/ProgressBar">
            <Anchor side="Top" relative="$parent/RatingLabel" pos="Max" offset="20"/>
            <Anchor side="Left" relative="$parent/RatingLabel" pos="Min" offset="0"/>
            <MinValue val="{$this/LeagueRatingFrame/@MinLeagueRating}"/>
            <MaxValue val="{$this/LeagueRatingFrame/@MaxLeagueRating}"/>
            <Value val="{$this/LeagueRatingFrame/@Rating}"/>

            <Frame type="LeagueRatingFrame" name="LeagueRatingFrame">
                <Anchor relative="$parent"/>
                <IsArrangedTeam val="{$parent/$parent/@IsArrangedTeam}"/>
                <Rating val="{$parent/$parent/@Rating}"/>
                <QueueId val="{$parent/$parent/@QueueId}"/>
            </Frame>
        </Frame>
    </Frame>
    
    <Frame type="UserProfileSummaryTeamsFrame" name="UserProfileSummaryTeamsFrame">
        <QueueId val="{$this/ModePulldown/@QueueId}"/>
        
        <Animation name="ViewLadderTeam">
            <Event event="OnClick" action="Reset,Play" frame="TileList/Item0/ViewLadderTeamButton"/>

            <Controller type="Event" end="Pause">
                <Key type="Event" time="0" event="ViewLadderTeamRequest"/>
            </Controller>
        </Animation>
        
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_TableLargeHeaderBg"/>
            <TextureType val="NineSlice"/>
            <Tiled val="True"/>
            <Visible val="true"/>
        </Frame>

        <Frame type="Label" name="SeasonLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-12"/>
            <Text val="{$this/SeasonFrame/@SeasonName}"/>
            <Style val="@Arcade_Title"/>

            <Frame type="MatchmakingSeasonFrame" name="SeasonFrame">
                <Anchor relative="$parent"/>
                <SeasonId val="{$this/@CurrentSeasonId}"/>
            </Frame>
        </Frame>

        <!-- TODO: Display something instead when the tile list is hidden due to being unranked -->
        <Frame type="TileList" name="TileList">
            <Anchor side="Top" relative="$parent" pos="Min" offset="80"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-12"/>
            <Visible val="{$this/@HasSelectedData}"/>

            <Frame type="PageSelectorButtonList" name="PageSelector">
                <Anchor relative="$parent"/>
            </Frame>
            
            <Frame type="LadderTeamTileListItem" name="Item0" template="UserProfileSummaryTeamsFrame/LadderTeamTileListItem">
                <Anchor relative="$parent"/>
            </Frame>
        </Frame>

        <Frame type="BattleQueuePulldown" name="ModePulldown" template="StandardTemplates/StandardPulldownTemplate">
            <Anchor side="Top" relative="$parent/TileList" pos="Min" offset="13"/>
            <Anchor side="Right" relative="$parent/TileList" pos="Max" offset="-10"/>
            <TagString index="0" val="Ladd"/>
            <TagString index="1" val="LotV"/>
        </Frame>
    </Frame>
</Desc>
