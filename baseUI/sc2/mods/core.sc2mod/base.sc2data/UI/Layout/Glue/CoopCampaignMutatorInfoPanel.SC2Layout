                    <Anchor side="Left" relative="$parent/BountiesNote" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/BountiesNote" pos="Max" offset="0"/>
                    <Style val="Coop_CommanderMutatorBountiesTooltipWarning"/>
                    <Text val="@UI/ScreenCoopCampaign/MutatorRewardWarning"/>
                    <Alpha val="255"/>
                </Frame>
                
                <Frame type="Label" name="BountiesNote">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="-20"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
                    <Style val="Coop_CommanderMutatorBountiesTooltipNote"/>
                    <Text val="@UI/ScreenCoopCampaign/MutatorRewardNote"/>
                    <Alpha val="128"/>
                </Frame>

                <Animation name="Show">
                    <Event event="OnMouseEnter" action="Reset,Play" frame="$parent/BadgeContainer/BadgeBackground"/>
                    <Event event="OnMouseExit" action="Reset,Stop" frame="$parent/BadgeContainer/BadgeBackground"/>

                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="true"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>
        
        <Frame type="Button" name="MutatorMapButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="258"/>
            <Enabled val="False"/>
            <Text val="{$parent/InfoContainer/MapLabel/@Text}"/>
            
            <StateGroup name="MutatorButtonStateGroup">
                <State name="InactiveState">
                    <When type="AnimationState" frame="$parent" InfoPanelState="Inactive"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCoopCampaign/MutatorInactive"/>
                </State>
                
                <State name="DefaultState">
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Button" name="Button" template="StandardBattlenetTemplates/BattlenetLargeIconButtonTemplate">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-20"/>
            <Anchor side="Left" relative="$parent/$parent/DifficultyButton" pos="Max" offset="6"/>
            <Toggleable val="true"/>
            
            <Frame type="Image" name="Icon">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <DesaturateOnDisable val="False"/>
                <Texture val="@UI/Glues_Icons_Info_Highlight"/>
                
                <Animation name="Disable">
                    <Event event="OnDisabled" action="Reset,Play" frame="$parent"/>
                    <Event event="OnEnabled" action="Reset" frame="$parent"/>
                     
                    <Controller type="Texture" end="Pause">
                        <Key type="Image" time="0" image="@UI/Glues_Icons_Info_Disabled"/>
                    </Controller>
                </Animation>
            </Frame>

            <StateGroup name="MutatorButtonStateGroup">
                <State name="InactiveState">
                    <When type="AnimationState" frame="$parent" InfoPanelState="Inactive"/>
                    <Action type="SetProperty" frame="$this" Enabled="false"/>
                </State>
                
                <State name="DefaultState">
                    <Action type="SetProperty" frame="$this" Enabled="true"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="RemainingTimeLabel">
            <Anchor side="Top" relative="$parent/MutatorMapButton" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/MutatorMapButton" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/MutatorMapButton" pos="Mid" offset="0"/>
            <Style val="Coop_MapDifficultyLabel"/>
            <Text val="@UI/ScreenCoopCampaign/RemainingTime"/>
        </Frame>
    </Frame>
</Desc>
