                </Frame>
                
                <Frame type="Image" name="FlavorImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Width val="504"/>
                    <Height val="732"/>
                    <Alpha val="180"/>
                    <LayerCount val="2"/>
                    
                    <Texture val="Assets/Textures/SC2_UI_Login_FlavorImage_Mask.dds" layer="0"/>
                    <AlphaLayer val="True" layer="0"/>
                    <Texture val="Assets/Textures/SC2_UI_Login_FlavorImage_Terran_01.dds" layer="1"/>
                    <ColorAdjustMode val="Colorize"/>
                    <DarkAdjustColor val="85,117,171"/>
                    
                    <Frame type="MathFrame" name="Randomizer">
                        <Anchor relative="$parent"/>
                        <A val="0"/>
                        <B val="3"/>
                        <Operator val="RandomStl"/>
                    </Frame>
        
                    <StateGroup name="ImageAssigner">
                        <DefaultState val="Default"/>
                    
                        <State name="Terran01">
                            <When type="Property" frame="$this/Randomizer" ResultCeil="1"/>
                            <Action type="SetProperty" frame="$this" Texture="Assets/Textures/SC2_UI_Login_FlavorImage_Terran_01.dds" index="1"/>
                        </State>
                    
                        <State name="Zerg01">
                            <When type="Property" frame="$this/Randomizer" ResultCeil="2"/>
                            <Action type="SetProperty" frame="$this" Texture="Assets/Textures/SC2_UI_Login_FlavorImage_Zerg_01.dds" index="1"/>
                        </State>
                    
                        <State name="Protoss01">
                            <When type="Property" frame="$this/Randomizer" ResultCeil="3"/>
                            <Action type="SetProperty" frame="$this" Texture="Assets/Textures/SC2_UI_Login_FlavorImage_Protoss_01.dds" index="1"/>
                        </State>
                    
                        <State name="Default">
                            <Action type="SetProperty" frame="$this" Texture="Assets/Textures/SC2_UI_Login_FlavorImage_Terran_01.dds" index="1"/>
                        </State>
                    </StateGroup>
                </Frame>

                <Frame type="Image" name="LoginLogo">
