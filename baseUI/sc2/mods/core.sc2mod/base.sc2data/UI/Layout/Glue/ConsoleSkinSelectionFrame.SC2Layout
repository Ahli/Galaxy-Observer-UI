<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Constant name="TileListItemHeight" val="88"/>
    <Constant name="TileListItemOffsetRight" val="-2"/>
    <Constant name="TileListItemOffsetTop" val="2"/>

    <Frame type="ConsoleSkinTileListItem" name="ConsoleSkinTileListItemTemplate">
        <Height val="#TileListItemHeight"/>
        <HideWhenCleared val="true"/>
        <SelectedSound val="@UI_FileSelectionSelect"/>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
            <Alpha val="255"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Over"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Selected"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="ConsoleSkinCacheFrame" name="ConsoleSkinCacheFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <ConsoleSkin val="{$parent/@ConsoleSkin}"/>
        </Frame>

        <Frame type="GameContentFrame" name="GameContentFrame">
            <Anchor relative="$parent"/>
            <GameContent val="{$parent/ConsoleSkinCacheFrame/@GameContent}"/>
        </Frame>

        <Frame type="StoreCacheFrame" name="StoreCacheFrame">
            <Anchor relative="$parent"/>
            <GameContent val="{$parent/ConsoleSkinCacheFrame/@GameContent}"/>
        </Frame>

        <Frame type="RewardCacheFrame" name="RewardCacheFrame">
            <Anchor relative="$parent"/>
            <Reward val="{$parent/ConsoleSkinCacheFrame/@RequiredReward}"/>
        </Frame>

        <Frame type="Image" name="IconImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
            <Height val="78"/>
            <Width val="93"/>
            <Alpha val="130"/>

            <Animation name="SelectedIconStyle">
                <Event event="OnShown" action="Reset" frame="$parent/SelectedImage"/>
                <Event event="OnShown" action="Play" frame="$parent/SelectedImage"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="130" out="Fast"/>
                    <Key type="Curve" time=".2" value="255" in="Slow"/>
                </Controller>
            </Animation>

            <Animation name="NormalIconStyle">
                <Event event="OnHidden" action="Reset" frame="$parent/SelectedImage"/>
                <Event event="OnHidden" action="Play" frame="$parent/SelectedImage"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" out="Fast"/>
                    <Key type="Curve" time=".2" value="130" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Frame" name="LockedIcon">
            <Anchor side="Top" relative="$parent/IconImage" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/IconImage" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/IconImage" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/IconImage" pos="Max" offset="0"/>
            <Visible val="False"/>

            <Frame type="Image" name="Background">
                <Anchor relative="$parent"/>
                <LayerColor val="000000"/>
                <Alpha val="150"/>
            </Frame>

            <Frame type="Image" name="LockIcon">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Texture val="@UI/Glues_Icons_SmallLock"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="IconFrame">
            <Anchor side="Top" relative="$parent/IconImage" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/IconImage" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/IconImage" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/IconImage" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Collection_VoicePack_ImageFrame"/>
            <TextureType val="Nineslice"/>
        </Frame>

        <Frame type="Label" name="NameLabel">
            <Anchor side="Bottom" relative="$parent/IconImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/IconImage" pos="Max" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Style val="@Collection_ListItem_Name"/>
            <Text val="{$parent/ConsoleSkinCacheFrame/@NameText}"/>

            <StateGroup name="ListItemTextStates">
                <DefaultState val="NormalState"/>
            
                <State name="ToggledState">
                    <When type="Property" frame="$parent/SelectedImage" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name_Selected"/>
                </State>
                
                <State name="HoverState">
                    <When type="Property" frame="$parent/HoverImage" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name_Selected"/>
                </State>
        
                <State name="LockedState">
                    <When type="Property" frame="$parent/LockedIcon" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name_Locked"/>
                </State>

                <State name="NormalState">
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="CostLabel">
            <Anchor side="Top" relative="$parent/IconImage" pos="Mid" offset="2"/>
            <Anchor side="Left" relative="$parent/NameLabel" pos="Min" offset="1"/>
            <Anchor side="Right" relative="$parent/NameLabel" pos="Max" offset="0"/>
            <Style val="@Collection_ListItem_Cost"/>
            <Height val="23"/>
            <Text val="{$parent/StoreCacheFrame/@CostText}"/>
        </Frame>

        <Frame type="Label" name="StateLabel">
            <Anchor side="Top" relative="$parent/IconImage" pos="Mid" offset="2"/>
            <Anchor side="Left" relative="$parent/NameLabel" pos="Min" offset="1"/>
            <Anchor side="Right" relative="$parent/NameLabel" pos="Max" offset="0"/>
            <Style val="@Collection_ListItem_Desc"/>
            <Height val="23"/>

            <StateGroup name="ListItemTextStates">
                <DefaultState val="NormalState"/>
            
                <State name="ToggledState">
                    <When type="Property" frame="$parent/SelectedImage" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Selected"/>
                </State>
                
                <State name="HoverState">
                    <When type="Property" frame="$parent/HoverImage" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Selected"/>
                </State>
        
                <State name="LockedState">
                    <When type="Property" frame="$parent/LockedIcon" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Locked"/>
                </State>

                <State name="NormalState">
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Image" name="CheckMarkImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Collection_Icon_Equipped"/>
        </Frame>

        <StateGroup name="GameContentOwnedStateGroup">
            <DefaultState val="GameContentNotOwned"/>

            <State name="RewardSelected">
                <When type="AnimationState" frame="RewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="StateLabel" visible="true"/>
                <Action type="SetProperty" frame="StateLabel" text="@UI/ScreenCollection/Equipped"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="GameContentPurchased">
                <When type="Property" frame="$this/StoreCacheFrame" IsPurchased="True"/>
                <Action type="SetProperty" frame="StateLabel" visible="true"/>
                <Action type="SetProperty" frame="StateLabel" text="@UI/ScreenCollection/Owned"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="GameContentDefault">
                <When type="Property" frame="$this/ConsoleSkinCacheFrame" Default="true"/>
                <Action type="SetProperty" frame="StateLabel" visible="true"/>
                <Action type="SetProperty" frame="StateLabel" text="@UI/ScreenCollection/Default"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="GameContentAllowed">
                <When type="Property" frame="$this/GameContentFrame" IsAllowed="true"/>
                <Action type="SetProperty" frame="StateLabel" visible="true"/>
                <Action type="SetProperty" frame="StateLabel" text="@UI/ScreenCollection/Earned"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="False"/>
            </State>

            <State name="GameContentLocked">
                <When type="Property" frame="$this/StoreCacheFrame" CanPurchase="false"/>
                <Action type="SetProperty" frame="StateLabel" visible="true"/>
                <Action type="SetProperty" frame="StateLabel" text="@UI/ScreenCollection/Locked"/>
                <Action type="SetProperty" frame="CostLabel" visible="false"/>
                <Action type="SetProperty" frame="LockedIcon" visible="True"/>
            </State>

            <State name="GameContentNotOwned">
                <Action type="SetProperty" frame="StateLabel" visible="false"/>
                <Action type="SetProperty" frame="CostLabel" visible="true"/>
                <Action type="SetProperty" frame="LockedIcon" visible="True"/>
            </State>
        </StateGroup>

        <StateGroup name="RewardSelectedStateGroup">
            <DefaultState val="RewardNotSelected"/>

            <State name="RewardSelected">
                <When type="AnimationState" frame="RewardCacheFrame" SelectedState="Selected"/>
                <Action type="SetProperty" frame="CheckMarkImage" visible="true"/>
            </State>

            <State name="RewardNotSelected">
                <Action type="SetProperty" frame="CheckMarkImage" visible="false"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="TileList" name="ConsoleSkinTileListTemplate">
        <AcceptsMouse val="true"/>
        <RequireSelection val="true"/>

        <Frame type="ConsoleSkinTileListItem" name="Item0" template="ConsoleSkinSelectionFrame/ConsoleSkinTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="#TileListItemOffsetRight"/>
        </Frame>

        <Frame type="ConsoleSkinTileListItem" name="Item1" template="ConsoleSkinSelectionFrame/ConsoleSkinTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Max" offset="#TileListItemOffsetTop"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="#TileListItemOffsetRight"/>
        </Frame>

        <Frame type="ConsoleSkinTileListItem" name="Item2" template="ConsoleSkinSelectionFrame/ConsoleSkinTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item1" pos="Max" offset="#TileListItemOffsetTop"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="#TileListItemOffsetRight"/>
        </Frame>

        <Frame type="ConsoleSkinTileListItem" name="Item3" template="ConsoleSkinSelectionFrame/ConsoleSkinTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item2" pos="Max" offset="#TileListItemOffsetTop"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="#TileListItemOffsetRight"/>
        </Frame>

        <Frame type="ConsoleSkinTileListItem" name="Item4" template="ConsoleSkinSelectionFrame/ConsoleSkinTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item3" pos="Max" offset="#TileListItemOffsetTop"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="#TileListItemOffsetRight"/>
        </Frame>

        <Frame type="ConsoleSkinTileListItem" name="Item5" template="ConsoleSkinSelectionFrame/ConsoleSkinTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item4" pos="Max" offset="#TileListItemOffsetTop"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="#TileListItemOffsetRight"/>
        </Frame>

        <Frame type="ConsoleSkinTileListItem" name="Item6" template="ConsoleSkinSelectionFrame/ConsoleSkinTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item5" pos="Max" offset="#TileListItemOffsetTop"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="#TileListItemOffsetRight"/>
        </Frame>

        <Frame type="ScrollBar" name="ScrollBar" template="CollectionTemplates/ScrollBarTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="-3"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        </Frame>
    </Frame>

    <Frame type="ConsoleSkinSelectionFrame" name="ConsoleSkinSelectionFrameTemplate">
        <HideAllowed val="{$this/HidePurchasedCheckBox/@Checked}"/>
        <SortMethod val="{$this/SortPulldown/@SelectedSort}"/>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="98"/>
            <Anchor side="Left" relative="$Anchor_Header" pos="Max" offset="0"/>
            <Style val="@Arcade_Title"/>
            <Text val="PH - Console Skins"/>
        </Frame>

        <Frame type="CheckBox" name="HidePurchasedCheckBox" template="StandardTemplates/StandardCheckBoxTemplate">
            <Anchor side="Top" relative="$parent/SortPulldown" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/SortPulldown" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/SortPulldown" pos="Min" offset="-40"/>
        </Frame>

        <Frame type="Label" name="HidePurchasedLabel">
            <Anchor side="Top" relative="$parent/HidePurchasedCheckBox" pos="Mid" offset="1"/>
            <Anchor side="Bottom" relative="$parent/HidePurchasedCheckBox" pos="Mid" offset="1"/>
            <Anchor side="Right" relative="$parent/HidePurchasedCheckBox" pos="Min" offset="-5"/>
            <Style val="@Arcade_Subtitle"/>
            <Text val="@UI/ScreenCollection/HideOwned"/>
        </Frame>

        <Frame type="GameContentSortPulldown" name="SortPulldown" template="StandardBattlenetTemplates/BattlenetLargePulldownTemplate">
            <Anchor side="Bottom" relative="$parent/ConsoleSkinCacheFrame" pos="Min" offset="3"/>
            <Anchor side="Right" relative="$parent/ConsoleSkinCacheFrame" pos="Max" offset="-4"/>
        </Frame>

        <Frame type="ConsoleSkinCacheFrame" name="ConsoleSkinCacheFrame">
            <Anchor side="Top" relative="$parent/TileListFrameBg" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/ConsoleSkinTileList" pos="Max" offset="30"/>
            <Height val="619"/>
            <Width val="1024"/>
            <ConsoleSkin val="{$parent/@SelectedConsoleSkin}"/>
        </Frame>
        
        <Frame type="BlurFrame" name="TilelistFrameBgBlur">
            <Anchor side="Top" relative="$parent/TileListFrameBg" pos="Min" offset="4"/>
            <Anchor side="Left" relative="$parent/TileListFrameBg" pos="Min" offset="4"/>
            <Anchor side="Bottom" relative="$parent/TileListFrameBg" pos="Max" offset="-4"/>
            <Anchor side="Right" relative="$parent/TileListFrameBg" pos="Max" offset="-4"/>

            <Animation name="Blur">
                <Event event="OnShown" action="Reset,Play" frame="$parent" />

                <Controller type="Blur" end="Pause">
                    <Key type="Curve" time="0" value=".35" out="Fast"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="TileListFrameBg">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="15"/>
            <Anchor side="Left" relative="$Anchor_Standard_Left" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/ConsoleSkinCacheFrame" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainContainer"/>
            <TextureType val="NineSlice"/>
            <Width val="517"/>
        </Frame>

        <Frame type="TileList" name="ConsoleSkinTileList" template="ConsoleSkinSelectionFrame/ConsoleSkinTileListTemplate">
            <Anchor side="Top" relative="$parent/TileListFrameBg" pos="Min" offset="25"/>
            <Anchor side="Left" relative="$parent/TileListFrameBg" pos="Min" offset="24"/>
            <Anchor side="Bottom" relative="$parent/TileListFrameBg" pos="Max" offset="-25"/>
            <Anchor side="Right" relative="$parent/TileListFrameBg" pos="Max" offset="-12"/>
        </Frame>

        <Frame type="SelectUnlockableButton" name="SelectUnlockableButton" template="CollectionTemplates/SelectUnlockableButtonTemplate">
            <Anchor side="Bottom" relative="$Anchor_ActionButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$Anchor_ActionButton" pos="Max" offset="0"/>
            <Reward val="{$parent/ConsoleSkinCacheFrame/@RequiredReward}"/>
            <PresenceId val="{$BattleUI/@LocalToonPresenceId}"/>
            <Visible val="False"/>

            <Frame type="GameContentFrame" name="GameContentFrame">
                <GameContent val="{$parent/$parent/ConsoleSkinCacheFrame/@GameContent}"/>
            </Frame>
            
            <Frame type="StoreCacheFrame" name="StoreCacheFrame">
                <GameContent val="{$parent/$parent/ConsoleSkinCacheFrame/@GameContent}"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="EquippedWarningContainer" template="CollectionTemplates/EquippedWarningContainerTemplate">
            <Anchor side="Bottom" relative="$Anchor_ActionButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/SelectUnlockableButton" pos="Max" offset="5"/>

            <StateGroup name="EquippedStateGroup">
                <DefaultState val="UnequippedState"/>

                <State name="EquippedState">
                    <When type="Property" frame="$parent/SelectUnlockableButton" enabled="False"/>
                    <When type="Property" frame="$parent/SelectUnlockableButton" visible="True"/>
                    <When type="AnimationState" frame="$parent/SelectUnlockableButton/RewardCacheFrame" AllowedState="Allowed"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>
                
                <State name="UnequippedState">
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Frame" name="AchievementLockedWarningContainer" template="CollectionTemplates/AchievementLockedWarningContainerTemplate">
            <Anchor side="Bottom" relative="$Anchor_ActionButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/SelectUnlockableButton" pos="Max" offset="20"/>
            
            <StateGroup name="AchievementLockedStateGroup">
                <DefaultState val="NotAchievementLockedState"/>
            
                <State name="AchievementLockedState">
                    <When type="Property" frame="$parent/SelectUnlockableButton/StoreCacheFrame" IsPurchased="False"/>
                    <When type="Property" frame="$parent/SelectUnlockableButton/RewardCacheFrame" Default="False"/>
                    <When type="Property" frame="$parent/SelectUnlockableButton/StoreCacheFrame" CanPurchase="False"/>
                    <When type="AnimationState" frame="$parent/SelectUnlockableButton/RewardCacheFrame" AllowedState="NotAllowed"/>
                    <When type="Property" frame="$parent/SelectUnlockableButton/GameContentFrame" IsValid="True"/>
                    <Action type="SetProperty" frame="$this" visible="True"/>
                </State>
                
                <State name="NotAchievementLockedState">
                    <Action type="SetProperty" frame="$this" visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="BuyButton" name="BuyButton" template="CollectionTemplates/BuyButtonTemplate">
            <Anchor side="Bottom" relative="$Anchor_ActionButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$Anchor_ActionButton" pos="Max" offset="0"/>
            <GameContent val="{$parent/ConsoleSkinCacheFrame/@GameContent}"/>

            <StateGroup name="RewardEarnedStateGroup">
                <DefaultState val="RewardNotEarnedState"/>

                <State name="RewardEarnedState">
                    <When type="AnimationState" frame="$parent/SelectUnlockableButton/RewardCacheFrame" AllowedState="Allowed"/>
                    <Action type="SetProperty" frame="$this" visible="false"/>
                </State>

                <State name="RewardNotEarnedState">
                    <Action type="SetProperty" frame="$this" visible="true"/>
                </State>
            </StateGroup>
        </Frame>

        <StateGroup name="SwapButtonsGroup">
            <State name="BuyStateActive">
                <When type="Property" frame="BuyButton" visible="True"/>
                <Action type="SetProperty" frame="SelectUnlockableButton" visible="False"/>
            </State>
            <State name="EquipStateActive">
                <When type="Property" frame="BuyButton" visible="False"/>
                <When type="AnimationState" frame="SelectUnlockableButton/RewardCacheFrame" AllowedState="Allowed"/>
                <Action type="SetProperty" frame="SelectUnlockableButton" visible="True"/>
            </State>
            <State name="NeitherActive">
                <When type="Property" frame="BuyButton" visible="False"/>
                <When type="AnimationState" frame="SelectUnlockableButton/RewardCacheFrame" AllowedState="NotAllowed"/>
                <Action type="SetProperty" frame="SelectUnlockableButton" visible="True"/>
            </State>
        </StateGroup>
    </Frame>
</Desc>
