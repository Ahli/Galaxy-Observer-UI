    <Frame type="Frame" name="MinimapContainerTemplate">
        <Width val="464"/>
        <Height val="464"/>
        
        <Frame type="Image" name="BackgroundImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
            <Texture val="Assets\Textures\white.dds"/>
            <Color val="18,21,24"/>
        </Frame>
        
        <Frame type="RangeFrame" name="VerticalWidthLimit">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MinimapVertical" pos="Min" offset="0"/>
            <LowerWidth val="5"/>
        </Frame>
        
        <Frame type="Image" name="MinimapVertical">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Texture val="{$ancestor[@type=MultiplayerLoadingPanel]/Minimap/MinimapImage/@Texture}"/>
            <!-- <TextureCoords val="{$ancestor[@type=MultiplayerLoadingPanel]/Minimap/@MinimapTexCoords}"/> -->
            <ScaleWidthToTexture val="true"/>
        </Frame>
        
        <Frame type="Image" name="MinimapHorizontal">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Max" offset="0"/>
            <Texture val="{$parent/MinimapVertical/@Texture}"/>
            <!-- <TextureCoords val="{$parent/MinimapVertical/@TextureCoords}"/> -->
            <ScaleHeightToTexture val="true"/>
            <Visible val="false"/>
        </Frame>
        
        <!-- BORDER OUTER -->
        <Frame type="Image" name="MinimapBorderOuter">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Outer_16x9"/>
            <TextureType val="NineSlice"/>
            <Color val="0,0,0"/>
        </Frame>
        
        <!-- BORDER INNER -->
        <Frame type="Image" name="MinimapBorderInner">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Inner_16x9"/>
            <TextureType val="NineSlice"/>
            <Color val="36,107,169"/>
        </Frame>
        
        <StateGroup name="OrientationState">
            <DefaultState val="Vertical"/>
            
            <State name="Horizontal">
                <When type="Property" frame="VerticalWidthLimit" UnderWidth="true"/>
                <Action type="SetProperty" frame="MinimapVertical" Visible="false"/>
                <Action type="SetProperty" frame="MinimapHorizontal" Visible="true"/>
            </State>
            
            <State name="Vertical">
                <Action type="SetProperty" frame="MinimapVertical" Visible="true"/>
                <Action type="SetProperty" frame="MinimapHorizontal" Visible="false"/>
            </State>
        </StateGroup>
        
