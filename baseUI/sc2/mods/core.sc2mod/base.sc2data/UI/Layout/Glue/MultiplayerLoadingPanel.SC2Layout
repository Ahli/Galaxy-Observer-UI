    <!--
    =======================
    Multiplayer Loading Panel Template
    =======================
    
    Controls the look of title, minimap and players/teams on the multiplayer loading screens
    -->

    <Frame type="MultiplayerLoadingPanel" name="MultiplayerLoadingPanelTemplate">

        <Frame type="Image" name="BackgroundMeleeVignette">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/MultiplayerLoadingPanelVignette_Back"/>
            <Visible val="False"/>
        </Frame>
    
        <Frame type="TileList" name="TeamTileList" template="MultiplayerLoadingPanel/MultiplayerTeamTileListTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="125"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="-59"/>
        </Frame>
        
        <Frame type="Frame" name="MinimapContainer" template="MultiplayerLoadingPanel/MinimapContainerTemplate">
            <!-- <Anchor side="Top" relative="$parent/TeamTileList" pos="Min" offset="0"/> -->
            <!-- <Anchor side="Right" relative="$parent/TeamTileList" pos="Min" offset="-120"/> -->
            <Anchor side="Top" relative="$parent" pos="Min" offset="125"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Label" name="TournamentNameLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="35"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="MultiplayerLoadingPanelTournamentName"/>
            <CollapseLayout val="true"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Label" name="TournamentRoundLabel">
            <Anchor side="Top" relative="$parent/TournamentNameLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="MultiplayerLoadingPanelTournamentName"/>
            <CollapseLayout val="true"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Label" name="MapNameLabel">
            <Anchor side="Top" relative="$parent/Minimap" pos="Max" offset="60"/>
            <Anchor side="Left" relative="$parent/Minimap" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/Minimap" pos="Max" offset="0"/>
            <Style val="MultiplayerLoadingPanelMapName"/>
            <Options val="NoWrapping|ScaleToPreventTruncation"/>
			<Alpha val="{$parent/TeamTileList/@Alpha}"/>
        </Frame>
        
        <!-- Minimap -->
        <Frame type="Minimap" name="Minimap">
            <Anchor relative="$parent/FreeModeMinimapAnchor"/>
            <Alpha val="{$parent/TeamTileList/@Alpha}"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="MinimapImage">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
            </Frame>
        </Frame>

        <!-- Versus Mode -->
        <Frame type="Frame" name="VersusModeMinimapAnchor">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-100"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-100"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="#MapWidth"/>
        </Frame>
        
        <Frame type="Frame" name="VersusTeamsPlayerContainer" template="MultiplayerLoadingPanel/VersusTeamsPlayerContainerTemplate"/>
        <Frame type="Frame" name="Archon1v1PlayerContainer" template="MultiplayerLoadingPanel/Archon1v1PlayerContainerTemplate"/>

        <!-- Free Mode -->
        <Frame type="Frame" name="FreeModeMinimapAnchor">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-100"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-100"/>
            <!-- <Anchor side="Right" relative="$parent/FreeModeColumnAnchor0" pos="Min" offset="-40"/> -->
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="#MapWidth"/>
        </Frame>

        <!-- Single Mode -->
        <Frame type="Frame" name="SingleModeMinimapAnchor">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-100"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-100"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="-500"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="#MapWidth"/>
        </Frame>

        <!-- Extended Mode -->
        <Frame type="Frame" name="ExtendedModeMinimapAnchor">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-100"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-100"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="45"/>
            <Width val="#MapWidth"/>
        </Frame>
        
        <Frame type="Frame" name="FFAPlayerContainer" template="MultiplayerLoadingPanel/FFAPlayerContainerTemplate"/>
        
        <StateGroup name="GameTypeState">
            <DefaultState val="Extended"/>
            
            <State name="Offline">
                <When type="Property" IsOffline="true"/>
                <Action type="SetProperty" frame="Archon1v1PlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="VersusTeamsPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="FFAPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="TeamTileList" Alpha="255"/>
            </State>
            
            <!-- <State name="EditorTest"> -->
                <!-- <When type="Property" frame="Minimap" MinimapTexture=""/> -->
                <!-- <Action type="SetProperty" frame="Archon1v1PlayerContainer" Visible="false"/> -->
                <!-- <Action type="SetProperty" frame="VersusTeamsPlayerContainer" Visible="false"/> -->
                <!-- <Action type="SetProperty" frame="FFAPlayerContainer" Visible="false"/> -->
                <!-- <Action type="SetProperty" frame="TeamTileList" Alpha="0"/> -->
            <!-- </State> -->
            
            <State name="Archon1v1">
                <When type="Property" LoadingPanelMode="#e_multiplayerLoadingPanelModeVersus"/>      
                <When type="Property" TeamCount="2"/>
                <When type="Property" frame="TeamTileList/Item0/PlayerTileList" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item1/PlayerTileList" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item0/PlayerTileList/Item0/TandemPlayerTileList" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item1/PlayerTileList/Item0/TandemPlayerTileList" DataCount="1"/>
                <Action type="SetProperty" frame="Archon1v1PlayerContainer" Visible="true"/>
                <Action type="SetProperty" frame="VersusTeamsPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="FFAPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="TeamTileList" Alpha="0"/>
            </State>
            
            <State name="Versus">
                <When type="Property" LoadingPanelMode="#e_multiplayerLoadingPanelModeVersus"/>
                <When type="Property" TeamCount="2"/>
                <When type="Property" frame="TeamTileList/Item0/PlayerTileList" operator="LessThanOrEqual" DataCount="4"/>
                <When type="Property" frame="TeamTileList/Item1/PlayerTileList" operator="LessThanOrEqual" DataCount="4"/>
                <When type="Property" HasTandemPlayers="false"/>
                <Action type="SetProperty" frame="Archon1v1PlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="VersusTeamsPlayerContainer" Visible="true"/>
                <Action type="SetProperty" frame="FFAPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="TeamTileList" Alpha="0"/>
            </State>
            
            <State name="FFA8OrLess">
                <When type="Property" LoadingPanelMode="#e_multiplayerLoadingPanelModeFree"/>
                <When type="Property" operator="LessThanOrEqual" PlayerCount="8"/>
                <When type="Property" HasTandemPlayers="false"/>
                <When type="Property" frame="TeamTileList/Item0/PlayerTileList" operator="LessThanOrEqual" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item1/PlayerTileList" operator="LessThanOrEqual" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item2/PlayerTileList" operator="LessThanOrEqual" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item3/PlayerTileList" operator="LessThanOrEqual" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item4/PlayerTileList" operator="LessThanOrEqual" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item5/PlayerTileList" operator="LessThanOrEqual" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item6/PlayerTileList" operator="LessThanOrEqual" DataCount="1"/>
                <When type="Property" frame="TeamTileList/Item7/PlayerTileList" operator="LessThanOrEqual" DataCount="1"/>
                <Action type="SetProperty" frame="Archon1v1PlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="VersusTeamsPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="FFAPlayerContainer" Visible="true"/>
                <Action type="SetProperty" frame="TeamTileList" Alpha="0"/>
            </State>
            
            <State name="Single">
                <When type="Property" LoadingPanelMode="#e_multiplayerLoadingPanelModeSingle"/>
                <When type="Property" PlayerCount="1"/>
                <When type="Property" HasTandemPlayers="false"/>
                <Action type="SetProperty" frame="Archon1v1PlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="VersusTeamsPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="FFAPlayerContainer" Visible="true"/>
                <Action type="SetProperty" frame="TeamTileList" Alpha="0"/>
            </State>
            
            <!-- Catches everything else -->
            <State name="Extended">
                <Action type="SetProperty" frame="Archon1v1PlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="VersusTeamsPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="FFAPlayerContainer" Visible="false"/>
                <Action type="SetProperty" frame="TeamTileList" Alpha="255"/>
            </State>
        </StateGroup>
    </Frame>
</Desc>
