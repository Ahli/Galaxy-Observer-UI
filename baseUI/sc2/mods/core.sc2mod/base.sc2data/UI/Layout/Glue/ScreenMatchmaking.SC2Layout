<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="RaceTileListItem" name="MatchmakingRaceTileListItemTemplate" template="StandardBattlenetTemplates/MatchmakingTileListItemTemplate">
        <StateGroup name="RaceSelectEnabledGroup">
            <State name="RaceSelectionMatched">
                <When type="AnimationState" frame="$parent" MatchmakingQueueState="MatchmakingStateRaceSelectionMatched"/>
                <Action type="SetProperty" tooltip="@UI/ScreenMatchmaking/SoloSearchingTooltip"/>
            </State>

            <State name="RaceSelectionSoloSearching">
                <When type="AnimationState" frame="$parent" MatchmakingQueueState="MatchmakingStateRaceSelectionSearching"/>
                <When type="AnimationState" frame="$parent" IsSoloQueue="SoloQueueTrue"/>
                <Action type="SetProperty" tooltip="@UI/ScreenMatchmaking/SoloSearchingTooltip"/>
            </State>

            <State name="TandemFollower">
                <When type="AnimationState" frame="$parent" IsTandemFollower="TandemFollowerTrue"/>
                <Action type="SetProperty" tooltip="@UI/ScreenMatchmaking/QueueNotPartyLeaderTooltip"/>
            </State>

            <State name="RaceSelectionEnabled">
                <Action type="SetProperty" tooltip=""/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="LeaguePromotionDebugFrame" name="LeaguePromotionDebugFrameTemplate" requiredtoload="IS_TOURNAMENT_DEBUG_CODE_ENABLED">
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainContainer"/>
            <TextureType val="NineSlice"/>
        </Frame>
        
        <Frame type="Label" name="SourceLeagueLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset ="20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset ="20"/>
            <Text val="SOURCE LEAGUE"/>
        </Frame>
        
        <Frame type="Pulldown" name="SourceLeaguePulldown" template="StandardTemplates/StandardPulldownTemplate">
            <Anchor side="Top" relative="$parent/SourceLeagueLabel" pos="Max" offset ="5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset ="20"/>
        </Frame>
        
        <Frame type="Pulldown" name="SourceTierPulldown" template="StandardTemplates/StandardPulldownTemplate">
            <Width val="100"/>
            <Anchor side="Top" relative="$parent/SourceLeaguePulldown" pos="Min" offset ="0"/>
            <Anchor side="Left" relative="$parent/SourceLeaguePulldown" pos="Max" offset ="20"/>
        </Frame>

        <Frame type="Label" name="DestLeagueLabel">
            <Anchor side="Top" relative="$parent/SourceLeaguePulldown" pos="Max" offset ="20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset ="20"/>
            <Text val="DEST LEAGUE"/>
        </Frame>
        
        <Frame type="Pulldown" name="DestLeaguePulldown" template="StandardTemplates/StandardPulldownTemplate">
            <Anchor side="Top" relative="$parent/DestLeagueLabel" pos="Max" offset ="5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset ="20"/>
        </Frame>
        
        <Frame type="Pulldown" name="DestTierPulldown" template="StandardTemplates/StandardPulldownTemplate">
            <Width val="100"/>
            <Anchor side="Top" relative="$parent/DestLeaguePulldown" pos="Min" offset ="0"/>
            <Anchor side="Left" relative="$parent/DestLeaguePulldown" pos="Max" offset ="20"/>
        </Frame>

        <Frame type="Label" name="QueueIdLabel">
            <Anchor side="Top" relative="$parent/DestLeaguePulldown" pos="Max" offset ="20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset ="20"/>
            <Text val="QUEUE ID"/>
        </Frame>

        <Frame type="EditBox" name="QueueEditBox" template="StandardBattlenetTemplates/BattlenetEditBoxTemplate">
            <Anchor side="Top" relative="$parent/QueueIdLabel" pos="Max" offset ="5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Height val="68"/>
            <Width val ="315"/>
        </Frame>

        <Frame type="Button" name="ShowPromotionButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
            <Anchor side="Top" relative="$parent/QueueEditBox" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Width val="288"/>
            <Text val="Promote"/>
        </Frame>

        <Frame type="Button" name="SeasonPlacementButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
            <Anchor side="Top" relative="$parent/ShowPromotionButton" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Width val="288"/>
            <Text val="Season Placement"/>
        </Frame>

        <Frame type="Button" name="LadderReassignmentButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
            <Anchor side="Top" relative="$parent/SeasonPlacementButton" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Width val="288"/>
            <Text val="Ladder Reassignment"/>
        </Frame>

        <Frame type="Label" name="AchievementIdLabel">
            <Anchor side="Top" relative="$parent/LadderReassignmentButton" pos="Max" offset ="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset ="20"/>
            <Text val="ACHIEVEMENT ID"/>
        </Frame>

        <Frame type="EditBox" name="AchievementFanfareEditBox" template="StandardBattlenetTemplates/BattlenetEditBoxTemplate">
            <Anchor side="Top" relative="$parent/AchievementIdLabel" pos="Max" offset ="5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Height val="68"/>
            <Width val ="0"/>
        </Frame>

        <Frame type="Button" name="AchievementFanfareButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
            <Anchor side="Top" relative="$parent/AchievementFanfareEditBox" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AchievementFanfareEditBox" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Width val="0"/>
            <Text val="Achievement Fanfare"/>
        </Frame>

        <Frame type="Button" name="PauseFanfareButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
            <Anchor side="Top" relative="$parent/AchievementFanfareButton" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Width val="288"/>
            <Text val="Toggle Fanfare Pause"/>
            <Toggleable val="true"/>
        </Frame>

        <Frame type="Button" name="ScoreScreenButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
            <Anchor side="Top" relative="$parent/PauseFanfareButton" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Width val="288"/>
            <Text val="Score Screen"/>
        </Frame>
    </Frame>

    <Frame type="ScreenMatchmaking" name="ScreenMatchmaking">
        <Handle val="ScreenMatchmaking"/>
        <Anchor relative="$parent"/>
        <DisplayBackgroundModel val="false"/>
        <ShowingNotification val="false"/>
        
        <Frame type="CutsceneFrame" name="BackgroundCutscene">
            <Anchor relative="$parent"/>
            <File val="Cutscenes/UI_Screens_Multiplayer_Background.SC2Cutscene" preloadFilters="Protoss Terran Zerg Random Ring"/>
            <AutoPlay val="false"/>
            <RenderType val="HDR"/>
        </Frame>

        <Animation name="VersionState">
            <Event event="NewVersion" action="Reset,Play" frame="RankedDialogContainer/LadderSplashDialogNotificationFrame"/>
            <Event event="OldVersion" action="Reset,Stop" frame="RankedDialogContainer/LadderSplashDialogNotificationFrame"/>
            <Event event="InvalidVersion" action="Reset,Stop" frame="RankedDialogContainer/LadderSplashDialogNotificationFrame"/>

            <Controller type="Property" property="ShowingNotification" end="Pause">
                <Key type="Property" time="0" value="true"/>
            </Controller>
        </Animation>

        <!-- These animation events are the race's entry index -->
        <!-- 0=Terr, 1=Zerg, 2=Prot, 3=Rand -->
        <!-- Consider using the Cutscene controller SetCutscene action rather than setting visibility -->
        
        <Frame type="CutsceneFrame" name="ForegroundCutscene">
            <Anchor relative="$parent"/>
            <File val="Cutscenes/UI_Screens_Multiplayer_ForeGround.SC2Cutscene" preloadFilters="Protoss Terran Zerg Random"/>
            <AutoPlay val="false"/>
            <RenderType val="HDR"/>
            
            <Animation name="Terran">
                <Event event="0" action="Reset,Play" frame="$parent"/>
                <Event event="1" action="Reset,Stop" frame="$parent"/>
                <Event event="2" action="Reset,Stop" frame="$parent"/>
                <Event event="3" action="Reset,Stop" frame="$parent"/>

                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Terran"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Protoss"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Random"/>
                </Controller>
                
                <Controller type="Cutscene" end="Stop" frame="$parent/BackgroundCutscene">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Terran"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Random"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Protoss"/>
                    <Key type="Cutscene" action="Play" time="0.0"/>
                </Controller>
            </Animation>
            
            <Animation name="Zerg">
                <Event event="0" action="Reset,Stop" frame="$parent"/>
                <Event event="1" action="Reset,Play" frame="$parent"/>
                <Event event="2" action="Reset,Stop" frame="$parent"/>
                <Event event="3" action="Reset,Stop" frame="$parent"/>

                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Terran"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Protoss"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Random"/>
                </Controller>
                
                <Controller type="Cutscene" end="Stop" frame="$parent/BackgroundCutscene">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Random"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Protoss"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Terran"/>
                    <Key type="Cutscene" action="Play" time="0.0"/>
                </Controller>
            </Animation>
            
            <Animation name="Protoss">
                <Event event="0" action="Reset,Stop" frame="$parent"/>
                <Event event="1" action="Reset,Stop" frame="$parent"/>
                <Event event="2" action="Reset,Play" frame="$parent"/>
                <Event event="3" action="Reset,Stop" frame="$parent"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Protoss"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Terran"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Random"/>
                </Controller>

                <Controller type="Cutscene" end="Stop" frame="$parent/BackgroundCutscene">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Protoss"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Random"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Terran"/>
                    <Key type="Cutscene" action="Play" time="0.0"/>
                </Controller>
            </Animation>
            
            <Animation name="Random">
                <Event event="0" action="Reset,Stop" frame="$parent"/>
                <Event event="1" action="Reset,Stop" frame="$parent"/>
                <Event event="2" action="Reset,Stop" frame="$parent"/>
                <Event event="3" action="Reset,Play" frame="$parent"/>

                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Random"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Protoss"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Terran"/>
                </Controller>
                
                <Controller type="Cutscene" end="Stop" frame="$parent/BackgroundCutscene">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Random"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Protoss"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Terran"/>
                    <Key type="Cutscene" action="Play" time="0.0"/>
                </Controller>
            </Animation>

            <Animation name="PlayBirth">
                <Event event="PlayBirth" action="Reset,Play" frame="$parent"/>

                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="Bookmark" time="0.0" bookmark="CharacterBirthEnd"/>
                    <Key type="Cutscene" action="Bookmark" time="0.0" bookmark="CharacterBirthStart"/>
                    <Key type="Cutscene" action="Play" time="0.0"/>
                </Controller>
            </Animation>

            <Animation name="PlayIdle">
                <Event event="PlayIdle" action="Reset,Play" frame="$parent"/>

                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="Bookmark" time="0.0" bookmark="CharacterLoopStart"/>
                    <Key type="Cutscene" action="Play" time="0.0"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="Vignette">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Coop_Vignette"/>
            <Alpha val="150"/>
        </Frame>

        <StateGroup name="MatchmakingTitleStateGroup">
            <State name="TrainingState">
                <When type="AnimationState" frame="$this" ModeState="ModeTraining"/>
                <Action type="SetProperty" frame="TitleLabel" Text="@UI/ScreenMultiplayer/TrainingButton"/>
                <Action type="SetProperty" frame="SubtitleLabel" Text="@UI/ScreenMatchmaking/TrainingDescription"/>
                <Action type="SetProperty" frame="RankedDialogContainer" Visible="False"/>
                <Action type="SetProperty" frame="ContainerFrame/QueueTileList" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/ModeLabel" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/MapVetoButton" Visible="False"/>
                <Action type="SetProperty" frame="LoadingFrame/NoQueuesLabel" Text="@UI/ScreenMatchmaking/NoQueues"/>
            </State>

            <State name="VersusAIState">
                <When type="AnimationState" frame="$this" ModeState="ModeVersusAI"/>
                <Action type="SetProperty" frame="TitleLabel" Text="@UI/ScreenMultiplayer/VersusAIButton"/>
                <Action type="SetProperty" frame="SubtitleLabel" Text="@UI/ScreenMatchmaking/VersusAIDescription"/>
                <Action type="SetProperty" frame="RankedDialogContainer" Visible="False"/>
                <Action type="SetProperty" frame="ContainerFrame/QueueTileList" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/ModeLabel" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/MapVetoButton" Visible="False"/>
                <Action type="SetProperty" frame="LoadingFrame/NoQueuesLabel" Text="@UI/ScreenMatchmaking/NoQueues"/>
            </State>

            <State name="UnrankedState">
                <When type="AnimationState" frame="$this" ModeState="ModeUnranked"/>
                <Action type="SetProperty" frame="TitleLabel" Text="@UI/ScreenMultiplayer/UnrankedButton"/>
                <Action type="SetProperty" frame="SubtitleLabel" Text="@UI/ScreenMatchmaking/UnrankedDescription"/>
                <Action type="SetProperty" frame="RankedDialogContainer" Visible="False"/>
                <Action type="SetProperty" frame="ContainerFrame/QueueTileList" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/ModeLabel" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/MapVetoButton" Visible="True"/>
                <Action type="SetProperty" frame="LoadingFrame/NoQueuesLabel" Text="@UI/ScreenMatchmaking/NoQueues"/>
            </State>

            <State name="RankedState">
                <When type="AnimationState" frame="$this" ModeState="ModeRanked"/>
                <Action type="SetProperty" frame="TitleLabel" Text="@UI/ScreenMultiplayer/RankedButton"/>
                <Action type="SetProperty" frame="SubtitleLabel" Text="@UI/ScreenMatchmaking/RankedDescription"/>
                <Action type="SetProperty" frame="RankedDialogContainer" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/QueueTileList" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/ModeLabel" Visible="True"/>
                <Action type="SetProperty" frame="ContainerFrame/MapVetoButton" Visible="True"/>
                <Action type="SetProperty" frame="LoadingFrame/NoQueuesLabel" Text="@UI/ScreenMatchmaking/NoQueues"/>
            </State>

            <State name="TestingState">
                <When type="AnimationState" frame="$this" ModeState="ModeTesting"/>
                <Action type="SetProperty" frame="TitleLabel" Text="@UI/ScreenMultiplayer/TestingButton"/>
                <Action type="SetProperty" frame="SubtitleLabel" Text="@UI/ScreenMatchmaking/TestingDescription"/>
                <Action type="SetProperty" frame="RankedDialogContainer" Visible="False"/>
                <Action type="SetProperty" frame="ContainerFrame/QueueTileList" Visible="False"/>
                <Action type="SetProperty" frame="ContainerFrame/ModeLabel" Visible="False"/>
                <Action type="SetProperty" frame="ContainerFrame/MapVetoButton" Visible="True"/>
                <Action type="SetProperty" frame="LoadingFrame/NoQueuesLabel" Text="@UI/ScreenMatchmaking/NoTestQueues"/>
            </State>
        </StateGroup>
            
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$Anchor_TopHeader" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$Anchor_LeftHeader" pos="Max" offset="0"/>
            <Style val="@Tournament_Title"/>
        </Frame>
        
        <Frame type="Label" name="SubtitleLabel">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="8"/>
            <Anchor side="Left" relative="$parent/TitleLabel" pos="Min" offset="-2"/>
            <Style val="@Tournament_Subtitle"/>
        </Frame>

        <Frame type="Button" name="PromotionDebugButton" template="StandardTemplates/StandardButtonTemplate" requiredtoload="IS_LEAGUE_PROMOTION_DEBUG_ENABLED">
            <Anchor side="Top" relative="$parent" pos="Min" offset="150"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-50"/>
            <Text val="Debug"/>
            <Toggleable val="true"/>
        </Frame>

        <Frame type="LeaguePromotionDebugFrame" name="LeaguePromotionDebugFrame" template="ScreenMatchmaking/LeaguePromotionDebugFrameTemplate" requiredtoload="IS_LEAGUE_PROMOTION_DEBUG_ENABLED">
            <Anchor side="Top" relative="$parent/PromotionDebugButton" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/PromotionDebugButton" pos="Max" offset="0"/>
            <Height val="800"/>
            <Width val="500"/>
            <Visible val="{$parent/PromotionDebugButton/@Toggled}"/>
         </Frame>

        <Frame type="Frame" name="LoadingFrame">
            <Anchor relative="$parent"/>

            <Frame type="Frame" name="IdleSpinner" template="StandardBattlenetTemplates/IdleSpinnerTemplate">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Visible val="false"/>

                <Animation name="StateLoading">
                    <Event event="Loading" action="Reset,Play" frame="$parent/$parent"/>
                    <Event event="Loaded" action="Reset,Stop" frame="$parent/$parent"/>

                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="false"/>
                        <Key type="Bool" time="1.5" value="true"/>
                        <Key type="Bool" time="6" value="false"/>
                    </Controller>

                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" time="1.5" value="0" out="Fast"/>
                        <Key type="Curve" time="1.8" value="255" out="Fast"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Label" name="NoQueuesLabel">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Visible val="false"/>
                <Style val="@Arcade_ListError"/>
                <Text val="@UI/ScreenMatchmaking/NoQueues"/>

                <Animation name="StateLoading">
                    <Event event="Loading" action="Reset,Play" frame="$parent/$parent"/>
                    <Event event="Loaded" action="Reset,Stop" frame="$parent/$parent"/>

                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="false"/>
                        <Key type="Bool" time="6" value="true"/>
                    </Controller>

                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" time="6" value="0" out="Fast"/>
                        <Key type="Curve" time="6.3" value="255" out="Fast"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>
        
        <Frame type="Frame" name="ContainerFrame">
            <Anchor relative="$parent"/>
            <Alpha val="0"/>
            <Visible val="false"/>

            <Animation name="StateLoading">
                <Event event="Loading" action="Reset,Stop" frame="$parent"/>
                <Event event="Loaded" action="Reset,Play" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time="0.2" value="0" out="Fast"/>
                    <Key type="Curve" time="0.5" value="255" in="Slow"/>
                </Controller>

                <Controller name="Visibility" type="Visibility" end="Pause">
                    <Key type="Bool" time="0.00" value="false"/>
                    <Key type="Bool" time="0.01" value="true"/>
                    <Key type="Bool" time="0.50" value="true"/>
                </Controller>
            </Animation>

            <Frame type="Label" name="ModeLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="309"/>
                <Anchor side="Left" relative="$parent/$parent/SubtitleLabel" pos="Min" offset="0"/>
                <Style val="@Tournament_DetailTitle"/>
                <Text val="@UI/ScreenMatchmaking/SelectMode"/>
                <CollapseLayout val="true"/>
            </Frame>

            <Frame type="TileList" name="QueueTileList">
                <Anchor side="Top" relative="$parent/ModeLabel" pos="Max" offset="3"/>
                <Anchor side="Left" relative="$parent/ModeLabel" pos="Min" offset="-1"/>
                <Width val="1260"/>
                <Height val="63"/>
                <CollapseLayout val="true"/>

                <Frame type="MatchmakingQueueTileListItem" name="Item0" template="StandardBattlenetTemplates/MatchmakingTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                </Frame>

                <Frame type="MatchmakingQueueTileListItem" name="Item1" template="StandardBattlenetTemplates/MatchmakingTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="6"/>
                </Frame>

                <Frame type="MatchmakingQueueTileListItem" name="Item2" template="StandardBattlenetTemplates/MatchmakingTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="6"/>
                </Frame>

                <Frame type="MatchmakingQueueTileListItem" name="Item3" template="StandardBattlenetTemplates/MatchmakingTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item2" pos="Max" offset="6"/>
                </Frame>

                <Frame type="MatchmakingQueueTileListItem" name="Item4" template="StandardBattlenetTemplates/MatchmakingTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item3" pos="Max" offset="6"/>
                </Frame>

                <Frame type="MatchmakingQueueTileListItem" name="Item5" template="StandardBattlenetTemplates/MatchmakingTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item4" pos="Max" offset="6"/>
                </Frame>
            </Frame>

            <Frame type="Label" name="RaceLabel">
                <Anchor side="Top" relative="$parent/QueueTileList" pos="Max" offset="21"/>
                <Anchor side="Left" relative="$parent/QueueTileList" pos="Min" offset="0"/>
                <Style val="@Tournament_DetailTitle"/>
                <Text val="@UI/ScreenMatchmaking/SelectRace"/>
            </Frame>

            <Frame type="MatchmakingRaceTileList" name="RaceTileList">
                <Anchor side="Top" relative="$parent/RaceLabel" pos="Max" offset="3"/>
                <Anchor side="Left" relative="$parent/RaceLabel" pos="Min" offset="-1"/>
                <Width val="1260"/>
                <Height val="63"/>

                <Frame type="RaceTileListItem" name="Item0" template="ScreenMatchmaking/MatchmakingRaceTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                </Frame>

                <Frame type="RaceTileListItem" name="Item1" template="ScreenMatchmaking/MatchmakingRaceTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="6"/>
                </Frame>

                <Frame type="RaceTileListItem" name="Item2" template="ScreenMatchmaking/MatchmakingRaceTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="6"/>
                </Frame>

                <Frame type="RaceTileListItem" name="Item3" template="ScreenMatchmaking/MatchmakingRaceTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item2" pos="Max" offset="6"/>
                </Frame>

                <Frame type="RaceTileListItem" name="Item4" template="ScreenMatchmaking/MatchmakingRaceTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item3" pos="Max" offset="6"/>
                </Frame>

                <Frame type="RaceTileListItem" name="Item5" template="ScreenMatchmaking/MatchmakingRaceTileListItemTemplate">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/Item4" pos="Max" offset="6"/>
                </Frame>

                <StateGroup name="RaceSelectEnabledGroup">
                    <State name="RaceSelectionMatched">
                        <When type="AnimationState" MatchmakingQueueState="MatchmakingStateRaceSelectionMatched"/>
                        <Action type="SetProperty" enabled="false"/>
                    </State>

                    <State name="RaceSelectionSoloSearching">
                        <When type="AnimationState" MatchmakingQueueState="MatchmakingStateRaceSelectionSearching"/>
                        <When type="AnimationState" IsSoloQueue="SoloQueueTrue"/>
                        <Action type="SetProperty" enabled="false"/>
                    </State>

                    <State name="TandemFollower">
                        <When type="AnimationState" IsTandemFollower="TandemFollowerTrue"/>
                        <Action type="SetProperty" enabled="false"/>
                    </State>

                    <State name="RaceSelectionEnabled">
                        <Action type="SetProperty" enabled="true"/>
                    </State>
                </StateGroup>
            </Frame>
            
            <StateGroup name="MatchmakingPanelsGroup">
                <State name="NotSelected">
                    <When type="AnimationState" frame="$parent" SelectionState="NotSelected"/>
                    <Action type="SetProperty" frame="LeaguePanel" visible="false"/>
                    <Action type="SetProperty" frame="VersusAIPanel" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel" visible="false"/>
                </State>
                
                <State name="SelectedModeTraining">
                    <When type="AnimationState" frame="$parent" SelectionState="Selected"/>
                    <When type="AnimationState" frame="$parent" ModeState="ModeTraining"/>
                    <Action type="SetProperty" frame="LeaguePanel" visible="false"/>
                    <Action type="SetProperty" frame="VersusAIPanel" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel" visible="true"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/IconImage" visible="true"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/IconImageFrameAnchor" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/QueueInfoPanelContainmentBG_Testing" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/ContainerBGBlur_Testing" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/QueueInfoPanelContainmentBG" visible="true"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/ContainerBGBlur" visible="true"/>
                </State>

                <State name="SelectedModeVersusAI">
                    <When type="AnimationState" frame="$parent" SelectionState="Selected"/>
                    <When type="AnimationState" frame="$parent" ModeState="ModeVersusAI"/>
                    <Action type="SetProperty" frame="LeaguePanel" visible="false"/>
                    <Action type="SetProperty" frame="VersusAIPanel" visible="true"/>
                    <Action type="SetProperty" frame="QueueInfoPanel" visible="false"/>
                </State>

                <State name="SelectedModeUnranked">
                    <When type="AnimationState" frame="$parent" SelectionState="Selected"/>
                    <When type="AnimationState" frame="$parent" ModeState="ModeUnranked"/>
                    <Action type="SetProperty" frame="LeaguePanel" visible="false"/>
                    <Action type="SetProperty" frame="VersusAIPanel" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel" visible="false"/>
                </State>

                <State name="SelectedModeRanked">
                    <When type="AnimationState" frame="$parent" SelectionState="Selected"/>
                    <When type="AnimationState" frame="$parent" ModeState="ModeRanked"/>
                    <Action type="SetProperty" frame="LeaguePanel" visible="true"/>
                    <Action type="SetProperty" frame="VersusAIPanel" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel" visible="false"/>
                </State>

                <State name="SelectedModeTesting">
                    <When type="AnimationState" frame="$parent" SelectionState="Selected"/>
                    <When type="AnimationState" frame="$parent" ModeState="ModeTesting"/>
                    <Action type="SetProperty" frame="LeaguePanel" visible="false"/>
                    <Action type="SetProperty" frame="VersusAIPanel" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel" visible="true"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/IconImage" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/IconImageFrameAnchor" visible="true"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/QueueInfoPanelContainmentBG_Testing" visible="true"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/ContainerBGBlur_Testing" visible="true"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/QueueInfoPanelContainmentBG" visible="false"/>
                    <Action type="SetProperty" frame="QueueInfoPanel/ContainerBGBlur" visible="false"/>
                </State>
            </StateGroup>

            <Frame type="MatchmakingLeaguePanel" name="LeaguePanel" template="MatchmakingLeaguePanel/MatchmakingLeaguePanelTemplate">
                <Anchor side="Top" relative="$parent/RaceTileList" pos="Max" offset="33"/>
                <Anchor side="Left" relative="$Anchor_LeftNoBuffer" pos="Min" offset="0"/>
                <Visible val="false"/>
            </Frame>

            <Frame type="MatchmakingQueueInfoPanel" name="QueueInfoPanel" template="MatchmakingQueueInfoPanel/MatchmakingQueueInfoPanelTemplate">
                <Anchor side="Top" relative="$parent/RaceTileList" pos="Max" offset="33"/>
                <Anchor side="Left" relative="$parent/RaceTileList" pos="Min" offset="-38"/>
                <Visible val="false"/>

                <StateGroup name="MatchmakingQueueInfoPanelGroup">
                    <State name="Stage1WoLState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId81"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage1"/>
                    </State>

                    <State name="Stage2WoLState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId82"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage2"/>
                    </State>

                    <State name="Stage3WoLState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId83"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage3"/>
                    </State>
                    
                    <State name="Stage1HotSState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId181"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage1"/>
                    </State>

                    <State name="Stage2HotSState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId182"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage2"/>
                    </State>

                    <State name="Stage3HotSState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId183"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage3"/>
                    </State>

                    <State name="Stage1LotVState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId281"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage1"/>
                    </State>

                    <State name="Stage2LotVState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId282"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage2"/>
                    </State>

                    <State name="Stage3LotVState">
                        <When type="AnimationState" frame="$this" QueueIdState="QueueId283"/>
                        <Action type="SetProperty" frame="IconImage" Texture="@UI/Glues_Icons_GameMode_Training_Stage3"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="MatchmakingVersusAIPanel" name="VersusAIPanel" template="MatchmakingVersusAIPanel/MatchmakingVersusAIPanelTemplate">
                <Anchor side="Top" relative="$parent/RaceTileList" pos="Max" offset="33"/>
                <Anchor side="Left" relative="$parent/RaceTileList" pos="Min" offset="-28"/>
                <Visible val="false"/>
            </Frame>

            <StateGroup name="SelectionStateGroup">
                <State name="NotSelectedState">
                    <When type="AnimationState" frame="$parent" SelectionState="NotSelected"/>
                    <Action type="SetProperty" frame="PlayButton" Enabled="false"/>
                    <Action type="SetProperty" frame="PlayButton" Visible="true"/>
                    <Action type="SetProperty" frame="MapVetoButton" Enabled="false"/>
                    <Action type="SetProperty" frame="MapProfileButton" Enabled="false"/>
                </State>
                
                <State name="SelectedState">
                    <When type="AnimationState" frame="$parent" SelectionState="Selected"/>
                    <Action type="SetProperty" frame="PlayButton" Enabled="true"/>
                    <Action type="SetProperty" frame="PlayButton" Visible="true"/>
                    <Action type="SetProperty" frame="MapVetoButton" Enabled="true"/>
                    <Action type="SetProperty" frame="MapProfileButton" Enabled="true"/>
                </State>
            </StateGroup>

            <Frame type="GlueButton" name="PlayButton" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
                <Anchor side="Bottom" relative="$Anchor_BottomActionButton" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$Anchor_LeftActionButton" pos="Max" offset="0"/>
                <Width val="288"/>
                <Text val="@UI/ScreenMatchmaking/PlayButton"/>
                <HoverSound val="@UI_HoverTick"/>
            </Frame>

            <Frame type="GlueButton" name="MapVetoButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
                <Anchor side="Top" relative="$parent/PlayButton" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayButton" pos="Max" offset="5"/>
                <Width val="340"/>
                <Visible val="false"/>
                <CollapseLayout val="true"/>
                <HoverSound val="@UI_HoverTick"/>
                <Animation name="SelectionState">
                    <Event event="NotSelected" action="Reset,Play" frame="$parent/$parent"/>

                    <Controller type="Property" property="Text" end="Pause">
                        <Key type="Property" time="0" value="@UI/ScreenMatchmaking/MapVetoButtonNoQueue"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="ViewMapProfileButton" name="MapProfileButton" template="StandardBattlenetTemplates/BattlenetSmallButtonTemplate">
                <Anchor side="Bottom" relative="$parent/QueueInfoPanel/QueueInfoPanelContainmentBG_Testing" pos="Max" offset="-35"/>
                <Anchor side="Left" relative="$parent/QueueInfoPanel/QueueInfoPanelContainmentBG_Testing" pos="Min" offset="45"/>
                <Text val="@UI/ScreenMatchmaking/ViewMapProfile"/>
                <Visible val="false"/>
                <Width val="200"/>
                <CollapseLayout val="true"/>

                <StateGroup name="SelectionStateGroup">
                    <DefaultState val="TestingInactiveState"/>

                    <State name="TestingActiveState">
                        <When type="AnimationState" frame="$parent/$parent" ModeState="ModeTesting"/>
                        <When type="AnimationState" frame="$parent/$parent" SelectionState="Selected"/>
                        <When type="AnimationState" frame="$this" ViewMapProfileButton="Active"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>
                    
                    <State name="TestingInactiveState">
                        <Action type="SetProperty" frame="$this" Visible="false"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="LaunchURLButton" name="ForumsButton" template="StandardBattlenetTemplates/BattlenetSmallButtonTemplate">
                <Anchor side="Bottom" relative="$parent/QueueInfoPanel/QueueInfoPanelContainmentBG_Testing" pos="Max" offset="-35"/>
                <Anchor side="Left" relative="$parent/MapProfileButton" pos="Max" offset="0"/>
                <Text val="@UI/ScreenMatchmaking/ViewForums"/>
                <Nydus val="forum/general"/>
                <Visible val="false"/>
                <Width val="200"/>

                <StateGroup name="SelectionStateGroup">
                    <DefaultState val="TestingInactiveState"/>
                    
                    <State name="TestingActiveState">
                        <When type="AnimationState" frame="$parent/$parent" ModeState="ModeTesting"/>
                        <When type="AnimationState" frame="$parent/$parent" SelectionState="Selected"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>

                    <State name="TestingInactiveState">
                        <Action type="SetProperty" frame="$this" Visible="false"/>
                    </State>
                </StateGroup>
            </Frame>
            
            <Frame type="ExpansionLevelFrame" name="ExpansionIconFrame">
                <Width val="82"/>
                <Height val="87"/>
                <Anchor side="Top" relative="$parent/PlayButton" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/PlayButton" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/MapVetoButton" pos="Max" offset="10"/>
                <AcceptsMouseTooltip val="true"/>
                
                <Frame type="Image" name="ExpansionIcon">
                    <Anchor relative="$parent"/>
                </Frame>

                <StateGroup name="ExpansionStateGroup">
                    <State name="WoLQueueTagState">
                        <When type="AnimationState" frame="$parent/$parent" ExpansionTag="TagWoL"/>
                        <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_WoL"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipWoL"/>
                    </State>

                    <State name="HotSQueueTagState">
                        <When type="AnimationState" frame="$parent/$parent" ExpansionTag="TagHotS"/>
                        <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_HoTS"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipHotS"/>
                    </State>

                    <State name="LotVQueueTagState">
                        <When type="AnimationState" frame="$parent/$parent" ExpansionTag="TagLotV"/>
                        <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_LotV"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipLotV"/>
                    </State>
                    
                    <State name="StarterEditionState">
                        <When type="AnimationState" frame="$this" ExpansionLevel="ExpansionLevel0"/>
                        <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_SE"/>
                        <Action type="SetProperty" frame="$this" Tooltip=""/>
                    </State>
                    
                    <State name="WoLState">
                        <When type="AnimationState" frame="$this" ExpansionLevel="ExpansionLevel1"/>
                        <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_WoL"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipWoL"/>
                    </State>
                    
                    <State name="HotSState">
                        <When type="AnimationState" frame="$this" ExpansionLevel="ExpansionLevel2"/>
                        <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_HoTS"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipHotS"/>
                    </State>
                    
                    <State name="LotVState">
                        <When type="AnimationState" frame="$this" ExpansionLevel="ExpansionLevel3"/>
                        <Action type="SetProperty" frame="ExpansionIcon" Texture="@UI/Glues_Icons_ExpansionLevel_Large_LotV"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/ExpansionLevelIcon/TooltipLotV"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="MatchmakingMapVetoFrame" name="MapVetoFrame" template="MatchmakingMapVetoFrame/MatchmakingMapVetoFrameTemplate">
                <Anchor relative="$parent"/>
                <Visible val="false"/>

                <Animation name="ToggleMapVetoListFrameVisibility">
                    <Event event="OnClick" action="ToggleDirection,Play" frame="$parent/MapVetoButton"/>
                    <Event event="OnHidden" action="DirectionForward,Play" frame="$parent/MapVetoButton"/>
                    <Event event="OnClick" action="DirectionForward,Play" frame="CancelButton"/>
                    <Event event="OnClick" action="DirectionForward,Play" frame="AcceptButton"/>
                    <Event event="LeftTournament" action="DirectionForward,Play"/>
                    <Event event="JoinedMatchQueue" action="DirectionForward,Play"/>
                    <Controller name="Name1" type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="true"/>
                        <Key type="Bool" time=".01" value="false"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>
        
        <Frame type="Frame" name="RankedDialogContainer">
            <Anchor relative="$parent"/>
            <Visible val="False"/>
        
            <Frame type="VersionNotificationFrame" name="LadderSplashDialogNotificationFrame">
                <Anchor relative="$parent"/>
                <Visible val="False"/>
                <Version val="1"/>
                <SettingName val="LadderSplashDialog"/>
                
                <Animation name="VersionState">
                    <Event event="NewVersion" action="Reset,Play"/>
                    <Event event="OldVersion" action="Reset,Stop"/>
                    <Event event="InvalidVersion" action="Reset,Stop"/>

                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="True"/>
                    </Controller>
                </Animation>

                <!-- The name of this animation must be "ClearNotification" -->
                <!-- When played, it will update the version of this notification  -->
                <Animation name="ClearNotification">
                    <Event event="OnClick" action="Reset,Play" frame="LadderSplashDialog/CloseButton"/>

                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="0.0" event="NoOp"/>
                        <Key type="Event" time="1.0" event="NoOp"/>
                    </Controller>
                </Animation>
                
                <Frame type="Frame" name="LadderSplashDialog" template="LadderSplashDialog/LadderSplashDialog"/>
            </Frame>
        </Frame>
    </Frame>
</Desc>
