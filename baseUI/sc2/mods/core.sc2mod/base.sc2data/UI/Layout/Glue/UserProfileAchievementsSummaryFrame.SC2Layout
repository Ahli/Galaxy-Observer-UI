<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="ProgressBar" name="ProgressBarTemplate">
        <Height val="58"/>
        <Color val="FFFFFF"/>
        <ColorStep val="FFFFFF"/>
        <UseFillContainer val="true"/>
        <CollapseLayout val="true"/>

        <Frame type="Image" name="BorderImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Coop_XPBar"/>
            <TextureCoords top="0.0" left="0.0" right="1.0" bottom="0.5"/>
        </Frame>

        <Frame type="Frame" name="FillImageContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Image" name="FillImage">
                <Anchor relative="$parent/$parent"/>
                <Texture val="@UI/SC2_UI_Coop_XPBar"/>
                <TextureCoords top="0.5" left="0.0" right="1.0" bottom="1.0"/>
            </Frame>
        </Frame>
    </Frame>

    <Frame type="AchievementCategoryFrame" name="CategoryPointsFrameTemplate">
        <Width val="180"/>
        <Height val="200"/>
        <AcceptsMouseTooltip val="true"/>
        <PresenceId val="{$UserProfileAchievementsFrame/@PresenceId}"/>
        <PointsRecurseChildren val="true"/>
        <TooltipFrame val="BattlenetTooltip"/>

        <Frame type="Image" name="IconImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="100"/>
            <Height val="100"/>
            <Texture val="{$parent/AchievementTier0Frame/@Texture}"/>
            <TextureCoords val="{$parent/AchievementTier0Frame/@TextureCoords}"/>
            
            <StateGroup name="TierStateGroup">
                <State name="Tier3State">
                    <When type="Property" frame="$parent/AchievementTier3Frame" Awarded="true"/>
                    <Action type="SetProperty" Texture="{$parent/AchievementTier3Frame/@Texture}"/>
                    <Action type="SetProperty" TextureCoords="{$parent/AchievementTier3Frame/@TextureCoords}"/>
                </State>
                
                <State name="Tier2State">
                    <When type="Property" frame="$parent/AchievementTier2Frame" Awarded="true"/>
                    <Action type="SetProperty" Texture="{$parent/AchievementTier2Frame/@Texture}"/>
                    <Action type="SetProperty" TextureCoords="{$parent/AchievementTier2Frame/@TextureCoords}"/>
                </State>
                
                <State name="Tier1State">
                    <When type="Property" frame="$parent/AchievementTier1Frame" Awarded="true"/>
                    <Action type="SetProperty" Texture="{$parent/AchievementTier1Frame/@Texture}"/>
                    <Action type="SetProperty" TextureCoords="{$parent/AchievementTier1Frame/@TextureCoords}"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="NameLabel">
            <Anchor side="Top" relative="$parent/IconImage" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$parent/@Name}"/>
        </Frame>

        <Frame type="Label" name="PointsLabel">
            <Anchor side="Top" relative="$parent/NameLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$parent/@PointsEarned}"/>
        </Frame>

        <Frame type="ProgressBar" name="ProgressBar" template="UserProfileAchievementsSummaryFrame/ProgressBarTemplate">
            <Anchor side="Top" relative="$parent/PointsLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <MaxValue val="{$parent/@PointsTotal}"/>
            <Value val="{$parent/@PointsEarned}"/>
        </Frame>

        <Frame type="AchievementInfoFrame" name="AchievementTier3Frame">
            <Anchor relative="$parent"/>
            <AchievementId val="{$parent/@FeaturedAchievementId}"/>
            <PresenceId val="{$parent/@PresenceId}"/>
        </Frame>

        <Frame type="AchievementInfoFrame" name="AchievementTier2Frame">
            <Anchor relative="$parent"/>
            <AchievementId val="{$parent/AchievementTier3Frame/@SupersedesAchievementId}"/>
            <PresenceId val="{$parent/@PresenceId}"/>
        </Frame>

        <Frame type="AchievementInfoFrame" name="AchievementTier1Frame">
            <Anchor relative="$parent"/>
            <AchievementId val="{$parent/AchievementTier2Frame/@SupersedesAchievementId}"/>
            <PresenceId val="{$parent/@PresenceId}"/>
        </Frame>

        <Frame type="AchievementInfoFrame" name="AchievementTier0Frame">
            <Anchor relative="$parent"/>
            <AchievementId val="{$parent/AchievementTier1Frame/@SupersedesAchievementId}"/>
            <PresenceId val="{$parent/@PresenceId}"/>
        </Frame>
    </Frame>

    <Frame type="Frame" name="CategoriesContainerFrameTemplate">
        <Anchor side="Right" relative="$this/ArcadeCategory" pos="Max" offset="20"/>
        <Height val="500"/>

        <Frame type="StandardTooltip" name="BattlenetTooltip" template="StandardTooltip/StandardBattlenetTooltipTemplate"/>
        
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_TableLargeHeaderBg"/>
            <TextureType val="NineSlice"/>
            <Tiled val="True"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="25"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="20"/>
            <Style val="@Arcade_Title"/>
            <Text val="@UI/UserProfileAchievementsFrame/CategoriesTitle"/>
        </Frame>
        
        <Frame type="AchievementCategoryFrame" name="LibertyCategory" template="UserProfileAchievementsSummaryFrame/CategoryPointsFrameTemplate">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="25"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <CategoryId val="4325379"/>
            <Tooltip val="@UI/UserProfileAchievementsFrame/CategoryTooltipLiberty"/>
        </Frame>
        
        <Frame type="AchievementCategoryFrame" name="SwarmCategory" template="UserProfileAchievementsSummaryFrame/CategoryPointsFrameTemplate">
            <Anchor side="Top" relative="$parent/LibertyCategory" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/LibertyCategory" pos="Max" offset="0"/>
            <CategoryId val="4325410"/>
            <Tooltip val="@UI/UserProfileAchievementsFrame/CategoryTooltipSwarm"/>
        </Frame>

        <Frame type="AchievementCategoryFrame" name="VoidCategory" template="UserProfileAchievementsSummaryFrame/CategoryPointsFrameTemplate">
            <Anchor side="Top" relative="$parent/SwarmCategory" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/SwarmCategory" pos="Max" offset="0"/>
            <CategoryId val="4330138"/>
            <Tooltip val="@UI/UserProfileAchievementsFrame/CategoryTooltipVoid"/>
        </Frame>

        <Frame type="AchievementCategoryFrame" name="MissionPacksCategory" template="UserProfileAchievementsSummaryFrame/CategoryPointsFrameTemplate">
            <Anchor side="Top" relative="$parent/VoidCategory" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/VoidCategory" pos="Max" offset="0"/>
            <CategoryId val="4364473"/>
            <Tooltip val="@UI/UserProfileAchievementsFrame/CategoryTooltipMissionPacks"/>
        </Frame>

        <!-- Row 2 -->
        <Frame type="AchievementCategoryFrame" name="MultiplayerCategory" template="UserProfileAchievementsSummaryFrame/CategoryPointsFrameTemplate">
            <Anchor side="Top" relative="$parent/LibertyCategory" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/LibertyCategory" pos="Min" offset="0"/>
            <CategoryId val="4325377"/>
            <Tooltip val="@UI/UserProfileAchievementsFrame/CategoryTooltipMultiplayer"/>
        </Frame>

        <Frame type="AchievementCategoryFrame" name="CoopCategory" template="UserProfileAchievementsSummaryFrame/CategoryPointsFrameTemplate">
            <Anchor side="Top" relative="$parent/MultiplayerCategory" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/MultiplayerCategory" pos="Max" offset="0"/>
            <CategoryId val="4386911"/>
            <Tooltip val="@UI/UserProfileAchievementsFrame/CategoryTooltipCoop"/>
        </Frame>

        <Frame type="AchievementCategoryFrame" name="CustomCategory" template="UserProfileAchievementsSummaryFrame/CategoryPointsFrameTemplate">
            <Anchor side="Top" relative="$parent/CoopCategory" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/CoopCategory" pos="Max" offset="0"/>
            <CategoryId val="4325382"/>
            <Tooltip val="@UI/UserProfileAchievementsFrame/CategoryTooltipCustom"/>
        </Frame>

        <Frame type="AchievementCategoryFrame" name="ArcadeCategory" template="UserProfileAchievementsSummaryFrame/CategoryPointsFrameTemplate">
            <Anchor side="Top" relative="$parent/CustomCategory" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/CustomCategory" pos="Max" offset="0"/>
            <CategoryId val="4325408"/>
            <Tooltip val="@UI/UserProfileAchievementsFrame/CategoryTooltipArcade"/>
        </Frame>
    </Frame>

    <Frame type="AchievementInfoFrame" name="RecentAchievementFrame">
        <Anchor side="Bottom" relative="$this/IconBorder" pos="Max" offset="10"/>
        <TooltipFrame val="AchievementInfoTooltip"/>
        <AcceptsMouseTooltip val="true"/>

        <Frame type="Image" name="Icon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="13"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Width val="82"/>
            <Height val="82"/>
        </Frame>

        <Frame type="Image" name="IconBorder">
            <Anchor side="Top" relative="$parent/Icon" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Icon" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/Icon" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/Icon" pos="Mid" offset="0"/>
            <Width val="94"/>
            <Height val="94"/>
            <TextureType val="NineSlice"/>
            <Texture val="@UI/SC2_UI_Collection_AchievementIconFrame_Normal"/>
        </Frame>

        <Frame type="Label" name="Title">
            <Anchor side="Top" relative="$parent" pos="Min" offset="9"/>
            <Anchor side="Left" relative="$parent/IconBorder" pos="Max" offset="9"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-9"/>
            <Style val="@Collection_ItemFrame_AchievementInfo_Name"/>
            <Text val="{$parent/@TitleText}"/>
        </Frame>
    </Frame>
    
    <Frame type="UserProfileAchievementsListBoxItem" name="RecentAchievementItem">
        <Frame type="AchievementInfoFrame" name="AchievementFrame" template="UserProfileAchievementsSummaryFrame/RecentAchievementFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <AchievementId val="{$parent/@AchievementId}"/>
            <PresenceId val="{$BattleUI/@LocalToonPresenceId}"/>
            <CollapseLayout val="false"/>
            <Visible val="true"/>
        </Frame>

        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AchievementFrame" pos="Max" offset="0"/>
        </Frame>
    </Frame>

    <Frame type="AchievementShowcaseFrame" name="AchievementShowcaseFrame">
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Width val="130"/>
        <PresenceId val="{$UserProfileAchievementsFrame/@PresenceId}"/>

        <StateGroup name="LocalUserStateGroup">
            <State name="LocalUserState">
                <When type="Property" frame="$BattleUI" operator="NotEqual" LocalToonPresenceId="{$UserProfileAchievementsFrame/@PresenceId}"/>
                <Action type="SetProperty" frame="EditButton" Visible="false"/>
            </State>
        </StateGroup>

        <Frame type="AchievementInfoFrame" name="AchievementFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="130"/>
            <PresenceId val="{$parent/@PresenceId}"/>
            <AchievementId val="{$parent/@AchievementId}"/>
            <AcceptsMouseTooltip val="true"/>
            <TooltipFrame val="AchievementInfoTooltip"/>

            <Frame type="Image" name="Icon">
                <Anchor relative="$parent"/>
                <Texture val="{$parent/@Texture}"/>
                <TextureCoords val="{$parent/@TextureCoords}"/>
            </Frame>
        </Frame>
        
        <Frame type="Button" name="EditButton" template="StandardBattlenetTemplates/OptionsButton">
            <Anchor side="Top" relative="$parent/AchievementFrame" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AchievementFrame" pos="Max" offset="32"/>
            <Anchor side="Left" relative="$parent/AchievementFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AchievementFrame" pos="Max" offset="0"/>
            <Width val="0"/>
            <Height val="0"/>
            
            <Frame type="Image" name="NormalImage">
                <Anchor side="Top" relative="$this" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Width val="32"/>
                <Height val="32"/>
            </Frame>

            <Frame type="Image" name="HoverImage">
                <Anchor side="Top" relative="$this" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Width val="32"/>
                <Height val="32"/>
            </Frame>
        </Frame>
    
        <Frame type="Button" name="ViewAchievementButton">
            <Anchor relative="$parent/AchievementFrame"/>
        </Frame>
            
        <StateGroup name="AchievementStateGroup">
            <State name="AchievementState">
                <When type="Property" frame="$this" AchievementId="0"/>
                <Action type="SetProperty" frame="AchievementFrame" AcceptsMouseTooltip="false"/>
                <Action type="SetProperty" frame="ViewAchievementButton" Visible="false"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="Frame" name="AchievementShowcaseContainer">
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_TableLargeHeaderBg"/>
            <TextureType val="NineSlice"/>
            <Tiled val="True"/>
            <Visible val="true"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="25"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="20"/>
            <Style val="@Arcade_Title"/>
            <Text val="@UI/UserProfileAchievementsFrame/Showcase"/>
        </Frame>
        
        <Frame type="AchievementShowcaseFrame" name="ShowcaseFrame0" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="80"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <ShowcaseIndex val="0"/>
        </Frame>

        <Frame type="AchievementShowcaseFrame" name="ShowcaseFrame1" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseFrame">
            <Anchor side="Top" relative="$parent/ShowcaseFrame0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/ShowcaseFrame0" pos="Max" offset="10"/>
            <ShowcaseIndex val="1"/>
        </Frame>

        <Frame type="AchievementShowcaseFrame" name="ShowcaseFrame2" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseFrame">
            <Anchor side="Top" relative="$parent/ShowcaseFrame1" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/ShowcaseFrame1" pos="Max" offset="10"/>
            <ShowcaseIndex val="2"/>
        </Frame>

        <Frame type="AchievementShowcaseFrame" name="ShowcaseFrame3" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseFrame">
            <Anchor side="Top" relative="$parent/ShowcaseFrame2" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/ShowcaseFrame2" pos="Max" offset="10"/>
            <ShowcaseIndex val="3"/>
        </Frame>
        
        <Frame type="AchievementShowcaseFrame" name="ShowcaseFrame4" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseFrame">
            <Anchor side="Top" relative="$parent/ShowcaseFrame3" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/ShowcaseFrame3" pos="Max" offset="10"/>
            <ShowcaseIndex val="4"/>
        </Frame>
    </Frame>

    <Frame type="AchievementShowcaseTileListItem" name="AchievementShowcaseTileListItem">
        <Anchor side="Bottom" relative="$this/AchievementFrame" pos="Max" offset="0"/>
        <Height val="0"/>
        
        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
