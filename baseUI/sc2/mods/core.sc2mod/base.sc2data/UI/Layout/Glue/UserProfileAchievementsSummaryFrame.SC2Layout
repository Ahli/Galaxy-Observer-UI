        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Over"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Selected"/>
            <TextureType val="NineSlice"/>
        </Frame>
        
        <Frame type="AchievementInfoFrame" name="AchievementFrame" template="UserProfileAchievementsSummaryFrame/RecentAchievementFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <PresenceId val="{$BattleUI/@LocalToonPresenceId}"/>
            <AchievementId val="{$parent/@AchievementId}"/>
        </Frame>
    </Frame>

    <Frame type="AchievementShowcaseSelectionFrame" name="AchievementShowcaseSelectionFrame">
        <!-- This tooltip is just for blocking tooltips behind this panel -->
        <Frame type="StandardTooltip" name="BackgroundTooltip">
            <Frame type="Label" name="Label">
                <Anchor relative="$parent"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="FullscreenBlackoutImage">
            <Anchor relative="$parent"/>
            <Color val="0,0,0"/>
            <Alpha val="180"/>
            <AcceptsMouse val="true"/>
            <TooltipFrame val="BackgroundTooltip"/>
        </Frame>
        
        <Frame type="Image" name="LeftBGGlowPattern">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Min" offset="40"/>
            <Texture val="@UI/Glues_Battlenet_DialogBox_BackGlow"/>
        </Frame>

        <Frame type="Image" name="RightBGGlowPattern">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Max" offset="-40"/>
            <Texture val="@UI/Glues_Battlenet_DialogBox_BackGlow"/>
            <TextureCoords top="0.0" left="1.0" bottom="1.0" right="0.0"/>
        </Frame>

        <Frame type="Image" name="BackgroundImage">
            <Anchor relative="$parent/AnchorFrame"/>
            <Texture val="@UI/Glues_Battlenet_DialogBox_StandardBG"/>
            <TextureType val="NineSlice"/>
            <Tiled val="false"/>
        </Frame>
        
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="700"/>
            <Height val="900"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="30"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="30"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-30"/>
            <Style val="@Arcade_Title"/>
            <Text val="@UI/AchievementShowcaseSelectionFrame/Title"/>
        </Frame>
        
        <Frame type="Pulldown" name="CategoryPulldown" template="StandardBattlenetTemplates/StandardBattlenetPulldownTemplate">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="20"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="20"/>
        </Frame>

        <Frame type="Pulldown" name="SubCategoryPulldown" template="StandardBattlenetTemplates/StandardBattlenetPulldownTemplate">
            <Anchor side="Top" relative="$parent/CategoryPulldown" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/CategoryPulldown" pos="Max" offset="10"/>
            <Visible val="false"/>

            <StateGroup name="VisibleStateGroup">
                <State name="VisibleState">
                    <When type="Property" operator="GreaterThan" DataCount="1"/>
                    <Action type="SetProperty" Visible="true"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="TileList" name="TileList">
            <Anchor side="Top" relative="$parent/CategoryPulldown" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-20"/>
            <Anchor side="Bottom" relative="$parent/AcceptButton" pos="Min" offset="0"/>
            <AcceptsMouse val="true"/>

            <Frame type="AchievementShowcaseTileListItem" name="Item0" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseTileListItem">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="AchievementShowcaseTileListItem" name="Item1" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseTileListItem">
                <Anchor side="Top" relative="$parent/Item0" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="AchievementShowcaseTileListItem" name="Item2" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseTileListItem">
                <Anchor side="Top" relative="$parent/Item1" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="AchievementShowcaseTileListItem" name="Item3" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseTileListItem">
                <Anchor side="Top" relative="$parent/Item2" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="AchievementShowcaseTileListItem" name="Item4" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseTileListItem">
                <Anchor side="Top" relative="$parent/Item3" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="AchievementShowcaseTileListItem" name="Item5" template="UserProfileAchievementsSummaryFrame/AchievementShowcaseTileListItem">
                <Anchor side="Top" relative="$parent/Item4" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="ScrollBar" name="ScrollBar">
                <Anchor side="Top" relative="$parent" pos="Min" offset="-3"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-3"/>

                <Image val="$root/StandardBattlenetTemplates/GluesSocialScrollBarBackgroundImage"/>
                <Button val="$root/StandardBattlenetTemplates/GluesSocialScrollBarButtonTemplate"/>
                <Orientation val="Vertical"/>
                <Width val="16"/>
            </Frame>
        </Frame>

        <Frame type="Button" name="AcceptButton" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Mid" offset="-5"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="-20"/>
            <Text val="@UI/StandardDialog_Ok_Control"/>
            <Width val="280"/>
