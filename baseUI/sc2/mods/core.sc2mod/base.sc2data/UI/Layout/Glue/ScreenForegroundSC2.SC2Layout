            <Frame type="AspectRatioFrame" name="Chat16x9Anchor">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-753"/>
                <Width val="213"/>
                <CollapseLayout val="true"/>
                <MinVisibleAspectRatio val="1.75"/>
            </Frame>
            
            <Frame type="ChatPanel" name="ChatPanel" template="ChatPanel/ChatPanelTemplate">
                <Anchor side="Left" relative="$parent/Chat16x9Anchor" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

                <!-- The Maximize Animation. It's important not to change the name because it is referenced by code -->
                <Animation name="MaximizeChat">
                    <Event event="MaximizeChat" action="DirectionForward"/>
                    <Event event="MaximizeChat" action="Play"/>
                    <Event event="MinimizeChat" action="DirectionReverse"/>
                    <Event event="MinimizeChat" action="Play"/>
                    <Event event="OnClick" action="DirectionForward" frame="ChatManagerPanel/MaximizeChatButton"/>
                    <Event event="OnClick" action="Play" frame="ChatManagerPanel/MaximizeChatButton"/>
                    <Event event="OnClick" action="DirectionReverse" frame="ChatManagerPanel/MinimizeChatButton"/>
                    <Event event="OnClick" action="Play" frame="ChatManagerPanel/MinimizeChatButton"/>
                    <Event event="EditBoxFocusLostImmediate" action="Reset,Stop"/>

                    <Controller type="Anchor" end="Pause" side="Top" relative="true">
                        <Key type="Curve" time="0.0" value="0" out="Step"/>
                        <Key type="Curve" time="0.1" value="-426" out="Step"/>
                    </Controller>
                    
                    <Controller type="Event" end="Pause" frame="ChatManagerPanel/MaximizeChatButton">
                        <Key type="Event" time="0.0" event="MinimizeChat"/>
                        <Key type="Event" time="0.1" event="MaximizeChat"/>
                    </Controller>

                    <Controller type="Event" end="Pause" frame="ChatManagerPanel/MinimizeChatButton">
                        <Key type="Event" time="0.0" event="MinimizeChat"/>
                        <Key type="Event" time="0.1" event="MaximizeChat"/>
                    </Controller>

                    <Controller type="Event" end="Pause">
                        <Key type="Event" time="0.0" event="ChatMaximizeStart"/>
                        <Key type="Event" time="0.1" event="ChatMaximizeEnd"/>
                    </Controller>
                </Animation>

                <!-- The Fade Lock Animation. It's important not to change the name because it is referenced by code -->
                <!-- This decouples the fade behavior from chat maximize, it uses a dummy controller to notify code of animation events -->
                <!-- We are disabling this behavior for now -->
                
                <Animation name="FadeLock">
                    <Event event="LockFade" action="DirectionForward"/>
                    <Event event="LockFade" action="Play"/>
                    <Event event="UnlockFade" action="DirectionReverse"/>
                    <Event event="UnlockFade" action="Play"/>
                    <Event event="OnClick" action="DirectionForward" frame="ChatManagerPanel/LockFadeButton"/>
                    <Event event="OnClick" action="Play" frame="ChatManagerPanel/LockFadeButton"/>
                    <Event event="OnClick" action="DirectionReverse" frame="ChatManagerPanel/UnlockFadeButton"/>
                    <Event event="OnClick" action="Play" frame="ChatManagerPanel/UnlockFadeButton"/>
                    <Event event="EditBoxFocusLostImmediate" action="Reset,Stop"/>

                    <Controller type="Event" end="Pause">
                        <Key type="Event" time="0.0" event="NoOp"/>
                        <Key type="Event" time="0.1" event="NoOp"/>
                    </Controller>

                    <Controller type="Event" end="Pause" frame="ChatManagerPanel/LockFadeButton">
                        <Key type="Event" time="0.0" event="UnlockFade"/>
                        <Key type="Event" time="0.1" event="LockFade"/>
                    </Controller>

                    <Controller type="Event" end="Pause" frame="ChatManagerPanel/UnlockFadeButton">
                        <Key type="Event" time="0.0" event="UnlockFade"/>
                        <Key type="Event" time="0.1" event="LockFade"/>
                    </Controller>
                </Animation>

                <Frame type="ChatManagerPanel" name="ChatManagerPanel">
                    <Frame type="Button" name="MaximizeChatButton" template="StandardBattlenetTemplates/BattlenetIconButtonTemplate">
                        <Anchor side="Right" relative="$parent" pos="Max" offset="-13"/>
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Width val="36"/>
                        <Height val="34"/>

                        <Frame type="Image" name="NormalImage">
                            <Texture val="@UI/Glues_Icons_Fullscreen"/>
                        </Frame>

                        <Frame type="Image" name="HoverImage">
                            <Texture val="@UI/Glues_Icons_Fullscreen_Hover"/>
                        </Frame>

                        <Animation name="ToggleButton">
                            <Event event="MaximizeChat" action="Reset"/>
                            <Event event="MaximizeChat" action="Play"/>
                            <Event event="MinimizeChat" action="Reset"/>
                            <Event event="MinimizeChat" action="Stop"/>

                            <Controller type="Visibility" end="Pause">
                                <Key type="Bool" time="0" value="false"/>
                            </Controller>
                        </Animation>
                    </Frame>

                    <Frame type="Button" name="MinimizeChatButton" template="StandardBattlenetTemplates/BattlenetIconButtonTemplate">
                        <Anchor relative="$parent/MaximizeChatButton"/>
                        <Visible val="false"/>

                        <Frame type="Image" name="NormalImage">
                            <Texture val="@UI/Glues_Icons_Minimize"/>
                        </Frame>

                        <Frame type="Image" name="HoverImage">
                            <Texture val="@UI/Glues_Icons_Minimize_Hover"/>
                        </Frame>

                        <Animation name="ToggleButton">
                            <Event event="MaximizeChat" action="Reset"/>
                            <Event event="MaximizeChat" action="Play"/>
                            <Event event="MinimizeChat" action="Reset"/>
                            <Event event="MinimizeChat" action="Stop"/>

                            <Controller type="Visibility" end="Pause">
                                <Key type="Bool" time="0" value="true"/>
                            </Controller>
                        </Animation>
                    </Frame>
                    
                    <Frame type="Button" name="LockFadeButton" template="StandardBattlenetTemplates/BattlenetIconButtonTemplate">
                        <Anchor side="Right" relative="$parent/MinimizeChatButton" pos="Min" offset="-2"/>
                        <Anchor side="Top" relative="$parent/MinimizeChatButton" pos="Min" offset="1"/>
                        <Width val="32"/>
                        <Height val="31"/>

                        <Frame type="Image" name="NormalImage">
                            <Texture val="@UI/Glues_Icons_Pin"/>
                        </Frame>

                        <Frame type="Image" name="HoverImage">
                            <Texture val="@UI/Glues_Icons_Pin_Hover"/>
                        </Frame>

                        <Animation name="ToggleButton">
                            <Event event="LockFade" action="Reset"/>
                            <Event event="LockFade" action="Play"/>
                            <Event event="UnlockFade" action="Reset"/>
                            <Event event="UnlockFade" action="Stop"/>

                            <Controller type="Visibility" end="Pause">
                                <Key type="Bool" time="0" value="false"/>
                            </Controller>
                        </Animation>
                    </Frame>

                    <Frame type="Button" name="UnlockFadeButton" template="StandardBattlenetTemplates/BattlenetIconButtonTemplate">
                        <Anchor relative="$parent/LockFadeButton"/>
                        <Visible val="false"/>

                        <Frame type="Image" name="NormalImage">
                            <Texture val="@UI/Glues_Icons_Unpin"/>
                        </Frame>

                        <Frame type="Image" name="HoverImage">
                            <Texture val="@UI/Glues_Icons_Unpin_Hover"/>
                        </Frame>

                        <Animation name="ToggleButton">
                            <Event event="LockFade" action="Reset"/>
                            <Event event="LockFade" action="Play"/>
                            <Event event="UnlockFade" action="Reset"/>
                            <Event event="UnlockFade" action="Stop"/>

                            <Controller type="Visibility" end="Pause">
                                <Key type="Bool" time="0" value="true"/>
                            </Controller>
                        </Animation>
                    </Frame>
                </Frame>

                <!-- Align edit box with rest of buttons -->
                <Frame type="Frame" name="ChatEditBoxFrame">
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/$parent/SocialButton" pos="Min" offset="7"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                </Frame>
            </Frame>

            <Frame type="SocialPanel" name="SocialPanel" template="SocialPanel/SocialPanelTemplate">
                <Anchor side="Bottom" relative="$parent/$parent" pos="Max" offset="-56"/>
                <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="0"/>
                <Height val="910"/>
