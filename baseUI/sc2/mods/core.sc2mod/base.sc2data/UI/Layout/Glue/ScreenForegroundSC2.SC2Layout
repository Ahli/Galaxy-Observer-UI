        <Frame type="MatchmakingInvitePanel" name="MatchmakingInvitePanel" template="MatchmakingInvitePanel/MatchmakingInvitePanelTemplate">
            <Anchor relative="$parent"/>
        </Frame>

        <Frame type="Frame" name="SocialContainer">
