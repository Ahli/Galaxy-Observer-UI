                <Frame type="IGRBenefitsPanel" name="IGRBenefitHeaderPanel" template="IGRBenefitsPanel/IGRBenefitHeaderTemplate">
                    <Frame type="Label" name="IGRBenefitTitle">
                        <Visible val="False"/>
                    </Frame>

                    <Frame type="Label" name="IGRBenefitDescription">
