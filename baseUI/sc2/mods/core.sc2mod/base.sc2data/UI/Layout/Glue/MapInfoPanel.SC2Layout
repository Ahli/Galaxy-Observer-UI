<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="MapInfoPanel" name="MapInfoPanelTemplate">
        <Frame type="Minimap" name="MinimapImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Width val="128"/>
            <Height val="128"/>
        </Frame>

        <Frame type="Label" name="NameLabel">
            <Anchor side="Top" relative="$parent/MinimapImage" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/MinimapImage" pos="Max" offset="10"/>
            <Style val="GlueLabel"/>
            <Text val="@UI/Name_Colon"/>
        </Frame>

        <Frame type="Label" name="NameValue">
            <Anchor side="Top" relative="$parent/NameLabel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/NameLabel" pos="Max" offset="10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Options val="NoWrapping"/>            
            <Style val="GlueValue"/>
        </Frame>
        
        <Frame type="Label" name="SizeLabel">
            <Anchor side="Top" relative="$parent/NameLabel" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/NameLabel" pos="Min" offset="0"/>
            <Style val="GlueLabel"/>
            <Text val="@UI/Size_Colon"/>
        </Frame>

        <Frame type="Label" name="SizeValue">
            <Anchor side="Top" relative="$parent/SizeLabel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/SizeLabel" pos="Max" offset="10"/>
            <Style val="GlueValue"/>
        </Frame>

        <Frame type="Label" name="TileLabel">
            <Anchor side="Top" relative="$parent/SizeLabel" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/SizeLabel" pos="Min" offset="0"/>
            <Style val="GlueLabel"/>
            <Text val="@UI/Tile_Colon"/>
        </Frame>

        <Frame type="Label" name="TileValue">
            <Anchor side="Top" relative="$parent/TileLabel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TileLabel" pos="Max" offset="10"/>
            <Style val="GlueValue"/>
        </Frame>

        <Frame type="Label" name="DescriptionLabel">
            <Anchor side="Top" relative="$parent/TileLabel" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/TileLabel" pos="Min" offset="0"/>
            <Style val="GlueLabel"/>
            <Text val="@UI/Description_Colon"/>
        </Frame>

        <Frame type="Label" name="DescriptionValue">
            <Anchor side="Top" relative="$parent/DescriptionLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/DescriptionLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-5"/>
            <Style val="GlueValue"/>
        </Frame>
    </Frame>
</Desc>
