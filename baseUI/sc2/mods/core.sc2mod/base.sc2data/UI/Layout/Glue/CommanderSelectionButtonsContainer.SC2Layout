    <Frame type="CommanderTileListItem" name="CommanderTileListItemBottomTemplate" template="CommanderSelectionButtonsContainer/CommanderTileListItemTemplate">
    
        <!-- AllyNameArrow -->
        <Frame type="Image" name="AllyNameArrow">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Max" offset="-24"/>
            <Anchor side="Bottom" relative="$this" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <TextureCoords top="1.0" left="0.0" bottom="0.0" right="1.0"/>
            
            <!-- AllyNameArrowShimmer -->
            <Frame type="Image" name="AllyNameArrowShimmer">
                <Anchor relative="$parent"/>
                <DesaturateOnDisable val="false"/>
                <LayerCount val="2"/>
                <AlphaLayer val="True" layer="0"/>
                <Visible val="True"/>
                <Texture val="@UI/SC2_UI_Coop_Nameplate_Arrow_Shimmer" layer="0"/>
                <Texture val="@UI/SC2_UI_Coop_Nameplate_Arrow_Ally_Glow" layer="1"/>
                <TextureCoords top="1.0" left="0.0" bottom="0.0" right="1.0" layer="0"/>
                <TextureCoords top="1.0" left="0.0" bottom="0.0" right="1.0" layer="1"/>

                <Animation name="Slide">
                    <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                    <Event event="OnShown" action="Play" frame="$parent"/>
             
                    <Controller type="LayerUV" layer="0" side="Top" relative="False" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="1" out="fast"/>
                        <Key type="Curve" time="2" value="-1" in="fast"/>
                        <Key type="Curve" time="2.5" value="-1" in="fast"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>
        
