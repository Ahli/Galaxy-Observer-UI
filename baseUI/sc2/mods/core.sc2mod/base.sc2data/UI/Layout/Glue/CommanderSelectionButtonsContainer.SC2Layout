            <Anchor side="Left" relative="$parent" pos="Mid" offset="1"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="1"/>
            <Anchor side="Top" relative="$parent/AllyNameArrow" pos="Mid" offset="31"/>
            <Anchor side="Bottom" relative="$parent/AllyNameArrow" pos="Mid" offset="31"/>
        </Frame>
    </Frame>

    
    
