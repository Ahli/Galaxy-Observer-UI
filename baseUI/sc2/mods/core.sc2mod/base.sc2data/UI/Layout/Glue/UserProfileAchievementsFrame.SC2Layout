            <Anchor side="Left" relative="$parent/AchievementSeriesFrame8" pos="Max" offset="9"/>
            <AchievementId val="{$parent/AchievementSeriesFrame6/@SupersedesAchievementId}"/>
        </Frame>

        <Frame type="AchievementInfoFrame" name="AchievementSeriesFrame8" template="UserProfileAchievementsFrame/AchievementSeriesFrameTemplate">
            <Anchor side="Top" relative="$parent/AchievementSeriesFrame9" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/AchievementSeriesFrame9" pos="Max" offset="9"/>
            <AchievementId val="{$parent/AchievementSeriesFrame7/@SupersedesAchievementId}"/>
        </Frame>

        <Frame type="AchievementInfoFrame" name="AchievementSeriesFrame9" template="UserProfileAchievementsFrame/AchievementSeriesFrameTemplate">
            <Anchor side="Top" relative="$parent/AchievementSeriesFrame19" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/AchievementSeriesFrame19" pos="Min" offset="0"/>
