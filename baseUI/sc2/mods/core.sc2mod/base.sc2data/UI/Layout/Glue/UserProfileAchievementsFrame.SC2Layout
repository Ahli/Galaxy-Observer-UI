                <When type="Property" frame="ExpandFrame/CriteriaContainerFrame/CriteriaRow0/CriteriaFrameRight" CriteriaId="0"/> <!-- We check the Right criteria because we need more than 1 to display -->
                <When type="Property" SupersedesAchievementId="0"/>
                <When type="Property" SeriesTailAchievementId="0"/>
            </State>
            
            <State name="HasContent"/>
        </StateGroup>
        
