    <!--
    ============================
    Achievements List Box Item Template
    ============================
    
    The containing list item for an achievement
    -->

    <Frame type="UserProfileAchievementsListBoxItem" name="AchievementsListBoxItemTemplate">
        <Frame type="Frame" name="HitTestFrame">
            <Anchor relative="$parent/AchievementFrame"/>
        </Frame>
        
        <Frame type="AchievementInfoFrame" name="CompareAchievementFrame" template="UserProfileAchievementsFrame/CompareAchievementFrameTemplate">
            <Anchor side="Top" relative="$parent/AchievementFrame" pos="Min" offset="1"/>
            <Anchor side="Left" relative="$ancestor[@type=ListBox]/ListBoxComparisonBackground" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$ancestor[@type=ListBox]/ListBoxComparisonBackground" pos="Max" offset="0"/>
            <AchievementId val="{$parent/@AchievementId}"/>
            <PresenceId val="{$BattleUI/@LocalToonPresenceId}"/>
            <Visible val="false"/>
        </Frame>
        
        <Frame type="AchievementInfoFrame" name="AchievementFrame" template="UserProfileAchievementsFrame/AchievementInfoFrameTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="810"/>
            <AchievementId val="{$parent/@AchievementId}"/>
            <PresenceId val="{$UserProfileAchievementsFrame/@PresenceId}"/>
        </Frame>

        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AchievementFrame" pos="Max" offset="0"/>
        </Frame>

        <StateGroup name="LocalUserState">
            <State name="LocalUserState">
                <When type="Property" frame="$BattleUI" operator="NotEqual" LocalToonPresenceId="{$UserProfileAchievementsFrame/@PresenceId}"/>
                <Action type="SetProperty" frame="CompareAchievementFrame" Visible="true"/>
            </State>
        </StateGroup>
    </Frame>
    
