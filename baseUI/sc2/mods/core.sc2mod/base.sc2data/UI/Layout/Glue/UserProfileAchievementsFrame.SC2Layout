            <Anchor side="Left" relative="$parent/PointsLabel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/PointsLabel" pos="Mid" offset="0"/>
            <Texture val="@UI/SC2_UI_UserProfile_Achievements_FeatIcon"/>
            
            <StateGroup name="VisibilityState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="Property" frame="$parent" SeriesPoints="0"/>
                    <Action type="SetProperty" visible="True"/>
                </State>
                
