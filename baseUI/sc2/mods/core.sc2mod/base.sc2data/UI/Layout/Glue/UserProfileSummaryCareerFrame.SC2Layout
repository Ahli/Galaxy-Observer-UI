<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="UserProfileSummaryCareerFrame" name="UserProfileSummaryCareerFrame">
        <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-12"/>
        <Height val="70"/>
        <PresenceId val="{$ScreenUserProfile/@PresenceId}"/>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
            <Alpha val="255"/>
        </Frame>
        
        <Frame type="MatchmakingLeagueIcon" name="LeagueIcon" template="MatchmakingLeagueIcon/MatchmakingLeagueIconMiniTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
            <League val="{$parent/@League}"/>
            <AcceptsMouseTooltip val="true"/>
            <Tooltip val="{$this/TooltipFormatFrame/@Text}"/>

            <Frame type="TextFormatFrame" name="TooltipFormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/ScreenUserProfile/SummaryCareer/Tooltip"/>
                <Replacement index="0" val="{$parent/@LeagueName}"/>
                <Replacement index="1" val="{$parent/$parent/@EarnedCount}"/>
            </Frame>
        </Frame>

        <Frame type="TextFormatFrame" name="LeagueFormatFrame">
            <Anchor relative="$parent"/>
            <Replacement index="0" val="{$parent/LeagueIcon/@LeagueName}"/>
        </Frame>

        <Frame type="Label" name="LeagueLabel">
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LeagueIcon" pos="Max" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$parent/LeagueFormatFrame/@Text}"/>
        </Frame>

        <Frame type="Label" name="UnrankedLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LeagueIcon" pos="Max" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="@UI/ScreenUserProfile/SummaryCareer/NoCareerFinishes"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Label" name="SeasonLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LeagueIcon" pos="Max" offset="10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$this/SeasonFrame/@SeasonName}"/>

            <Frame type="MatchmakingSeasonFrame" name="SeasonFrame">
                <Anchor relative="$parent"/>
                <SeasonId val="{$parent/$parent/@SeasonId}"/>
            </Frame>
        </Frame>

        <StateGroup name="UnrankedStateGroup">
            <State name="UnrankedState">
                <When type="Property" EarnedCount="0"/>
                <Action type="SetProperty" frame="UnrankedLabel" Visible="true"/>
                <Action type="SetProperty" frame="SeasonLabel" Visible="false"/>
                <Action type="SetProperty" frame="LeagueIcon" AcceptsMouseTooltip="false"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="Frame" name="UserProfileSummaryCareerContainerFrame">
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_TableLargeHeaderBg"/>
            <TextureType val="NineSlice"/>
            <Tiled val="True"/>
            <Visible val="true"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Text val="@UI/ScreenUserProfile/SummaryCareer/Title"/>
            <Style val="Tournament_TrophyCaseTitle"/>
        </Frame>
        
        <Frame type="UserProfileSummaryCareerFrame" name="SoloCareerFrame" template="UserProfileSummaryCareerFrame/UserProfileSummaryCareerFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="80"/>
            <IsSoloQueue val="true"/>

            <Frame type="TextFormatFrame" name="LeagueFormatFrame">
                <FormatText val="@UI/ScreenUserProfile/SummaryCareer/LeagueSolo"/>
            </Frame>
        </Frame>
        
        <Frame type="UserProfileSummaryCareerFrame" name="TeamCareerFrame" template="UserProfileSummaryCareerFrame/UserProfileSummaryCareerFrame">
            <Anchor side="Top" relative="$parent/SoloCareerFrame" pos="Max" offset="0"/>
            <IsSoloQueue val="false"/>

            <Frame type="TextFormatFrame" name="LeagueFormatFrame">
                <FormatText val="@UI/ScreenUserProfile/SummaryCareer/LeagueTeam"/>
            </Frame>
        </Frame>
    </Frame>
</Desc>
