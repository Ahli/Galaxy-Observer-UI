            <Anchor side="Right" relative="$parent/$parent/$parent/$parent/QuantityHeaderLabel" pos="Max" offset="-5"/>
            <Text val="1"/>
            <Style val="@Campaign_PurchaseDialogQuantity"/>
        </Frame>
    
        <StateGroup name="PurchasedState">
            <DefaultState val="NotPurchased"/>

            <State name="Purchased">
                <When type="Property" frame="$this/GameContentFrame" IsPurchased="True"/>
                <Action type="SetProperty" frame="ProductNameLabel" style="@Campaign_PurchaseDialogItemTitle_Owned"/>
                <Action type="SetProperty" frame="QuantityLabel" visible="false"/>
                <Action type="SetProperty" frame="OwnedLabel" visible="true"/>
            </State>

            <State name="NotPurchased">
                <Action type="SetProperty" frame="ProductNameLabel" style="@Campaign_PurchaseDialogItemTitle"/>
                <Action type="SetProperty" frame="QuantityLabel" visible="true"/>
                <Action type="SetProperty" frame="OwnedLabel" visible="false"/>
            </State>
        </StateGroup>
    </Frame>
    
    <Frame type="Label" name="ProductHeaderTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Height val="34"/>
    </Frame>
    
