                    <Event event="OnHidden" action="Stop"/>
                    
                    <Controller type="Sound" end="Stop">
                        <Key type="Sound" time="0.1" sound="*@UI_WarChest_Purchased_AS"/>
                    </Controller>
                </Animation>
            </Frame>

