                <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="60"/>
                <Text val="@UI/ProductPurchase/ConfirmButton"/>
                <ClickSound val="@UI_ActionButtonSelect"/>
            </Frame>

            <Frame type="Button" name="CancelButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
                <Anchor side="Top" relative="$parent/ConfirmButton" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-60"/>
                <Text val="@UI/ProductPurchase/CancelButton"/>
                <Shortcut val="@UI/ScreenNavigationBack_Hotkey"/>
                <ClickSound val="@UI_ActionButtonSelect"/>
            </Frame>
            
            <Animation name="ToggleVisibility">
                <Event event="OnShown" action="Reset" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
                <Event event="OnHidden" action="Reset" frame="$parent"/>
                <Event event="OnHidden" action="DirectionReverse" frame="$parent"/>
                <Event event="OnHidden" action="Play" frame="$parent"/>
                
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".5" value="0" out="Fast"/>
                    <Key type="Curve" time="1" value="255" out="Fast"/>
                </Controller>
            </Animation>

            <Animation name="CompletePurchaseVisbility">
                <Event event="CompletePurchase" action="Reset,Play" frame="$parent"/>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
            </Animation>
        
            <StateGroup name="BundleFrameVisibility">
                <DefaultState val="NotBundle"/>

                <State name="Bundle">
