        </Frame>

        <Frame type="Button" name="Item9" template="CustomGamesMelee/MeleeListItemTemplate">
            <Anchor side="Top" relative="$parent/Item8" pos="Max" offset="#TileListItem_Spacing_Vertical"/>
        </Frame>
        
        <Frame type="Button" name="Item10" template="CustomGamesMelee/MeleeListItemTemplate">
            <Anchor side="Top" relative="$parent/Item9" pos="Max" offset="#TileListItem_Spacing_Vertical"/>
        </Frame>
        
        <Frame type="Button" name="Item11" template="CustomGamesMelee/MeleeListItemTemplate">
            <Anchor side="Top" relative="$parent/Item10" pos="Max" offset="#TileListItem_Spacing_Vertical"/>
        </Frame>
        
        <Frame type="Button" name="Item12" template="CustomGamesMelee/MeleeListItemTemplate">
            <Anchor side="Top" relative="$parent/Item11" pos="Max" offset="#TileListItem_Spacing_Vertical"/>
        </Frame>
        
        <Frame type="Button" name="Item13" template="CustomGamesMelee/MeleeListItemTemplate">
            <Anchor side="Top" relative="$parent/Item12" pos="Max" offset="#TileListItem_Spacing_Vertical"/>
        </Frame>
        
        <Frame type="Button" name="Item14" template="CustomGamesMelee/MeleeListItemTemplate">
            <Anchor side="Top" relative="$parent/Item13" pos="Max" offset="#TileListItem_Spacing_Vertical"/>
        </Frame>

        <Frame type="ScrollBar" name="ScrollBar" template="StandardTileListTemplates/StandardTileListScrollBarTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="98"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-20"/>
        </Frame>
    </Frame>
    
    <!-- 
