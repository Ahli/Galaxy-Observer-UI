<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Constant name="CustomGamesMelee_ListItem_Height" val="40"/>
    
    <!-- 
    ========================
    FILTER EDITBOX TEMPLATE
    ========================
    -->
    
    <Frame type="EditBox" name="SearchEditBoxTemplate" template="StandardBattlenetTemplates/BattlenetEditBoxTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="8"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="9"/>
        <Anchor side="Right" relative="$parent/ToggleMapTypeButton" pos="Min" offset="-4"/>
        <Height val="44"/>
        <LoseFocusOnEnterOrEsc val="true"/>
        <Tooltip val="@UI/ScreenArcadeJoin/FilterTooltip"/>
        <TooltipAnchorPosition val="TopCenter"/>
        <Insets top="5" left="15" bottom="5" right="30"/>
        <Visible val="True"/>
        <RenderPriority val="600"/>
        
        <Frame type="Image" name="Image">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_FilterBG"/>
            <TextureType val="Nineslice"/>
            <StateCount val="1"/>
            <Tiled val="true"/>
        </Frame>

        <Frame type="Frame" name="LabelContainerFrame">
            <Frame type="Label" name="Label">
                <Style val="ArcadeFilterInput"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="SearchTextLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="8"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
            <Style val="ArcadeFilterInfo"/>
            <Text val="@UI/ScreenArcadeJoin/FilterText"/>
        </Frame>

        <Frame type="Button" name="ClearTextButton">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
            <Height val="31"/>
            <Width val="30"/>

            <Frame type="Image" name="NormalImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Texture val="@UI/Glues_Icons_CloseButton" layer="0"/>
                <TextureType val="Normal"/>
                <Tiled val="false"/>
                <StateCount val="1"/>
            </Frame>

            <Frame type="Image" name="HoverImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Texture val="@UI/Glues_Icons_CloseButtonHover" layer="0"/>
                <TextureType val="Normal"/>
                <Tiled val="false"/>
                <StateCount val="1"/>
            </Frame>

            <Frame type="Frame" name="HitTestFrame">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            </Frame>
        </Frame>

        <!-- Doing the edit box animation this way because apparently you lose focus on the edit box otherwise -->
        <Animation name="ListEmptyFilterPrep">
            <Event event="EmptyFilter" action="DirectionForward,Play" frame="$parent"/>
            <Event event="NotEmpty" action="DirectionForward,Play" frame="$parent"/>
            <Event event="Empty" action="DirectionReverse,Play" frame="$parent"/>
            <Event event="Loading" action="DirectionReverse,Play" frame="$parent"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0.00" value="False"/>
                <Key type="Bool" time="0.01" value="True"/>
            </Controller>
        </Animation>
    </Frame>
    
    <!-- 
    ========================
    MELEE LIST ITEM TEMPLATE
    ========================
    -->
    <Frame type="Button" name="MeleeListItemTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
        <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="-2"/>
        <Toggleable val="True"/>
        <Toggled val="False"/>
        
        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/MapIconContainer" pos="Max" offset="4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <TextureType val="NineSlice"/>
            <Alpha val="255"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/MapIconContainer" pos="Max" offset="4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/MapIconContainer" pos="Max" offset="4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <TextureType val="NineSlice"/>
            <Visible val="{$parent/@Toggled}"/>
        </Frame>
        
        <!-- Melee/Arcade Icon -->
        <Frame type="Frame" name="MapIconContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="#CustomGamesMelee_ListItem_Height"/>
            <CollapseLayout val="True"/>
            <Visible val="False"/>
            
            <Frame type="Image" name="NormalImage">
                <Anchor relative="$parent"/>
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
                <TextureType val="NineSlice"/>
                <Visible val="{$parent/$parent/NormalImage/@Visible}"/>
            </Frame>

            <Frame type="Image" name="HoverImage">
                <Anchor relative="$parent"/>
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Over"/>
                <TextureType val="NineSlice"/>
                <Visible val="{$parent/$parent/HoverImage/@Visible}"/>
            </Frame>

            <Frame type="Image" name="SelectedImage">
                <Anchor relative="$parent"/>
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Selected"/>
                <TextureType val="NineSlice"/>
                <Visible val="{$parent/$parent/SelectedImage/@Visible}"/>
            </Frame>
        
            <Frame type="Image" name="MapTypeIcon">
                <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-2"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="2"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-2"/>
                <Alpha val="100"/>
                <Color val="60,120,200"/>
            </Frame>
        </Frame>
        
        <!-- Map/Game Icon -->
        <Frame type="Image" name="MapIcon">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/NormalImage" pos="Min" offset="2"/>
            <Width val="54"/>
            <Height val="36"/>
            <Alpha val="170"/>
            <Color val="255,255,255"/>
            <Texture val="EditorData\Images\UI_Arcade_IconTMarine.tga"/>
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Image" name="MapIconBorder">
            <Anchor side="Top" relative="$parent/MapIcon" pos="Min" offset="-2"/>
            <Anchor side="Left" relative="$parent/MapIcon" pos="Min" offset="-2"/>
            <Anchor side="Right" relative="$parent/MapIcon" pos="Max" offset="2"/>
            <Anchor side="Bottom" relative="$parent/MapIcon" pos="Max" offset="2"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_ImageBorder"/>
            <TextureType val="NineSlice"/>
            <Alpha val="0"/>
        </Frame>
        
        <Frame type="Image" name="MapIconDivider">
            <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-2"/>
            <Anchor side="Left" relative="$parent/MapIcon" pos="Max" offset="0"/>
            <Width val="2"/>
            <Color val="20,40,60"/>
            <Visible val="False"/>
        </Frame>
        
        <!-- Map/Game Name -->
        <Frame type="Label" name="MapName">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/MapName" pos="Min" offset="0"/>
            <Width val="200"/>
            <Height val="20"/>
            <Style val="@CustomGames_ListItem_Normal"/>
            <Text val="Map/Game Name"/>
        </Frame>
        
        <!-- Lobby Name -->
         <Frame type="Label" name="LobbyName">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/LobbyName" pos="Min" offset="0"/>
            <Width val="200"/>
            <Height val="20"/>
            <Style val="@CustomGames_ListItem_Normal"/>
            <Text val="2v2 No Newsb!!!!"/>
        </Frame>
        
        <!-- Player Count -->
         <Frame type="Label" name="PlayerCount">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/PlayerCountIcon" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/PlayerCountIcon" pos="Mid" offset="0"/>
            <Width val="30"/>
            <Height val="40"/>
            <Style val="@CustomGames_Melee_PlayerCount"/>
            <Text val="2"/>
        </Frame>
        
        <!-- Host Name -->
         <Frame type="Label" name="HostName">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/HostName" pos="Min" offset="0"/>
            <Width val="200"/>
            <Height val="20"/>
            <Style val="@CustomGames_ListItem_Normal"/>
            <Text val="HostName"/>
        </Frame>

        <StateGroup name="SelectedState">
            <State name="Selected">
                <When type="Property" frame="$this" Toggled="True"/>
                <Action type="SetProperty" frame="$this/MapIcon" Alpha="255"/>
                <Action type="SetProperty" frame="$this/MapName" Style="@CustomGames_ListItem_Selected"/>
                <Action type="SetProperty" frame="$this/LobbyName" Style="@CustomGames_ListItem_Selected"/>
                <Action type="SetProperty" frame="$this/PlayerCount" Style="@CustomGames_Melee_PlayerCount"/>
                <Action type="SetProperty" frame="$this/HostName" Style="@CustomGames_ListItem_Selected"/>
            </State>
        
            <State name="Hover">
                <When type="Property" frame="$this/HoverImage" Visible="True"/>
                <Action type="SetProperty" frame="$this/MapIcon" Alpha="255"/>
                <Action type="SetProperty" frame="$this/MapName" Style="@CustomGames_ListItem_Hover"/>
                <Action type="SetProperty" frame="$this/LobbyName" Style="@CustomGames_ListItem_Hover"/>
                <Action type="SetProperty" frame="$this/PlayerCount" Style="@CustomGames_Melee_PlayerCount"/>
                <Action type="SetProperty" frame="$this/HostName" Style="@CustomGames_ListItem_Hover"/>
            </State>
            
            <State name="Normal">
                <When type="Property" frame="$this" Toggled="False"/>
                <Action type="SetProperty" frame="$this/MapIcon" Alpha="120"/>
                <Action type="SetProperty" frame="$this/MapName" Style="@CustomGames_ListItem_Normal"/>
                <Action type="SetProperty" frame="$this/LobbyName" Style="@CustomGames_ListItem_Normal"/>
                <Action type="SetProperty" frame="$this/PlayerCount" Style="@CustomGames_Melee_PlayerCount"/>
                <Action type="SetProperty" frame="$this/HostName" Style="@CustomGames_ListItem_Normal"/>
            </State>
        </StateGroup>
    </Frame>
    
    <!-- 
    ========================
