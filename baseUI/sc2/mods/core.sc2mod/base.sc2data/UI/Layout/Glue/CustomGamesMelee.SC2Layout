        </Frame>

        <Frame type="Button" name="Item1" template="CustomGamesMelee/MeleeListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Max" offset="#TileListItem_Spacing_Vertical"/>
        </Frame>

        <Frame type="Button" name="Item2" template="CustomGamesMelee/MeleeListItemTemplate">
