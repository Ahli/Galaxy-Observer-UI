        <StateGroup name="TitleStateGroup">
            <DefaultState val="DefaultState"/>
            
            <State name="SearchingState">
                <When type="AnimationState" frame="$this" SearchState="Searching"/>
                <Action type="SetProperty" frame="ModLabel" visible="false"/>
                <Action type="SetProperty" frame="ModeLabel" visible="false"/>
                <Action type="SetProperty" frame="CategoryLabel" visible="true"/>
                <Action type="SetProperty" frame="GameInfoPanel/ModeLabel" Alpha="0"/>
