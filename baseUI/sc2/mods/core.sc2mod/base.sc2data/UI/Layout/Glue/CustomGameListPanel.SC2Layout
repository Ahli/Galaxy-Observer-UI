                <Action type="SetProperty" frame="LadderFilterLabel" visible="false"/>
                <Action type="SetProperty" frame="RefreshButton" visible="false"/>
                <Action type="SetProperty" frame="BookmarkCountFrame" visible="false"/>
            </State>
