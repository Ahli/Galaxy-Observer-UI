            </State>
            
            <State name="FilterOpenGamesState">
                <When type="AnimationState" frame="$this" FilterState="OpenGames"/>
                <Action type="SetProperty" frame="ModLabel" visible="true"/>
                <Action type="SetProperty" frame="ModeLabel" visible="true"/>
                <Action type="SetProperty" frame="CategoryLabel" visible="false"/>
                <Action type="SetProperty" frame="GameInfoPanel/ModeLabel" Alpha="255"/>
                <Action type="SetProperty" frame="GameInfoPanel/GenreLabel" Alpha="0"/>
            </State>
            
            <State name="FilterRecentlyPlayedState">
                <When type="AnimationState" frame="$this" FilterState="RecentlyPlayed"/>
                <Action type="SetProperty" frame="ModLabel" visible="true"/>
                <Action type="SetProperty" frame="ModeLabel" visible="true"/>
                <Action type="SetProperty" frame="CategoryLabel" visible="false"/>
                <Action type="SetProperty" frame="GameInfoPanel/ModeLabel" Alpha="255"/>
                <Action type="SetProperty" frame="GameInfoPanel/GenreLabel" Alpha="0"/>
            </State>

            <State name="DefaultState">
                <Action type="SetProperty" frame="ModLabel" visible="false"/>
                <Action type="SetProperty" frame="ModeLabel" visible="false"/>
                <Action type="SetProperty" frame="CategoryLabel" visible="true"/>
                <Action type="SetProperty" frame="GameInfoPanel/ModeLabel" Alpha="0"/>
                <Action type="SetProperty" frame="GameInfoPanel/GenreLabel" Alpha="255"/>
            </State>
        </StateGroup>

        <StateGroup name="LoadingStateGroup">
            <DefaultState val="NotEmptyState"/>
            
            <State name="LoadingState">
                <When type="AnimationState" frame="$this" GameList="Loading"/>
                <Action type="SetProperty" frame="Loading" visible="true"/>
                <Action type="SetProperty" frame="ListEmpty" visible="false"/>
            </State>
            
            <State name="EmptyState">
                <When type="AnimationState" frame="$this" GameList="Empty"/>
                <Action type="SetProperty" frame="Loading" visible="false"/>
                <Action type="SetProperty" frame="ListEmpty" visible="true"/>
            </State>

            <State name="EmptyFilterState">
                <When type="AnimationState" frame="$this" GameList="EmptyFilter"/>
                <Action type="SetProperty" frame="Loading" visible="false"/>
                <Action type="SetProperty" frame="ListEmpty" visible="true"/>
            </State>

            <State name="NotEmptyState">
                <When type="AnimationState" frame="$this" GameList="NotEmpty"/>
                <Action type="SetProperty" frame="Loading" visible="false"/>
                <Action type="SetProperty" frame="ListEmpty" visible="false"/>
            </State>
        </StateGroup>
