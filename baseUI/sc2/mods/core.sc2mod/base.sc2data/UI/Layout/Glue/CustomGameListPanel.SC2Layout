            <State name="FilterTopPlayedState">
                <When type="AnimationState" frame="$this" FilterState="TopPlayed"/>
                <Action type="SetProperty" frame="ModLabel" visible="true"/>
                <Action type="SetProperty" frame="ModeLabel" visible="true"/>
                <Action type="SetProperty" frame="CategoryLabel" visible="false"/>
                <Action type="SetProperty" frame="GameInfoPanel/ModeLabel" Alpha="255"/>
