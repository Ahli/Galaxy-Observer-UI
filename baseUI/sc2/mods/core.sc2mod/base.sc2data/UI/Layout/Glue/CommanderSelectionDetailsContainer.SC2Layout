                <Anchor side="Bottom" relative="$parent/LevelLabel" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/LevelLabel" pos="Mid" offset="-1"/>
                <Anchor side="Right" relative="$parent/LevelLabel" pos="Mid" offset="-1"/>
                <Width val="156"/>
                <Height val="156"/>
                <Color val="FFFFFF"/>
                <ColorStep val="FFFFFF"/>
                <UseFillContainer val="False"/>
                <Circular val="True"/>
