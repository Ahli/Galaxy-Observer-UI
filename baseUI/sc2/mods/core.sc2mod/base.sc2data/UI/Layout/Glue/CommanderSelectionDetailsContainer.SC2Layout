    <!--
    
    
    
    Details Complete Container
    
    
    
    -->
    
    <Frame type="Frame" name="DetailsContainerTemplate">
        <Width val="1000"/>
        <Height val="300"/>
        
        <Frame type="Button" name="InfoButton" template="CommanderSelectionDetailsContainerItems/TabButtonTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="112"/>
            <HoverSound val="@UI_HoverTick"/>
            
            <Frame type="Label" name="Label">
                <Text val="@UI/ScreenCoopCampaign/Information"/>
            </Frame>
            
            <Animation name="Show">
                <Event event="OnShown" action="Reset" frame="$parent/InfoContainer"/>
                <Event event="OnShown" action="Play" frame="$parent/InfoContainer"/>
                <Event event="OnHidden" action="Reset" frame="$parent/InfoContainer"/>
                <Event event="OnHidden" action="Stop" frame="$parent/InfoContainer"/>

                <Controller type="Style" end="Pause" style1="Coop_CommanderTabButton_Selected" style2="Coop_CommanderTabButton_Selected" frame="Label">
                    <Key type="Curve" time="0" value="0"/>
                </Controller>
                
                <Controller type="Visibility" end="Pause" frame="SelectedFrameImage">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Button" name="UnitsButton" template="CommanderSelectionDetailsContainerItems/TabButtonMidTemplate">
            <Anchor side="Top" relative="$parent/InfoButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/InfoButton" pos="Max" offset="-20"/>
            <HoverSound val="@UI_HoverTick"/>
            
            <Frame type="Label" name="Label">
                <Text val="@UI/ScreenCoopCampaign/Units"/>
            </Frame>

            <Animation name="Show">
                <Event event="OnShown" action="Reset" frame="$parent/UnitsContainer"/>
                <Event event="OnShown" action="Play" frame="$parent/UnitsContainer"/>
                <Event event="OnHidden" action="Reset" frame="$parent/UnitsContainer"/>
                <Event event="OnHidden" action="Stop" frame="$parent/UnitsContainer"/>

                <Controller type="Style" end="Pause" style1="Coop_CommanderTabButton_Selected" style2="Coop_CommanderTabButton_Selected" frame="Label">
                    <Key type="Curve" time="0" value="0"/>
                </Controller>
                
                <Controller type="Visibility" end="Pause" frame="SelectedFrameImage">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Button" name="UpgradesButton" template="CommanderSelectionDetailsContainerItems/TabButtonRightTemplate">
            <Anchor side="Top" relative="$parent/UnitsButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/UnitsButton" pos="Max" offset="-20"/>
            <HoverSound val="@UI_HoverTick"/>
            
            <Frame type="Label" name="Label">
                <Text val="@UI/ScreenCoopCampaign/Upgrades"/>
            </Frame>

            <Animation name="Show">
                <Event event="OnShown" action="Reset" frame="$parent/UpgradesContainer"/>
                <Event event="OnShown" action="Play" frame="$parent/UpgradesContainer"/>
                <Event event="OnHidden" action="Reset" frame="$parent/UpgradesContainer"/>
                <Event event="OnHidden" action="Stop" frame="$parent/UpgradesContainer"/>

                <Controller type="Style" end="Pause" style1="Coop_CommanderTabButton_Selected" style2="Coop_CommanderTabButton_Selected" frame="Label">
                    <Key type="Curve" time="0" value="0"/>
                </Controller>
                
                <Controller type="Visibility" end="Pause" frame="SelectedFrameImage">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>
        </Frame>
        
        
        <!-- Info Detail Container -->

        <Frame type="Frame" name="InfoContainer" template="CommanderSelectionDetailsContainer/InfoDetailContainerTemplate">
            <Anchor side="Top" relative="$parent/UpgradesContainer" pos="Min" offset="20"/>
            <Anchor side="Left" relative="$parent/UpgradesContainer" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

            <Animation name="Show">
                <Event event="OnClick" action="DirectionForward" frame="$parent/UpgradesButton"/>
                <Event event="OnClick" action="Play" frame="$parent/UpgradesButton"/>
                <Event event="OnClick" action="DirectionReverse" frame="$parent/InfoButton"/>
                <Event event="OnClick" action="Play" frame="$parent/InfoButton"/>
                <Event event="OnClick" action="DirectionForward" frame="$parent/UnitsButton"/>
                <Event event="OnClick" action="Play" frame="$parent/UnitsButton"/>
                <!-- When you level up, we want to display the talents you've earned, hide the info panel -->
                <Event event="LevelUp" action="DirectionForward" frame="$parent/$parent"/>
                <Event event="LevelUp" action="Play" frame="$parent/$parent"/>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time=".001" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        
        <!-- Units Detail Container -->

        <Frame type="Frame" name="UnitsContainer" template="CommanderSelectionDetailsContainer/UnitsDetailContainerTemplate">
            <Anchor side="Top" relative="$parent/UpgradesContainer" pos="Min" offset="26"/>
            <Anchor side="Left" relative="$parent/UpgradesContainer" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Visible val="false"/>

            <Animation name="Show">
                <Event event="OnClick" action="DirectionForward" frame="$parent/UpgradesButton"/>
                <Event event="OnClick" action="Play" frame="$parent/UpgradesButton"/>
                <Event event="OnClick" action="DirectionForward" frame="$parent/InfoButton"/>
                <Event event="OnClick" action="Play" frame="$parent/InfoButton"/>
                <Event event="OnClick" action="DirectionReverse" frame="$parent/UnitsButton"/>
                <Event event="OnClick" action="Play" frame="$parent/UnitsButton"/>
                <!-- When you level up, we want to display the talents you've earned, hide the units panel -->
                <Event event="LevelUp" action="DirectionForward" frame="$parent/$parent"/>
                <Event event="LevelUp" action="Play" frame="$parent/$parent"/>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time=".001" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        
        <!-- Upgrades Detail Container -->
        
        <Frame type="Frame" name="UpgradesContainer" template="CommanderSelectionDetailsContainer/UpgradesDetailContainerTemplate">
            <Anchor side="Top" relative="$parent/UpgradesButton" pos="Max" offset="-12"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Visible val="false"/>
            
            <Animation name="Show">
                <Event event="OnClick" action="DirectionForward" frame="$parent/UpgradesButton"/>
                <Event event="OnClick" action="Play" frame="$parent/UpgradesButton"/>
                <Event event="OnClick" action="DirectionReverse" frame="$parent/InfoButton"/>
                <Event event="OnClick" action="Play" frame="$parent/InfoButton"/>
                <Event event="OnClick" action="DirectionReverse" frame="$parent/UnitsButton"/>
                <Event event="OnClick" action="Play" frame="$parent/UnitsButton"/>
                <!-- When you level up, we want to display the talents you've earned, show the upgrades panel -->
                <Event event="LevelUp" action="DirectionForward" frame="$parent/$parent"/>
                <Event event="LevelUp" action="Play" frame="$parent/$parent"/>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".001" value="true"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

</Desc>