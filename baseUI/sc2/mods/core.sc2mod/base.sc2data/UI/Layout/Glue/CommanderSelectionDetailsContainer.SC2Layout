            <StateGroup name="RandomState">
                <DefaultState val="NotRandom"/>
                
                <State name="Random">
                    <When type="Property" frame="$ancestor[@type=CommanderSelectionPanel]" Commander="Random"/>
                    <Action type="SetProperty" Visible="False"/>
                </State>
                
                <State name="NotRandom">
                    <Action type="SetProperty" Visible="True"/>
                </State>
            </StateGroup>
            
            <StateGroup name="PressedState">
                <DefaultState val="NotPressed"/>
                
                <State name="Pressed">
                    <When type="Property" Pushed="True"/>
                    <Action type="SetProperty" Style="Coop_CommanderPreview_Pressed"/>
                </State>
                
                <State name="NotPressed">
                    <Action type="SetProperty" Style="Coop_CommanderPreview"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Label" name="DescriptionLabel">
