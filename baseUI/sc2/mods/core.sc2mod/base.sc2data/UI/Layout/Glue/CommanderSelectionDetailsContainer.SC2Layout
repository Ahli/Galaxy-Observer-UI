    <!-- 
    
    Commander Trait Ability Info Container Template 
    
    -->
    
    <Frame type="Frame" name="AbilContainerTemplate">
        <Width val="504"/>
        <Height val="415"/>

        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Coop_TooltipBG"/>
            <TextureType val="NineSlice"/>
        </Frame>
        
        <Frame type="Image" name="HoriztonalBorder">
            <Anchor side="Top" relative="$parent/LevelCapWarningLabel" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-4"/>
            <Height val="2"/>
            <LayerColor val="00a8ff"/>
            <Alpha val="102"/>
            <PreserveAnchorOffset val="True"/>
            
            <StateGroup name="AnchorState">
                <DefaultState val="TitleAnchored"/>
                
                <State name="CapAnchored">
                    <When type="Property" frame="$parent/LevelCapWarningLabel" Visible="True"/>
                    <Action type="SetAnchor" side="Top" relative="$parent/LevelCapWarningLabel" pos="Max" offset="5"/>
                </State>
                
                <State name="TitleAnchored">
                    <Action type="SetAnchor" side="Top" relative="$parent/LevelCapWarningLabel" pos="Max" offset="9"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="LevelLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="14"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
            <Style val="@Coop_AbilTooltip_LevelLabel"/>
            <Text val="{$this/FormatFrame/@Text}"/>

            <Frame type="TextFormatFrame" name="FormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/ScreenCoopCampaign/LevelTooltip"/>
                <Replacement index="0" val="{$CoopStandardGlue_CommanderSelectionPanel/@Level}"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="ExperienceLabel">
            <Anchor side="Top" relative="$parent/LevelLabel" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/LevelLabel" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LevelLabel" pos="Max" offset="5"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Style val="@Coop_AbilTooltip_ExperienceLabel"/>

            <Frame type="TextFormatFrame" name="FormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/ScreenCoopCampaign/ExperienceTooltip"/>
                <Replacement index="0" val="{$CoopStandardGlue_CommanderSelectionPanel/@CurrentXP}"/>
                <Replacement index="1" val="{$CoopStandardGlue_CommanderSelectionPanel/@TotalXP}"/>
            </Frame>

            <StateGroup name="MaxLevelStateGroup">
                <DefaultState val="NotMaxLevelState"/>
                
                <State name="MaxLevelState">
                    <When type="AnimationState" frame="$CoopStandardGlue_CommanderSelectionPanel" CommanderMaxLevel="MaxLevel"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCoopCampaign/ExperienceMaxTooltip"/>
                </State>

                <State name="NotMaxLevelState">
                    <When type="AnimationState" frame="$CoopStandardGlue_CommanderSelectionPanel" CommanderMaxLevel="NotMaxLevel"/>
                    <Action type="SetProperty" frame="$this" Text="{$this/FormatFrame/@Text}"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Label" name="LevelCapWarningLabel">
            <Anchor side="Top" relative="$parent/LevelLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/LevelLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-50"/>
            <Text val="@UI/ScreenCoopCampaign/XPCappedHint"/>
            <Style val="Coop_TraitTooltip_Warning"/>
            <CollapseLayout val="True"/>
            <Visible val="{$parent/$parent/DetailsContainer/DetailsBackground/CappedLevelLabel/@Visible}"/>
        </Frame>
        
        <Frame type="Image" name="CappedIcon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="19"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-19"/>
            <Texture val="@UI/SC2_UI_Coop_CappedIcon_Small_Overlay"/>
            <Visible val="{$parent/LevelCapWarningLabel/@Visible}"/>
        </Frame>

        <Frame type="Label" name="Title">
            <Anchor side="Top" relative="$parent/HoriztonalBorder" pos="Max" offset="15"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@Coop_TraitTooltip_Title"/>
            <Text val="{$CoopStandardGlue_CommanderSelectionPanel/@Commander[CommanderAbilTitle]}"/>
        </Frame>

        <Frame type="TileList" name="TileList">
            <Anchor side="Top" relative="$parent/Title" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

            <Frame type="CommanderAbilTileListItem" name="Item0" template="CommanderSelectionDetailsContainerItems/CommanderAbilTileListItemTemplate">
