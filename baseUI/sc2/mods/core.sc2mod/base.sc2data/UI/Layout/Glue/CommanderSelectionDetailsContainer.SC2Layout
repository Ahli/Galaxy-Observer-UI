                <CircularEndOffset val="360"/>
                <MaxValue val="{$CoopStandardGlue_CommanderSelectionPanel/@TotalXP}"/>
                <Value val="{$CoopStandardGlue_CommanderSelectionPanel/@CurrentXP}"/>
                
                <Frame type="Image" name="BorderImage">
