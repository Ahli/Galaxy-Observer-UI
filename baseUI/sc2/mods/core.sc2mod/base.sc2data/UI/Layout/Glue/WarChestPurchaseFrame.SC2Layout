                    <Key type="Curve" time="0.3" value="0" out="Fast"/>
                    <Key type="Curve" time="0.7" value="255" in="Slow"/>
                </Controller>
            </Animation>
            
            <StateGroup name="ProtossSelectableState">
                <DefaultState val="Selectable"/>
                
                <State name="TerranZergOwned">
                    <When type="StateGroup" frame="$TerranWarChest/WarChestNode0" OwnedState="Owned"/>
                    <When type="StateGroup" frame="$ZergWarChest/WarChestNode0" OwnedState="Owned"/>
