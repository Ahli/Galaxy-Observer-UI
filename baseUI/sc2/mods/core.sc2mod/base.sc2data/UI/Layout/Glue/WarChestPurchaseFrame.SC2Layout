                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0.0" value="0" out="Fast"/>
                    <Key type="Curve" time="0.3" value="0" out="Fast"/>
                    <Key type="Curve" time="0.7" value="255" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Label" name="TypeLabel">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="8"/>
