                </State>
                
                <State name="ProtossOwned">
                    <When type="StateGroup" frame="$ProtossWarChest/WarChestNode0" OwnedState="Owned"/>
                    <Action type="SetProperty" frame="$this/Item2" Enabled="False"/>
                </State>
                
                <State name="Selectable">
                    <Action type="SetProperty" frame="$this/Item2" Enabled="True"/>
                </State>
            </StateGroup>
            
            <StateGroup name="ZergSelectableState">
                <DefaultState val="Selectable"/>
                
                <State name="TerranProtossOwned">
                    <When type="StateGroup" frame="$TerranWarChest/WarChestNode0" OwnedState="Owned"/>
