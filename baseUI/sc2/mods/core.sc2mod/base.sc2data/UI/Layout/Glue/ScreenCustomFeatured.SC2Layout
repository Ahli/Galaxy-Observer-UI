        <Frame type="TileList" name="BookmarkTileList" template="ScreenCustomFeatured/ArcadeGamesListTemplate">
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="-8"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="8"/>
        </Frame>
        
        <Frame type="BattleMapInfoPanel" name="DetailsPanel" template="ScreenCustomFeatured/DetailsPanelTemplate"/>

        <Frame type="CustomGamePlayButton" name="CreateButton" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Max" offset="11"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="4"/>
            <Width val="288"/>
            <Text val="@UI/ScreenCustomGames/CreateButton"/>
            <MapHandle val="{$parent/@SelectedMapHandle}"/>
            <UseDefaultVariant val="true"/>

            <Animation name="ItemDoubleClicked">
                <Event event="OnItemDoubleClicked" action="Reset,Play" frame="$parent/BookmarkTileList"/>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0" event="ControlClickRequest"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="CustomGamePlayButton" name="AutoJoinButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Bottom" relative="$parent/CreateButton" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/CreateButton" pos="Max" offset="8"/>
            <Width val="288"/>
            <Text val="@UI/ScreenCustomGames/AutoJoinButton"/>
            <Tooltip val="@UI/ScreenCustomGames/AutoJoinTooltip"/>
            <AcceptsMouseTooltip val="True"/>
            <TooltipDelayed val="True"/>
            <MapHandle val="{$parent/@SelectedMapHandle}"/>
            <UseDefaultVariant val="False"/>
            <VariantIndex val="{$parent/VariantPulldown/@VariantIndex}"/>
            <PlayMode val="JoinOrCreate"/>

            <Animation name="ItemDoubleClicked">
                <Event event="OnItemDoubleClicked" action="Reset,Play" frame="$parent/BookmarkTileList"/>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0" event="ControlClickRequest"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="BattleMapVariantPulldown" name="VariantPulldown" template="StandardBattlenetTemplates/BattlenetLargePulldownTemplate">
            <Anchor side="Bottom" relative="$parent/CreateButton" pos="Max" offset="-4"/>
            <Anchor side="Left" relative="$parent/AutoJoinButton" pos="Max" offset="8"/>
            <Width val="288"/>
            <MapHandle val="{$parent/@SelectedMapHandle}"/>
                
            <StateGroup name="EnabledState">
                <DefaultState val="Disabled"/>
            
                <State name="Enabled">
                    <When type="Property" frame="$parent/AutoJoinButton" Enabled="True"/>
                    <Action type="SetProperty" frame="$this" Enabled="True"/>
                </State>
                
                <State name="Disabled">
                    <Action type="SetProperty" frame="$this" Enabled="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="ViewMapProfileButton" name="GameInfoButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Bottom" relative="$parent/CreateButton" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/VariantPulldown" pos="Max" offset="8"/>
            <Width val="288"/>
            <Text val="@UI/ScreenCustomGames/MapInfoButton"/>
            <HoverSound val="@UI_HoverTick"/>
            <MapHandle val="{$parent/@SelectedMapHandle}"/>
            <UseHyperLink val="true"/>
            <HyperLinkIndex val="#MapInfoIndex_Premium"/>

            <StateGroup name="GameInfoButtonState">
                <State name="MapHandleInvalid">
                    <When type="Property" frame="$parent" SelectedMapHandle="0,0"/>
                    <Action type="SetProperty" frame="$this" Enabled="false"/>
                </State>
                <State name="MapHandleValid">
                    <Action type="SetProperty" frame="$this" Enabled="true"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="BattleMapContextMenu" name="ItemContextMenu" template="StandardTemplates/StandardContextMenuTemplate">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <ShowMapInfo val="true"/>
            <ShowCreateOffline val="false"/>
            <MapHandle val="{$parent/@SelectedMapHandle}"/>
        </Frame>
    </Frame>
    
    <!-- 
    ======================================
    MELEE LIST ITEM TEMPLATE
    ======================================
    -->
    <Frame type="BookmarkSelectionTileListItem" name="MeleeListItemTemplate" template="CustomGamesTemplates/BookmarkSelectionTileListItemTemplate">
        <ContextMenu val="$parent/$parent/ItemContextMenu"/>
        <TooltipFrame val="BattleMapInfoTooltip"/>
        <UseTooltip val="False"/>
        <HideWhenCleared val="False"/>
        <AllowOpenOnClick val="False"/>
        
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent/$sibling-1/AnchorFrame" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="16"/>
            <Anchor side="Right" relative="$parent/$parent/ScrollBar" pos="Min" offset="-2"/>
        </Frame>

        <Frame type="BattleMapFrame" name="BattleMapFrame">
            <Anchor relative="$parent"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>
        
        <!-- Author Name Label -->
        <Frame type="Label" name="AuthorLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="830"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-15"/>
            <Height val="20"/>
            <Style val="{$parent/MapName/@Style}"/>
            <Text val="{$parent/BattleMapFrame/@AuthorName}"/>

            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="Property" frame="$parent/BattleMapFrame" IsBlizzardMap="False"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>

                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Image" name="BlizzardAuthoredIcon">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/AuthorLabel" pos="Min" offset="0"/>
            <Width val="44"/>
            <Height val="20"/>
            <Texture val="@UI/Glues_Icons_BlizzardAuthored_Normal"/>
            <Visible val="False"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="Property" frame="$parent/BattleMapFrame" IsBlizzardMap="True"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>

                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
            
            <StateGroup name="DisplayState">
                <DefaultState val="Normal"/>
                
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Icons_BlizzardAuthored_Selected"/>
                </State>
                
                <State name="Hovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Icons_BlizzardAuthored_Hovered"/>
                </State>
                
                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Icons_BlizzardAuthored_Normal"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Image" name="BookmarkIcon">
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="Property" frame="$parent/BattleMapFrame" IsBookmarked="True"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    <!-- 
    ======================================
    MELEE LIST TEMPLATE
    ======================================
    -->
    <Frame type="TileList" name="MeleeGamesListTemplate">
        <Width val="1058"/>
        <Height val="486"/>
        <AcceptsMouse val="true"/>
        <RequireSelection val="true"/>
            
        <Frame type="BookmarkSelectionTileListItem" name="Item0" template="ScreenCustomFeatured/MeleeListItemTemplate">
            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="4"/>
            </Frame>
        </Frame>
        
        <Frame type="BookmarkSelectionTileListItem" name="Item1" template="ScreenCustomFeatured/MeleeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item2" template="ScreenCustomFeatured/MeleeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item3" template="ScreenCustomFeatured/MeleeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item4" template="ScreenCustomFeatured/MeleeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item5" template="ScreenCustomFeatured/MeleeListItemTemplate"/>
