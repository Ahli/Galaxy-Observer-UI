<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <!--
    ======================================
    DETAILS PANEL TEMPLATE
    ======================================
    -->
    <Frame type="BattleMapInfoPanel" name="DetailsPanelTemplate">
        <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="8"/>
        <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="-8"/>
        <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-8"/>
        <Width val="412"/>
        <MapHandle val="{$parent/@SelectedMapHandle}"/>
        
        <Frame type="BattleMapFrame" name="BattleMapFrame">
            <Anchor relative="$parent"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>
                
        <Frame type="BattleMapGameInfoScreenshotTileList" name="ScreenshotTileList" template="CustomGamesTemplates/ScreenshotTileListTemplate">
            <Width val="384"/>
            <Height val="290"/>
            <MapHandle val="{$parent/@MapHandle}"/>

            <Frame type="BattleMapScreenshotTileListItem" name="Item0">
                <Width val="378"/>
                <Height val="284"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="MapNameLabel">
            <Anchor side="Top" relative="$parent/ScreenshotTileList" pos="Max" offset="8"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Height val="32"/>
            <Style val="@CustomGames_MapInfoPreview_Title"/>
            <Text val="{$parent/BattleMapFrame/@Name}"/>
            <AcceptsMouse val="True"/>
            <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="AuthorLabel">
            <Anchor side="Top" relative="$parent/MapNameLabel" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Author"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="Property" frame="$parent/BattleMapFrame" IsBlizzardMap="False"/>
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>

                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Image" name="BlizzardIcon">
            <Anchor side="Top" relative="$parent/MapNameLabel" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="1"/>
            <Width val="40"/>
            <ScaleHeightToTexture val="True"/>
            <Texture val="@UI/Glues_Icons_BlizzardAuthored_Normal"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent/BattleMapFrame" IsBlizzardMap="True"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>

                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="ModeLabel">
            <Anchor side="Top" relative="$parent/AuthorLabel" pos="Max" offset="18"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Info"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="Label" name="PlayerLabel">
            <Anchor side="Top" relative="$parent/ModeLabel" pos="Max" offset="18"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Info"/>
            <Visible val="False"/>
        </Frame>
 
        <Frame type="TextFormatFrame" name="FormattedInfoText">
            <Anchor relative="$parent"/>
            <Replacement index="0" val="{$parent/PlayerLabel/@Text}"/>
            <Replacement index="1" val="{$parent/BattleMapFrame/@Genre}"/>
            <FormatText val="%0% | %1%"/>
        </Frame>

        <Frame type="Label" name="InfoLabel">
            <Anchor side="Top" relative="$parent/AuthorLabel" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent/AuthorLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/VersionLabel" pos="Min" offset="-30"/>
            <AcceptsMouseTooltip val="True"/>
            <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
            <Style val="@CustomGames_MapInfoPreview_Info"/>
            <Text val="{$parent/FormattedInfoText/@Text}"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="VersionLabel">
            <Anchor side="Top" relative="$parent/InfoLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Info"/>
            <Text val="{$parent/FormattedVersionText/@Text}"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
 
        <Frame type="TextFormatFrame" name="FormattedVersionText">
            <Anchor relative="$parent"/>
            <Replacement index="0" val="{$parent/BattleMapFrame/@MajorVersionNum}"/>
            <Replacement index="1" val="{$parent/BattleMapFrame/@MinorVersionNum}"/>
            <FormatText val="@UI/ScreenCustomGames/VersionNumFormatText"/>
        </Frame>

        <Frame type="Label" name="DescriptionLabel">
            <Anchor side="Top" relative="$parent/InfoLabel" pos="Max" offset="11"/>
            <Anchor side="Bottom" relative="$parent/WebsiteLabel" pos="Min" offset="-10"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Description"/>
            <Text val="{$parent/BattleMapFrame/@Description}"/>
            <AcceptsMouse val="True"/>
            <Options val="Ellipsis | TooltipIfTruncated | NewLineIfTruncated"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="WebsiteLabel">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-14"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Height val="24"/>
            <Style val="@CustomGames_MapInfoPreview_Website"/>
            <AcceptsMouse val="true"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- ERROR LABEL -->
        <Frame type="Label" name="ErrorLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-25"/>
            <Style val="@InternationalListItemTitle_Small_Locked_Normal_Centered"/>
            <Text val=""/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="Equal"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame Type="BattleUserContextMenu" name="UserContextMenu" template="BattleUserContextMenu/BattleUserContextMenu">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>
                    
        <!-- Determines whether or not both the Lobby preview and game info have been fully loaded -->
        <StateGroup name="LoadingState">
            <DefaultState val="NotLoaded"/>
        
            <State name="Loaded">
                <When type="Property" operator="NotEqual" MapHandle="0,0"/>
                <When type="AnimationState" GameInfo="Loaded"/>
            </State>
        
            <State name="Loading">
                <When type="Property" operator="NotEqual" MapHandle="0,0"/>
            </State>
            
            <State name="NotLoaded"/>
        </StateGroup>
    </Frame>
    
    
    <!-- 
    ======================================
    ARCADE LIST ITEM TEMPLATE
    ======================================
    -->
    <Frame type="BookmarkSelectionTileListItem" name="ArcadeListItemTemplate" template="CustomGamesTemplates/ArcadeListItemTemplate">
        <ContextMenu val="$parent/$parent/ItemContextMenu"/>
        <TooltipFrame val="BattleMapInfoTooltip"/>
        <AllowOpenOnClick val="False"/>
        
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent/$sibling-1/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/$sibling-1/AnchorFrame" pos="Max" offset="8"/>
            <Height val="237"/>
        </Frame>

        <Frame type="BattleMapFrame" name="BattleMapFrame">
            <Anchor relative="$parent"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>
        
        <Frame type="Image" name="TextBackground">
            <Height val="101"/>
        </Frame>
        
        <Frame type="Image" name="Divider">
            <Anchor side="Top" relative="$parent/UnlockableFrame" pos="Max" offset="-19"/>
        </Frame>
        
        <Frame type="Label" name="TitleLabel">
            <Text val="{$parent/BattleMapFrame/@Name}"/>
        </Frame>
        
        <StateGroup name="EquippedState">
            <DefaultState val="NotEquipped"/>
            <State name="Equipped">
                <When type="Property" frame="$this/BattleMapFrame" IsBookmarked="True"/>
            </State>
            <State name="NotEquipped">
            </State>
        </StateGroup>
    </Frame>
    
    <!-- 
    ======================================
    ARCADE LIST TEMPLATE
    ======================================
    -->
    <Frame type="TileList" name="ArcadeGamesListTemplate">
        <Width val="1058"/>
        <Height val="510"/>
        <AcceptsMouse val="true"/>
        <ScrollStepValue val="5"/>
        <RequireSelection val="true"/>

        <Frame type="BattleMapInfoTooltip" name="BattleMapInfoTooltip" template="BattleMapInfoTooltip/BattleMapInfoTooltipTemplate"/>

        <!-- Row 1 -->
        <Frame type="BookmarkSelectionTileListItem" name="Item0" template="ScreenCustomFeatured/ArcadeListItemTemplate">
            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="14"/>
                <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="14"/>
            </Frame>
        </Frame>
        
        <Frame type="BookmarkSelectionTileListItem" name="Item1" template="ScreenCustomFeatured/ArcadeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item2" template="ScreenCustomFeatured/ArcadeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item3" template="ScreenCustomFeatured/ArcadeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item4" template="ScreenCustomFeatured/ArcadeListItemTemplate"/>

        <!-- Row 2 -->
        <Frame type="BookmarkSelectionTileListItem" name="Item5" template="ScreenCustomFeatured/ArcadeListItemTemplate">
            <Frame type="Frame" name="AnchorFrame">
                <Anchor side="Top" relative="$parent/$parent/Item0/AnchorFrame" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/$parent/Item0/AnchorFrame" pos="Min" offset="0"/>
            </Frame>
        </Frame>
        
        <Frame type="BookmarkSelectionTileListItem" name="Item6" template="ScreenCustomFeatured/ArcadeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item7" template="ScreenCustomFeatured/ArcadeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item8" template="ScreenCustomFeatured/ArcadeListItemTemplate"/>
        <Frame type="BookmarkSelectionTileListItem" name="Item9" template="ScreenCustomFeatured/ArcadeListItemTemplate"/>
            
        <Frame type="Image" name="ScrollBarBackground" template="StandardTileListTemplates/StandardTileListScrollBarBackgroundTemplate"/>

        <Frame type="ScrollBar" name="ScrollBar" template="StandardTileListTemplates/StandardTileListScrollBarTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="14"/>
            <Anchor side="Bottom" relative="$parent/Item9" pos="Max" offset="-14"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-2"/>
        </Frame>
    </Frame>
    
    <!--
    ======================================
    ARCADE MULTIPLE CONTAINER
    ======================================
    -->
    <Frame type="UserProfileMapFavoritesSelectionFrame" name="ArcadeMultipleContainerTemplate">
        <Anchor relative="$parent"/>
        <Visible val="False"/>
        <Arcade val="True"/>
        <ToonEmail val="{$CustomGamesFeaturedPanelContentFeed/@ToonEmail}"/>
        <RandomizeContent val="{$CustomGamesFeaturedPanelContentFeed/@Shuffle}"/>
        
        <Frame type="Frame" name="AnchorFrame">
            <Anchor relative="$parent/$parent/ContentMultipleBackground"/>
        </Frame>

