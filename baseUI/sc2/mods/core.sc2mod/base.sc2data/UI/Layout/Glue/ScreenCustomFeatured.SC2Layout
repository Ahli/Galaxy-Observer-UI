    <!--
    ======================================
    CUSTOM GAMES FEATURED PANEL
    ======================================
    -->
    <Frame type="ScreenCustomFeatured" name="ScreenCustomFeatured">
        <Anchor relative="$parent"/>
        <Handle val="ScreenCustomFeatured"/>
        <RenderPriority val="630"/>
        <Visible val="False"/>
        
        <Frame type="Frame" name="ExtendedExperienceBG" template="StandardExtendedExperienceTemplates/StandardSizeExtendedExperienceTemplate"/>
        <Frame type="Button" name="CloseButton" template="ScreenCustomFeatured/CloseButtonTemplate"/>
        <Frame type="Label" name="TitleLabel" template="ScreenCustomFeatured/TitleLabelTemplate"/>
        <Frame type="Label" name="SubtitleLabel" template="ScreenCustomFeatured/SubtitleLabelTemplate"/>
        <Frame type="Frame" name="ContentMultipleBackground" template="ScreenCustomFeatured/ContentMultipleBackgroundTemplate"/>
        <Frame type="UserProfileMapFavoritesSelectionFrame" name="ArcadeMultipleContainer" template="ScreenCustomFeatured/ArcadeMultipleContainerTemplate"/>
        <Frame type="UserProfileMapFavoritesSelectionFrame" name="MeleeMultipleContainer" template="ScreenCustomFeatured/MeleeMultipleContainerTemplate"/>
        <Frame type="UserProfileMapFavoritesSelectionFrame" name="DualPremiumContainer" template="ScreenCustomFeatured/DualPremiumContainerTemplate"/>
        <Frame type="UserProfileMapFavoritesSelectionFrame" name="SingleMapContainer" template="ScreenCustomFeatured/SingleMapContainerTemplate"/>
        
        <StateGroup name="DisplayState">
            <DefaultState val="Empty"/>
            
            <State name="GroupArcade">
                <When type="Property" frame="$CustomGamesFeaturedPanelContentFeed" FeaturedType="GroupArcade"/>
                <Action type="SetProperty" frame="$this/ArcadeMultipleContainer" Visible="True"/>
                <Action type="SetProperty" frame="$this/MeleeMultipleContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/DualPremiumContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/SingleMapContainer" Visible="False"/>
            </State>
            
            <State name="GroupMelee">
                <When type="Property" frame="$CustomGamesFeaturedPanelContentFeed" FeaturedType="GroupMelee"/>
                <Action type="SetProperty" frame="$this/ArcadeMultipleContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/MeleeMultipleContainer" Visible="True"/>
                <Action type="SetProperty" frame="$this/DualPremiumContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/SingleMapContainer" Visible="False"/>
            </State>
            
            <State name="DualPremium">
                <When type="Property" frame="$CustomGamesFeaturedPanelContentFeed" FeaturedType="DualPremium"/>
                <Action type="SetProperty" frame="$this/ArcadeMultipleContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/MeleeMultipleContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/DualPremiumContainer" Visible="True"/>
                <Action type="SetProperty" frame="$this/SingleMapContainer" Visible="False"/>
            </State>
            
            <State name="SingleMap">
                <When type="Property" frame="$CustomGamesFeaturedPanelContentFeed" FeaturedType="SingleMap"/>
                <Action type="SetProperty" frame="$this/ArcadeMultipleContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/MeleeMultipleContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/DualPremiumContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/SingleMapContainer" Visible="True"/>
            </State>
            
            <State name="Empty">
                <Action type="SetProperty" frame="$this/ArcadeMultipleContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/MeleeMultipleContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/DualPremiumContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/SingleMapContainer" Visible="False"/>
            </State>
        </StateGroup>
        
        <StateGroup name="VisibilityVersionTrigger">
            <DefaultState val="NotVisible"/>
            
            <State name="Visible">
                <When type="Property" frame="$CustomFeaturedVersionNotificationFrame" Visible="True"/>
                <When type="Property" frame="$ScreenCustom" Visible="True"/>
                <Action type="SendEvent" frame="$GlueUI" event="ShowFeaturedMapContent"/>
            </State>
            
            <State name="NotVisible"/>
        </StateGroup>
        
        <!-- this animation must be called Open so the game knows to close this window when you logout -->
        <Animation name="Open">
            <Event event="ShowFeaturedMapContent" frame="$GlueUI" action="Reset,Play"/>
            
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="$CustomFeaturedVersionNotificationFrame">
                <Key type="Event" time="0" event="SetVersion"/>
            </Controller>
        </Animation>
    </Frame>
</Desc>
