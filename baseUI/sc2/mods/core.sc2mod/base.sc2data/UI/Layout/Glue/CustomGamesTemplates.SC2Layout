<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Constant name="CustomGamesTemplates_LobbyPreview_TeamList_Spacing_Horizontal" val="8"/>
    <Constant name="CustomGamesTemplates_LobbyPreview_TeamList_Spacing_Horizontal_16x9" val="6"/>
    
    <!-- 
    ========================
    HEADER PANEL TEMPLATE
    ========================
    -->
    <Frame type="Frame" name="HeaderPanelTemplate">
        <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent/DividerLeft" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent/DividerTop" pos="Min" offset="0"/>
    </Frame>
    
    <!-- 
    ========================
    PRIMARY PANEL TEMPLATE
    ========================
    -->
    <Frame type="Frame" name="PrimaryPanelTemplate">
        <Anchor side="Top" relative="$parent/DividerLeft" pos="Max" offset="0"/>
        <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent/DividerBottom" pos="Min" offset="0"/>
    </Frame>
    
    <!-- 
    ========================
    SECONDARY PANEL TEMPLATE
    ========================
    -->
    <Frame type="Frame" name="SecondaryPanelTemplate">
        <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent/DividerBottom" pos="Max" offset="0"/>
        <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
    </Frame>
    
    <!-- 
    ===========================================================
    
    CUSTOM GAMES BASE TEMPLATE
    
    ===========================================================
    -->
    <Frame type="Frame" name="CustomGamesBaseTemplate">
        <Anchor relative="$parent"/>
        
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$Anchor_Header" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$Anchor_Header" pos="Max" offset="0"/>
            <Style val="@BattlenetTitle"/>
        </Frame>
        
        <Frame type="Label" name="SubtitleLabel">
            <Anchor side="Top" relative="$Anchor_Subheader" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$Anchor_Subheader" pos="Max" offset="0"/>
            <Style val="@BattlenetSubtitle"/>
        </Frame>
        
        <Frame type="Frame" name="ContentContainer" template="StandardBattlenetTemplates/BattlenetStandardContentContainerTemplate">
            <Anchor side="Right" relative="$Anchor_CustomGamesContentContainer_Right" pos="Max" offset="0"/>
        
            <Frame type="Image" name="HeaderHighlight">
                <Anchor side="Right" relative="$parent/DividerAnchor" pos="Max" offset="2"/>
                <Texture val="@UI/Glues_Battlenet_PageAssets_StandardContainer_Mask_SolidCorner" layer="0"/>
                <TextureCoords top="0" left="1" bottom="1" right="0" layer="0"/>
            </Frame>
            
            <Frame type="Image" name="BodyShadow">
                <Anchor side="Right" relative="$parent/DividerAnchor" pos="Max" offset="0"/>
            </Frame>
            
            <Frame type="Image" name="DividerAnchor" template="StandardBattlenetTemplates/BattlenetStandardContentContainerDividerTemplate">
                <Anchor side="Top" relative="$Anchor_StandardContentHeader_Bottom" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$Anchor_CustomGamesContentSidebar_Right" pos="Max" offset="0"/>
                <Width val="2"/>
                <Height val="2"/>
            </Frame>
            
            <Frame type="Image" name="DividerRight">
                <Visible val="False"/>
            </Frame>

            <Frame type="Frame" name="HeaderPanel" template="CustomGamesTemplates/HeaderPanelTemplate"/>
            <Frame type="Frame" name="PrimaryPanel" template="CustomGamesTemplates/PrimaryPanelTemplate"/>
            <Frame type="Frame" name="SecondaryPanel" template="CustomGamesTemplates/SecondaryPanelTemplate"/>
            
            <StateGroup name="AspectRatioState">
                <DefaultState val="16x9"/>
                
                <State name="4x3">
                    <When type="StateGroup" frame="$GlueUI" AspectRatioState="4x3"/>
                    <Action type="SetProperty" frame="$this" Width="550"/>
                </State>

                <State name="16x10">
                    <When type="StateGroup" frame="$GlueUI" AspectRatioState="16x10"/>
                    <Action type="SetProperty" frame="$this" Width="550"/>
                </State>
                
                <State name="16x9">
                    <Action type="SetProperty" frame="$this" Width="855"/> 
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    
    <!-- 
    ========================
    SEARCH EDITBOX TEMPLATE
    ========================
    -->
    <Frame type="EditBox" name="SearchEditBoxTemplate" template="StandardSmallButtonTemplates/StandardSmallSearchBoxTemplate">
    
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent/$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/DividerAnchor" pos="Min" offset="-9"/>
        </Frame>

        <!-- Doing the edit box animation this way because apparently you lose focus on the edit box otherwise -->
        <Animation name="ListEmptyFilterPrep">
            <Event event="EmptyFilter" action="DirectionForward,Play" frame="$parent"/>
            <Event event="NotEmpty" action="DirectionForward,Play" frame="$parent"/>
            <Event event="Empty" action="DirectionReverse,Play" frame="$parent"/>
            <Event event="Loading" action="DirectionReverse,Play" frame="$parent"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0.00" value="False"/>
                <Key type="Bool" time="0.01" value="True"/>
            </Controller>
        </Animation>
    </Frame>
    
    
    <!-- 
    ========================
    FILTER EDITBOX TEMPLATE
    ========================
    -->
    <Frame type="EditBox" name="FilterEditBoxTemplate" template="StandardSmallButtonTemplates/StandardSmallFilterBoxTemplate">
    
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent/$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/DividerAnchor" pos="Min" offset="-9"/>
        </Frame>

        <!-- Doing the edit box animation this way because apparently you lose focus on the edit box otherwise -->
        <Animation name="ListEmptyFilterPrep">
            <Event event="EmptyFilter" action="DirectionForward,Play" frame="$parent"/>
            <Event event="NotEmpty" action="DirectionForward,Play" frame="$parent"/>
            <Event event="Empty" action="DirectionReverse,Play" frame="$parent"/>
            <Event event="Loading" action="DirectionReverse,Play" frame="$parent"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0.00" value="False"/>
                <Key type="Bool" time="0.01" value="True"/>
            </Controller>
        </Animation>
    </Frame>
    
    
    <!-- 
    ========================
    BATTLEMAPINFOPANEL
    ========================
    -->
    <Frame type="BattleMapInfoPanel" name="BattleMapInfoPanelTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <MapHandle val="{$ancestor[@name=DetailsPanel]/BattleMapFrame/@MapHandle}"/>
        
        <Frame type="BattleMapFrame" name="BattleMapFrame">
            <Anchor relative="$parent"/>
            <MapHandle val="{$parent/@MapHandle}"/>
        </Frame>

        <Frame type="Label" name="MapNameLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Height val="32"/>
            <Style val="@CustomGames_MapInfoPreview_Title"/>
            <Text val="{$parent/BattleMapFrame/@Name}"/>
            <AcceptsMouse val="True"/>
            <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="AuthorLabel">
            <Anchor side="Top" relative="$parent/MapNameLabel" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Author"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="Property" frame="$parent/BattleMapFrame" IsBlizzardMap="False"/>
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>

                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Image" name="BlizzardIcon">
            <Anchor side="Top" relative="$parent/MapNameLabel" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="1"/>
            <Width val="40"/>
            <ScaleHeightToTexture val="True"/>
            <Texture val="@UI/Glues_Icons_BlizzardAuthored_Normal"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent/BattleMapFrame" IsBlizzardMap="True"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>

                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="ModeLabel">
            <Anchor side="Top" relative="$parent/AuthorLabel" pos="Max" offset="18"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Info"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="Label" name="PlayerLabel">
            <Anchor side="Top" relative="$parent/ModeLabel" pos="Max" offset="18"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Info"/>
            <Visible val="False"/>
        </Frame>
 
        <Frame type="TextFormatFrame" name="FormattedInfoText">
            <Anchor relative="$parent"/>
            <Replacement index="0" val="{$parent/PlayerLabel/@Text}"/>
            <Replacement index="1" val="{$parent/BattleMapFrame/@Genre}"/>
            <FormatText val="%0% | %1%"/>
        </Frame>

        <Frame type="Label" name="InfoLabel">
            <Anchor side="Top" relative="$parent/AuthorLabel" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent/AuthorLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/VersionLabel" pos="Min" offset="-30"/>
            <AcceptsMouseTooltip val="True"/>
            <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
            <Style val="@CustomGames_MapInfoPreview_Info"/>
            <Text val="{$parent/FormattedInfoText/@Text}"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="VersionLabel">
            <Anchor side="Top" relative="$parent/InfoLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Info"/>
            <Text val="{$parent/FormattedVersionText/@Text}"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
 
        <Frame type="TextFormatFrame" name="FormattedVersionText">
            <Anchor relative="$parent"/>
            <Replacement index="0" val="{$parent/BattleMapFrame/@MajorVersionNum}"/>
            <Replacement index="1" val="{$parent/BattleMapFrame/@MinorVersionNum}"/>
            <FormatText val="@UI/ScreenCustomGames/VersionNumFormatText"/>
        </Frame>

        <Frame type="Label" name="DescriptionLabel">
            <Anchor side="Top" relative="$parent/InfoLabel" pos="Max" offset="11"/>
            <Anchor side="Bottom" relative="$parent/WebsiteLabel" pos="Min" offset="-10"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Style val="@CustomGames_MapInfoPreview_Description"/>
            <Text val="{$parent/BattleMapFrame/@Description}"/>
            <AcceptsMouse val="True"/>
            <Options val="Ellipsis | TooltipIfTruncated | NewLineIfTruncated"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="WebsiteLabel">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-14"/>
            <Anchor side="Left" relative="$parent/MapNameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MapNameLabel" pos="Max" offset="0"/>
            <Height val="24"/>
            <Style val="@CustomGames_MapInfoPreview_Website"/>
            <AcceptsMouse val="true"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- ERROR LABEL -->
        <Frame type="Label" name="ErrorLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-25"/>
            <Style val="@InternationalListItemTitle_Small_Locked_Normal_Centered"/>
            <Text val=""/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="Equal"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame Type="BattleUserContextMenu" name="UserContextMenu" template="BattleUserContextMenu/BattleUserContextMenu">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>
            
        <StateGroup name="ExtensionModState">
            <DefaultState val="NoExtensionMod"/>
            <State name="ExtensionMod"/>
            <State name="NoExtensionMod"/>
        </StateGroup>
    </Frame>
    
    <!-- 
    ================================
    BATTLEMAPGAMEINFOSCREENSHOTTILELIST
    ================================
    -->
    <Frame type="BattleMapGameInfoScreenshotTileList" name="ScreenshotTileListTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="14"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-14"/>
        <Width val="472"/>
        <Height val="356"/>

        <Frame type="Image" name="BattleMapInfoBg">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Item0" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/Item0" pos="Max" offset="0"/>
            <LayerColor val="02060b"/>
            <Alpha val="190"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <!-- Displays while loading -->
        <Frame type="Frame" name="LoadingSpinner" template="StandardBattlenetTemplates/IdleSpinnerTemplate">
            <Anchor side="Top" relative="$parent/BattleMapInfoBg" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BattleMapInfoBg" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BattleMapInfoBg" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BattleMapInfoBg" pos="Mid" offset="0"/>
            <Visible val="false"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loading"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <!-- This shows if the map has no screenshots -->
        <Frame type="BattleMapIcon" name="MapIcon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Image" name="Image">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <ScaleWidthToTexture val="true"/>
                <TextureType val="Normal"/>
                <AutoSizeClamps maxWidth="466"/>
            </Frame>
                
            <Animation name="FadeIn">
                <Event event="OnShown" action="Reset,Play"/>

                <Controller name="FadeIn" type="Fade" end="Pause">
                    <Key type="Curve" time="0.0" value="0" in="Auto"/>
                    <Key type="Curve" time="0.20" value="255" in="Auto"/>
                </Controller>
            </Animation>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="StateGroup" frame="$ancestor[@name=DetailsPanel]" LoadingState="Loaded"/>
                    <When type="Property" frame="$parent" DataCount="0"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <When type="AnimationState" frame="$this" IconState="Show"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>

                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="BattleMapScreenshotTileListItem" name="Item0">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="466"/>
            <Height val="350"/>
            
            <Frame type="Image" name="ScreenshotImage">
                <Anchor relative="$parent"/>
            </Frame>

            <Frame type="Label" name="CaptionLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
                <Style val="@GlueLabel"/>
                <Visible val="False"/>
            </Frame>
                
            <Animation name="FadeIn">
                <Event event="OnShown" action="Reset,Play"/>

                <Controller name="FadeIn" type="Fade" end="Pause">
                    <Key type="Curve" time="0.0" value="0" in="Auto"/>
                    <Key type="Curve" time="0.20" value="255" in="Auto"/>
                </Controller>
            </Animation>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="Property" frame="$parent" DataCount="0" operator="NotEqual"/>
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="PageSelectorButtonList" name="PageSelector" template="StandardElementsTemplates/PageSelectorButtonListTemplate">

            <Animation name="HideDisabled">
                <Event event="OnEnabled" action="Reset,Stop"/>
                <Event event="OnDisabled" action="Reset,Play"/>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        <!-- BORDER -->
        <Frame type="Frame" name="IconBorder" template="StandardElementsTemplates/ImageBorderTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <RenderPriority val="521"/>
        </Frame>
        
        <!-- EMPTY LABEL -->
        <Frame type="Label" name="EmptyLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Style val="@InternationalListItemTitle_Small_Locked_Normal_Centered"/>
            <Text val="@UI/ScreenCustomGames/InfoTabs/NoSelection"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="Property" frame="$parent" MapHandle="0,0" operator="Equal"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    <!-- 
    ========================
    LOBBY PREVIEW TEMPLATES
    ========================
    -->
    
    <Frame type="LobbyPreviewSlotTileListItem" name="LobbyPreviewPlayerSlotTemplate">
        <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
        <Height val="20"/>
        <AcceptsMouse val="False"/>
        
        <Frame type="Label" name="Label">
            <Anchor relative="$parent"/>
            <Style val="@CustomGames_LobbyPreview_PlayerSlot_Inactive"/>
            <Text val="{$parent/@PlayerName}"/>
            <AcceptsMouse val="False"/>
        </Frame>

        <StateGroup name="LobbyActiveState">
            <DefaultState val="PlayerSlotNotOpen"/>
            
            <State name="PlayerSlotOccupiedPlayer">
                <When type="Property" LobbySlotPreviewState="OCCUPIED"/>
                <Action type="SetProperty" frame="Label" Style="@CustomGames_LobbyPreview_PlayerSlot_Active"/>
                <Action type="SetProperty" frame="Label" Text="{$parent/@PlayerName}"/>
            </State>
            <State name="PlayerSlotOccupiedAI">
                <When type="Property" LobbySlotPreviewState="COMPUTER"/>
                <Action type="SetProperty" frame="Label" Style="@CustomGames_LobbyPreview_PlayerSlot_AI"/>
                <Action type="SetProperty" frame="Label" Text="@UI/ScreenCustomGames/Lobbies/AIPlayer"/>
            </State>
            <State name="PlayerSlotOpen">
                <When type="Property" LobbySlotPreviewState="OPEN"/>
                <Action type="SetProperty" frame="Label" Style="@CustomGames_LobbyPreview_PlayerSlot_Inactive"/>
