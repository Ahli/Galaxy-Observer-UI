                <Action type="SetProperty" frame="OwnedLabel" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="OwnedLabel" LightAdjustColor="#WarChestNodeColor_ProgressBar_Primary"/>
                <Action type="SetProperty" frame="LockIcon" Visible="false"/>
            </State>
        </StateGroup>
    </Frame>
    
    
