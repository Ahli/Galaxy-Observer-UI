    <!--
    =================================
    War Chest Tier Node Tooltip Template
    =================================
    -->
    <Frame type="WarChestTierTooltip" name="WarChestNodeTooltipTemplate">
        <FadeTime val="0"/>
        
        <Frame type="WarChestTierFrame" name="WarChestTierFrame">
            <Anchor relative="$parent"/>
            <Season val="{$parent/@Season}"/>
            <Round val="{$parent/@Round}"/>
            <Tier val="{$parent/@Tier}"/>
            <RewardGroup val="{$parent/@RewardGroup}"/>
        </Frame>
        
        <!--
        CurrentXPVal and GoalXPVal do the subtraction math to convert absolute Min/Max XP vals into a local range (0 to 300k)
        CurrentXP binds the result and clamps it between 0 and the max (300k)
        CurrentXPWithCommas and GoalXPWithCommas add commas to the values and output as text
        CurrentXPFinalBuffer evaluates the final current xp and sets it to 0 in the case that it is at or below zero. This avoids some bugs with countdown labels and property binding replacement indeces.
        -->
        <Frame type="TextFormatFrame" name="FormattedXPText">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <FormatText val="@UI/ScreenCollection/WarChest/Tooltip/MinMaxXP"/>
            <Replacement val="{$this/CurrentXPFinalBuffer/@Text}" index="0"/>
            <Replacement val="{$this/GoalXPWithCommas/@Text}" index="1"/>
            
            <Frame type="Label" name="CurrentXP">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
                <Text val="0"/>
            </Frame>
            
            <Frame type="Label" name="CurrentXPFinalBuffer">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
                <Text val="{$parent/CurrentXPWithCommas/@Text}"/>
            </Frame>
            
            <Frame type="MathFrame" name="CurrentXPVal">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
                <A val="{$WarChestFrame/@EarnedXP}"/>
                <B val="{$parent/$parent/WarChestTierFrame/@MinXP}"/>
                <Operator val="Subtract"/>
            </Frame>
            
            <Frame type="MathFrame" name="GoalXPVal">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
                <A val="{$parent/$parent/WarChestTierFrame/@MaxXP}"/>
                <B val="{$parent/$parent/WarChestTierFrame/@MinXP}"/>
                <Operator val="Subtract"/>
            </Frame>
        
            <Frame type="CountdownLabel" name="CurrentXPWithCommas">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
                <FormatNumber val="true"/>
                <CurrentValue val="{$parent/CurrentXP/@Text}"/>
            </Frame>
            
            <Frame type="CountdownLabel" name="GoalXPWithCommas">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
                <FormatNumber val="true"/>
                <CurrentValue val="{$parent/GoalXPVal/@ResultInt}"/>
            </Frame>
            
            <StateGroup name="CurrentXPRangeState">
                <DefaultState val="BelowMin"/>
                
                <State name="BetweenMinMax">
                    <When type="Property" frame="CurrentXPVal" Operator="LessThanOrEqual" Result="{$this/GoalXPVal/@Result}"/>
                    <When type="Property" frame="CurrentXPVal" Operator="GreaterThan" Result="0"/>
                    <Action type="SetProperty" frame="CurrentXP" Text="{$parent/CurrentXPVal/@ResultInt}"/>
                    <Action type="SetProperty" frame="CurrentXPFinalBuffer" Text="{$parent/CurrentXPWithCommas/@Text}"/>
                </State>
                
                <State name="AboveMax">
                    <When type="Property" frame="CurrentXPVal" Operator="GreaterThan" Result="{$this/GoalXPVal/@Result}"/>
                    <Action type="SetProperty" frame="CurrentXP" Text="{$parent/GoalXPVal/@ResultInt}"/>
                    <Action type="SetProperty" frame="CurrentXPFinalBuffer" Text="{$parent/CurrentXPWithCommas/@Text}"/>
                </State>
                
                <State name="BelowMin">
                    <Action type="SetProperty" frame="CurrentXP" Text="0"/>
                    <Action type="SetProperty" frame="CurrentXPFinalBuffer" Text="0"/>
                </State>
            </StateGroup>
        </Frame>
        
