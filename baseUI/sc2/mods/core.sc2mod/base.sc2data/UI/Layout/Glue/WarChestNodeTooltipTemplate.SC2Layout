                <Key type="Curve" time="0.000" value="0" inOut="Linear"/>
                <Key type="Curve" time="#WarChestNodeTooltip_BottomReveal_StartTimestamp" value="0" inOut="Linear"/>
                <Key type="Curve" time="#WarChestNodeTooltip_BottomReveal_StopTimestamp" value="255" inOut="Linear"/>
            </Controller>
            
            <Controller type="Fade" frame="BottomBar" end="Pause">
                <Key type="Curve" time="0.000" value="0" inOut="Linear"/>
                <Key type="Curve" time="#WarChestNodeTooltip_BottomReveal_StartTimestamp" value="0" inOut="Linear"/>
                <Key type="Curve" time="#WarChestNodeTooltip_BottomReveal_StopTimestamp" value="255" inOut="Linear"/>
            </Controller>
            
            <Controller type="Fade" frame="OwnedLabel" end="Pause">
                <Key type="Curve" time="0.000" value="0" inOut="Linear"/>
                <Key type="Curve" time="#WarChestNodeTooltip_BottomReveal_StartTimestamp" value="0" inOut="Linear"/>
                <Key type="Curve" time="#WarChestNodeTooltip_BottomReveal_StopTimestamp" value="255" inOut="Linear"/>
            </Controller>
            
            <Controller type="Fade" frame="ClickPromptLabel" end="Pause">
                <Key type="Curve" time="0.000" value="0" inOut="Linear"/>
                <Key type="Curve" time="#WarChestNodeTooltip_BottomReveal_StartTimestamp" value="0" inOut="Linear"/>
                <Key type="Curve" time="#WarChestNodeTooltip_BottomReveal_StopTimestamp" value="255" inOut="Linear"/>
            </Controller>
        </Animation>
        
        <StateGroup name="ListHeightStateGroup">
            <DefaultState val="NotTooTall"/>
        
            <State name="TooTall">
                <When type="Property" frame="ListRangeFrame" OverHeight="true"/>
                <Action type="SetProperty" frame="SelectedBundle/TileList" Alpha="0"/>
                <Action type="SetProperty" frame="SelectedBundleLong/TileList" Alpha="255"/>
                <Action type="SetAnchor" frame="DescriptionLabel" side="Bottom" relative="$parent/SelectedBundleLong" pos="Min" offset="-9"/>
            </State>
        
            <State name="NotTooTall">
                <Action type="SetProperty" frame="SelectedBundle/TileList" Alpha="255"/>
                <Action type="SetProperty" frame="SelectedBundleLong/TileList" Alpha="0"/>
                <Action type="SetAnchor" frame="DescriptionLabel" side="Bottom" relative="$parent/SelectedBundle" pos="Min" offset="-9"/>
            </State>
        </StateGroup>
    </Frame>
    
