    <Frame type="Frame" name="MultiplayerVersusAIFrameTemplate" template="MultiplayerMatchmakingFrame/MultiplayerMatchmakingFrameTemplate">
        <Frame type="Label" name="TitleLabel">
            <Text val="@UI/ScreenMultiplayer/VersusAIButton"/>
        </Frame>

        <Frame type="Label" name="SubtitleLabel">
            <Text val="@UI/ScreenMatchmaking/VersusAIDescription"/>
        </Frame>
        
        <Frame type="MatchmakingQueueTileList" name="QueueTileList">
            <TagString index="0" val="Coop"/>
            <LastQueueSettingName val="PlayGame.LastCoopGameTypeQueue"/>

            <Frame type="MatchmakingQueueTileListItem" name="Item0" template="MultiplayerMatchmakingFrame/MatchmakingQueueTileListItemLeft">
                <Width val="208"/>
            </Frame>

            <Frame type="MatchmakingQueueTileListItem" name="Item1" template="MultiplayerMatchmakingFrame/MatchmakingQueueTileListItemMiddle">
                <Width val="220"/>
            </Frame>

            <Frame type="MatchmakingQueueTileListItem" name="Item2" template="MultiplayerMatchmakingFrame/MatchmakingQueueTileListItemMiddle">
                <Width val="220"/>
            </Frame>

            <Frame type="MatchmakingQueueTileListItem" name="Item3" template="MultiplayerMatchmakingFrame/MatchmakingQueueTileListItemRight">
                <Width val="208"/>
            </Frame>
        </Frame>

        <Frame type="MatchmakingVersusAIPanel" name="VersusAIPanel" template="MatchmakingVersusAIPanel/MatchmakingVersusAIPanelTemplate">
            <Anchor side="Top" relative="$parent/QueueTileList" pos="Max" offset="-31"/>
            <Anchor side="Left" relative="$Anchor_Standard_Left" pos="Min" offset="0"/>
            <Visible val="True"/>
            <QueueHandle val="{$parent/QueueTileList/@SelectedQueueHandle}"/>
            
            <!-- When you are in a group and not the party leader, we'll display the league panel info for 1vAI -->
            <StateGroup name="QueueSelectabilityState">
                <DefaultState val="Selectable"/>
                
                <State name="NotSelectable3">
                    <When type="Property" frame="$parent/QueueTileList" HasSelectedData="False"/>
                    <When type="Property" frame="$ScreenForeground_PartyPanel/TileList" operator="GreaterThanOrEqual" DataCount="3"/>
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/Item3/@QueueHandle}"/>
                </State>
                
                <State name="NotSelectable2">
                    <When type="Property" frame="$parent/QueueTileList" HasSelectedData="False"/>
                    <When type="Property" frame="$ScreenForeground_PartyPanel/TileList" operator="GreaterThanOrEqual" DataCount="2"/>
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/Item2/@QueueHandle}"/>
                </State>
                
                <State name="NotSelectable1">
                    <When type="Property" frame="$parent/QueueTileList" HasSelectedData="False"/>
                    <When type="Property" frame="$ScreenForeground_PartyPanel/TileList" operator="GreaterThanOrEqual" DataCount="1"/>
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/Item0/@QueueHandle}"/>
                </State>
                
                <State name="Selectable">
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/@SelectedQueueHandle}"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>

    <Frame type="Frame" name="MultiplayerTeamFrameTemplate" template="MultiplayerMatchmakingFrame/MultiplayerMatchmakingFrameTemplate">
        <Frame type="Label" name="TitleLabel">
            <Text val="@UI/ScreenMultiplayer/TeamButton"/>
        </Frame>

        <Frame type="Label" name="SubtitleLabel">
            <Text val="@UI/ScreenMatchmaking/TeamDescription"/>
        </Frame>
        
        <Frame type="MatchmakingQueueTileList" name="QueueTileList">
            <TagString index="0" val="Norm"/>
            <TagString index="1" val="Comp"/>
            <TagString index="2" val="Ladd"/>
            <TagString index="3" val="Team"/>
            <LastQueueSettingName val="PlayGame.LastGameType"/>
        </Frame>

        <Frame type="MatchmakingLeaguePanel" name="LeaguePanel" template="MatchmakingLeaguePanel/MatchmakingLeaguePanelTemplate">
            <Anchor side="Top" relative="$parent/QueueTileList" pos="Max" offset="-31"/>
            <Anchor side="Left" relative="$Anchor_Standard_Left" pos="Min" offset="0"/>
            <Visible val="True"/>
            <QueueHandle val="{$parent/QueueTileList/@SelectedQueueHandle}"/>
            <Race val="{$parent/RaceTileList/@Race}"/>
            
            <!-- When you are in a group and not the party leader, we'll display the league panel info for 2v2 -->
            <StateGroup name="QueueSelectabilityState">
                <DefaultState val="Selectable"/>
                
                <State name="NotSelectable4">
                    <When type="Property" frame="$parent/QueueTileList" HasSelectedData="False"/>
                    <When type="Property" frame="$ScreenForeground_PartyPanel/TileList" operator="GreaterThanOrEqual" DataCount="4"/>
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/Item3/@QueueHandle}"/>
                </State>
                
                <State name="NotSelectable3">
                    <When type="Property" frame="$parent/QueueTileList" HasSelectedData="False"/>
                    <When type="Property" frame="$ScreenForeground_PartyPanel/TileList" operator="GreaterThanOrEqual" DataCount="3"/>
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/Item2/@QueueHandle}"/>
                </State>
                
                <State name="NotSelectable2">
                    <When type="Property" frame="$parent/QueueTileList" HasSelectedData="False"/>
                    <When type="Property" frame="$ScreenForeground_PartyPanel/TileList" operator="GreaterThanOrEqual" DataCount="2"/>
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/Item1/@QueueHandle}"/>
                </State>
                
                <State name="Selectable">
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/@SelectedQueueHandle}"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="MatchmakingPlayButton" name="PlayButton" template="MultiplayerMatchmakingFrame/PlayRankedButtonTemplate">
            <Anchor side="Bottom" relative="$Anchor_ActionButton" pos="Min" offset="23"/>
        </Frame>

        <Frame type="MatchmakingPlayButton" name="PlayUnrankedButton" template="MultiplayerMatchmakingFrame/PlayUnrankedButtonTemplate">
            <Anchor side="Top" relative="$parent/PlayButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayButton" pos="Max" offset="20"/>
        </Frame>

        <Frame type="GlueButton" name="MapVetoButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Top" relative="$parent/PlayUnrankedButton" pos="Min" offset="1"/>
            <Anchor side="Left" relative="$parent/PlayUnrankedButton" pos="Max" offset="20"/>
            <Width val="180"/>
            <CollapseLayout val="true"/>
            <HoverSound val="@UI_HoverTick"/>
            <Text val="@UI/ScreenMatchmaking/MapVetoButtonShort"/>
            <Enabled val="{$parent/QueueTileList/@HasSelectedData}"/>

            <Frame type="BattleQueueFrame" name="QueueFrame">
                <Anchor relative="$parent"/>
                <QueueHandle val="{$parent/$parent/QueueTileList/@SelectedQueueHandle}"/>
            </Frame>

            <Frame type="TextFormatFrame" name="FormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/ScreenMatchmaking/MapVetoButton"/>
                <Replacement index="0" val="{$parent/QueueFrame/@ShortName}"/>
            </Frame>
        </Frame>

        <Frame type="Button" name="ActivityButton">
            <Anchor side="Top" relative="$parent/LeaguePanel/LeaguePanelContainmentBG/AnchorFrame" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent/LeaguePanel/LeaguePanelContainmentBG/AnchorFrame" pos="Max" offset="-10"/>
            <Width val="46"/>
            <Height val="46"/>
            <HoverSound val="@UI_HoverTick"/>
            <Visible val="False"/>
            
            <Frame type="Image" name="Icon">
                <Anchor relative="$parent"/>
                <Texture val="@UI/Glues_Icons_Popout_Highlight"/>
            </Frame>
        </Frame>

        <Frame type="MatchmakingMapVetoFrame" name="MapVetoFrame" template="MatchmakingMapVetoFrame/MatchmakingMapVetoFrameTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <QueueHandle val="{$parent/QueueTileList/@SelectedQueueHandle}"/>

            <Animation name="ToggleMapVetoListFrameVisibility">
                <Event event="OnClick" action="ToggleDirection,Play" frame="$parent/MapVetoButton"/>
                <Event event="OnHidden" action="DirectionForward,Play" frame="$parent/MapVetoButton"/>
                <Event event="OnClick" action="DirectionForward,Play" frame="CancelButton"/>
                <Event event="OnClick" action="DirectionForward,Play" frame="AcceptButton"/>
                <Controller name="Name1" type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time=".01" value="false"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="MatchmakingActivityFrame" name="ActivityFrame" template="MatchmakingActivityFrame/MatchmakingActivityFrameTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>

            <Animation name="ToggleMapVetoListFrameVisibility">
                <Event event="OnClick" action="ToggleDirection,Play" frame="$parent/ActivityButton"/>
                <Event event="OnHidden" action="DirectionForward,Play" frame="$parent/ActivityButton"/>
                <Event event="OnClick" action="DirectionForward,Play" frame="CloseButton"/>
                <Controller name="Name1" type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time=".01" value="false"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <Frame type="Frame" name="MultiplayerSoloFrameTemplate" template="MultiplayerMatchmakingFrame/MultiplayerTeamFrameTemplate">
        <Frame type="Label" name="TitleLabel">
            <Text val="@UI/ScreenMultiplayer/SoloButton"/>
        </Frame>

        <Frame type="Label" name="SubtitleLabel">
            <Text val="@UI/ScreenMatchmaking/SoloDescription"/>
        </Frame>

        <Frame type="Label" name="ModeLabel">
            <Visible val="false"/>
        </Frame>

        <Frame type="MatchmakingQueueTileList" name="QueueTileList">
            <TagString index="0" val="Norm"/>
            <TagString index="1" val="Comp"/>
            <TagString index="2" val="Ladd"/>
            <TagString index="3" val="Solo"/>
            <LastQueueSettingName val="PlayGame.SoloGameType"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Label" name="RaceLabel">
            <Anchor side="Top" relative="$parent/ModeLabel" pos="Min" offset="0"/>
        </Frame>
        
        <Frame type="MatchmakingPlayButton" name="PlayUnrankedButton">
            <EnabledWhileInParty val="False"/>
        </Frame>

        <Frame type="MatchmakingLeaguePanel" name="LeaguePanel" template="MatchmakingLeaguePanel/MatchmakingLeaguePanelTemplate">
            <Visible val="True"/>
            
            <StateGroup name="QueueSelectabilityState">
                <DefaultState val="Selectable"/>
                
                <State name="NotSelectable4">
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/@SelectedQueueHandle}"/>
                </State>
                
                <State name="NotSelectable3">
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/@SelectedQueueHandle}"/>
                </State>
                
                <State name="NotSelectable2">
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/@SelectedQueueHandle}"/>
                </State>
                
                <State name="Selectable">
                    <Action type="SetProperty" QueueHandle="{$parent/QueueTileList/@SelectedQueueHandle}"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>

    <Frame type="Frame" name="MultiplayerTestingFrameTemplate" template="MultiplayerMatchmakingFrame/MultiplayerMatchmakingFrameTemplate">
        <Frame type="Label" name="TitleLabel">
            <Text val="@UI/ScreenMultiplayer/TestingButton"/>
        </Frame>

        <Frame type="Label" name="SubtitleLabel">
            <Text val="@UI/ScreenMatchmaking/TestingDescription"/>
        </Frame>

        <Frame type="Label" name="ModeLabel">
            <Visible val="false"/>
        </Frame>

        <Frame type="MatchmakingQueueTileList" name="QueueTileList">
            <TagString index="0" val="Test"/>
            <TagString index="1" val="Comp"/>
            <LastQueueSettingName val="PlayGame.LastTestingGameTypeQueue"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Label" name="RaceLabel">
            <Anchor side="Top" relative="$parent/ModeLabel" pos="Min" offset="0"/>
        </Frame>

        <Frame type="BattleQueueFrame" name="QueueInfoPanel" template="MatchmakingQueueInfoPanel/MatchmakingQueueInfoPanelTemplate">
            <Anchor side="Top" relative="$parent/QueueTileList" pos="Max" offset="-31"/>
            <Anchor side="Left" relative="$Anchor_Standard_Left" pos="Min" offset="0"/>
            <QueueHandle val="{$parent/QueueTileList/@SelectedQueueHandle}"/>
            <Visible val="{$parent/QueueTileList/@HasSelectedData}"/>

            <Frame type="BlurFrame" name="ContainerBGBlur">
                <Visible val="true"/>
            </Frame>

            <Frame type="Image" name="QueueInfoPanelContainmentBG">
                <Visible val="true"/>
            </Frame>

            <Frame type="Image" name="IconImage">
                <Visible val="false"/>
            </Frame>

            <Frame type="Frame" name="IconImageFrameAnchor">
                <Visible val="true"/>
            </Frame>

            <Frame type="ViewMapProfileButton" name="MapProfileButton" template="StandardBattlenetTemplates/BattlenetSmallButtonTemplate">
                <Anchor side="Bottom" relative="$parent/QueueInfoPanelContainmentBG" pos="Max" offset="-15"/>
                <Anchor side="Left" relative="$parent/TitleLabel" pos="Min" offset="-5"/>
                <Text val="@UI/ScreenMatchmaking/ViewMapProfile"/>
                <Width val="200"/>
                <CollapseLayout val="true"/>
                <MapHandle val="{$parent/@ModHandle}"/>
            </Frame>

            <Frame type="LaunchURLButton" name="ForumsButton" template="StandardBattlenetTemplates/BattlenetSmallButtonTemplate">
                <Anchor side="Bottom" relative="$parent/QueueInfoPanelContainmentBG" pos="Max" offset="-15"/>
                <Anchor side="Left" relative="$parent/MapProfileButton" pos="Max" offset="0"/>
                <Text val="@UI/ScreenMatchmaking/ViewForums"/>
                <Nydus val="forum/general"/>
                <Width val="200"/>
            </Frame>
        </Frame>

        <Frame type="GlueButton" name="MapVetoButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Top" relative="$parent/PlayButton" pos="Min" offset="1"/>
            <Anchor side="Left" relative="$parent/PlayButton" pos="Max" offset="20"/>
            <Width val="180"/>
            <CollapseLayout val="true"/>
            <HoverSound val="@UI_HoverTick"/>
            <Text val="@UI/ScreenMatchmaking/MapVetoButtonShort"/>
            <Enabled val="{$parent/QueueTileList/@HasSelectedData}"/>

            <Frame type="BattleQueueFrame" name="QueueFrame">
                <Anchor relative="$parent"/>
                <QueueHandle val="{$parent/$parent/QueueTileList/@SelectedQueueHandle}"/>
            </Frame>

            <Frame type="TextFormatFrame" name="FormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/ScreenMatchmaking/MapVetoButton"/>
                <Replacement index="0" val="{$parent/QueueFrame/@ShortName}"/>
            </Frame>
        </Frame>

        <Frame type="MatchmakingMapVetoFrame" name="MapVetoFrame" template="MatchmakingMapVetoFrame/MatchmakingMapVetoFrameTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <QueueHandle val="{$parent/QueueTileList/@SelectedQueueHandle}"/>

            <Animation name="ToggleMapVetoListFrameVisibility">
                <Event event="OnClick" action="ToggleDirection,Play" frame="$parent/MapVetoButton"/>
                <Event event="OnHidden" action="DirectionForward,Play" frame="$parent/MapVetoButton"/>
                <Event event="OnClick" action="DirectionForward,Play" frame="CancelButton"/>
                <Event event="OnClick" action="DirectionForward,Play" frame="AcceptButton"/>
                <Controller name="Name1" type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time=".01" value="false"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
</Desc>
