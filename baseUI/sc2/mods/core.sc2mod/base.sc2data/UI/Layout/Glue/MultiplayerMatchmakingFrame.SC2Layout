            </Animation>
                
            <StateGroup name="OptInState">
                <DefaultState val="Invalid"/>
                
                <State name="On">
                    <When type="Property" frame="$parent/LeaguePanel" PreventAgentMatch="False"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenMatchmaking/AgentsOnLadderOptIn/PopupButtonOptedIn"/>
