                        <Action type="SetProperty" enabled="true"/>
                    </State>
                </StateGroup> -->
            </Frame>

            <Frame type="MatchmakingQueueTileListItem" name="Item2" template="MultiplayerMatchmakingFrame/MatchmakingQueueTileListItemMiddle">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Item3" pos="Min" offset="21"/>
            </Frame>

            <Frame type="MatchmakingQueueTileListItem" name="Item3" template="MultiplayerMatchmakingFrame/MatchmakingQueueTileListItemRight">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent/RaceTileList" pos="Max" offset="-39"/>
            </Frame>
        </Frame>

        <Frame type="MatchmakingPlayButton" name="PlayButton" template="MultiplayerMatchmakingFrame/PlayButtonTemplate">
