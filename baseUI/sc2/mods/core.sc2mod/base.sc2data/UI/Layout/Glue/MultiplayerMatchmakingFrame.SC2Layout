    <Frame type="Frame" name="MultiplayerMatchmakingFrameTemplate">
        <Anchor relative="$parent"/>
        
        <!-- These animation events are the race's entry index -->
        <!-- 0=Terr, 1=Zerg, 2=Prot, 3=Rand -->
        <StateGroup name="SelectedRaceStateGroup">
            <!-- TERRAN -->
            <State name="TerranState">
                <When type="Property" frame="$ScreenMultiplayer" Visible="true"/>
                <When type="Property" frame="$this" Visible="true"/>
                <When type="Property" frame="RaceTileList" RaceIndex="0"/>
                <Action type="SetState" frame="$MatchmakingCutsceneContainer" group="RaceEventStateGroup" state="TerranState"/>
            </State>

            <!-- ZERG -->
            <State name="ZergState">
                <When type="Property" frame="$ScreenMultiplayer" Visible="true"/>
                <When type="Property" frame="$this" Visible="true"/>
                <When type="Property" frame="RaceTileList" RaceIndex="1"/>
                <Action type="SetState" frame="$MatchmakingCutsceneContainer" group="RaceEventStateGroup" state="ZergState"/>
            </State>

            <!-- PROTOSS -->
            <State name="ProtossState">
                <When type="Property" frame="$ScreenMultiplayer" Visible="true"/>
                <When type="Property" frame="$this" Visible="true"/>
                <When type="Property" frame="RaceTileList" RaceIndex="2"/>
                <Action type="SetState" frame="$MatchmakingCutsceneContainer" group="RaceEventStateGroup" state="ProtossState"/>
            </State>

            <!-- RANDOM -->
            <State name="RandomState">
                <When type="Property" frame="$ScreenMultiplayer" Visible="true"/>
                <When type="Property" frame="$this" Visible="true"/>
                <When type="Property" frame="RaceTileList" RaceIndex="3"/>
                <Action type="SetState" frame="$MatchmakingCutsceneContainer" group="RaceEventStateGroup" state="RandomState"/>
            </State>
        </StateGroup>
        
        <Frame type="Image" name="Vignette">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Coop_Vignette"/>
            <Alpha val="150"/>
        </Frame>
            
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$Anchor_Header" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$Anchor_Header" pos="Max" offset="0"/>
            <Style val="@BattlenetTitle"/>
        </Frame>
        
        <Frame type="Label" name="SubtitleLabel">
            <Anchor side="Top" relative="$Anchor_Subheader" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$Anchor_Subheader" pos="Max" offset="0"/>
            <Style val="@BattlenetSubtitle"/>
        </Frame>

        <Frame type="Button" name="PromotionDebugButton" template="StandardTemplates/StandardButtonTemplate" requiredtoload="IS_LEAGUE_PROMOTION_DEBUG_ENABLED">
            <Anchor side="Top" relative="$parent" pos="Min" offset="150"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-50"/>
            <Text val="Debug"/>
