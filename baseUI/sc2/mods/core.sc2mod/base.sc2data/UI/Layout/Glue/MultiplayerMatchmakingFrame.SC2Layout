    <Frame type="MatchmakingPlayButton" name="PlayButtonTemplate" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
        <Anchor side="Bottom" relative="$Anchor_ActionButton" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$Anchor_ActionButton" pos="Max" offset="0"/>
        <Width val="288"/>
        <Text val="@UI/ScreenMatchmaking/PlayButton"/>
        <HoverSound val="@UI_HoverTick"/>
        <QueueHandle val="{$parent/QueueTileList/@SelectedQueueHandle}"/>

        <Frame type="BattleQueueFrame" name="QueueFrame">
            <Anchor relative="$parent"/>
            <QueueHandle val="{$parent/@QueueHandle}"/>
        </Frame>

        <StateGroup name="TooltipStateGroup">
            <DefaultState val="Allowed"/>
            <State name="NotHasSelectedData">
                <When type="Property" frame="$parent/QueueTileList" HasSelectedData="false"/>
                <Action type="SetProperty" frame="$this" Tooltip="{$parent/QueueTileList/Item0/@Tooltip}"/>
                <Action type="SetProperty" frame="$this" Enabled="false"/>
            </State>
            <State name="NotAllowed">
                <When type="Property" frame="$this" IsRanked="true"/>
                <When type="Property" frame="QueueFrame" IsAllowed="false"/>
                <Action type="SetProperty" frame="$this" Tooltip="@UI/ScreenMatchmaking/QueueNotAllowedTooltip"/>
                <Action type="SetProperty" frame="$this" Enabled="false"/>
            </State>
            <State name="Allowed">
                <Action type="SetProperty" frame="$this" Tooltip=""/>
                <Action type="SetProperty" frame="$this" Enabled="true"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="MatchmakingPlayButton" name="PlayRankedButtonTemplate" template="MultiplayerTemplates/RankedQueueButtonTemplate">
        <Anchor side="Bottom" relative="$Anchor_ActionButton" pos="Min" offset="0"/>
