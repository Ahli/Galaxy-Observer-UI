        <Frame type="Image" name="Icon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="186"/>
            <Height val="124"/>
            <Alpha val="100"/>
            <Texture val="EditorData\Images\UI_Arcade_IconTMarine.tga"/>
        </Frame>
        
        <Frame type="Image" name="InfoDarkBg">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-2"/>
            <Anchor side="Left" relative="$parent/Icon" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/Icon" pos="Max" offset="0"/>
            <Height val="24"/>
            <LayerColor val="000000"/>
            <Alpha val="130"/>
        </Frame>

        <Frame type="Label" name="Name">
            <Anchor side="Top" relative="$parent/InfoDarkBg" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/InfoDarkBg" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/InfoDarkBg" pos="Min" offset="8"/>
            <Anchor side="Right" relative="$parent/InfoDarkBg" pos="Max" offset="-8"/>
            <Height val="23"/>
            <Style val="@CustomGames_Arcade_ListItem_Normal"/>
            <AcceptsMouseTooltip val="true"/>
            <Options val="Ellipsis | TooltipIfTruncated | NoWrapping"/>
            <Text val="Game Name Here"/>
        </Frame>
        
        <Frame type="Image" name="CountBg">
            <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="2"/>
            <Anchor side="Right" relative="$parent/CountLabel" pos="Max" offset="2"/>
            <Height val="21"/>
            <Texture val="Assets/Textures/White.dds"/>
            <Color val="0,0,0"/>
            <Alpha val="200"/>
        </Frame>
        
        <Frame type="Label" name="CountLabel">
            <Anchor side="Top" relative="$parent/CountBg" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/CountBg" pos="Min" offset="2"/>
            <Anchor side="Bottom" relative="$parent/CountBg" pos="Max" offset="0"/>
            <Style val="@CustomGames_Arcade_ListItem_Number"/>
            <Text val="2"/>
        </Frame>

        <Frame type="Image" name="BookmarkedIcon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="-1"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
            <Width val="30"/>
            <ScaleHeightToTexture val="True"/>
            <Texture val="@UI/Glues_Icons_ArcadeBookmarkIcon"/>
        </Frame>

        <StateGroup name="BookmarkedStateGroup">
            <State name="BookmarkedState">
                <When type="AnimationState" frame="$this" BookmarkedState="Bookmarked"/>
                <Action type="SetProperty" frame="BookmarkedIcon" Visible="true"/>
            </State>
            
            <State name="NotBookmarkedState">
                <When type="AnimationState" frame="$this" BookmarkedState="NotBookmarked"/>
                <Action type="SetProperty" frame="BookmarkedIcon" Visible="true"/>
            </State>
        </StateGroup>

        <StateGroup name="SelectedState">
            <State name="Selected">
                <When type="Property" frame="$this" Toggled="True"/>
                <Action type="SetProperty" frame="$this/Icon" Alpha="255"/>
                <Action type="SetProperty" frame="$this/Name" Style="@CustomGames_Arcade_ListItem_Selected"/>
            </State>
        
            <State name="Hover">
                <When type="Property" frame="$this/HoverImage" Visible="True"/>
                <Action type="SetProperty" frame="$this/Icon" Alpha="255"/>
                <Action type="SetProperty" frame="$this/Name" Style="@CustomGames_Arcade_ListItem_Hover"/>
            </State>
            
            <State name="Normal">
                <When type="Property" frame="$this" Toggled="False"/>
                <Action type="SetProperty" frame="$this/Icon" Alpha="100"/>
                <Action type="SetProperty" frame="$this/Name" Style="@CustomGames_Arcade_ListItem_Normal"/>
            </State>
        </StateGroup>
    </Frame>
    
