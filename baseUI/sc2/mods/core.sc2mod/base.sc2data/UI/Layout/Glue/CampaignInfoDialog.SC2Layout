    <!--
    
    
    ACTUAL DIALOG
    
    
    -->

    <Frame type="CampaignInfoDialog" name="CampaignInfoDialog">
        <Anchor relative="$parent"/>
        <VisibilityOnFade val="true"/>
        <AcceptsMouse val="true"/>
        
        <Frame type="Image" name="Blackout">
            <Anchor relative="$parent"/>
            <LayerColor val="000000"/>
            <Alpha val="170"/>
    
            <Animation name="DialogAnimation">
                <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
                <Event event="DialogDismissed" action="DirectionReverse" frame="$parent"/>
                <Event event="DialogDismissed" action="Play" frame="$parent"/>
                
                <Controller type="Anchor" end="Pause" side="Top" relative="true">
                    <Key type="Curve" time="0" value="-20" out="Slow"/>
                    <Key type="Curve" time=".2" value="0" in="Slow"/>
                </Controller>
                
                <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                    <Key type="Curve" time="0" value="40" out="Slow"/>
                    <Key type="Curve" time=".2" value="0" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="BlurFrame" name="BackgroundFrameBlur">
            <Anchor side="Top" relative="$parent/BackgroundFrame" pos="Min" offset="13"/>
            <Anchor side="Left" relative="$parent/BackgroundFrame" pos="Min" offset="13"/>
            <Anchor side="Bottom" relative="$parent/BackgroundFrame" pos="Max" offset="-13"/>
            <Anchor side="Right" relative="$parent/BackgroundFrame" pos="Max" offset="-13"/>

            <Animation name="Blur">
                <Event event="OnShown" action="Reset,Play" frame="$parent" />

                <Controller type="Blur" end="Pause">
                    <Key type="Curve" time="0" value=".35" out="Fast"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="LeftBGGlowPattern">
            <Anchor side="Top" relative="$parent/BackgroundFrame" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundFrame" pos="Min" offset="10"/>
            <Texture val="@UI/Glues_Battlenet_DialogBox_BackGlow"/>
        </Frame>

        <Frame type="Image" name="RightBGGlowPattern">
            <Anchor side="Top" relative="$parent/BackgroundFrame" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundFrame" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundFrame" pos="Max" offset="-10"/>
            <Texture val="@UI/Glues_Battlenet_DialogBox_BackGlow"/>
            <TextureCoords top="0.0" left="1.0" bottom="1.0" right="0.0"/>
        </Frame>

        <Frame type="Frame" name="BackgroundFrame" template="StandardTemplates/GlueDialogTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Height val="860"/>
            <Width val="1270"/>
        </Frame>


        <Frame type="Frame" name="WingsofLibertyInfo" template="CampaignInfoDialog/LibertyInfo">
            <Visible val="false"/>
        </Frame>

        <Frame type="Frame" name="HeartoftheSwarmInfo" template="CampaignInfoDialog/SwarmInfo">
            <Visible val="false"/>
        </Frame>

        <Frame type="Frame" name="LegacyoftheVoidInfo" template="CampaignInfoDialog/VoidInfo">
            <Visible val="false"/>
        </Frame>

        <Frame type="Frame" name="CompleteEditionInfo" template="CampaignInfoDialog/CompleteInfo">
            <Visible val="false"/>
        </Frame>

        <Frame type="Frame" name="NovaCovertOpsAndBundleInfo" template="CampaignInfoDialog/NovaCovertOpsAndBundleInfo">
            <Visible val="false"/>
        </Frame>

        <Frame type="Frame" name="NovaCovertOpsInfo" template="CampaignInfoDialog/NovaCovertOpsInfo">
            <Visible val="false"/>
        </Frame>

        <StateGroup name="InfoVisibility">
            <State name="Liberty">
                <When type="AnimationState" SelectedCampaign="WingsOfLiberty" />
                <Action type="SetProperty" frame="WingsofLibertyInfo" visible="true"/>
            </State>

            <State name="Swarm">
                <When type="AnimationState" SelectedCampaign="HeartOfTheSwarm" />
                <Action type="SetProperty" frame="HeartoftheSwarmInfo" visible="true"/>
            </State>

            <State name="Void">
                <When type="AnimationState" SelectedCampaign="LegacyOfTheVoid" />
                <Action type="SetProperty" frame="LegacyoftheVoidInfo" visible="true"/>
            </State>

            <State name="CompleteEdition">
                <When type="AnimationState" SelectedCampaign="CompleteEdition" />
                <Action type="SetProperty" frame="CompleteEditionInfo" visible="true"/>
            </State>

            <State name="NovaDLC1Bundle">
                <When type="AnimationState" SelectedCampaign="NovaDLC1" />
                <When type="AnimationState" ProductType="MissionBundle" />
                <Action type="SetProperty" frame="NovaCovertOpsAndBundleInfo" visible="true"/>
                <Action type="SetProperty" frame="NovaCovertOpsAndBundleInfo/InfoTabController" SelectedIndex="0"/>
            </State>

            <State name="NovaDLC1">
                <When type="AnimationState" SelectedCampaign="NovaDLC1" />
                <Action type="SetProperty" frame="NovaCovertOpsAndBundleInfo" visible="true"/>
                <Action type="SetProperty" frame="NovaCovertOpsAndBundleInfo/InfoTabController" SelectedIndex="1"/>
            </State>

            <State name="NovaDLC1NoBundle">
                <When type="AnimationState" SelectedCampaign="NovaDLC1NoBundle" />
                <Action type="SetProperty" frame="NovaCovertOpsInfo" visible="true"/>
                <Action type="SetProperty" frame="NovaCovertOpsInfo/InfoTabController" SelectedIndex="0"/>
            </State>

            <State name="NovaDLC2">
                <When type="AnimationState" SelectedCampaign="NovaDLC2" />
                <Action type="SetProperty" frame="NovaCovertOpsInfo" visible="true"/>
                <Action type="SetProperty" frame="NovaCovertOpsInfo/InfoTabController" SelectedIndex="1"/>
            </State>

            <State name="NovaDLC3">
                <When type="AnimationState" SelectedCampaign="NovaDLC3" />
                <Action type="SetProperty" frame="NovaCovertOpsInfo" visible="true"/>
                <Action type="SetProperty" frame="NovaCovertOpsInfo/InfoTabController" SelectedIndex="2"/>
            </State>

            <State name="NovaDLCBundle">
                <When type="AnimationState" SelectedCampaign="NovaDLCBundle" />
                <Action type="SetProperty" frame="NovaCovertOpsAndBundleInfo" visible="true"/>
                <Action type="SetProperty" frame="NovaCovertOpsAndBundleInfo/InfoTabController" SelectedIndex="0"/>
            </State>
        </StateGroup>

        <Frame type="Button" name="CloseButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Bottom" relative="$parent/BackgroundFrame" pos="Max" offset="-42"/>
            <Anchor side="Left" relative="$parent/BackgroundFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundFrame" pos="Mid" offset="0"/>
            <RenderPriority val="530"/>
            <Shortcut val="@UI/Close_Hotkey"/>
            <Shortcut val="@UI/ScreenNavigationBack_Hotkey"/>
            <Text val="@UI/Close_Control"/>
            
            <Frame type="Label" name="Label">
                <Options val="StripHotkeys"/>
            </Frame>
        </Frame>
    </Frame>
</Desc>

