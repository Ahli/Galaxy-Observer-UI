                <Action type="SetProperty" frame="InfoTabController" SelectedIndex="0"/>
            </State>
            <State name="DeluxeEdition" >
                <When type="AnimationState" frame="$parent" ProductType="DeluxeEdition" />
                <Action type="SetProperty" frame="InfoTabController" SelectedIndex="1"/>
            </State>
        </StateGroup>
