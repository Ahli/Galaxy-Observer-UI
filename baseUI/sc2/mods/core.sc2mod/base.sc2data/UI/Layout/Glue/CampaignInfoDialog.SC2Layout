                        <Event event="OnShown" action="Reset,Play" frame="$parent/$parent/InfoPage1"/>
                        <Event event="OnHidden" action="Reset,Stop" frame="$parent/$parent/InfoPage1"/>

                        <Controller type="Style" end="Pause" style1="BattlenetSmallTabButtonSelected" style2="BattlenetSmallTabButtonSelected" frame="Label">
                            <Key type="Curve" time="0" value="0"/>
                        </Controller>
                    </Animation>
