                    <Anchor side="Top" relative="$parent/ReadyButton" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/ReadyButton" pos="Max" offset="8"/>
                    <Enabled val="false"/>
                    <Text val="@UI/ScreenCoopCampaign/MapSelectionPanel/RandomMap"/>
                    <Tooltip val="@UI/BattleTradeShow/CoopCampaign/MapDisabled"/>
                </Frame>
                
                <Frame type="GlueButton" name="ReadyButton">
                    <EnabledDuringRestrictedState val="true"/>
                </Frame>
                
                <!-- Countdown panel for tradeshow specifically, since there's no holopanel -->
                <Frame type="Frame" name="CountdownPanel">
                    <Anchor relative="$parent"/>
                    <Visible val="False"/>

                    <Animation name="Countdown">
                        <Event event="CountdownStart" action="Reset" frame="$parent"/>
                        <Event event="CountdownStart" action="Play" frame="$parent"/>
                        <Event event="CountdownEnd" action="Reset" frame="$parent"/>
                        <Event event="CountdownEnd" action="Stop" frame="$parent"/>

                        <Controller type="Visibility" end="Pause">
                            <Key type="Bool" time="0" value="true"/>
                        </Controller>

                        <Controller type="Visibility" end="Pause" frame="CountdownAnim">
                            <Key type="Bool" time="0" value="true"/>
                        </Controller>
                        
                        <Controller type="Sound" end="Stop">
                            <Key type="Sound" time="0.2" sound="@AC_UI_Countdown_5MxFull"/>
                        </Controller>
                    </Animation>

                    <Frame type="Frame" name="CountdownAnim" template="StandardBattlenetTemplates/CountdownPanelTemplate">
                    </Frame>
                </Frame>
                
                <!-- We need to set the standard light for the Foreground Cutscene because it is not receiving the mode from code which would normally set this using a StateGroup -->
                
                <Frame type="CutsceneFrame" name="ForegroundCutscene">
                    <Animation name="SetStandardLightTradeshow">
                        <Event event="OnShown" action="Reset,Play"/>
                        
                        <Controller type="Cutscene" end="Pause">
                            <Key type="Cutscene" action="AddFilter" time="0.0" filter="Standard"/>
                            <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Mutator"/>
                        </Controller>
                    </Animation>
                </Frame>
                
                <Frame type="Button" name="DifficultyButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
                    <Anchor side="Top" relative="$parent/ReadyButton" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/ReadyButton" pos="Max" offset="5"/>
                </Frame>
                
                <!-- Hiding the difficulty bonus label since players can't really earn XP in tradeshow -->
                <Frame type="Label" name="DifficultyBonusLabel">
                    <Visible val="False"/>
                </Frame>
                
                <Frame type="Frame" name="CommanderInfoContainer">
                    <Frame type="Label" name="NameLabel">
                        <Text val="{$parent/$parent/@Commander[Name]}"/>
                    </Frame>
                </Frame>
                
                <Frame type="Frame" name="DetailsContainer">
                    <Frame type="Frame" name="DetailsBackground">
                        <Frame type="Image" name="CommanderDetailsBackground">
                            <Texture val="{$parent/$parent/$parent/@Commander[TraitIcon]}" layer="1"/>
                        </Frame>
                        <Frame type="Label" name="LevelLabel">
                            <Text val="{$parent/$parent/$parent/@Level}"/>
                        </Frame>
                    </Frame>
                    <Frame type="Frame" name="InfoContainer">
                        <Frame type="Label" name="DescriptionLabel">
                            <Text val="{$parent/$parent/$parent/@Commander[Description]}"/>
                        </Frame>
                        <Frame type="Label" name="DetailsLabel">
                            <Text val="{$parent/$parent/$parent/@Commander[Details]}"/>
                        </Frame>
                    </Frame>
                </Frame>
            </Frame>
            
            <Frame type="Button" name="ReadyButton">
                <Visible val="false"/>
            </Frame>
        </Frame>
    </Frame>

