<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="BattleUserFrame" name="BattleUserFrame">
        <Anchor side="Right" relative="$this/NameLabel" pos="Max" offset="0"/>
        <CollapseLayout val="true"/>

        <StateGroup name="VisibleStateGroup">
            <State name="VisibleState">
                <When type="Property" frame="$this" PresenceId="0"/>
                <Action type="SetProperty" Visible="false"/>
            </State>
        </StateGroup>

        <Frame type="RaceCacheFrame" name="RaceFrame">
            <Anchor relative="$parent"/>
        </Frame>

        <Frame type="Image" name="RaceIcon">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="30"/>
            <Height val="30"/>
            <Texture val="{$parent/RaceFrame/@RaceIcon}"/>
        </Frame>

        <Frame type="Label" name="NameLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/RaceIcon" pos="Max" offset="5"/>
            <Text val="{$parent/@DisplayName}"/>
            <AcceptsMouse val="true"/>
            <ContextMenu val="$parent/UserContextMenu"/>
            <ContextMenuButtons val="Left|Right"/>
        </Frame>

        <Frame type="BattleUserContextMenu" name="UserContextMenu" template="BattleUserContextMenu/BattleUserContextMenu">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <PresenceId val="{$parent/@PresenceId}"/>
        </Frame>
    </Frame>

    <Frame type="LadderTeamTileListItem" name="LadderTeamSelectionTileListItem">
        <Height val="50"/>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
            <Alpha val="255"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Over"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Selected"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="MatchmakingLeagueIcon" name="LeagueIcon" template="MatchmakingLeagueIcon/MatchmakingLeagueIconMiniTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <League val="{$parent/@League}"/>
            <LeagueTier val="{$parent/@Tier}"/>
        </Frame>

        <Frame type="Label" name="QueueLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LeagueIcon" pos="Max" offset="0"/>
            <Width val="70"/>
            <Text val="{$this/QueueFrame/@ShortName}"/>

            <Frame type="BattleQueueFrame" name="QueueFrame">
                <Anchor relative="$parent"/>
                <QueueId val="{$parent/$parent/@QueueId}"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="RankLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/QueueLabel" pos="Max" offset="10"/>
            <Width val="70"/>
            <Text val="{$parent/@Rank}"/>
        </Frame>

        <!-- TODO: Make this dynamic width based on the number of players -->
        <!-- This assumes no more than 4 players a team -->
        <Frame type="BattleUserFrame" name="PlayerFrame0" template="UserProfileLadderFriendsFrame/BattleUserFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/RankLabel" pos="Max" offset="10"/>
            <PresenceId val="{$parent/@PresenceId[0]}"/>

            <Frame type="RaceCacheFrame" name="RaceFrame">
                <RaceFourCC val="{$parent/$parent/@Race[0]}"/>
            </Frame>
        </Frame>

        <Frame type="BattleUserFrame" name="PlayerFrame1" template="UserProfileLadderFriendsFrame/BattleUserFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayerFrame0" pos="Max" offset="0"/>
            <PresenceId val="{$parent/@PresenceId[1]}"/>

            <Frame type="RaceCacheFrame" name="RaceFrame">
                <RaceFourCC val="{$parent/$parent/@Race[1]}"/>
            </Frame>
        </Frame>

        <Frame type="BattleUserFrame" name="PlayerFrame2" template="UserProfileLadderFriendsFrame/BattleUserFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayerFrame1" pos="Max" offset="0"/>
            <PresenceId val="{$parent/@PresenceId[2]}"/>

            <Frame type="RaceCacheFrame" name="RaceFrame">
                <RaceFourCC val="{$parent/$parent/@Race[2]}"/>
            </Frame>
        </Frame>

        <Frame type="BattleUserFrame" name="PlayerFrame3" template="UserProfileLadderFriendsFrame/BattleUserFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayerFrame2" pos="Max" offset="0"/>
            <PresenceId val="{$parent/@PresenceId[3]}"/>

            <Frame type="RaceCacheFrame" name="RaceFrame">
                <RaceFourCC val="{$parent/$parent/@Race[3]}"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="TeamTypeLabel">
            <Anchor side="Top" relative="$parent/PlayerFrame3" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/PlayerFrame3" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayerFrame3" pos="Max" offset="0"/>
            <Text val="@UI/UserProfileLadderFriendsFrame/Random"/>
            <Visible val="false"/>

            <StateGroup name="VisibleStateGroup">
                <State name="VisibleState">
                    <When type="Property" frame="$parent" IsArrangedTeam="false"/>
                    <Action type="SetProperty" Visible="true"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="DivisionLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Text val="{$parent/@DivisionName}"/>
        </Frame>
    </Frame>

    <Frame type="LadderTeamShowcaseSelectionFrame" name="LadderTeamShowcaseSelectionFrame">
        <!-- This tooltip is just for blocking tooltips behind this panel -->
        <Frame type="StandardTooltip" name="BackgroundTooltip">
            <Frame type="Label" name="Label">
                <Anchor relative="$parent"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="FullscreenBlackoutImage">
            <Anchor relative="$parent"/>
            <Color val="0,0,0"/>
            <Alpha val="180"/>
            <AcceptsMouse val="true"/>
            <TooltipFrame val="BackgroundTooltip"/>
        </Frame>
        
        <Frame type="Image" name="LeftBGGlowPattern">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Min" offset="40"/>
            <Texture val="@UI/Glues_Battlenet_DialogBox_BackGlow"/>
        </Frame>

        <Frame type="Image" name="RightBGGlowPattern">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Max" offset="-40"/>
            <Texture val="@UI/Glues_Battlenet_DialogBox_BackGlow"/>
            <TextureCoords top="0.0" left="1.0" bottom="1.0" right="0.0"/>
        </Frame>

        <Frame type="Image" name="BackgroundImage">
            <Anchor relative="$parent/AnchorFrame"/>
            <Texture val="@UI/Glues_Battlenet_DialogBox_StandardBG"/>
            <TextureType val="NineSlice"/>
            <Tiled val="false"/>
        </Frame>
        
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="900"/>
            <Height val="700"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="30"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="30"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-30"/>
            <Style val="@Arcade_Title"/>
            <Text val="{$this/FormatFrame/@Text}"/>

            <Frame type="TextFormatFrame" name="FormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/LadderTeamShowcaseSelectionFrame/Title"/>
                <Replacement index="0" val="{$parent/IndexFrame/@ResultInt}"/>
            </Frame>

            <Frame type="MathFrame" name="IndexFrame">
                <Anchor relative="$parent"/>
                <A val="{$parent/$parent/@ShowcaseIndex}"/>
                <B val="1"/>
                <Operator val="Add"/>
            </Frame>
        </Frame>

        <Frame type="TileList" name="TileList">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-20"/>
            <Anchor side="Bottom" relative="$parent/AcceptButton" pos="Min" offset="0"/>
            <AcceptsMouse val="true"/>

            <Frame type="LadderTeamTileListItem" name="Item0" template="UserProfileLadderSummaryFrame/LadderTeamSelectionTileListItem">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item1" template="UserProfileLadderSummaryFrame/LadderTeamSelectionTileListItem">
                <Anchor side="Top" relative="$parent/Item0" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item2" template="UserProfileLadderSummaryFrame/LadderTeamSelectionTileListItem">
                <Anchor side="Top" relative="$parent/Item1" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item3" template="UserProfileLadderSummaryFrame/LadderTeamSelectionTileListItem">
                <Anchor side="Top" relative="$parent/Item2" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item4" template="UserProfileLadderSummaryFrame/LadderTeamSelectionTileListItem">
                <Anchor side="Top" relative="$parent/Item3" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item5" template="UserProfileLadderSummaryFrame/LadderTeamSelectionTileListItem">
                <Anchor side="Top" relative="$parent/Item4" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
            </Frame>

            <Frame type="ScrollBar" name="ScrollBar">
                <Anchor side="Top" relative="$parent" pos="Min" offset="-3"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-3"/>

                <Image val="$root/StandardBattlenetTemplates/GluesSocialScrollBarBackgroundImage"/>
                <Button val="$root/StandardBattlenetTemplates/GluesSocialScrollBarButtonTemplate"/>
                <Orientation val="Vertical"/>
                <Width val="16"/>
            </Frame>
        </Frame>

        <Frame type="Button" name="AcceptButton" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Mid" offset="-5"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="-20"/>
            <Text val="@UI/StandardDialog_Ok_Control"/>
            <Width val="280"/>
        </Frame>

        <Frame type="Button" name="CancelButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Mid" offset="5"/>
            <Anchor side="Top" relative="$parent/AcceptButton" pos="Min" offset="0"/>
            <Text val="@UI/StandardDialog_Cancel_Control"/>
            <Width val="280"/>
            <Shortcut val="@UI/ScreenNavigationBack_Hotkey"/>
        </Frame>
    </Frame>

    <Frame type="LadderTeamTileListItem" name="LadderTeamShowcaseTileListItem">
        <Width val="350"/>
        <Height val="300"/>

        <Frame type="Image" name="RaceBackground">
            <Anchor relative="$parent"/>

            <Frame type="RaceCacheFrame" name="RaceCacheFrame">
                <Anchor relative="$parent"/>
                <RaceFourCC val="{$parent/$parent/@PrimaryRace}"/>
            </Frame>

            <!-- TODO: Set the race background image -->
            <StateGroup name="RaceStateGroup">
                <State name="TerranState">
                    <When type="Property" frame="RaceCacheFrame" Race="Terr"/>
                </State>

                <State name="ZergState">
                    <When type="Property" frame="RaceCacheFrame" Race="Terr"/>
                </State>

                <State name="ProtossState">
                    <When type="Property" frame="RaceCacheFrame" Race="Terr"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
            <Alpha val="255"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Over"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Selected"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="MatchmakingLeagueIcon" name="LeagueIcon" template="MatchmakingLeagueIcon/MatchmakingLeagueIconTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="-100"/>
            <League val="{$parent/@League}"/>
            <LeagueTier val="{$parent/@Tier}"/>
            <HideBanner val="true"/>
        </Frame>

        <Frame type="Label" name="QueueLabel">
            <Anchor side="Top" relative="$parent/LeagueIcon" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/LeagueIcon" pos="Max" offset="-70"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Text val="{$this/QueueFrame/@ShortName}"/>
            <Style val="@Arcade_Title"/>

            <Frame type="BattleQueueFrame" name="QueueFrame">
                <Anchor relative="$parent"/>
                <QueueId val="{$parent/$parent/@QueueId}"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="RankLabel">
            <Anchor side="Top" relative="$parent/QueueLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/QueueLabel" pos="Min" offset="0"/>
            <Width val="70"/>
            <Text val="{$this/FormatFrame/@Text}"/>

            <Frame type="TextFormatFrame" name="FormatFrame">
                <Anchor relative="$parent"/>
                <FormatText val="@UI/LadderTeamShowcaseFrame/Rank"/>
                <Replacement index="0" val="{$parent/$parent/@Rank}"/>
            </Frame>
        </Frame>

        <!-- TODO: Make this dynamic width based on the number of players -->
        <!-- This assumes no more than 4 players a team -->
        <Frame type="BattleUserFrame" name="PlayerFrame0" template="UserProfileLadderFriendsFrame/BattleUserFrame">
            <Anchor side="Top" relative="$parent/RankLabel" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent/RankLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="30"/>
            <PresenceId val="{$parent/@PresenceId[0]}"/>

            <Frame type="RaceCacheFrame" name="RaceFrame">
                <RaceFourCC val="{$parent/$parent/@Race[0]}"/>
            </Frame>
        </Frame>

        <Frame type="BattleUserFrame" name="PlayerFrame1" template="UserProfileLadderFriendsFrame/BattleUserFrame">
            <Anchor side="Top" relative="$parent/PlayerFrame0" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayerFrame0" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="30"/>
            <PresenceId val="{$parent/@PresenceId[1]}"/>

            <Frame type="RaceCacheFrame" name="RaceFrame">
                <RaceFourCC val="{$parent/$parent/@Race[1]}"/>
            </Frame>
        </Frame>

        <Frame type="BattleUserFrame" name="PlayerFrame2" template="UserProfileLadderFriendsFrame/BattleUserFrame">
            <Anchor side="Top" relative="$parent/PlayerFrame1" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayerFrame1" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="30"/>
            <PresenceId val="{$parent/@PresenceId[2]}"/>

            <Frame type="RaceCacheFrame" name="RaceFrame">
                <RaceFourCC val="{$parent/$parent/@Race[2]}"/>
            </Frame>
        </Frame>

        <Frame type="BattleUserFrame" name="PlayerFrame3" template="UserProfileLadderFriendsFrame/BattleUserFrame">
            <Anchor side="Top" relative="$parent/PlayerFrame2" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PlayerFrame2" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="30"/>
            <PresenceId val="{$parent/@PresenceId[3]}"/>

            <Frame type="RaceCacheFrame" name="RaceFrame">
                <RaceFourCC val="{$parent/$parent/@Race[3]}"/>
            </Frame>
        </Frame>
        
        <Frame type="Button" name="EditButton" template="StandardBattlenetTemplates/OptionsButton">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Visible val="false"/>
            
            <StateGroup name="LocalUserStateGroup">
                <State name="LocalUserState">
                    <When type="Property" frame="$BattleUI" LocalToonPresenceId="{$parent/$parent/$parent/@PresenceId}"/>
                    <Action type="SetProperty" Visible="true"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    <Frame type="LadderTeamShowcaseFrame" name="LadderTeamShowcaseFrame">
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_TableLargeHeaderBg"/>
            <TextureType val="NineSlice"/>
            <Tiled val="True"/>
            <Visible val="true"/>
        </Frame>

        <Frame type="Label" name="SeasonLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-12"/>
            <Text val="{$this/SeasonFrame/@SeasonName}"/>
            <Style val="@Arcade_Title"/>

            <Frame type="MatchmakingSeasonFrame" name="SeasonFrame">
                <Anchor relative="$parent"/>
                <SeasonId val="{$this/@CurrentSeasonId}"/>
            </Frame>
        </Frame>

        <Frame type="TileList" name="TileList">
            <Anchor side="Top" relative="$parent" pos="Min" offset="80"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-12"/>

            <Frame type="LadderTeamTileListItem" name="Item0" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseTileListItem">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/Item1" pos="Min" offset="-10"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item1" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseTileListItem">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item2" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseTileListItem">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="10"/>
            </Frame>
        </Frame>
    </Frame>

    <Frame type="Frame" name="UserProfileLadderSummaryFrame">
        <Frame type="LadderTeamShowcaseFrame" name="LadderTeamShowcaseFrame" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseFrame">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Mid" offset="0"/>
            <TeamHash val="{$parent/LadderTeamShowcaseSelectionFrame/@TeamHash}"/>

            <Animation name="Show">
                <Event event="OnClick" action="Reset,Play" frame="$parent/LadderTeamShowcaseSelectionFrame/AcceptButton"/>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0" event="UpdateShowcaseRequest"/>
                </Controller>
            </Animation>

            <Animation name="Index0">
                <Event event="OnClick" action="Reset,Play" frame="TileList/Item0/EditButton"/>

                <Controller type="Property" frame="TileList" property="SelectedIndex" end="Pause">
                    <Key type="Property" time="0" value="0"/>
                </Controller>
            </Animation>

            <Animation name="Index1">
                <Event event="OnClick" action="Reset,Play" frame="TileList/Item1/EditButton"/>

                <Controller type="Property" frame="TileList" property="SelectedIndex" end="Pause">
                    <Key type="Property" time="0" value="1"/>
                </Controller>
            </Animation>

            <Animation name="Index2">
                <Event event="OnClick" action="Reset,Play" frame="TileList/Item2/EditButton"/>

                <Controller type="Property" frame="TileList" property="SelectedIndex" end="Pause">
                    <Key type="Property" time="0" value="2"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="UserProfileLadderPlacementsFrame" name="UserProfileLadderPlacementsFrame" template="UserProfileLadderPlacementsFrame/UserProfileLadderPlacementsFrameTemplate">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
        </Frame>

        <Frame type="LadderTeamShowcaseSelectionFrame" name="LadderTeamShowcaseSelectionFrame" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseSelectionFrame">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <ShowcaseIndex val="{$parent/LadderTeamShowcaseFrame/TileList/@SelectedIndex}"/>
            <QueueId val="{$parent/LadderTeamShowcaseFrame/@QueueId}"/>
            <TeamHash val="{$parent/LadderTeamShowcaseFrame/@TeamHash}"/>

            <Animation name="Show">
                <Event event="OnClick" action="Reset,Play" frame="$parent/LadderTeamShowcaseFrame/TileList/Item0/EditButton"/>
                <Event event="OnClick" action="Reset,Play" frame="$parent/LadderTeamShowcaseFrame/TileList/Item1/EditButton"/>
                <Event event="OnClick" action="Reset,Play" frame="$parent/LadderTeamShowcaseFrame/TileList/Item2/EditButton"/>
                <Event event="OnClick" action="DirectionReverse,Play" frame="AcceptButton"/>
                <Event event="OnClick" action="DirectionReverse,Play" frame="CancelButton"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time=".175" value="255" in="Auto"/>
                </Controller>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".001" value="true"/>
                    <Key type="Bool" time=".175" value="true"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
</Desc>
