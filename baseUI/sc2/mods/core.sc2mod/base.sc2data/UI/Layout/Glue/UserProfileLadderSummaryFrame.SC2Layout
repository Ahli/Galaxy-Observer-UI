                    <Replacement index="0" val="{$ancestor[@type=LadderTeamTileListItem]/@Rank}"/>
                </Frame>
            </Frame>
        </Frame>
        
        <Frame type="Frame" name="TeamContentContainer">
            <Anchor relative="$parent"/>
            
            <Frame type="Image" name="PlayerBackground0">
                <Anchor side="Bottom" relative="$parent/PlayerBackground1" pos="Min" offset="1"/>
                <Anchor side="Left" relative="$parent/PlayerBackground1" pos="Min" offset="0"/>
                <Texture val="@UI/SC2_UI_UserProfile_Ladders_Showcase_TeamBg"/>
                <Visible val="{$parent/PlayerFrame0/@Visible}"/>
            </Frame>
            
            <Frame type="Image" name="PlayerBackground1">
                <Anchor side="Bottom" relative="$parent/PlayerBackground2" pos="Min" offset="1"/>
                <Anchor side="Left" relative="$parent/PlayerBackground2" pos="Min" offset="0"/>
                <Texture val="@UI/SC2_UI_UserProfile_Ladders_Showcase_TeamBg"/>
                <Visible val="{$parent/PlayerFrame1/@Visible}"/>
            </Frame>
            
            <Frame type="Image" name="PlayerBackground2">
                <Anchor side="Bottom" relative="$parent/PlayerBackground3" pos="Min" offset="1"/>
                <Anchor side="Left" relative="$parent/PlayerBackground3" pos="Min" offset="0"/>
                <Texture val="@UI/SC2_UI_UserProfile_Ladders_Showcase_TeamBg"/>
                <Visible val="{$parent/PlayerFrame2/@Visible}"/>
            </Frame>
            
            <Frame type="Image" name="PlayerBackground3">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-36"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="27"/>
                <Texture val="@UI/SC2_UI_UserProfile_Ladders_Showcase_TeamBg"/>
                <Visible val="{$parent/PlayerFrame3/@Visible}"/>
            </Frame>

            <!-- TODO: Make this dynamic width based on the number of players -->
            <!-- This assumes no more than 4 players a team -->
            <Frame type="BattleUserFrame" name="PlayerFrame0" template="UserProfileLadderSummaryFrame/BattleUserLargeFrame">
                <Anchor side="Top" relative="$parent/PlayerBackground0" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/PlayerBackground0" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayerBackground0" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="30"/>
                <PresenceId val="{$ancestor[@type=LadderTeamTileListItem]/@PresenceId[0]}"/>

                <Frame type="RaceCacheFrame" name="RaceFrame">
                    <RaceFourCC val="{$ancestor[@type=LadderTeamTileListItem]/@Race[0]}"/>
                </Frame>
            </Frame>

            <Frame type="BattleUserFrame" name="PlayerFrame1" template="UserProfileLadderSummaryFrame/BattleUserLargeFrame">
                <Anchor side="Top" relative="$parent/PlayerBackground1" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/PlayerBackground1" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayerBackground1" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="30"/>
                <PresenceId val="{$ancestor[@type=LadderTeamTileListItem]/@PresenceId[1]}"/>

                <Frame type="RaceCacheFrame" name="RaceFrame">
                    <RaceFourCC val="{$ancestor[@type=LadderTeamTileListItem]/@Race[1]}"/>
                </Frame>
            </Frame>

            <Frame type="BattleUserFrame" name="PlayerFrame2" template="UserProfileLadderSummaryFrame/BattleUserLargeFrame">
                <Anchor side="Top" relative="$parent/PlayerBackground2" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/PlayerBackground2" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayerBackground2" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="30"/>
                <PresenceId val="{$ancestor[@type=LadderTeamTileListItem]/@PresenceId[2]}"/>

                <Frame type="RaceCacheFrame" name="RaceFrame">
                    <RaceFourCC val="{$ancestor[@type=LadderTeamTileListItem]/@Race[2]}"/>
                </Frame>
            </Frame>

            <Frame type="BattleUserFrame" name="PlayerFrame3" template="UserProfileLadderSummaryFrame/BattleUserLargeFrame">
                <Anchor side="Top" relative="$parent/PlayerBackground3" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/PlayerBackground3" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayerBackground3" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="30"/>
                <PresenceId val="{$ancestor[@type=LadderTeamTileListItem]/@PresenceId[3]}"/>

                <Frame type="RaceCacheFrame" name="RaceFrame">
                    <RaceFourCC val="{$ancestor[@type=LadderTeamTileListItem]/@Race[3]}"/>
                </Frame>
            </Frame>
            
            <StateGroup name="VisibilityState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" IsArrangedTeam="True"/>
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" IsSoloQueue="False"/>
                    <Action type="SetProperty" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Image" name="MedalBackground">
            <Anchor side="Top" relative="$parent" pos="Min" offset="-9"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="-11"/>
            <Texture val="@UI/SC2_UI_UserProfile_Ladders_MedalBg_Unranked"/>
            
            <StateGroup name="MedalState">
                <DefaultState val="Unranked"/>
                
                <State name="GrandMaster">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" League="6"/>
                    <Action type="SetProperty" Texture="@UI/SC2_UI_UserProfile_Ladders_MedalBg_GrandMaster"/>
                </State>
                
                <State name="Master">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" League="5"/>
                    <Action type="SetProperty" Texture="@UI/SC2_UI_UserProfile_Ladders_MedalBg_Platinum"/>
                </State>
                
                <State name="Diamond">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" League="4"/>
                    <Action type="SetProperty" Texture="@UI/SC2_UI_UserProfile_Ladders_MedalBg_Platinum"/>
                </State>
                
                <State name="Platinum">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" League="3"/>
                    <Action type="SetProperty" Texture="@UI/SC2_UI_UserProfile_Ladders_MedalBg_Platinum"/>
                </State>
                
                <State name="Bronze">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" League="2"/>
                    <Action type="SetProperty" Texture="@UI/SC2_UI_UserProfile_Ladders_MedalBg_Gold"/>
                </State>
                
                <State name="Silver">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" League="1"/>
                    <Action type="SetProperty" Texture="@UI/SC2_UI_UserProfile_Ladders_MedalBg_Silver"/>
                </State>
                
                <State name="Bronze">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" League="0"/>
                    <Action type="SetProperty" Texture="@UI/SC2_UI_UserProfile_Ladders_MedalBg_Bronze"/>
                </State>
                
                <State name="Unranked">
                    <Action type="SetProperty" Texture="@UI/SC2_UI_UserProfile_Ladders_MedalBg_Unranked"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor relative="$parent"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor relative="$parent"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="MatchmakingLeagueIcon" name="LeagueIcon" template="MatchmakingLeagueIcon/MatchmakingLeagueIconResizableTemplate">
            <Anchor side="Top" relative="$parent/MedalBackground" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent/MedalBackground" pos="Mid" offset="-6"/>
            <Anchor side="Right" relative="$parent/MedalBackground" pos="Mid" offset="-6"/>
            <Width val="240"/>
            <League val="{$parent/@League}"/>
            <LeagueTier val="{$parent/@Tier}"/>
            <HideBanner val="true"/>
            
            <StateGroup name="AnchorState">
                <DefaultState val="Ranked"/>
                
                <State name="Unranked">
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" League="7"/>
                    <Action type="SetAnchor" side="Top" relative="$parent/MedalBackground" pos="Min" offset="21"/>
                </State>
                
                <State name="Ranked">
                    <Action type="SetAnchor" side="Top" relative="$parent/MedalBackground" pos="Min" offset="13"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Button" name="EditButton" template="UserProfileTemplates/ConfigButtonTemplate">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Visible val="false"/>
            
            <StateGroup name="VisibilityState">
                <DefaultState val="NotVisible"/>
                
                <State name="Visible">
                    <When type="Property" frame="$BattleUI" LocalToonPresenceId="{$parent/$parent/$parent/@PresenceId}"/>
                    <When type="Property" frame="$ancestor[@type=LadderTeamTileListItem]" HasData="True"/>
                    <Action type="SetProperty" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    <Frame type="LadderTeamShowcaseFrame" name="LadderTeamShowcaseFrame">
            
        <Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_TableSmallHeaderBg"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Label" name="SeasonLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="29"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-29"/>
            <Text val="{$this/SeasonFrame/@SeasonName}"/>
            <Style val="Profile_Achievement_SectionHeader"/>

            <Frame type="MatchmakingSeasonFrame" name="SeasonFrame">
                <Anchor relative="$parent"/>
                <SeasonId val="{$this/@CurrentSeasonId}"/>
            </Frame>
        </Frame>

        <Frame type="TileList" name="TileList">
            <Anchor side="Top" relative="$parent" pos="Min" offset="46"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>

            <Frame type="LadderTeamTileListItem" name="Item0" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseTileListItem">
                <Anchor side="Top" relative="$parent/Item1" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/Item1" pos="Min" offset="5"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item1" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseTileListItem">
                <Anchor side="Top" relative="$parent" pos="Min" offset="13"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            </Frame>

            <Frame type="LadderTeamTileListItem" name="Item2" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseTileListItem">
                <Anchor side="Top" relative="$parent/Item1" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="-5"/>
            </Frame>
        </Frame>
    </Frame>

    <Frame type="Frame" name="UserProfileLadderSummaryFrame">
        <Frame type="LadderTeamShowcaseFrame" name="LadderTeamShowcaseFrame" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseFrame">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Height val="458"/>
            <TeamHash val="{$parent/LadderTeamShowcaseSelectionFrame/@TeamHash}"/>
            <QueueId val="{$parent/LadderTeamShowcaseSelectionFrame/@QueueId}"/>

            <Animation name="Show">
                <Event event="OnClick" action="Reset,Play" frame="$parent/LadderTeamShowcaseSelectionFrame/AcceptButton"/>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0" event="UpdateShowcaseRequest"/>
                </Controller>
            </Animation>

            <Animation name="Index0">
                <Event event="OnClick" action="Reset,Play" frame="TileList/Item0/EditButton"/>

                <Controller type="Property" frame="TileList" property="SelectedIndex" end="Pause">
                    <Key type="Property" time="0" value="0"/>
                </Controller>
            </Animation>

            <Animation name="Index1">
                <Event event="OnClick" action="Reset,Play" frame="TileList/Item1/EditButton"/>

                <Controller type="Property" frame="TileList" property="SelectedIndex" end="Pause">
                    <Key type="Property" time="0" value="1"/>
                </Controller>
            </Animation>

            <Animation name="Index2">
                <Event event="OnClick" action="Reset,Play" frame="TileList/Item2/EditButton"/>

                <Controller type="Property" frame="TileList" property="SelectedIndex" end="Pause">
                    <Key type="Property" time="0" value="2"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="UserProfileLadderPlacementsFrame" name="UserProfileLadderPlacementsFrame" template="UserProfileLadderPlacementsFrame/UserProfileLadderPlacementsFrameTemplate">
            <Anchor side="Top" relative="$parent/LadderTeamShowcaseFrame" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Height val="339"/>
        </Frame>

        <Frame type="LadderTeamShowcaseSelectionFrame" name="LadderTeamShowcaseSelectionFrame" template="UserProfileLadderSummaryFrame/LadderTeamShowcaseSelectionFrame">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <ShowcaseIndex val="{$parent/LadderTeamShowcaseFrame/TileList/@SelectedIndex}"/>
            <QueueId val="{$parent/LadderTeamShowcaseFrame/@QueueId}"/>
            <TeamHash val="{$parent/LadderTeamShowcaseFrame/@TeamHash}"/>

            <Animation name="Show">
                <Event event="OnClick" action="Reset,Play" frame="$parent/LadderTeamShowcaseFrame/TileList/Item0/EditButton"/>
                <Event event="OnClick" action="Reset,Play" frame="$parent/LadderTeamShowcaseFrame/TileList/Item1/EditButton"/>
                <Event event="OnClick" action="Reset,Play" frame="$parent/LadderTeamShowcaseFrame/TileList/Item2/EditButton"/>
                <Event event="OnClick" action="DirectionReverse,Play" frame="AcceptButton"/>
                <Event event="OnClick" action="DirectionReverse,Play" frame="CancelButton"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time=".175" value="255" in="Auto"/>
                </Controller>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".001" value="true"/>
                    <Key type="Bool" time=".175" value="true"/>
                </Controller>
            </Animation>
            
            <Animation name="OpenSound">
                <Event event="OnShown" action="Play"/>
                
                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0" sound="@UI_BnetWindowOpen"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
</Desc>
