                    <Anchor relative="$parent"/>
                    <QueueId val="{$ancestor[@type=LadderTeamTileListItem]/@QueueId}"/>
                </Frame>
            </Frame>

            <Frame type="Label" name="RankLabel">
                <Anchor side="Top" relative="$parent/QueueLabel" pos="Max" offset="-7"/>
                <Anchor side="Left" relative="$parent/QueueLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/QueueLabel" pos="Max" offset="0"/>
