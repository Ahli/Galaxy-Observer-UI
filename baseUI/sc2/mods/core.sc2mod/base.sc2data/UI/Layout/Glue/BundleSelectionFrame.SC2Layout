                <Action type="SetProperty" frame="LockedItemIcon" Visible="False"/>
            </State>

            <State name="NotOwned">
                <Action type="SetProperty" frame="StateLabel" Visible="False"/>
                <Action type="SetProperty" frame="LockedItemIcon" Visible="True"/>
            </State>
        </StateGroup>
        
        <StateGroup name="ItemDiscountState">
            <State name="Empty">
