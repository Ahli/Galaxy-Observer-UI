                <Action type="SetProperty" frame="StateLabel" Visible="False"/>
                <Action type="SetProperty" frame="LockedItemIcon" Visible="False"/>
            </State>
    
            <State name="Owned">
                <When type="Property" frame="StoreCacheFrame" IsPurchased="True"/>
                
