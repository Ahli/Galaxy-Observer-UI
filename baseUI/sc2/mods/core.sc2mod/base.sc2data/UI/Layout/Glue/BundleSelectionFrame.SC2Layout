            <State name="NotEmpty">
                <Action type="SetProperty" frame="NameLabel" Visible="True"/>
                <Action type="SetProperty" frame="IconImageContainer" Visible="True"/>
                <Action type="SetProperty" frame="TextBackground" Visible="True"/>
                <Action type="SetProperty" frame="EmptyImage" Visible="False"/>
