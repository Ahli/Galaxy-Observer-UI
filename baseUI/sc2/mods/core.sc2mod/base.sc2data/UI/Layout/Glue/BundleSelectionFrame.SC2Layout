            <Frame type="ScrollBar" name="ScrollBar" template="CollectionTemplates/ScrollBarTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="-3"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            </Frame>
        </Frame>

        <Frame type="SelectedBundleFrame" name="SelectedBundleFrame" template="SelectedBundleFrame/SelectedBundleFrameTemplate">
            <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="0"/>

            <Frame type="Button" name="CloseButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
                <Animation name="ClearBundle">
                    <Event event="OnClick" action="Restart,Play"/>

                    <Controller type="Property" frame="$parent/$parent/BigTileList" property="SelectedIndex" end="Pause">
                        <Key type="Property" time="0" value="#InvalidTileListIndex"/>
                    </Controller>

                    <Controller type="Property" frame="$parent/$parent/ContentTileList" property="SelectedIndex" end="Pause">
                        <Key type="Property" time="0" value="#InvalidTileListIndex"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>

        <StateGroup name="StoreStatusStates">
            <DefaultState val="StoreDown"/>
            
            <State name="StoreAvailable">
                <When type="Property" frame="StoreCacheFrame" IsStoreAvailable="True"/>
                <Action type="SetProperty" frame="ContentTileList" Visible="True"/>
                <Action type="SetProperty" frame="BigTileList" Visible="True"/>
            </State>
            
            <State name="StoreDown">
                <Action type="SetProperty" frame="ContentTileList" Visible="False"/>
                <Action type="SetProperty" frame="BigTileList" Visible="False"/>
            </State>
        </StateGroup>

        <StateGroup name="SelectedBundleVisibility">
            <DefaultState val="SelectedBundleNotVisible"/>
            
            <State name="SelectedBundleVisible">
                <When type="Property" frame="SelectedBundleFrame" operator="NotEqual" Bundle=""/>
                <Action type="SetProperty" frame="SelectedBundleFrame" visible="true"/>
            </State>
            
            <State name="SelectedBundleNotVisible">
                <Action type="SetProperty" frame="SelectedBundleFrame" visible="false"/>
            </State>
        </StateGroup>

        <StateGroup name="MultipleTileListStates">
            <State name="SmallTileListOnly">
                <When type="Property" frame="BigTileList" DataCount="0"/>
                <Action type="SetProperty" frame="ContentTileList" MaxItemPerPage="12"/>

                <Action type="SetAnchor" side="Left" frame="ContentTileList" relative="$parent/BigTileList/Item0" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Right" frame="ContentTileList" relative="$parent" pos="Max" offset="0"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item0" relative="$parent" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item0" relative="$parent" pos="0" offset="0"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item2" relative="$parent/Item0" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item2" relative="$parent/Item1" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item4" relative="$parent/Item3" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item4" relative="$parent/Item3" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item5" relative="$parent/Item4" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item5" relative="$parent/Item4" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item6" relative="$parent/Item0" pos="Max" offset="#Collection_Spacing_StandardItem_Vertical"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item6" relative="$parent/Item0" pos="Min" offset="0"/>

                <Action type="SetAnchor" side="Right" frame="ContentTileList/ScrollBar" relative="$parent/Item5" pos="Max" offset="26"/>
            </State>
            <State name="OneBigTileList">
                <When type="Property" frame="BigTileList" DataCount="1"/>
                <Action type="SetProperty" frame="ContentTileList" MaxItemPerPage="8"/>

                <Action type="SetAnchor" side="Left" frame="ContentTileList" relative="$parent/BigTileList/Item0" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>
                <Action type="SetAnchor" side="Right" frame="ContentTileList" relative="$parent" pos="Max" offset="0"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item0" relative="$parent" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item0" relative="$parent" pos="0" offset="0"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item2" relative="$parent/Item0" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item2" relative="$parent/Item1" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item4" relative="$parent/Item0" pos="Max" offset="#Collection_Spacing_StandardItem_Vertical"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item4" relative="$parent" pos="Min" offset="0"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item5" relative="$parent/Item4" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item5" relative="$parent/Item4" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item6" relative="$parent/Item5" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item6" relative="$parent/Item5" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>

                <Action type="SetAnchor" side="Right" frame="ContentTileList/ScrollBar" relative="$parent/Item7" pos="Max" offset="26"/>
            </State>
            <State name="TwoBigTileList">
                <When type="Property" frame="BigTileList" operator="GreaterThan" DataCount="1"/>
                <Action type="SetProperty" frame="ContentTileList" MaxItemPerPage="4"/>

                <Action type="SetAnchor" side="Left" frame="ContentTileList" relative="$parent/BigTileList/Item1" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>
                <Action type="SetAnchor" side="Right" frame="ContentTileList" relative="$parent" pos="Max" offset="0"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item0" relative="$parent" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item0" relative="$parent" pos="0" offset="0"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item2" relative="$parent/Item0" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item2" relative="$parent" pos="Min" offset="0"/>

                <Action type="SetAnchor" side="Top" frame="ContentTileList/Item4" relative="$parent/Item3" pos="Min" offset="0"/>
                <Action type="SetAnchor" side="Left" frame="ContentTileList/Item4" relative="$parent/Item3" pos="Max" offset="#Collection_Spacing_StandardItem_Horizontal"/>

                <Action type="SetAnchor" side="Right" frame="ContentTileList/ScrollBar" relative="$parent/Item3" pos="Max" offset="23"/>
            </State>
        </StateGroup>
       
    </Frame>
</Desc>
