                <Action type="SetProperty" frame="CurrentPriceLabel" Visible="True"/>
                <Action type="SetProperty" frame="OriginalPriceLabel" Visible="False"/>
                <Action type="SetProperty" frame="DiscountSlash" Visible="False"/>
                <Action type="SetProperty" frame="DiscountPercentageLabel" Visible="False"/>
            </State>

            <State name="UnpurchasedStandardBundleCanPurchase">
                <When type="StateGroup" frame="$this" ItemTypeState="StandardBundle"/>
                <When type="Property" frame="StoreCacheFrame" CanPurchase="True"/>

                <Action type="SetProperty" frame="CurrentPriceLabel" Visible="True"/>
                <Action type="SetProperty" frame="OriginalPriceLabel" Visible="True"/>
                <Action type="SetProperty" frame="DiscountSlash" Visible="True"/>
                <Action type="SetProperty" frame="DiscountPercentageLabel" Visible="True"/>
            </State>

            <State name="UnpurchasedDynamicBundle">
                <When type="StateGroup" frame="$this" ItemTypeState="DynamicBundle"/>

                <Action type="SetProperty" frame="CurrentPriceLabel" Visible="True"/>
                <Action type="SetProperty" frame="OriginalPriceLabel" Visible="True"/>
                <Action type="SetProperty" frame="DiscountSlash" Visible="True"/>
                <Action type="SetProperty" frame="DiscountPercentageLabel" Visible="True"/>
            </State>

            <State name="UnpurchasedItem">
                <Action type="SetProperty" frame="CurrentPriceLabel" Visible="True"/>
                <Action type="SetProperty" frame="OriginalPriceLabel" Visible="False"/>
                <Action type="SetProperty" frame="DiscountSlash" Visible="False"/>
                <Action type="SetProperty" frame="DiscountPercentageLabel" Visible="False"/>
            </State>
        </StateGroup>
