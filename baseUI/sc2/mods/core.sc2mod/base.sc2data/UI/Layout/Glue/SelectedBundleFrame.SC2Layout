<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Constant name="Collection_Spacing_StandardItem_Horizontal" val="15"/>
    <Constant name="Collection_Spacing_StandardItem_Vertical" val="9"/>
    
    <Frame type="SelectedBundleItemTileListItem" name="SelectedBundleItemTileListItemTemplate">
        <Height val="94"/>
        <HideWhenCleared val="false"/>
        <SelectedSound val="@UI_FileSelectionSelect"/>

        <Animation name="HoverSound">
            <Event event="OnMouseEnter" action="Reset,Play"/>
            <Event event="OnMouseExit" action="Reset, Stop"/>
            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0" sound="@UI_HoverTick"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Over"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Selected"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Image" name="EmptyImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainListItem_Normal"/>
            <TextureType val="NineSlice"/>
            <Visible val="False"/>
            <Alpha val="80"/>
        </Frame>
        
        <Frame type="TextFormatFrame" name="TypeLabelFormatFrame">
            <Anchor relative="$parent"/>
            <Replacement index="0" val="{$parent/GameContentFrame/@Name}"/>
            
            <StateGroup name="ItemType">
                <DefaultState val="Default"/>

                <State name="Announcer">
                    <When type="Property" frame="$parent" TypeName="VoicePack"/>
                    <Action type="SetProperty" frame="$this" FormatText="@UI/Generic/GenderFormattedAnnouncerTitle"/>
                </State>
                
                <State name="Commander">
                    <When type="Property" frame="$parent" TypeName="Commander"/>
                    <Action type="SetProperty" frame="$this" FormatText="@UI/Generic/GenderFormattedCommanderTitle"/>
                </State>
                
                <State name="Default">
                    <Action type="SetProperty" frame="$this" FormatText="{$parent/@TypeNameStr}"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="TypeLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Style val="@Collection_Bundle_ItemTypeLabel"/>
            <Text val="{$parent/TypeLabelFormatFrame/@Text}"/>
        </Frame>

        <Frame type="Label" name="NameLabel">
            <Anchor side="Top" relative="$parent/TypeLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Height val="23"/>
            <Style val="@Collection_ListItem_Name"/>
            <AcceptsMouseTooltip val="True"/>
            <Options val="Ellipsis |NoWrapping | TooltipIfTruncated"/>

            <StateGroup name="ListItemText">
                <DefaultState val="HasShortName"/>
                
                <State name="NoShortName">
                    <When type="Property" frame="$parent/GameContentFrame" ShortName=""/>
                    <Action type="SetProperty" frame="$this" Text="{$parent/GameContentFrame/@Name}"/>
                </State>

                <State name="HasShortName">
                    <Action type="SetProperty" frame="$this" Text="{$parent/GameContentFrame/@ShortName}"/>
                </State>
            </StateGroup>

            <StateGroup name="ListItemTextStates">
                <DefaultState val="NormalState"/>
            
                <State name="ToggledState">
                    <When type="Property" frame="$parent/SelectedImage" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name_Selected"/>
                </State>
                
                <State name="HoverState">
                    <When type="Property" frame="$parent/HoverImage" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name_Selected"/>
                </State>
        
                <State name="LockedState">
                    <When type="Property" frame="$parent" Allowed="false"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name_Locked"/>
                </State>

                <State name="NormalState">
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Name"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Label" name="StatusLabel">
            <Anchor side="Top" relative="$parent/NameLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Style val="@Collection_ListItem_Desc"/>

            <StateGroup name="ListItemTextStates">
                <DefaultState val="NormalState"/>
            
                <State name="ToggledState">
                    <When type="Property" frame="$parent/SelectedImage" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Selected"/>
                </State>
                
                <State name="HoverState">
                    <When type="Property" frame="$parent/HoverImage" visible="True"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Selected"/>
                </State>
        
                <State name="LockedState">
                    <When type="Property" frame="$parent" Allowed="false"/>
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc_Locked"/>
                </State>

                <State name="NormalState">
                    <Action type="SetProperty" frame="$this" Style="@Collection_ListItem_Desc"/>
                </State>
            </StateGroup>
             
            <StateGroup name="ListItemOwnershipState">
                <DefaultState val="NotOwnedState"/>
            
                <State name="OwnedState">
                    <When type="Property" frame="$parent" IsPurchased="True"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCollection/Owned"/>
                </State>

                <State name="NotOwnedState">
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCollection/Unowned"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="GameContentFrame" name="GameContentFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <GameContent val="{$parent/@GameContent}"/>
        </Frame>
 
        <StateGroup name="HasDataStateGroup">
            <State name="NoDataState">
                <When type="Property" frame="$this" HasData="False"/>
                <Action type="SetProperty" frame="EmptyImage" Visible="true"/>
                <Action type="SetProperty" frame="NormalImage" Alpha="0"/>
                <Action type="SetProperty" frame="NameLabel" Alpha="0"/>
                <Action type="SetProperty" frame="TypeLabel" Alpha="0"/>
                <Action type="SetProperty" frame="StatusLabel" Alpha="0"/>
            </State>

            <State name="HasDataState">
                <When type="Property" frame="$this" HasData="True"/>
                <Action type="SetProperty" frame="EmptyImage" Visible="false"/>
                <Action type="SetProperty" frame="NormalImage" Alpha="255"/>
                <Action type="SetProperty" frame="NameLabel" Alpha="255"/>
                <Action type="SetProperty" frame="TypeLabel" Alpha="255"/>
                <Action type="SetProperty" frame="StatusLabel" Alpha="255"/>
            </State>
        </StateGroup>
    </Frame>

