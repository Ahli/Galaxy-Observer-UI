    <Frame type="SelectedBundleItemTileListItem" name="SelectedBundleItemTileListItemTemplate" template="StandardTileListTemplates/StandardMediumTileListItemTemplate">
    
        <Frame type="Frame" name="AnchorFrame">
            <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="16"/>
            <Anchor side="Right" relative="$parent/$parent/ScrollBar" pos="Min" offset="-2"/>
            <Height val="84"/>
        </Frame>
        
        <Frame type="Label" name="NameLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="19"/>
            <Anchor side="Left" relative="$parent/TitleLabel" pos="Min" offset="0"/>
            <Style val="Collection_BundleItem_TileList_Type"/>
            <Text val="{$parent/@TypeNameStr}"/>
            
            <StateGroup name="StyleState">
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Style="Collection_BundleItem_TileList_Type_Selected"/>
                </State>
                
                <State name="Hovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Style="Collection_BundleItem_TileList_Type_Hovered"/>
                </State>
                
                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Style="Collection_BundleItem_TileList_Type"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/NameLabel" pos="Max" offset="-3"/>
            <Anchor side="Bottom" relative="$this" pos="Min" offset="0"/>
            <Text val="{$parent/GameContentFrame/@ShortName}"/>
            
            <StateGroup name="GameContentNameState">
                <DefaultState val="ShortName"/>
                
                <State name="Name">
                    <When type="Property" frame="$parent/GameContentFrame" ShortName=""/>
                    <Action type="SetProperty" frame="$this" Text="{$parent/GameContentFrame/@Name}"/>
                </State>
                
                <State name="ShortName">
                    <Action type="SetProperty" frame="$this" Text="{$parent/GameContentFrame/@ShortName}"/>
                </State>
            </StateGroup>
            
            <StateGroup name="StyleState">
                
                <State name="Locked">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Locked_Normal"/>
                </State>
            
                <State name="EquippedSelected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Selected"/>
                </State>
            
                <State name="PurchasableSelected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Selected"/>
                </State>
            
                <State name="Selected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Selected"/>
                </State>
                
                <State name="EquippedHovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Hovered"/>
                </State>
            
                <State name="PurchasableHovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Hovered"/>
                </State>
                
                <State name="Hovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Hovered"/>
                </State>

                <State name="EquippedNormal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Normal"/>
                </State>
            
                <State name="PurchasableNormal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Normal"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Normal"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Label" name="StatusLabel">
            <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="-4"/>
            <Anchor side="Left" relative="$parent/TitleLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/TitleLabel" pos="Max" offset="0"/>
            <Style val="Collection_BundleItem_TileList_Included"/>
            <Text val="@UI/ScreenCollection/BundleItem/Included"/>           
            <StateGroup name="StyleState">
                <DefaultState val="Normal"/>
            
                <State name="OwnedSelected">
                    <When type="StateGroup" frame="$parent" OwnedState="Owned"/>
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Style="Collection_BundleItem_TileList_Owned_Selected"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCollection/BundleItem/Owned"/>
                </State>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Style="Collection_BundleItem_TileList_Included_Selected"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCollection/BundleItem/Included"/>
                </State>
                
                <State name="OwnedHovered">
                    <When type="StateGroup" frame="$parent" OwnedState="Owned"/>
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Style="Collection_BundleItem_TileList_Owned_Hovered"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCollection/BundleItem/Owned"/>
                </State>
                
                <State name="Hovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Style="Collection_BundleItem_TileList_Included_Hovered"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCollection/BundleItem/Included"/>
                </State>

                <State name="Owned">
                    <When type="StateGroup" frame="$parent" OwnedState="Owned"/>
