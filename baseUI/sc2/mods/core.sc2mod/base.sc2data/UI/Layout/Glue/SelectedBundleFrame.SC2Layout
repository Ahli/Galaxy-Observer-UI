                <Action type="SetProperty" frame="SelectedStimpackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedConsoleSkinFrame" visible="false"/>
            </State>

            <State name="SkinPackSelected">
                <When type="Property" SelectedType="SkinPack"/>

                <Action type="SetProperty" frame="SelectedEmoticonPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedPortraitPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedRaceBannerPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedDecalPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedSprayPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedCampaignFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedCommanderFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedVoicePackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedSkinPackFrame" visible="true"/>
                <Action type="SetProperty" frame="SelectedStimpackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedConsoleSkinFrame" visible="false"/>
            </State>

            <State name="StimpackSelected">
                <When type="Property" SelectedType="Stimpack"/>

                <Action type="SetProperty" frame="SelectedEmoticonPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedPortraitPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedRaceBannerPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedDecalPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedSprayPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedCampaignFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedCommanderFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedVoicePackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedSkinPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedStimpackFrame" visible="true"/>
                <Action type="SetProperty" frame="SelectedConsoleSkinFrame" visible="false"/>
            </State>

            <State name="ConsoleSkinSelected">
                <When type="Property" SelectedType="ConsoleSkin"/>

                <Action type="SetProperty" frame="SelectedEmoticonPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedPortraitPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedRaceBannerPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedDecalPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedSprayPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedCampaignFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedCommanderFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedVoicePackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedSkinPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedStimpackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedConsoleSkinFrame" visible="true"/>
            </State>

            <State name="NothingSelected">
                <Action type="SetProperty" frame="SelectedEmoticonPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedPortraitPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedRaceBannerPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedDecalPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedSprayPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedCampaignFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedCommanderFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedVoicePackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedSkinPackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedStimpackFrame" visible="false"/>
                <Action type="SetProperty" frame="SelectedConsoleSkinFrame" visible="false"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="SelectedBundleFrame" name="SelectedBundleFrameTemplate" template="SelectedBundleFrame/BaseBundleFrameTemplate">
            
        <Frame type="Label" name="DynamicBundleTitleLabel">
            <Anchor side="Top" relative="$parent/ContentContainer" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/BundleTitleLabel" pos="Min" offset="0"/>
            <Style val="@Collection_Bundle_DynamicBundleTitleLabel"/>
            <Text val="@UI/ScreenCollection/DynamicBundle"/>
        </Frame>
            
        <Frame type="Label" name="DynamicBundleItemCountLabel">
            <Anchor side="Top" relative="$parent/DynamicBundleTitleLabel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/DynamicBundleTitleLabel" pos="Max" offset="5"/>
            <Style val="@Collection_Bundle_DynamicBundleCountLabel"/>
            <Text val="{$parent/@DynamicBundleItemCountStr}"/>
        </Frame>

        <Frame type="Label" name="OwnedBundleTitleLabel">
            <Anchor side="Top" relative="$parent/TileList" pos="Max" offset="15"/>
            <Anchor side="Left" relative="$parent/BundleTitleLabel" pos="Min" offset="1"/>
            <Style val="@Collection_Bundle_DynamicBundleTitleLabel"/>
            <Text val="@UI/ScreenCollection/Bundle/Owned"/>
        </Frame>

        <Frame type="BuyButton" name="BuyButton" template="CollectionTemplates/BuyButtonTemplate">
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Max" offset="-40"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Mid" offset="-10"/>
            <ShowPrice val="true"/>
            <AlwaysShow val="true"/>
                
            <GameContent val="{$parent/$parent/@GameContent}"/>
            <CollapseLayout val="false"/>
            <ClickSound val="@UI_ActionButtonSelect"/>
            <HoverSound val="@UI_HoverTick"/>
        </Frame>
         
        <Frame type="ExternalBuyButton" name="ExternalBuyButton" template="CollectionTemplates/ExternalBuyButtonTemplate">
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Max" offset="-40"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Mid" offset="-10"/>
            <!-- piggy back off BuyButton since we need the same data, doing same as BuyButton above outputs bind errors -->
            <GameContent val="{$parent/BuyButton/@GameContent}"/>
            <CollapseLayout val="false"/>
            <ClickSound val="@UI_ActionButtonSelect"/>
            <HoverSound val="@UI_HoverTick"/>
            <Visible val="{$this/@ContentIsExternal}"/>
            <NydusLink val="external-link"/>
            <LinkId val="0"/>
        </Frame>

        <Frame type="Image" name="BundleDiscountBadge">
            <Anchor side="Top" relative="$parent/BuyButton" pos="Mid" offset="-1"/>
            <Anchor side="Bottom" relative="$parent/BuyButton" pos="Mid" offset="-1"/>
            <Anchor side="Right" relative="$parent/BuyButton" pos="Min" offset="30"/>
            <Texture val="@UI/SC2_UI_Collection_DiscountBadge"/>
        </Frame>

        <Frame type="TextFormatFrame" name="DiscountPercentageFormat">
            <Anchor relative="$parent"/>
            <Visible val="False"/>
            <FormatText val="@UI/ScreenCollection/DiscountPercentFormat"/>
            <Replacement index="0" val="{$parent/StoreCacheFrame/@CatalogDiscountPercent}"/>
        </Frame>

        <Frame type="Label" name="BundleDiscountPercentLabel">
            <Anchor side="Top" relative="$parent/BundleDiscountBadge" pos="Min" offset="1"/>
            <Anchor side="Bottom" relative="$parent/BundleDiscountBadge" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/BundleDiscountBadge" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/BundleDiscountBadge" pos="Max" offset="0"/>
            <Style val="@Collection_Bundle_DynamicBundleDiscountLabel"/>
            <Text val="{$parent/DiscountPercentageFormat/@Text}"/>
        </Frame>
        
        <StateGroup name="OwnedLabelVisibility">
            <DefaultState val="NotOwned"/>
        
            <State name="OwnedNotBundle">
                <When type="Property" frame="StoreCacheFrame" IsPurchased="True"/>
                <When type="Property" frame="BundleCacheFrame" ProductCount="1"/>
                <Action type="SetProperty" frame="OwnedBundleTitleLabel" visible="true"/>
                <Action type="SetProperty" frame="OwnedBundleTitleLabel" Text="@UI/ScreenCollection/Item/Owned"/>
            </State>

            <State name="OwnedBundle">
                <When type="Property" frame="StoreCacheFrame" IsPurchased="True"/>
                <Action type="SetProperty" frame="OwnedBundleTitleLabel" visible="true"/>
                <Action type="SetProperty" frame="OwnedBundleTitleLabel" Text="@UI/ScreenCollection/Bundle/Owned"/>
            </State>

            <State name="NotOwned">
                <Action type="SetProperty" frame="OwnedBundleTitleLabel" visible="false"/>
            </State>
        </StateGroup>

        <StateGroup name="BundleDiscountLabelState">
            <DefaultState val="HasDiscount"/>

            <State name="Owned">
                <When type="Property" frame="StoreCacheFrame" IsPurchased="True"/>
                <Action type="SetProperty" frame="BundleDiscountPercentLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleDiscountBadge" visible="false"/>
            </State>

            <State name="NoDiscount">
                <When type="Property" frame="StoreCacheFrame" CatalogDiscountPercent="0"/>
                <Action type="SetProperty" frame="BundleDiscountPercentLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleDiscountBadge" visible="false"/>
            </State>

            <State name="DiscountSuppressed">
                <When type="Property" frame="BundleCacheFrame" SuppressDiscount="true"/>
                <Action type="SetProperty" frame="BundleDiscountPercentLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleDiscountBadge" visible="false"/>
            </State>

            <State name="HasDiscount">
                <Action type="SetProperty" frame="BundleDiscountPercentLabel" visible="true"/>
                <Action type="SetProperty" frame="BundleDiscountBadge" visible="true"/>
            </State>
        </StateGroup>
        
        <StateGroup name="ExternalState">
            <DefaultState val="NotExternal"/>
            <State name="External">
                <When type="Property" frame="ExternalBuyButton" ContentIsExternal="True"/>
            </State>
            <State name="NotExternal"/>
        </StateGroup>

        <StateGroup name="DynamicBundleItemState">
            <DefaultState val="NotDynamicBundle"/>

            <State name="AlreadyOwnedDynamicBundle">
                <When type="Property" frame="BundleCacheFrame" ShowDynamicBundle="true"/>
                <When type="Property" frame="StoreCacheFrame" IsPurchased="True"/>
                <Action type="SetProperty" frame="DynamicBundleTitleLabel" visible="false"/>
                <Action type="SetProperty" frame="DynamicBundleItemCountLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleSubLabel" visible="true"/>
                <Action type="SetProperty" frame="BundleVerticalSpacingAnchor" visible="false"/>
            </State>

            <State name="AlreadyOwnedSingleItem">
                <When type="Property" frame="StoreCacheFrame" IsPurchased="True"/>
                <When type="Property" frame="BundleCacheFrame" ProductCount="1"/>
                <Action type="SetProperty" frame="DynamicBundleTitleLabel" visible="false"/>
                <Action type="SetProperty" frame="DynamicBundleItemCountLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleSubLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleVerticalSpacingAnchor" visible="true"/>
            </State>

            <State name="AlreadyOwnedNonDynamicBundle">
                <When type="Property" frame="StoreCacheFrame" IsPurchased="True"/>
                <Action type="SetProperty" frame="DynamicBundleTitleLabel" visible="false"/>
                <Action type="SetProperty" frame="DynamicBundleItemCountLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleSubLabel" visible="true"/>
                <Action type="SetProperty" frame="BundleVerticalSpacingAnchor" visible="false"/>
            </State>
                        
            <State name="DynamicBundleEmptyCount">
                <When type="Property" frame="BundleCacheFrame" ShowDynamicBundle="true"/>
                <When type="Property" DynamicBundleItemCountStr=""/>
                <Action type="SetProperty" frame="DynamicBundleTitleLabel" visible="false"/>
                <Action type="SetProperty" frame="DynamicBundleItemCountLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleSubLabel" visible="true"/>
                <Action type="SetProperty" frame="BundleVerticalSpacingAnchor" visible="false"/>
            </State>

            <State name="DynamicBundle">
                <When type="Property" frame="BundleCacheFrame" ShowDynamicBundle="true"/>
                <Action type="SetProperty" frame="DynamicBundleTitleLabel" visible="true"/>
                <Action type="SetProperty" frame="DynamicBundleItemCountLabel" visible="true"/>
                <Action type="SetProperty" frame="BundleSubLabel" visible="true"/>
                <Action type="SetProperty" frame="BundleVerticalSpacingAnchor" visible="false"/>
            </State>

            <State name="SingleItem">
                <When type="Property" frame="BundleCacheFrame" ProductCount="1"/>
                <Action type="SetProperty" frame="DynamicBundleTitleLabel" visible="false"/>
                <Action type="SetProperty" frame="DynamicBundleItemCountLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleSubLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleVerticalSpacingAnchor" visible="true"/>
            </State>

            <State name="NotDynamicBundle">
                <Action type="SetProperty" frame="DynamicBundleTitleLabel" visible="false"/>
                <Action type="SetProperty" frame="DynamicBundleItemCountLabel" visible="false"/>
                <Action type="SetProperty" frame="BundleSubLabel" visible="true"/>
                <Action type="SetProperty" frame="BundleVerticalSpacingAnchor" visible="false"/>
            </State>
        </StateGroup>
    </Frame>
</Desc>