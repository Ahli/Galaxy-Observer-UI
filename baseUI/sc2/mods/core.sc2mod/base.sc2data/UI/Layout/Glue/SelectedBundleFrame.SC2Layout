            <Reward val ="{$parent/@RewardIdentifier}"/>
        </Frame>

        <Frame type="UnlockableFrame" name="UnlockableFrame">
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Mid" offset="25"/>
            
            <Frame type="Image" name="UnlockableImage">
                <Texture val="{$parent/$parent/RewardCacheFrame/@Texture}"/>
                <TextureCoords val="{$parent/$parent/RewardCacheFrame/@TextureCoords}"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Text val="{$parent/RewardCacheFrame/@Name}"/>
            
            <StateGroup name="StyleState">
            
                <State name="EquippedSelected">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Equipped_Selected"/>
                </State>
            
                <State name="PurchasableSelected">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Standard_Selected"/>
                </State>
            
                <State name="Selected">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Standard_Selected"/>
                </State>
                
                <State name="EquippedHovered">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Equipped_Hovered"/>
                </State>
            
                <State name="PurchasableHovered">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Standard_Hovered"/>
                </State>
                
                <State name="Hovered">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Standard_Hovered"/>
                </State>

                <State name="EquippedNormal">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Equipped_Normal"/>
                </State>
            
                <State name="PurchasableNormal">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Standard_Normal"/>
                </State>
                
                <State name="Locked">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Locked_Normal"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Style="@GraphicListItemTitle_UnlockableLarge_Standard_Normal"/>
                </State>
            </StateGroup>
        </Frame>
                
        <!-- LockedState -->
        <StateGroup name="LockedState">
            <DefaultState val="NotLocked"/>
            <State name="Locked">
                <When type="StateGroup" EmptyState="Empty"/>
                <Action type="SetProperty" frame="UnlockableFrame/UnlockableImage" Texture="Assets/Textures/blank.dds"/>
            </State>
            <State name="NotLocked">
                <Action type="SetProperty" frame="UnlockableFrame/UnlockableImage" Texture="{$parent/$parent/RewardCacheFrame/@Texture}"/>
            </State>
        </StateGroup>
    </Frame>

