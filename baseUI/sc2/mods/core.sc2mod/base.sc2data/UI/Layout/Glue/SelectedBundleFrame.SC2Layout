            <Anchor side="Bottom" relative="$parent/Pip2" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/Pip2" pos="Min" offset="2"/>
        </Frame>
    
        <Animation name="ScreenshotRotationAnim">
            <Event event="OnShown" action="Reset,Play"/>
            <Event event="OnHidden" action="Pause"/>
            <Event event="OnSelectedBundleItemClick" frame="$ancestor[@type=SelectedBundleFrame]" action="Reset"/>
            <Event event="OnMouseEnter" action="Pause"/>
            <Event event="OnMouseExit" action="Play"/>
            <Event event="OnPipClick" action="Stop,Reset"/>
            
            <Controller type="Event" end="Loop">
                <Key type="Event" time="3" event="IncrementSelectedIndex"/>
            </Controller>
        </Animation>
        
        <Animation name="ScreenshotResetAnim">
            <Event event="OnShown" action="Reset,Play"/>
            <Event event="OnSelectedBundleItemClick" frame="$ancestor[@type=SelectedBundleFrame]" action="Reset,Play"/>
            
            <Controller type="Property" property="SelectedIndex" end="Stop">
                <Key type="Property" time="0" value="0"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="RewardItemTileListItem" name="RewardItemTileListItemTemplate" template="StandardTileListTemplates/UnlockableTileListItemTemplate">
        <UseTooltip val="False"/>
        
        <Frame type="Frame" name="AnchorFrame">
