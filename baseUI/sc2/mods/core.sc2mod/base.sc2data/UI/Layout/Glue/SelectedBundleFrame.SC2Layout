    <Frame type="Image" name="CampaignScreenshotTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="3"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="-3"/>
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <ScaleWidthToTexture val="true"/>
        
        <StateGroup name="ContentState">
            <DefaultState val="NoContent"/>
            
            <State name="HasContent">
                <When type="Property" operator="NotEqual" Texture=""/>
            </State>
            
            <State name="NoContent"/>
        </StateGroup>
    </Frame>
    
    <Frame type="Button" name="ScreenshotPipTemplate">
        <Width val="18"/>
        <Height val="18"/>
        <Toggleable val="True"/>
        <CollapseLayout val="True"/>

        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_EmoticonPanel_InGame_PagePip_Inactive"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="60,170,255"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_EmoticonPanel_InGame_PagePip_Active"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="60,170,255"/>
        </Frame>
        
        <Frame type="Image" name="SelectedImage">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Battlenet_Glue_EmoticonPanel_InGame_PagePip_Active"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="60,170,255"/>
            <Visible val="false"/>
        </Frame>
    
        <Animation name="ClickAnim">
            <Event event="OnClick" action="Reset,Play"/>
            
            <Controller type="Event" frame="$parent" end="Stop">
                <Key type="Event" time="0" event="OnPipClick"/>
            </Controller>
        </Animation>
        
        <StateGroup name="ToggleState">
            <DefaultState val="NotToggled"/>
            
            <State name="Toggled">
                <When type="Property" Toggled="True"/>
                <Action type="SetProperty" frame="SelectedImage" Visible="True"/>
                <Action type="SetProperty" frame="NormalImage" Alpha="0"/>
                <Action type="SetProperty" frame="HoverImage" Alpha="0"/>
            </State>
            
            <State name="NotToggled">
                <Action type="SetProperty" frame="SelectedImage" Visible="False"/>
                <Action type="SetProperty" frame="NormalImage" Alpha="255"/>
                <Action type="SetProperty" frame="HoverImage" Alpha="255"/>
            </State>
        </StateGroup>
    </Frame>
    
    <Frame type="TabControl" name="ScreenshotTabControlTemplate">
        <Anchor relative="$parent"/>
        <AcceptsMouse val="True"/>
        <Wrap val="True"/>
                
        <Frame type="Button" name="Pip5" template="SelectedBundleFrame/ScreenshotPipTemplate">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-8"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-8"/>
        </Frame>
        
        <Frame type="Button" name="Pip4" template="SelectedBundleFrame/ScreenshotPipTemplate">
            <Anchor side="Bottom" relative="$parent/Pip5" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/Pip5" pos="Min" offset="2"/>
        </Frame>
        
        <Frame type="Button" name="Pip3" template="SelectedBundleFrame/ScreenshotPipTemplate">
            <Anchor side="Bottom" relative="$parent/Pip4" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/Pip4" pos="Min" offset="2"/>
