                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="BottomClassic"/>
                </Controller>
            </Animation>
            
            <Animation name="TrophyTopBlackOps">
                <Event event="TrophyTopBlackOps" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="TopBlackOps"/>
