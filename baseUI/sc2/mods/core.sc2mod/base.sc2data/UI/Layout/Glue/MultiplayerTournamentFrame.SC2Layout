    <Frame type="MultiplayerTournamentFrame" name="MultiplayerTournamentFrame">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        
        <Frame type="CutsceneFrame" name="BackgroundCutscene">
            <Anchor relative="$parent"/>
            <File val="Cutscenes/UI_Screens_Multiplayer_Background.SC2Cutscene" preloadFilters="Protoss Terran Zerg Random Ring"/>
            <AutoPlay val="True"/>
            <RenderType val="HDR"/>
            
            <Animation name="DisplayLandingScreen">
                <Event event="OnShown" action="Reset,Play"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Zerg"/>
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="Terran"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Random"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Protoss"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="Vignette">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Tournament_Background_Vignette"/>
            <Alpha val="127"/>
            <RenderType val="HDR"/>
        </Frame>
        
        <Frame type="Image" name="VignetteRing">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Coop_Vignette"/>
            <Alpha val="80"/>
            <RenderType val="HDR"/>
            <RenderPriority val="515"/>
        </Frame>
        
        <Frame type="CutsceneFrame" name="TrophyCutscene">
            <Anchor relative="$parent"/>
            <File val="Cutscenes/UI_Screens_Tournament_Start.SC2Cutscene"/>
            <AutoPlay val="True"/>
            <RenderType val="HDR"/>
            
            <Animation name="DisplayLandingScreen">
                <Event event="OnShown" action="Reset,Play" frame="$parent/TournamentPortalFrame"/>

                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="SecondPlace"/>
                    <Key type="Cutscene" action="Bookmark" time="0.0" bookmark="BirthStart"/>
                </Controller>
            </Animation>
            
            <Animation name="DisplayTournament">
                <Event event="OnHidden" action="Reset,Play" frame="$parent/TournamentPortalFrame"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="FirstPlace"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="SecondPlace"/>
                </Controller>
            </Animation>
            
            <Animation name="TrophyManagementWon">
                <Event event="OnShown" action="Reset,Play" frame="$parent/TournamentPortalFrame/TrophyTallyFrame"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="FirstPlace"/>
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="NotVictory"/>
                </Controller>
            </Animation>
            
            <Animation name="TrophyManagementNotWon">
                <Event event="OnHidden" action="Reset,Play" frame="$parent/TournamentPortalFrame/TrophyTallyFrame"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="FirstPlace"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="SecondPlace"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Victory"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="NotVictory"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Bronze"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Master"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Silver"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Gold"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Platinum"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Diamond"/>
                    <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Grandmaster"/>
                </Controller>
            </Animation>
            
            <StateGroup name="TrophyDisplayTrigger">
                <DefaultState val="Reset"/>
                
                <State name="Trigger">
                    <When type="Property" frame="$parent/TournamentPortalFrame/TrophyTallyFrame" visible="True"/>
                </State>
                
                <State name="Reset">
                    <When type="Property" frame="$parent/TournamentPortalFrame/TrophyTallyFrame" visible="False"/>
                </State>
            </StateGroup>
            
            <StateGroup name="TrophyTopState">
                
                <State name="Reset">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Reset"/>
                </State>
                
                <State name="Battlecruiser">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="14"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopBattlecruiser"/>
                </State>
                
                <State name="Colossus">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="13"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopColossus"/>
                </State>
                
                <State name="Minigun">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="12"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopMinigun"/>
                </State>
                
                <State name="HydraSkull">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="11"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopHydraSkull"/>
                </State>
                
                <State name="Viking">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="10"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopViking"/>
                </State>
                
                <State name="Ultralisk">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="9"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopUltralisk"/>
                </State>
                
                <State name="VoidRay">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="8"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopVoidRay"/>
                </State>
                
                <State name="SiegeTank">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="7"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopSiegeTank"/>
                </State>
                
                <State name="Mutalisk">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="6"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopMutalisk"/>
                </State>
                
                <State name="Carrier">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="5"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopCarrier"/>
                </State>
                
                <State name="BlackOps">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="4"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopBlackOps"/>
                </State>
                
                <State name="Griffin">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="3"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopGriffin"/>
                </State>
                
                <State name="Nova">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="2"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopNova"/>
                </State>
                
                <State name="Classic">
                    <When type="StateGroup" frame="$this" TrophyDisplayTrigger="Trigger"/>
                    <When type="Property" frame="$TournamentPortalFrame" FirstPlaceTrophyDefinitionId="1"/>
                    <Action type="SendEvent" frame="$this" event="TrophyTopClassic"/>
                </State>
            </StateGroup>
            
            <Animation name="TrophyTopBattlecruiser">
                <Event event="TrophyTopBattlecruiser" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="TopBattlecruiser"/>
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="BottomClassic"/>
                </Controller>
            </Animation>
            
            <Animation name="TrophyTopColossus">
                <Event event="TrophyTopColossus" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="TopColossus"/>
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="BottomClassic"/>
