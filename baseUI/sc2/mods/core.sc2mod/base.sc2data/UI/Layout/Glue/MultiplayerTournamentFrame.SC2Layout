            </Animation>
            
            <Animation name="TrophyTopUltralisk">
                <Event event="TrophyTopUltralisk" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="TopUltralisk"/>
