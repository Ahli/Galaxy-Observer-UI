                </Controller>
            </Animation>
            
            <Animation name="TrophyTopVoidRay">
                <Event event="TrophyTopVoidRay" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="TopVoidRay"/>
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="BottomClassic"/>
                </Controller>
            </Animation>
            
            <Animation name="TrophyTopSiegeTank">
                <Event event="TrophyTopSiegeTank" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="TopSiegeTank"/>
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="BottomClassic"/>
                </Controller>
            </Animation>
            
            <Animation name="TrophyTopMutalisk">
                <Event event="TrophyTopMutalisk" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="TopMutalisk"/>
