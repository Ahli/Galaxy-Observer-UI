        <Frame type="Frame" name="EmptyTrophyFrame">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-92"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-120"/>
            <Width val="668"/>
            <Height val="972"/>
            <Visible val="{$parent/TournamentPortalFrame/MatchmakingLeaguePanel/@Visible}"/>
            
            <Frame type="Image" name="EmptyTrophyGlow">
                <Anchor relative="$parent"/>
                <Texture val="@UI/SC2_UI_Tournament_EmptyTrophy_BackGlow"/>
                <Alpha val="120"/>
                
                <Animation name="Pulse">
                    <Event event="OnShown" action="Reset, Play"/>
                    <Controller type="Fade" end="Loop">
                        <Key type="Curve" time="0" value="120" out="Fast"/>
                        <Key type="Curve" time="2" value="255" in="Slow"/>
                        <Key type="Curve" time="4" value="120" out="Fast"/>
                        <Key type="Curve" time="6" value="255" in="Slow"/>
                        <Key type="Curve" time="8" value="120" out="Fast"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="EmptyTrophySmoke">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
