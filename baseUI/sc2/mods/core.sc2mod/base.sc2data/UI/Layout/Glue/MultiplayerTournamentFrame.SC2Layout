            <Animation name="TrophyTopViking">
                <Event event="TrophyTopViking" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" action="AddFilter" time="0.0" filter="TopViking"/>
