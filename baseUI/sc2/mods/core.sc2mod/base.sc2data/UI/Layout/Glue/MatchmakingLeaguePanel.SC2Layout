                <Anchor side="Bottom" relative="$parent/LockIcon" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/LockIcon" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/LockIcon" pos="Mid" offset="0"/>
                <Width val="150"/>
                <Height val="150"/>
                <Texture val="@UI/Glues_Battlenet_PageAssets_StandardElements_ArtBackground_LockGlow"/>
