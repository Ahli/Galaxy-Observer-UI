<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Frame type="Frame" name="MatchmakingLeagueContainerTemplate">
        <Anchor relative="$parent"/>
        <Visible val="false"/>
            
        
    </Frame>

    <Frame type="MatchmakingLeaguePanel" name="MatchmakingLeaguePanelTemplate">
        <Width val="915"/>
        <Height val="500"/>
        <ProvisionalGames val="25"/>
        
        <!-- League Icon Animations: -->
        <!-- "Bronze", "Silver", "Gold", "Platinum", "Diamond", "Master","Grandmaster" -->
        
        <Frame type="BlurFrame" name="ContainerBGBlur">
            <Anchor side="Top" relative="$parent/LeaguePanelContainmentBG" pos="Min" offset="5"/>
            <Anchor side="Left" relative="$parent/LeaguePanelContainmentBG" pos="Min" offset="6"/>
            <Anchor side="Bottom" relative="$parent/LeaguePanelContainmentBG" pos="Max" offset="-5"/>
            <Anchor side="Right" relative="$parent/LeaguePanelContainmentBG" pos="Max" offset="-6"/>
            <AlphaMaskTexture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ContainerBlurMask"/>

            <Animation name="Blur">
                <Event event="OnShown" action="Reset,Play" frame="$parent" />

                <Controller type="Blur" end="Pause">
                    <Key type="Curve" time="0" value=".35" out="Fast"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="LeaguePanelContainmentBG">
            <Anchor side="Top" relative="$parent" pos="Min" offset="18"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="128"/>
            <Texture val="@UI/SC2_UI_Tournament_LeaguePanelBG"/>
            <TextureType val="NineSlice"/>
            <Width val="712"/>
            <Height val="264"/>
        </Frame>
        
        <!-- Season Placement Container -->

        <Frame type="Frame" name="SeasonPlacementContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Frame" name="LeagueIconImage" template="LadderTemplates/LeagueIcon_Placement">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="-32"/>
            </Frame>

            <Frame type="Label" name="SeasonPlacementLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="35"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="193"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
            </Frame>

            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/SeasonPlacementLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/SeasonPlacementLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/SeasonPlacement"/>
            </Frame>
        </Frame>
        
        <!-- GM Placement Container -->

        <Frame type="Frame" name="GrandMasterPlacementContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>

            <Frame type="Frame" name="LeagueIconImage" template="LadderTemplates/LeagueIcon_Placement">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="-32"/>
            </Frame>

            <Frame type="Label" name="HeaderLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="35"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="193"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
                <Text val="@UI/MatchmakingLeaguePanel/GrandMasterPlacementTitle"/>
            </Frame>

            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/HeaderLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/HeaderLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/GrandMasterPlacementDescription"/>
            </Frame>
        </Frame>
        
        <!-- Placement Container -->
        
        <Frame type="Frame" name="PlacementContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Frame" name="LeagueIconImage" template="LadderTemplates/LeagueIcon_Placement">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="-32"/>
            </Frame>
            
            <Frame type="Label" name="HeaderLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="35"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="193"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
                <Text val="@UI/MatchmakingLeaguePanel/Placement"/>
            </Frame>
            
            <Frame type="Label" name="RemainingMatchesLabel">
                <Anchor side="Top" relative="$parent/HeaderLabel" pos="Max" offset="1"/>
                <Anchor side="Left" relative="$parent/HeaderLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Subtitle"/>
                <Text val="@UI/MatchmakingLeaguePanel/RemainingMatches"/>
            </Frame>
            
            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/RemainingMatchesLabel" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/RemainingMatchesLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/LeaguePlacement"/>
            </Frame>

            <Frame type="Image" name="InfoIcon">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="13"/>
                <Anchor side="Right" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Max" offset="-15"/>
                <Width val="43"/>
                <Height val="43"/>
                <Texture val="@UI/Glues_Icons_GenericInfo"/>
                <AcceptsMouseTooltip val="true"/>
                <Tooltip val="@UI/MatchmakingLeaguePanel/ProvisionalTooltip"/>
            </Frame>
        </Frame>
        
        <!-- League Container -->
        
        <Frame type="Frame" name="LeagueContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            

            <Frame type="MatchmakingLeagueIcon" name="LeagueIcon" template="MatchmakingLeagueIcon/MatchmakingLeagueIconTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="-134"/>
            </Frame>

            <Frame type="Label" name="LeagueLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="35"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="193"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
            </Frame>

            <Frame type="Label" name="TierLabel">
                <Anchor side="Top" relative="$parent/LeagueLabel" pos="Max" offset="1"/>1
                <Anchor side="Left" relative="$parent/LeagueLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Subtitle"/>
                <Text val="@UI/MatchmakingLeaguePanel/Tier"/>
                <CollapseLayout val="true"/>
            </Frame>

            <Frame type="Image" name="InfoIcon">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="13"/>
                <Anchor side="Right" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Max" offset="-15"/>
                <Width val="43"/>
                <Height val="43"/>
                <Texture val="@UI/Glues_Icons_GenericInfo"/>
                <AcceptsMouseTooltip val="true"/>
                <Tooltip val="@UI/MatchmakingLeaguePanel/BonusPointsTooltip"/>
                
                <StateGroup name="ProvisionalStateGroup">
                    <State name="ProvisionalGrandmasterState">
                        <When type="AnimationState" frame="$parent/$parent" ProvisionalState="Provisional"/>
                        <When type="AnimationState" frame="$parent/LeagueIcon" LeagueIcon="Grandmaster"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/MatchmakingLeaguePanel/ProvisionalTooltip"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>
                    
                    <State name="GrandmasterState">
                        <When type="AnimationState" frame="$parent/LeagueIcon" LeagueIcon="Grandmaster"/>
                        <Action type="SetProperty" frame="$this" Visible="false"/>
                    </State>
                    
                    <State name="ProvisionalState">
                        <When type="AnimationState" frame="$parent/$parent" ProvisionalState="Provisional"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/MatchmakingLeaguePanel/ProvisionalBonusPointsTooltip"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>

                    <State name="NotProvisionalState">
                        <When type="AnimationState" frame="$parent/$parent" ProvisionalState="NotProvisional"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/MatchmakingLeaguePanel/BonusPointsTooltip"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="Label" name="BonusPointsLabel">
                <Anchor side="Top" relative="$parent/TierLabel" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/TierLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/BonusPoints"/>
                <CollapseLayout val="true"/>
            </Frame>

            <StateGroup name="GrandmasterStateGroup">
                <DefaultState val="DefaultState"/>

                <State name="GrandmasterState">
                    <When type="AnimationState" frame="$this/LeagueIcon" LeagueIcon="Grandmaster"/>
                    <Action type="SetProperty" frame="BonusPointsLabel" Visible="false"/>
                    <Action type="SetProperty" frame="TierLabel" Visible="false"/>
                </State>

                <State name="DefaultState">
                    <Action type="SetProperty" frame="BonusPointsLabel" Visible="true"/>
                    <Action type="SetProperty" frame="TierLabel" Visible="true"/>
                </State>
            </StateGroup>

            <Frame type="Label" name="WinsLabel">
                <Anchor side="Top" relative="$parent/BonusPointsLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/BonusPointsLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/Wins"/>
            </Frame>

            <Frame type="Label" name="SeasonLabel">
                <Anchor side="Top" relative="$parent/WinsLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/WinsLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/Season"/>
            </Frame>

            <Frame type="Label" name="SeasonEndLabel">
                <Anchor side="Top" relative="$parent/SeasonLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/SeasonLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/SeasonEnd"/>
            </Frame>

            <Frame type="Button" name="ViewLadderButton">
                <Anchor relative="$parent/LeagueIcon"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="TeamMembersLabel">
            <Anchor side="Top" relative="$parent/LeagueContainer/SeasonEndLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/LeagueContainer/SeasonEndLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
            <Style val="@MatchmakingLeaguePanel_Details"/>
            <Text val="@UI/MatchmakingLeaguePanel/TeamFormatRandom"/>
            <Visible val="false"/>

            <StateGroup name="TeamMembersGroup">
                <State name="LeagueStateEmpty">
                    <When type="AnimationState" frame="$parent" LeagueState="Empty"/>
                    <Action type="SetProperty" frame="$this" visible="false"/>
                </State>

                <State name="TeamStateSoloQueue">
                    <When type="AnimationState" frame="$parent" TeamState="SoloQueue"/>
                    <Action type="SetProperty" frame="$this" visible="false"/>
                </State>

                <State name="TeamStateTeamQueue">
                    <When type="AnimationState" frame="$parent" TeamState="TeamQueue"/>
                    <Action type="SetProperty" frame="$this" visible="true"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!--
        
        
        LEAGUE STATE ANIMATIONS
        
        
        -->
        
        <!-- Empty -->
        
        <Animation name="LeagueStateEmpty">
            <Event event="Empty" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- Placement -->
        
        <Animation name="LeagueStatePlacement">
            <Event event="Placement" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- SeasonPlacement -->
        
        <Animation name="LeagueStateSeasonPlacement">
            <Event event="SeasonPlacement" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>

        <!-- GrandMasterPlacement -->

        <Animation name="LeagueStateGrandMasterPlacement">
            <Event event="GrandMasterPlacement" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- League -->
        
        <Animation name="LeagueStateLeague">
            <Event event="League" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
        </Animation>
        
        <!--
        LEAGUE ICON ANIMATIONS
        TODO: Update the LeagueIconTierImage textures.  They should be different per league and tier.
        Maybe this should be in a template somewhere?
        
        -->
    </Frame>
</Desc>
