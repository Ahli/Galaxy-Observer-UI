            <StateGroup name="TeamMembersGroup">
                <State name="LeagueStateEmpty">
                    <When type="AnimationState" frame="$parent" LeagueState="Empty"/>
                    <Action type="SetProperty" frame="$this" visible="false"/>
                </State>

                <State name="TeamStateSoloQueue">
                    <When type="AnimationState" frame="$parent" TeamState="SoloQueue"/>
                    <Action type="SetProperty" frame="$this" visible="false"/>
                </State>

                <State name="TeamStateTeamQueue">
                    <When type="AnimationState" frame="$parent" TeamState="TeamQueue"/>
                    <Action type="SetProperty" frame="$this" visible="true"/>
                </State>
            </StateGroup>
            
            <StateGroup name="AnchorState">
                <DefaultState val="League"/>
                
                <State name="SeasonPlacement">
                    <When type="Property" frame="$parent/SeasonPlacementContainer" Visible="True"/>
                    <Action type="SetAnchor" side="Top" relative="$parent/SeasonPlacementContainer/DescriptionLabel" pos="Max" offset="0"/>
                </State>
                
                <State name="GrandMasterPlacement">
                    <When type="Property" frame="$parent/GrandMasterPlacementContainer" Visible="True"/>
                    <Action type="SetAnchor" side="Top" relative="$parent/GrandMasterPlacementContainer/DescriptionLabel" pos="Max" offset="0"/>
                </State>
                
                <State name="Placement">
                    <When type="Property" frame="$parent/PlacementContainer" Visible="True"/>
                    <Action type="SetAnchor" side="Top" relative="$parent/PlacementContainer/DescriptionLabel" pos="Max" offset="20"/>
                </State>
                
                <State name="League">
                    <Action type="SetAnchor" side="Top" relative="$parent/LeagueContainer/Divider3" pos="Max" offset="1"/>
                </State>
            </StateGroup>
        </Frame>
      
        <!--
        
        
        LEAGUE STATE
        
        
        -->
            
        <StateGroup name="LeagueState">
            <DefaultState val="LeagueStateEmpty"/>
            
            <State name="RankedLocked">
                <When type="AnimationState" frame="$this" TeamState="SoloQueue"/>
                <When type="Property" frame="$this/RankedLockedContainer/AchievementFrame" Awarded="False"/>
                <When type="Property" frame="$this/RankedLockedContainer/AchievementFrame/QueueFrame" IsAllowed="False"/>
                <Action type="SetProperty" frame="$this/SeasonPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/GrandMasterPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/PlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/LeagueContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/RankedLockedContainer" Visible="True"/>
            </State>
            
            <State name="LeagueStateLeague">
                <When type="AnimationState" frame="$this" LeagueState="League"/>
                <Action type="SetProperty" frame="$this/SeasonPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/GrandMasterPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/PlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/LeagueContainer" Visible="True"/>
                <Action type="SetProperty" frame="$this/RankedLockedContainer" Visible="False"/>
            </State>
            
            <State name="LeagueStateGrandMasterPlacement">
                <When type="AnimationState" frame="$this" LeagueState="GrandMasterPlacement"/>
                <Action type="SetProperty" frame="$this/SeasonPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/GrandMasterPlacementContainer" Visible="True"/>
                <Action type="SetProperty" frame="$this/PlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/LeagueContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/RankedLockedContainer" Visible="False"/>
            </State>
            
            <State name="LeagueStateSeasonPlacement">
                <When type="AnimationState" frame="$this" LeagueState="SeasonPlacement"/>
                <Action type="SetProperty" frame="$this/SeasonPlacementContainer" Visible="True"/>
                <Action type="SetProperty" frame="$this/GrandMasterPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/PlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/LeagueContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/RankedLockedContainer" Visible="False"/>
            </State>
            
            <State name="LeagueStatePlacement">
                <When type="AnimationState" frame="$this" LeagueState="Placement"/>
                <Action type="SetProperty" frame="$this/SeasonPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/GrandMasterPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/PlacementContainer" Visible="True"/>
                <Action type="SetProperty" frame="$this/LeagueContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/RankedLockedContainer" Visible="False"/>
            </State>
            
            <State name="LeagueStateEmpty">
                <When type="AnimationState" frame="$this" LeagueState="Empty"/>
                <Action type="SetProperty" frame="$this/SeasonPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/GrandMasterPlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/PlacementContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/LeagueContainer" Visible="False"/>
                <Action type="SetProperty" frame="$this/RankedLockedContainer" Visible="False"/>
            </State>
        </StateGroup>
    </Frame>
    
    
