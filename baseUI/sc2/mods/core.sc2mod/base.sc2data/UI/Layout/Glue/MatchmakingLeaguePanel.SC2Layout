<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Frame type="Frame" name="MatchmakingLeagueContainerTemplate">
        <Anchor relative="$parent"/>
        <Visible val="false"/>
            
        
    </Frame>

    <Frame type="MatchmakingLeaguePanel" name="MatchmakingLeaguePanelTemplate">
        <Width val="800"/>
        <Height val="300"/>
        
        <!-- League Icon Animations: -->
        <!-- "Bronze", "Silver", "Gold", "Platinum", "Diamond", "Master","Grandmaster" -->
        
        <Frame type="BlurFrame" name="ContainerBGBlur">
            <Anchor side="Top" relative="$parent/LeaguePanelContainmentBG" pos="Min" offset="5"/>
            <Anchor side="Left" relative="$parent/LeaguePanelContainmentBG" pos="Min" offset="6"/>
            <Anchor side="Bottom" relative="$parent/LeaguePanelContainmentBG" pos="Max" offset="-5"/>
            <Anchor side="Right" relative="$parent/LeaguePanelContainmentBG" pos="Max" offset="-6"/>
            <AlphaMaskTexture val="@UI/Battlenet_Glue_Multiplayer_Difficulty_ContainerBlurMask"/>

            <Animation name="Blur">
                <Event event="OnShown" action="Reset,Play" frame="$parent" />

                <Controller type="Blur" end="Pause">
                    <Key type="Curve" time="0" value=".35" out="Fast"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="LeaguePanelContainmentBG">
            <Anchor side="Top" relative="$parent" pos="Min" offset="18"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="42"/>
            <Texture val="@UI/SC2_UI_Tournament_LeaguePanelBG"/>
            <TextureType val="NineSlice"/>
            <Width val="712"/>
            <Height val="264"/>
        </Frame>
        
        <!-- Season Placement Container -->

        <Frame type="Frame" name="SeasonPlacementContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="LeagueIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                <Width val="212"/>
                <Height val="216"/>
                <Texture val="@UI/Glues_Icons_Ladder_Placement"/>
            </Frame>

            <Frame type="Label" name="SeasonPlacementLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="35"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="193"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
            </Frame>

            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/SeasonPlacementLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/SeasonPlacementLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/SeasonPlacement"/>
            </Frame>
        </Frame>
        
        <!-- GM Placement Container -->

        <Frame type="Frame" name="GrandMasterPlacementContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="LeagueIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                <Width val="212"/>
                <Height val="216"/>
                <Texture val="@UI/Glues_Icons_Ladder_Placement"/>
            </Frame>

            <Frame type="Label" name="HeaderLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="35"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="193"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
                <Text val="@UI/MatchmakingLeaguePanel/GrandMasterPlacementTitle"/>
            </Frame>

            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/HeaderLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/HeaderLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/GrandMasterPlacementDescription"/>
            </Frame>
        </Frame>
        
        <!-- Placement Container -->
        
        <Frame type="Frame" name="PlacementContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="LeagueIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                <Width val="212"/>
                <Height val="216"/>
                <Texture val="@UI/Glues_Icons_Ladder_Placement"/>
            </Frame>
            
            <Frame type="Label" name="HeaderLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="35"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="193"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
                <Text val="@UI/MatchmakingLeaguePanel/Placement"/>
            </Frame>
            
            <Frame type="Label" name="RemainingMatchesLabel">
                <Anchor side="Top" relative="$parent/HeaderLabel" pos="Max" offset="1"/>
                <Anchor side="Left" relative="$parent/HeaderLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Subtitle"/>
                <Text val="@UI/MatchmakingLeaguePanel/RemainingMatches"/>
            </Frame>
            
            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/RemainingMatchesLabel" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/RemainingMatchesLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/LeaguePlacement"/>
            </Frame>
        </Frame>
        
        <!-- League Container -->
        
        <Frame type="Frame" name="LeagueContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="LeagueIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                <Width val="212"/>
                <Height val="216"/>
                <Texture val="@UI/Glues_Icons_Ladder_Placement"/>
            </Frame>
            
            <Frame type="Image" name="LeagueIconTier0Image">
                <Anchor side="Bottom" relative="$parent/LeagueIconImage" pos="Max" offset="17"/>
                <Anchor side="Left" relative="$parent/LeagueIconImage" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/LeagueIconImage" pos="Mid" offset="0"/>
                <Width val="228"/>
                <Height val="188"/>
                <Texture val="@UI/Glues_Icons_Ladder_Bronze_0"/>
                <Visible val="False"/>
            </Frame>
            
            <Frame type="Image" name="LeagueIconTier2Image">
                <Anchor side="Bottom" relative="$parent/LeagueIconImage" pos="Max" offset="-6"/>
                <Anchor side="Left" relative="$parent/LeagueIconImage" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/LeagueIconImage" pos="Mid" offset="0"/>
                <Width val="124"/>
                <Height val="76"/>
                <Texture val="@UI/Glues_Icons_Ladder_Bronze_2"/>
                <Visible val="False"/>
            </Frame>
            
            <Frame type="Image" name="LeagueIconTier1Image">
                <Anchor side="Bottom" relative="$parent/LeagueIconImage" pos="Max" offset="1"/>
                <Anchor side="Left" relative="$parent/LeagueIconImage" pos="Mid" offset="1"/>
                <Anchor side="Right" relative="$parent/LeagueIconImage" pos="Mid" offset="1"/>
                <Width val="212"/>
                <Height val="104"/>
                <Texture val="@UI/Glues_Icons_Ladder_Bronze_1"/>
                <Visible val="False"/>
            </Frame>
            
            <Frame type="Image" name="LeagueIconTier0StarImage">
                <Anchor side="Bottom" relative="$parent/LeagueIconImage" pos="Max" offset="6"/>
                <Anchor side="Left" relative="$parent/LeagueIconImage" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/LeagueIconImage" pos="Mid" offset="0"/>
                <Width val="75"/>
                <Height val="73"/>
                <Texture val="@UI/Glues_Icons_Ladder_Tier0_Star"/>
                <Visible val="False"/>
            </Frame>

            <Frame type="Label" name="LeagueLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="35"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="193"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
            </Frame>

            <Frame type="Label" name="RankLabel">
                <Anchor side="Top" relative="$parent/LeagueLabel" pos="Max" offset="1"/>
                <Anchor side="Left" relative="$parent/LeagueLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Subtitle"/>
                <Text val="@UI/MatchmakingLeaguePanel/Rank"/>
            </Frame>

            <Frame type="Image" name="InfoIcon">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="13"/>
                <Anchor side="Right" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Max" offset="-15"/>
