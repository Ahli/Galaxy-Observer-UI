            </Frame>
            
            <Frame type="Image" name="LockIcon">
                <Anchor side="Top" relative="$parent/IconBackground" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/IconBackground" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/IconBackground" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/IconBackground" pos="Mid" offset="0"/>
                <Width val="84"/>
                <Height val="88"/>
                <Texture val="@UI/Glues_Battlenet_PageAssets_StandardElements_ArtBackground_Lock"/>
            </Frame>

            <Frame type="Label" name="TitleLabel">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Min" offset="24"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG/DividerAnchor" pos="Max" offset="24"/>
                <Anchor side="Right" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Max" offset="-66"/>
