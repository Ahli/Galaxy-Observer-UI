<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <Frame type="Frame" name="MatchmakingLeagueContainerTemplate">
        <Anchor relative="$parent"/>
        <Visible val="false"/>
            
        
    </Frame>

    <Frame type="MatchmakingLeaguePanel" name="MatchmakingLeaguePanelTemplate">
        <Width val="800"/>
        <Height val="300"/>
        
        <!-- League Icon Animations: -->
        <!-- "Bronze", "Silver", "Gold", "Platinum", "Diamond", "Master","Grandmaster" -->
        
        <Frame type="Image" name="LeaguePanelContainmentBG">
            <Anchor side="Top" relative="$parent" pos="Min" offset="16"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="29"/>
            <Texture val="@UI/SC2_UI_Tournament_LeaguePanelBG"/>
            <TextureType val="NineSlice"/>
            <Width val="712"/>
            <Height val="264"/>
        </Frame>
        
        <!-- Season Placement Container -->

        <Frame type="Frame" name="SeasonPlacementContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="LeagueIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Width val="212"/>
                <Height val="212"/>
                <Texture val="@UI/Glues_Icons_League_Matchmaking_Placement"/>
            </Frame>

            <Frame type="Label" name="SeasonNameLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="55"/>
                <Anchor side="Left" relative="$parent/LeagueIconImage" pos="Max" offset="9"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
            </Frame>

            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/SeasonNameLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/SeasonNameLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/SeasonPlacementTournaments"/>
            </Frame>
        </Frame>
        
        <!-- Placement Container -->
        
        <Frame type="Frame" name="PlacementContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="LeagueIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Width val="212"/>
                <Height val="212"/>
                <Texture val="@UI/Glues_Icons_League_Matchmaking_Placement"/>
            </Frame>
            
            <Frame type="Label" name="HeaderLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="55"/>
                <Anchor side="Left" relative="$parent/LeagueIconImage" pos="Max" offset="9"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
                <Text val="@UI/MatchmakingLeaguePanel/Placement"/>
            </Frame>
            
            <Frame type="Label" name="RemainingMatchesLabel">
                <Anchor side="Top" relative="$parent/HeaderLabel" pos="Max" offset="1"/>
                <Anchor side="Left" relative="$parent/HeaderLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Subtitle"/>
                <Text val="@UI/MatchmakingLeaguePanel/RemainingMatches"/>
            </Frame>
            
            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/RemainingMatchesLabel" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/RemainingMatchesLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/SeasonPlacementTournaments"/>
            </Frame>
        </Frame>
        
        <!-- League Container -->
        
        <Frame type="Frame" name="LeagueContainer" template="MatchmakingLeaguePanel/MatchmakingLeagueContainerTemplate">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <Frame type="Image" name="LeagueIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Width val="212"/>
                <Height val="212"/>
                <Texture val="@UI/Glues_Icons_League_Matchmaking_Placement"/>
            </Frame>
            
            <Frame type="Label" name="LeagueLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="55"/>
                <Anchor side="Left" relative="$parent/LeagueIconImage" pos="Max" offset="9"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Title"/>
            </Frame>
            
            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/LeagueLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/LeagueLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/SeasonPlacementTournaments"/>
            </Frame>
        </Frame>
        
        <!--
        
        
        LEAGUE STATE ANIMATIONS
        
        
        -->
        
        <!-- Empty -->
        
        <Animation name="LeagueStateEmpty">
            <Event event="Empty" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- Placement -->
        
        <Animation name="LeagueStatePlacement">
            <Event event="Placement" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- SeasonPlacement -->
        
        <Animation name="LeagueStateSeasonPlacement">
            <Event event="SeasonPlacement" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- League -->
        
        <Animation name="LeagueStateLeague">
            <Event event="League" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
        </Animation>
        
        <!--
        
        
        LEAGUE ICON ANIMATIONS
        
        
        -->
        
        <!-- Bronze -->
        
        <Animation name="Bronze">
            <Event event="Bronze" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="LeagueContainer/LeagueIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_League_Matchmaking_Bronze"/>
            </Controller>
        </Animation>
        
        <!-- Silver -->
        
        <Animation name="Silver">
            <Event event="Silver" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="LeagueContainer/LeagueIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_League_Matchmaking_Silver"/>
            </Controller>
        </Animation>
        
        <!-- Gold -->
        
        <Animation name="Gold">
            <Event event="Gold" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="LeagueContainer/LeagueIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_League_Matchmaking_Gold"/>
            </Controller>
        </Animation>
        
        <!-- Platinum -->
        
        <Animation name="Platinum">
            <Event event="Platinum" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="LeagueContainer/LeagueIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_League_Matchmaking_Platinum"/>
            </Controller>
        </Animation>
        
        <!-- Diamond -->
        
        <Animation name="Diamond">
            <Event event="Diamond" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="LeagueContainer/LeagueIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_League_Matchmaking_Diamond"/>
            </Controller>
        </Animation>
        
        <!-- Master -->
        
        <Animation name="Master">
            <Event event="Master" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="LeagueContainer/LeagueIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_League_Matchmaking_Master"/>
            </Controller>
        </Animation>
        
        <!-- Grandmaster -->
        
        <Animation name="Grandmaster">
            <Event event="Grandmaster" action="Reset,Play"/>

            <Controller type="Texture" end="Pause" frame="LeagueContainer/LeagueIconImage">
                <Key type="Image" time="0" image="@UI/Glues_Icons_League_Matchmaking_Grandmaster"/>
            </Controller>
        </Animation>
    </Frame>
</Desc>
