                <Height val="43"/>
                <Texture val="@UI/Glues_Icons_GenericInfo"/>
                <AcceptsMouseTooltip val="true"/>
                <Tooltip val="@UI/MatchmakingLeaguePanel/BonusPointsTooltip"/>
            </Frame>

            <Frame type="Label" name="BonusPointsLabel">
                <Anchor side="Top" relative="$parent/RankLabel" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/RankLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/BonusPoints"/>
            </Frame>

            <Frame type="Label" name="WinsLabel">
                <Anchor side="Top" relative="$parent/BonusPointsLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/BonusPointsLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/Wins"/>
            </Frame>

            <Frame type="Label" name="SeasonLabel">
                <Anchor side="Top" relative="$parent/WinsLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/WinsLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/Season"/>
            </Frame>

            <Frame type="Label" name="SeasonEndLabel">
                <Anchor side="Top" relative="$parent/SeasonLabel" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/SeasonLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/SeasonEnd"/>
            </Frame>

            <Frame type="Button" name="ViewLadderButton">
                <Anchor relative="$parent/LeagueIconImage"/>
            </Frame>
        </Frame>

        <Frame type="Label" name="TeamMembersLabel">
            <Anchor side="Top" relative="$parent/LeagueContainer/SeasonEndLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/LeagueContainer/SeasonEndLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-100"/>
            <Style val="@MatchmakingLeaguePanel_Details"/>
            <Text val="@UI/MatchmakingLeaguePanel/TeamFormatRandom"/>
            <Visible val="false"/>

            <StateGroup name="TeamMembersGroup">
                <State name="LeagueStateEmpty">
                    <When type="AnimationState" frame="$parent" LeagueState="Empty"/>
                    <Action type="SetProperty" frame="$this" visible="false"/>
                </State>

                <State name="TeamStateSoloQueue">
                    <When type="AnimationState" frame="$parent" TeamState="SoloQueue"/>
                    <Action type="SetProperty" frame="$this" visible="false"/>
                </State>

                <State name="TeamStateTeamQueue">
                    <When type="AnimationState" frame="$parent" TeamState="TeamQueue"/>
                    <Action type="SetProperty" frame="$this" visible="true"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!--
        
        
        LEAGUE STATE ANIMATIONS
        
        
        -->
        
        <!-- Empty -->
        
        <Animation name="LeagueStateEmpty">
            <Event event="Empty" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- Placement -->
        
        <Animation name="LeagueStatePlacement">
            <Event event="Placement" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- SeasonPlacement -->
        
        <Animation name="LeagueStateSeasonPlacement">
            <Event event="SeasonPlacement" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>

        <!-- GrandMasterPlacement -->

        <Animation name="LeagueStateGrandMasterPlacement">
            <Event event="GrandMasterPlacement" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
        </Animation>
        
        <!-- League -->
        
        <Animation name="LeagueStateLeague">
            <Event event="League" action="Reset,Play"/>

            <Controller type="Visibility" end="Pause" frame="SeasonPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="GrandMasterPlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="PlacementContainer">
                <Key type="Bool" time="0" value="False"/>
            </Controller>
            
            <Controller type="Visibility" end="Pause" frame="LeagueContainer">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
        </Animation>
        
