                <StateGroup name="ProvisionalStateGroup">
                    <State name="ProvisionalGrandmasterState">
                        <When type="AnimationState" frame="$parent/$parent" ProvisionalState="Provisional"/>
                        <When type="AnimationState" frame="$parent/LeagueIcon" LeagueIcon="Grandmaster"/>
                        <Action type="SetProperty" frame="$this" Tooltip="@UI/MatchmakingLeaguePanel/ProvisionalTooltip"/>
