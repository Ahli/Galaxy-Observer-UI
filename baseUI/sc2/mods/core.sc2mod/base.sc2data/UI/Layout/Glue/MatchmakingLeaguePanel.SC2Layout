                        <Action type="SetProperty" frame="$this" Tooltip="@UI/MatchmakingLeaguePanel/BonusPointsTooltip"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>
                </StateGroup>
            </Frame>

            <StateGroup name="GrandmasterStateGroup">
