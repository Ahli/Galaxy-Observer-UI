                <Text val="@UI/MatchmakingLeaguePanel/Placement"/>
            </Frame>

            <Frame type="Label" name="SubtitleLabel">
                <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="-1"/>
                <Anchor side="Left" relative="$parent/TitleLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Max" offset="-24"/>
                <Style val="@MatchmakingLeaguePanel_Subtitle"/>
                <Text val="@UI/MatchmakingLeaguePanel/Locked"/>
                <CollapseLayout val="true"/>
            </Frame>

            <Frame type="Label" name="DescriptionLabel">
                <Anchor side="Top" relative="$parent/SubtitleLabel" pos="Max" offset="16"/>
                <Anchor side="Left" relative="$parent/SubtitleLabel" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent/LeaguePanelContainmentBG" pos="Max" offset="-24"/>
                <Style val="@MatchmakingLeaguePanel_Details"/>
                <Text val="@UI/MatchmakingLeaguePanel/RankedUnlockDetailsTooltip"/>
