            <Frame type="AchievementInfoFrame" name="AchievementFrame">
                <Anchor side="Top" relative="$parent/$parent/LeaguePanelContainmentBG/AnchorFrame" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent/$parent/LeaguePanelContainmentBG/AnchorFrame" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent/LeaguePanelContainmentBG/AnchorFrame" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent/LeaguePanelContainmentBG/AnchorFrame" pos="Max" offset="0"/>
                <PresenceId val="{$BattleUI/@LocalToonPresenceId}"/>
                <AchievementId val="91475336794577"/>
                <Visible val="True"/>

                <Frame type="BattleQueueFrame" name="QueueFrame">
                    <Anchor relative="$parent"/>
                    <QueueHandle val="{$ancestor[@type=MatchmakingLeaguePanel]/@QueueHandle}"/>
                </Frame>

                <StateGroup name="AllowedStateGroup">
                    <State name="AllowedState">
                        <When type="Property" frame="QueueFrame" IsAllowed="false"/>
                        <Action type="SetProperty" frame="$this" Visible="true"/>
                    </State>
                </StateGroup>
            
                <Frame type="Frame" name="ProgressBar" template="StandardElementsTemplates/SimpleProgressBarContainerTemplate">
                    <Frame type="Frame" name="AnchorFrame">
                        <Anchor side="Bottom" relative="$parent/$parent/$parent/$parent/LeaguePanelContainmentBG" pos="Max" offset="-32"/>
                        <Anchor side="Left" relative="$parent/$parent/$parent/$parent/LeaguePanelContainmentBG/DividerAnchor" pos="Max" offset="26"/>
                        <Width val="430"/>
                        <Height val="10"/>
                    </Frame>
                
                    <Frame type="ProgressBar" name="ProgressBar">
                        <MinValue val="0"/>
                        <MaxValue val="{$parent/$parent/@CriteriaTotal}"/>
                        <Value val="{$parent/$parent/@CriteriaEarned}"/>
                    </Frame>
                </Frame>

                <Frame type="Label" name="ProgressLabel">
                    <Anchor side="Top" relative="$parent/ProgressBar" pos="Mid" offset="1"/>
                    <Anchor side="Bottom" relative="$parent/ProgressBar" pos="Mid" offset="1"/>
                    <Anchor side="Left" relative="$parent/ProgressBar" pos="Max" offset="7"/>
                    <Style val="@MatchmakingLeaguePanel_RankedUnlockCount"/>
                    <Text val="{$parent/FormatFrame/@Text}"/>
                </Frame>

                <Frame type="TextFormatFrame" name="FormatFrame">
                    <Anchor relative="$parent"/>
                    <FormatText val="%0%/%1%"/>
                    <Replacement index="0" val="{$parent/@CriteriaEarned}"/>
                    <Replacement index="1" val="{$parent/@CriteriaTotal}"/>
                </Frame>
