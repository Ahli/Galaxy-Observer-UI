    <Frame type="Frame" name="WarChestPhaseFrame1Template" template="WarChestPhaseFrameTemplate/WarChestPhaseFrameTemplate">
        <Anchor side="Top" relative="$WarChestFrame/ZergWarChest/WarChestNode2" pos="Mid" offset="-70"/>
        <Anchor side="Bottom" relative="$WarChestFrame/ZergWarChest/WarChestNode2" pos="Mid" offset="-70"/>
        <Anchor side="Left" relative="$WarChestFrame/ZergWarChest/WarChestNode2" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$WarChestFrame/ZergWarChest/WarChestNode2" pos="Mid" offset="0"/>
        
        <StateGroup name="LastPhaseStateGroup">
            <DefaultState val="LastPhaseActive"/>
            
            <State name="LastPhaseActive">
