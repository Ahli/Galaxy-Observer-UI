    <Frame type="Frame" name="PhaseFramesContainerTemplate">
        <Anchor relative="$parent"/>

        <Animation name="Shimmer">
            <Event event="OnShown" action="Reset,Play" frame="$this"/>
             
            <Controller type="Event" end="Loop" frame="PhaseFrame1">
                <Key type="Event" time="0" event=""/>
                <Key type="Event" time="0" event="ShimmerA"/>
                <Key type="Event" time="0" event="ShimmerB"/>
                <Key type="Event" time="7" event=""/>
            </Controller>
            
            <Controller type="Event" end="Loop" frame="PhaseFrame2">
                <Key type="Event" time="0" event=""/>
                <Key type="Event" time="4.666" event="ShimmerA"/>
                <Key type="Event" time="4.666" event="ShimmerB"/>
                <Key type="Event" time="7" event=""/>
            </Controller>
            
            <Controller type="Event" end="Loop" frame="PhaseFrame3">
                <Key type="Event" time="0" event=""/>
                <Key type="Event" time="2.750" event="ShimmerA"/>
                <Key type="Event" time="2.750" event="ShimmerB"/>
                <Key type="Event" time="7" event=""/>
            </Controller>
        </Animation>
        
        <Frame type="Frame" name="PhaseFrame1" template="WarChestPhaseFrameTemplate/WarChestPhaseFrame1Template"/>
        <Frame type="Frame" name="PhaseFrame2" template="WarChestPhaseFrameTemplate/WarChestPhaseFrame2Template"/>
        <Frame type="Frame" name="PhaseFrame3" template="WarChestPhaseFrameTemplate/WarChestPhaseFrame3Template"/>
        
        <Frame type="Frame" name="LoadingDelayTriggerFrame">
            <Anchor relative="$parent"/>
            
            <Animation name="LoadingDelay">
                <Event event="OnShown" frame="$WarChestFrame" action="Reset,Play"/>
                <Event event="OnHidden" frame="$WarChestFrame" action="Reset"/>
                
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time=".01" value="false"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
