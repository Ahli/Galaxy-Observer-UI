            <State name="LastPhaseInactive"/>
        </StateGroup>
        
        <StateGroup name="PhaseUnlockedStateGroup">
            <DefaultState val="Locked"/>
            
            <State name="Unlocked">
                <When type="Property" frame="$ancestor[@name=WarChestFrame]/WarChestDataControlCenter/CurrentPhaseBind" operator="GreaterThanOrEqual" A="1"/>
            </State>
            
            <State name="Locked"/>
        </StateGroup>
        
        <StateGroup name="UnlockSeenStateGroup">
            <DefaultState val="Unseen"/>
            
