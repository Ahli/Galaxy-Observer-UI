                <Key type="Curve" time="0.000" value="255" inOut="Linear"/>
                <Key type="Curve" time="1.167" value="255" inOut="Linear"/>
                <Key type="Curve" time="1.217" value="191" inOut="Linear"/>
                <Key type="Curve" time="1.267" value="230" inOut="Linear"/>
                <Key type="Curve" time="1.317" value="208" inOut="Linear"/>
                <Key type="Curve" time="1.367" value="255" inOut="Linear"/>
