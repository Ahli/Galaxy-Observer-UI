            <Height val="100"/>
        </Frame>
        
        <Frame type="CountdownLabel" name="ExperienceCounter">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Handle val="WarChestExperienceCounter"/>
            <FormatNumber val="true"/>
            <Style val="Coop_ScoreScreen_RewardAmount"/>
            <StartValue val="{$WarChestLastSeenXPBind/@A}"/>
            <TargetValue val="{$WarChestEarnedXPBind/@A}"/>
            <Alpha val="0"/>
            <CountdownSound val=""/>
            
            <Animation name="ResetCurrentValue">
                <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                
                <Controller type="Event" end="Stop" frame="$this">
                    <Key type="Event" time="0" event="CountdownLabelResetRequest"/>
                </Controller>
            </Animation>

            <Frame type="MathFrame" name="AnimTime">
                <Anchor relative="$parent"/>
                <A val="0"/>
            </Frame>
            
            <Animation name="ProgressFill" speed="#WarChestNodeAnimationSpeed">
                <Event event="StartXP" action="Reset,Play" frame="$this"/>
                <Event event="PauseXP" action="Pause" frame="$parent"/>
                <Event event="ResumeXP" action="Play" frame="$parent"/>
                
                <Controller type="Counter" end="Pause">
                    <Key type="Curve" time="0.000" value="0.0" out="Slow"/>
                    <Key type="Curve" timeinc="4.0000" value="1.0" in="Slow"/>
                </Controller>
            </Animation>
            
            <StateGroup name="ChangedXPState">
                <DefaultState val="None"/>
                
                <State name="OnHoldWhileWarChestLoading">
                    <When type="Property" frame="$WarChestFrame/PhaseFrames/LoadingDelayTriggerFrame" Visible="true"/>
                </State>
                
                <State name="OnHoldWhilePhaseUnlocksMoveLinePhase1">
                    <When type="Property" frame="$WarChestLastSeenPhaseBind" Operator="LessThan" A="{$WarChestCurrentPhaseBind/@A}"/>
                    <When type="Property" frame="$WarChestLastSeenPhaseBind" A="1"/>
                    <When type="Property" frame="$WarChestLastSeenXPBind" Operator="GreaterThanOrEqual" A="{$WarChestLastSeenActiveNodeMaxXPBind/@A}"/>
                    <When type="StateGroup" frame="$WarChestFrame" operator="NotEqual" PhaseAnimationState="Finished"/>
                    <When type="StateGroup" frame="$WarChestFrame" LineMovedState="NotFinished"/>
                    <Action type="SendEvent" frame="$WarChestFrame" event="MoveLine4" on="Exit"/>
                </State>
                
                <State name="OnHoldWhilePhaseUnlocksMoveLinePhase2">
                    <When type="Property" frame="$WarChestLastSeenPhaseBind" Operator="LessThan" A="{$WarChestCurrentPhaseBind/@A}"/>
                    <When type="Property" frame="$WarChestLastSeenPhaseBind" A="2"/>
                    <When type="Property" frame="$WarChestLastSeenXPBind" Operator="GreaterThanOrEqual" A="{$WarChestLastSeenActiveNodeMaxXPBind/@A}"/>
                    <When type="StateGroup" frame="$WarChestFrame" operator="NotEqual" PhaseAnimationState="Finished"/>
                    <When type="StateGroup" frame="$WarChestFrame" LineMovedState="NotFinished"/>
                    <Action type="SendEvent" frame="$WarChestFrame" event="MoveLine9" on="Exit"/>
                </State>
                
                <State name="OnHoldWhilePhaseUnlocksDoNotMoveLine">
                    <When type="Property" frame="$WarChestLastSeenPhaseBind" Operator="LessThan" A="{$WarChestCurrentPhaseBind/@A}"/>
                    <When type="StateGroup" frame="$WarChestFrame" operator="NotEqual" PhaseAnimationState="Finished"/>
                </State>
                
                <State name="VerySmall">
                    <When type="Property" frame="$WarChestFrame" Visible="true"/>
                    <When type="Property" frame="$WarChestChangedXPBind" operator="GreaterThanOrEqual" A="1"/>
                    <Action type="SetProperty" frame="$this" CountdownSound="UI_WarChest_XP_tick"/>
                    <Action type="SendEvent" frame="$this" event="StartXP"/>
                </State>
                
                <State name="None">
                    <When type="Property" frame="$WarChestChangedXPBind" A="0"/>
                    <Action type="SetProperty" frame="$this" CountdownSound=""/>
                    <Action type="SendEvent" frame="$this" event="StartXP"/>
                </State>
            </StateGroup>
        </Frame>
        
