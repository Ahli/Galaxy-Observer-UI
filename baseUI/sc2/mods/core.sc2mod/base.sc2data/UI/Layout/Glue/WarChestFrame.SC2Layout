    <Frame type="Image" name="WarChestVerticalLineTemplate">
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <Width val="16"/>
        <Texture val="@UI/SC2_UI_Collection_WarChest_ProgressLines_Vertical_Unowned"/>
        <TextureType val="NineSlice"/>
        
        <StateGroup name="WarChestOwnedState">
            <DefaultState val="Unowned"/>
            
            <State name="TerranOwned">
                <When type="Property" frame="$TerranWarChest/WarChestNode0/WarChestOwnedAchievement" Awarded="True"/>
                <Action type="SetProperty" frame="$this" Texture="@UI/SC2_UI_Collection_WarChest_ProgressLines_Vertical_Owned"/>
            </State>
            
            <State name="ZergOwned">
                <When type="Property" frame="$ZergWarChest/WarChestNode0/WarChestOwnedAchievement" Awarded="True"/>
                <Action type="SetProperty" frame="$this" Texture="@UI/SC2_UI_Collection_WarChest_ProgressLines_Vertical_Owned"/>
            </State>
            
            <State name="ProtossOwned">
                <When type="Property" frame="$ProtossWarChest/WarChestNode0/WarChestOwnedAchievement" Awarded="True"/>
                <Action type="SetProperty" frame="$this" Texture="@UI/SC2_UI_Collection_WarChest_ProgressLines_Vertical_Owned"/>
            </State>
            
            <State name="Unowned">
                <Action type="SetProperty" frame="$this" Texture="@UI/SC2_UI_Collection_WarChest_ProgressLines_Vertical_Unowned"/>
            </State>
        </StateGroup>
    </Frame>
    
