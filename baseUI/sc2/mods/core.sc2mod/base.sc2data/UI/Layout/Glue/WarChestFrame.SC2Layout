        <Frame type="GlueButton" name="PurchaseButton" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
            <Anchor side="Bottom" relative="$Anchor_ActionButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$Anchor_ActionButton" pos="Max" offset="0"/>
            <Text val="@UI/ScreenCollection/WarChest/Purchase"/>
            <Width val ="288"/>
            <CollapseLayout val="True"/>
            <HoverSound val="@UI_HoverTick"/>
            <EnabledDuringRestrictedState val="true"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="Visible"/>
                
                <State name="NotVisibleCantPurchase">
                    <When type="Property" frame="$WarChestFrame" IsPurchaseAvailable="False"/>
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
                
                <State name="NotVisibleOwnAll">
                    <When type="StateGroup" frame="$TerranWarChest/WarChestNode0" OwnedState="Owned"/>
                    <When type="StateGroup" frame="$ZergWarChest/WarChestNode0" OwnedState="Owned"/>
                    <When type="StateGroup" frame="$ProtossWarChest/WarChestNode0" OwnedState="Owned"/>
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
                
                <State name="Visible">
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
            </StateGroup>
            
            <StateGroup name="UpgradeState">
                <DefaultState val="Upgrade"/>
                
                <State name="Purchase">
                    <When type="StateGroup" frame="$TerranWarChest/WarChestNode0" OwnedState="NotOwned"/>
                    <When type="StateGroup" frame="$ZergWarChest/WarChestNode0" OwnedState="NotOwned"/>
                    <When type="StateGroup" frame="$ProtossWarChest/WarChestNode0" OwnedState="NotOwned"/>
                    <Action type="ApplyTemplate" frame="$this" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCollection/WarChest/Purchase"/>
                </State>
                
                <State name="Upgrade">
                    <Action type="ApplyTemplate" frame="$this" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate"/>
                    <Action type="SetProperty" frame="$this" Text="@UI/ScreenCollection/WarChest/Upgrade"/>
                </State>
            </StateGroup>
        </Frame>

