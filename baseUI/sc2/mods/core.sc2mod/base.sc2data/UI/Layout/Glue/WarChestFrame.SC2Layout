                    <SubpixelRendering val="True"/>
            
                    <TextureCoords top="-1.69" left="-1.69" bottom="2.69" right="2.69" layer="0"/>
                    <TextureCoords top="-1.830" left="-1.830" bottom="2.830" right="2.830" layer="1"/>
                    <TextureCoords top="0.00" left="0.00" bottom="1.00" right="1.00" layer="2"/>
                    <TextureType val="Circular" layer="3"/>
                </Frame>
            </Frame>
        
            <Animation name="WarChestCompleteAnimation" speed="#WarChestWheelAnimationSpeed">
                <Event event="WarChestCompleteAnimStart" action="Reset,Play" frame="$WarChestFrame"/>
                <Event event="WarChestCompleteAnimReset" action="Reset" frame="$WarChestFrame"/>
                
                <Controller type="Property" property="LayerAlpha" curve="True" index="3"  end="Pause" frame="$this/FillImageContainer/FillImage">
                    <Key type="Property" time="0.000" value="255" inout="Linear"/>
                    <Key type="Property" time="3.567" value="255" inout="Linear"/>
                    <Key type="Property" time="3.933" value="0" inout="Linear"/>
                </Controller>
                
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause">
                    <Key type="Curve" time="0.000" value="100" inout="Linear"/>
                    <Key type="Curve" time="3.383" value="100" inout="Linear"/>
                    <Key type="Curve" time="3.567" value="106" inout="Slow"/>
                    <Key type="Curve" time="3.933" value="20" inout="Linear"/>
                </Controller>
                
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause">
                    <Key type="Curve" time="0.000" value="100" inout="Linear"/>
                    <Key type="Curve" time="3.383" value="100" inout="Linear"/>
                    <Key type="Curve" time="3.567" value="106" inout="Slow"/>
                    <Key type="Curve" time="3.933" value="20" inout="Linear"/>
                </Controller>
            </Animation>
            
            <StateGroup name="DataState">
                <DefaultState val="Current"/>
                
                <State name="LastSeen">
                    <When type="Property" frame="$WarChestLastSeenActiveNodeBind" A="{$WarChestVisibleActiveNodeBind/@A}"/>
                    <Action type="SetProperty" frame="$this" MinValue="{$WarChestLastSeenActiveNodeMinXPBind/@A}"/>
                    <Action type="SetProperty" frame="$this" MaxValue="{$WarChestLastSeenActiveNodeMaxXPBind/@A}"/>
                    <Action type="SetProperty" frame="$this" Value="{$WarChestCurrentXPBind/@A}"/>
                </State>
                
                <State name="Current">
                    <Action type="SetProperty" frame="$this" MinValue="{$WarChestVisibleActiveNodeMinXPBind/@A}"/>
                    <Action type="SetProperty" frame="$this" MaxValue="{$WarChestVisibleActiveNodeMaxXPBind/@A}"/>
                    <Action type="SetProperty" frame="$this" Value="{$WarChestCurrentXPBind/@A}"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!--
        =================================
        Progress Bar Start Point
        =================================
        -->
        
        <Frame type="Image" name="ProgressBarStartPoint" template="WarChestWheelTemplate/ImageTemplate">
            <Anchor side="Top" relative="$this" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$WarChestWheel" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$WarChestWheel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$WarChestWheel" pos="Mid" offset="0"/>
            <Width val="16"/>
            <Height val="128"/>
            <Color val="#WarChestWheel_SectionA_ProgressStartPoint_Color"/>
            <LayerAlpha val="#WarChestWheel_SectionA_ProgressStartPoint_Alpha" layer="3"/>
            <Rotation val="0"/>
            <RotationPosition val="BottomCenter"/>
            
            <TextureCoords top="0.015625" left="0.046875" bottom="-0.109375" right="0.0625" layer="2"/>
        
            <Animation name="WarChestCompleteAnimation" speed="#WarChestWheelAnimationSpeed">
                <Event event="WarChestCompleteAnimStart" action="Reset,Play" frame="$WarChestFrame"/>
                <Event event="WarChestCompleteAnimReset" action="Reset" frame="$WarChestFrame"/>
                
                <Controller type="Property" property="LayerAlpha" curve="True" index="3"  end="Pause" frame="$this">
                    <Key type="Property" time="0.000" value="255" inout="Linear"/>
                    <Key type="Property" time="3.317" value="255" inout="Linear"/>
                    <Key type="Property" time="3.450" value="0" inout="Linear"/>
                </Controller>
                
                <Controller type="Anchor" side="Bottom" relative="true" end="Pause" frame="$this">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time="3.200" value="0" inout="Linear"/>
                    <Key type="Curve" time="3.317" value="-7" inout="Slow"/>
                    <Key type="Curve" time="3.450" value="13" inout="Linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <!--
        =================================
        Progress Bar End Point
        =================================
        -->
        
        <Frame type="Image" name="ProgressBarEndPoint" template="WarChestWheelTemplate/ImageTemplate">
            <Anchor side="Top" relative="$this" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$WarChestWheel" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$WarChestWheel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$WarChestWheel" pos="Mid" offset="0"/>
            <Width val="16"/>
            <Height val="128"/>
            <!-- <Color val="#WarChestWheel_SectionA_ProgressEndPoint_Color"/> -->
            <Color val="#WarChestWheel_SectionA_ProgressStartPoint_Color"/>
            <LayerAlpha val="#WarChestWheel_SectionA_ProgressEndPoint_Alpha" layer="3"/>
            <Rotation val="{$WarChestProgressBarStandard/FillImageContainer/FillImage/@EndAngle}"/>
            <RotationPosition val="BottomCenter"/>
            
            <TextureCoords top="0.015625" left="0.046875" bottom="-0.109375" right="0.0625" layer="2"/>
        
            <Animation name="WarChestCompleteAnimation" speed="#WarChestWheelAnimationSpeed">
                <Event event="WarChestCompleteAnimStart" action="Reset,Play" frame="$WarChestFrame"/>
                <Event event="WarChestCompleteAnimReset" action="Reset" frame="$WarChestFrame"/>
                
                <Controller type="Property" property="LayerAlpha" curve="True" index="3"  end="Pause" frame="$this">
                    <Key type="Property" time="0.000" value="255" inout="Linear"/>
                    <Key type="Property" time="0.001" value="0" inout="Linear"/>
                </Controller>
            </Animation>
        </Frame>
        
