        <Frame type="Button" name="HideDebugPanelButton" template="StandardGreyboxTemplates/StandardTextButtonSmallTemplate" requiredtoload="IS_DEBUG_PANEL_ENABLED">
            <Anchor side="Top" relative="$parent/WarChestDebugPanel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/WarChestDebugPanel" pos="Min" offset="-5"/>
            <Handle val="WarChestToggleDebugPanelButton"/>
            <Width val="50"/>
            <Height val="50"/>
            <Toggleable val="True"/>
            <Toggled val="True"/>
            <Text val="Hide"/>
            
            <StateGroup name="AnchorState">
                <DefaultState val="Disabled"/>
            
                <State name="Enabled">
                    <When type="Property" frame="$this" toggled="True"/>
                    <Action type="SetProperty" frame="$this" text="Hide"/>
                    <Action type="SetProperty" frame="$this/NormalImage" alpha="255"/>
                    <Action type="SetProperty" frame="$this/Border" alpha="255"/>
                </State>
            
                <State name="Disabled">
                    <Action type="SetProperty" frame="$this" text=""/>
                    <Action type="SetProperty" frame="$this/NormalImage" alpha="0"/>
                    <Action type="SetProperty" frame="$this/Border" alpha="0"/>
