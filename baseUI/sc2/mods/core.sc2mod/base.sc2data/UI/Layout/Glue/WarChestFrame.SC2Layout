        <Texture val="@UI/SC2_UI_Collection_WarChest_ProgressLines_Horizontal_Dotted_Mask" layer="1"/>
        <Tiled val="True" layer="1"/>
        <AlphaLayer val="True" layer="1"/>
        
        <Texture val="@UI/SC2_UI_Collection_WarChest_ProgressLines_Horizontal_Dotted_Shimmer" layer="2"/>
        <TextureCoords top="0.000" left="0.000" bottom="1.000" right="24.000" layer="2"/>
        <LayerColor val="#WarChestProgressLines_Dotted_Color_Shimmer" layer="2"/>
        
        <Texture val="@UI/SC2_UI_Collection_WarChest_ProgressLines_Horizontal_Dotted_Mask" layer="3"/>
        <Tiled val="True" layer="3"/>
        <LayerAlpha val="0" layer="3"/>
        <LayerColor val="#WarChestProgressLines_Dotted_Color_Shimmer" layer="3"/>
        
        <Animation name="Shimmer">
            <Event event="OnShown" action="Reset,Play"/>
             
            <Controller type="LayerUV" layer="2" side="Left" relative="True" sidelock="True" end="Loop" >
                <Key type="Curve" time="0" value="0.25" inout="Linear"/>
                <Key type="Curve" time="#WarChestProgressLines_Dotted_StartTime" value="0.25" inout="Linear"/>
                <Key type="Curve" time="#WarChestProgressLines_Dotted_StopTime" value="-24" inout="Linear"/>
                <Key type="Curve" time="#WarChestProgressLines_Dotted_LoopTime" value="-24" inout="Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="PhaseUnlock">
            <Event event="PhaseUnlockLineStart" action="Reset,DirectionForward,Play" frame="$parent/$parent"/>
            
            <Controller type="Property" property="LayerAlpha" curve="True" index="3"  end="Pause" frame="$this">
                <Key type="Property" time="0.000" value="0" inout="Linear"/>
                <Key type="Property" time="0.150" value="204" inout="Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="PhaseUnlockEnd">
            <Event event="PhaseUnlockLineEnd" action="Reset,Play" frame="$parent/$parent"/>
            
            <Controller type="Property" property="LayerAlpha" curve="True" index="3"  end="Pause" frame="$this">
                <Key type="Property" time="0.000" value="204" inout="Linear"/>
                <Key type="Property" time="0.333" value="0" inout="Linear"/>
            </Controller>
        </Animation>
        
