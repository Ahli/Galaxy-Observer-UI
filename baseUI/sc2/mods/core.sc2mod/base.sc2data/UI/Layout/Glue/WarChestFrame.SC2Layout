        <StateGroup name="WarChestCompleteState">
            <DefaultState val="Incomplete"/>
            
            <State name="Complete">
                <When type="Property" frame="$WarChestCurrentXPBind" operator="GreaterThanOrEqual" A="{$TerranWarChest/WarChestNode14/WarChestTierFrame/@MaxXP}"/>
