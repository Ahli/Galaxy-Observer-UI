                <Animation name="TestPhaseUnlock">
                    <Event event="OnClick" action="Reset,Play"/>
                    
                    <Controller type="Event" end="Stop" frame="$TerranWarChest">
                        <Key type="Event" time="0" event="PurchaseIntro"/>
                    </Controller>
