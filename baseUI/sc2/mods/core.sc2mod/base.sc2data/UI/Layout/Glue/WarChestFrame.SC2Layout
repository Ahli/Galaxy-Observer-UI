            <Frame type="Button" name="WheelResetButton" template="StandardGreyboxTemplates/StandardTextButtonSmallTemplate">
                <Anchor side="Bottom" relative="$parent/CycleWheelUnitButton" pos="Min" offset="-5"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                <Width val="160"/>
                <Height val="30"/>
                <Text val="Reset Wheel Anim"/>
                
                <Animation name="ResetWheel">
                    <Event event="OnClick" action="Reset,Play" frame="$this" />

                    <Controller type="Visibility" end="Pause" frame="$WarChestWheel/SectionA">
                        <Key type="Bool" time="0" value="False"/>
                        <Key type="Bool" time=".01" value="True"/>
                    </Controller>
                    
                    <Controller type="Visibility" end="Pause" frame="$WarChestWheel/SectionB">
                        <Key type="Bool" time="0" value="False"/>
                        <Key type="Bool" time=".01" value="True"/>
                    </Controller>
                    
                    <Controller type="Visibility" end="Pause" frame="$WarChestWheel/SectionC">
                        <Key type="Bool" time="0" value="False"/>
                        <Key type="Bool" time=".01" value="True"/>
                    </Controller>
                    
                    <Controller type="Visibility" end="Pause" frame="$WarChestWheel/SectionD">
                        <Key type="Bool" time="0" value="False"/>
                        <Key type="Bool" time=".01" value="True"/>
                    </Controller>
                    
                    <Controller type="Visibility" end="Pause" frame="$WarChestWheel/SectionE">
                        <Key type="Bool" time="0" value="False"/>
                        <Key type="Bool" time=".01" value="True"/>
                    </Controller>
