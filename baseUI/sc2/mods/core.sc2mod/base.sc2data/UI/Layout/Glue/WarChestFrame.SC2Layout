        <Frame type="ContentFeedTileList" name="WarChesteSportsContentTileList" template="WarChesteSportsContentTileListTemplate/WarChesteSportsContentTileListTemplate">
            <Handle val="WarChestPlayerInfo"/>
        </Frame>
        
        <Frame type="Frame" name="PurchaseFrame" template="WarChestPurchaseFrame/WarChestPurchaseFrame">
            <Anchor relative="$parent"/>
            <Visible val="false"/>

            <Frame type="WarChestPurchaseTileList" name="TileList">
                <SeasonId val="{$ancestor[@name=WarChestFrame]/@CurrentSeasonId}"/>
            </Frame>
            
            <Animation name="ClickAnim">
                <Event event="OnClick" frame="$parent/PurchaseButton" action="Reset,Play"/>
                <Event event="OnClick" frame="CloseButton" action="Reset,Stop"/>
                <Event event="OnHidden" frame="$parent" action="Reset,Stop"/>
                <Event event="PurchaseComplete" frame="TileList" action="Reset,Stop"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time=".175" value="255" in="Auto"/>
                </Controller>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".001" value="true"/>
                    <Key type="Bool" time=".175" value="true"/>
                </Controller>
            </Animation>
        </Frame>
            
        <Frame type="Frame" name="LadderSplashDialog" template="WarChestLearnMoreDialog/WarChestLearnMoreDialog">
            
            <Animation name="ClickAnim">
                <Event event="OnClick" frame="$parent/LearnMoreButton" action="Reset,Play"/>
                <Event event="OnClick" frame="CloseButton" action="Reset,Stop"/>
                <Event event="OnHidden" frame="$parent" action="Reset,Stop"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time=".175" value="255" in="Auto"/>
                </Controller>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".001" value="true"/>
                    <Key type="Bool" time=".175" value="true"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Frame" name="WarChestCompleteTriggerFrame">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
        
            <Animation name="WarChestCompleteAnim">
                <Event event="WarChestCompleteAnimStart" frame="$parent" action="Reset,Play"/>
                
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time="10" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        <StateGroup name="WarChestCompletionState">
            <DefaultState val="Incomplete"/>
            
            <State name="PreparedToAnimate">
                <When type="StateGroup" frame="TerranWarChest/WarChestNode14" CompletionState="Complete"/>
                <When type="StateGroup" frame="TerranWarChest/WarChestNode14" UnlockAnimationState="Incomplete"/>
                <Action type="SetProperty" frame="Wheel/SectionF" Visible="false"/>
                <Action type="SetProperty" frame="Wheel/SectionA/ProgressUnder" Visible="true"/>
                <Action type="SetProperty" frame="WarChestSkinDisplaySphere" Visible="true"/>
                <Action type="SetProperty" frame="Wheel_ProgressBarGlow" Visible="true"/>
                <Action type="SetProperty" frame="Wheel_ProgressBarStandard" Visible="true"/>
                <Action type="SetProperty" frame="ProgressBarStartPoint" Visible="true"/>
                <Action type="SetProperty" frame="ProgressBarEndPoint" Visible="true"/>
                <Action type="SetProperty" on="Enter" frame="WarChestCompleteTriggerFrame" Visible="true"/>
            </State>
            
            <State name="Animating">
