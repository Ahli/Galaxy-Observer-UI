<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="ScreenCoopCampaign" name="ScreenCoopCampaign">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Handle val="ScreenCoopCampaign"/>
        <DisplayBackgroundModel val="false"/>
        <MasteryVersion val="3"/>
        
        <!--

        <Frame type="Image" name="BackgroundImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <ScaleWidthToTexture val="true"/>
            <TextureType val="Normal"/>
            <Texture val="@UI/GlueCampaignPanelBackground_Challenge"/>
        </Frame> 
        
        -->

        <Frame type="CommanderSelectionPanel" name="CommanderSelectionPanel" template="CommanderSelectionPanel/CommanderSelectionPanelTemplate">
            <Anchor relative="$parent"/>

            <Animation name="Show">
                <Event event="Show" action="Reset" frame="$parent/ScorePanel"/>
                <Event event="Show" action="Play" frame="$parent/ScorePanel"/>

                <Controller type="Event" end="Stop" frame="$this">
                    <Key type="Event" time="0" event="UISlideOut"/>
                </Controller>
            </Animation>

            <Animation name="Hide">
                <Event event="Hide" action="Reset" frame="$parent/ScorePanel"/>
                <Event event="Hide" action="Play" frame="$parent/ScorePanel"/>

                <Controller type="Event" end="Stop" frame="$this">
                    <Key type="Event" time="0" event="UISlideIn"/>
                </Controller>
            </Animation>

            <Animation name="MatchStarting">
                <Event event="Matched" action="Reset,Play" frame="$parent"/>
                <Event event="PartyReady" action="Reset,Play" frame="$parent"/>
                
                <Controller type="Enabled" end="Pause" frame="CancelButton">
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
            </Animation>

            <!-- This is a dummy button for when mutators are inactive -->
            <Frame type="Button" name="MutatorReadyButton" template="StandardBattlenetTemplates/BattlenetAlternateButtonTemplate">
                <Anchor relative="$parent/ReadyButton"/>
                <Text val="@UI/ScreenCoopCampaign/Ready"/>
                <Tooltip val="@UI/ScreenCoopCampaign/MutatorInactiveTooltip"/>
                <Visible val="false"/>
                <Enabled val="false"/>
            </Frame>

            <Frame type="CoopCampaignMutatorInfoPanel" name="MutatorInfoPanel" template="CoopCampaignMutatorInfoPanel/CoopCampaignMutatorInfoPanelTemplate">
                <Anchor relative="$parent"/>
                <Handle val="MutatorInfoPanel"/>
                
                <Animation name="UISlideOut" template="CommanderSelectionPanel/UISlideOutAnimationTemplate"/>
                <Animation name="UISlideIn" template="CommanderSelectionPanel/UISlideInAnimationTemplate"/>
                
                <Frame type="Button" name="MutatorMapButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
                    <Anchor side="Top" relative="$parent/$parent/MapSelectionPanel/MapButton" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/$parent/MapSelectionPanel/MapButton" pos="Min" offset="0"/>
                    <Width val="258"/>
                    <HoverSound val="@UI_HoverTick"/>

                    <Frame type="Label" name="Label">
                        <Anchor side="Left" relative="$parent" pos="Min" offset="14"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="-14"/>
                        <Options val="Ellipsis | TooltipIfTruncated"/>
                        <AcceptsMouseTooltip val="True"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <Frame type="Frame" name="CustomMutatorInfoPanel" template="CoopCampaignCustomMutatorInfoPanel/CoopCampaignCustomMutatorInfoPanelTemplate"/>
            
            <Frame type="Frame" name="StandardInfoPanel" template="CoopCampaignStandardInfoPanel/CoopCampaignStandardInfoPanelTemplate">
                <Anchor side="Bottom" relative="Button" pos="Max" offset="31"/>
                <Anchor side="Left" relative="$parent/ReadyButton" pos="Min" offset="-361"/>
                <Handle val="StandardInfoPanel"/>
                
                <Frame type="Button" name="Button">
                    <Anchor side="Bottom" relative="$parent/$parent/MutatorReadyButton" pos="Max" offset="-4"/>
                    <HoverSound val="@UI_HoverTick"/>
                    <Visible val="False"/>
                </Frame>
            </Frame>

            <Frame type="CoopCampaignMapSelectionPanel" name="MapSelectionPanel" template="CoopCampaignMapSelectionPanel/CoopCampaignMapSelectionPanelTemplate">
                <Anchor relative="$parent"/>
                
                <Frame type="Button" name="RetryMapButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
                    <!--Fake map button to replace the real one while retry is selected.-->
                    <Anchor side="Top" relative="$parent/MapButton" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/MapButton" pos="Min" offset="0"/>
                    <Width val="260"/>
                    <Enabled val="False"/>
                    <HoverSound val="@UI_HoverTick"/>

                    <Frame type="Label" name="Label">
                        <Anchor side="Left" relative="$parent" pos="Min" offset="14"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="-14"/>
                        <Options val="Ellipsis | TooltipIfTruncated"/>
                        <AcceptsMouseTooltip val="True"/>
                    </Frame>
                </Frame>
                
                
                <Frame type="Button" name="MapButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
                    <Anchor side="Top" relative="$parent/$parent/CommanderPurchaseButton" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent/$parent/CommanderPurchaseButton" pos="Max" offset="8"/>
                    <Width val="260"/>
                    <HoverSound val="@UI_HoverTick"/>

                    <Frame type="Label" name="Label">
                        <Anchor side="Left" relative="$parent" pos="Min" offset="14"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="-14"/>
                        <Options val="Ellipsis | TooltipIfTruncated"/>
                        <AcceptsMouseTooltip val="True"/>
                    </Frame>
                </Frame>
                
                <Animation name="UISlideOut" template="CommanderSelectionPanel/UISlideOutAnimationTemplate"/>
                <Animation name="UISlideIn" template="CommanderSelectionPanel/UISlideInAnimationTemplate"/>
            </Frame>

            <Frame type="Button" name="DifficultyButton" template="StandardBattlenetTemplates/BattlenetStandardButtonTemplate">
                <Anchor side="Left" relative="$parent/MapSelectionPanel/MapButton" pos="Max" offset="8"/>
                <Width val="260"/>
                <HoverSound val="@UI_HoverTick"/>
            </Frame>

            <StateGroup name="ButtonModeVisibilty">
                <State name="ButtonModeStandard">
                    <When type="AnimationState" frame="$parent" ModeState="ModeStandard"/>
                    <Action type="SetProperty" frame="MapSelectionPanel" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyButton" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyLabel" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyBonusLabel" visible="True"/>
                    <Action type="SetProperty" frame="StandardInfoPanel" visible="True"/>
                    <Action type="SetProperty" frame="MutatorInfoPanel" visible="False"/>
                    <Action type="SetProperty" frame="CustomMutatorInfoPanel" visible="False"/>
                </State>
              
                <State name="ButtonModeBrutalPlus">
                    <When type="AnimationState" frame="$parent" ModeState="ModeBrutalPlus"/>
                    <Action type="SetProperty" frame="MapSelectionPanel" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyButton" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyLabel" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyBonusLabel" visible="True"/>
                    <Action type="SetProperty" frame="StandardInfoPanel" visible="True"/>
                    <Action type="SetProperty" frame="MutatorInfoPanel" visible="False"/>
                    <Action type="SetProperty" frame="CustomMutatorInfoPanel" visible="False"/>
                </State>

                <State name="ButtonModeMutator">
                    <When type="AnimationState" frame="$parent" ModeState="ModeMutator"/>
                    <Action type="SetProperty" frame="MapSelectionPanel" visible="False"/>
                    <Action type="SetProperty" frame="DifficultyButton" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyLabel" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyBonusLabel" visible="True"/>
                    <Action type="SetProperty" frame="StandardInfoPanel" visible="False"/>
                    <Action type="SetProperty" frame="MutatorInfoPanel" visible="True"/>
                    <Action type="SetProperty" frame="CustomMutatorInfoPanel" visible="False"/>
                </State>

                <State name="ButtonModeCustom">
                    <When type="AnimationState" frame="$parent" ModeState="ModeCustom"/>
                    <Action type="SetProperty" frame="MapSelectionPanel" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyButton" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyLabel" visible="True"/>
                    <Action type="SetProperty" frame="DifficultyBonusLabel" visible="True"/>
                    <Action type="SetProperty" frame="StandardInfoPanel" visible="False"/>
                    <Action type="SetProperty" frame="MutatorInfoPanel" visible="False"/>
                    <Action type="SetProperty" frame="CustomMutatorInfoPanel" visible="True"/>
                </State>
            </StateGroup>

            <StateGroup name="TitleStateGroup">
                <State name="TitleModeStandard">
                    <When type="AnimationState" frame="$parent" ModeState="ModeStandard"/>
                    <Action type="SetProperty" frame="Title" text="@UI/ScreenCoopCampaign/Title"/>
                    <Action type="SetProperty" frame="Subtitle" text="@UI/ScreenCoopCampaign/Subtitle"/>
                </State>

                <State name="TitleModeMutator">
                    <When type="AnimationState" frame="$parent" ModeState="ModeMutator"/>
                    <Action type="SetProperty" frame="Title" text="@UI/ScreenCoopCampaign/MutatorTitle"/>
                    <Action type="SetProperty" frame="Subtitle" text="@UI/ScreenCoopCampaign/MutatorSubtitle"/>
                </State>

                <State name="TitleModeCustom">
                    <When type="AnimationState" frame="$parent" ModeState="ModeCustom"/>
                    <Action type="SetProperty" frame="Title" text="@UI/ScreenCoopCampaign/CustomTitle"/>
                    <Action type="SetProperty" frame="Subtitle" text="@UI/ScreenCoopCampaign/CustomSubtitle"/>
                </State>
            </StateGroup>

            <StateGroup name="ReadyButtonStateGroup">
                <!-- Commanders are always playable even if they're not purchased now. -->
                <!--
                <State name="CampaignPurchaseState">
                    <When type="AnimationState" CommanderCampaignPurchase="ShowCampaignPurchase"/>
                    <Action type="SetProperty" frame="ReadyButton" Visible="false"/>
                    <Action type="SetProperty" frame="PartyReadyButton" Visible="false"/>
                    <Action type="SetProperty" frame="CancelButton" Visible="false"/>
                    <Action type="SetProperty" frame="MutatorReadyButton" Visible="false"/>
                </State>
                
                <State name="CommanderPurchaseState">
                    <When type="AnimationState" CommanderPurchase="ShowCommanderPurchase"/>
                    <Action type="SetProperty" frame="ReadyButton" Visible="false"/>
                    <Action type="SetProperty" frame="PartyReadyButton" Visible="false"/>
                    <Action type="SetProperty" frame="CancelButton" Visible="false"/>
                    <Action type="SetProperty" frame="MutatorReadyButton" Visible="false"/>
                </State>
                -->
                
                <State name="MutatorDisabledState">
                    <When type="AnimationState" frame="MutatorInfoPanel" InfoPanelState="Inactive"/>
                    <When type="AnimationState" frame="$parent" ModeState="ModeMutator"/>
                    <Action type="SetProperty" frame="ReadyButton" Visible="false"/>
                    <Action type="SetProperty" frame="PartyReadyButton" Visible="false"/>
                    <Action type="SetProperty" frame="CancelButton" Visible="false"/>
                    <Action type="SetProperty" frame="MutatorReadyButton" Visible="true"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Button" name="DebugButton" template="StandardTemplates/StandardButtonTemplate" requiredtoload="IS_COOP_CAMPAIGN_DEBUG_CODE_ENABLED">
            <Anchor side="Top" relative="$parent" pos="Min" offset="150"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-50"/>
            <Width val ="315"/>
            <Text val="Debug"/>
            <Toggleable val="true"/>

            <StateGroup name="ToggleStateGroup">
                <State name="ToggledState">
                    <When type="Property" frame="$this" Toggled="true"/>
                    <Action type="SetProperty" frame="$parent/DebugFrame" Visible="true"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="CoopCampaignDebugFrame" name="DebugFrame" requiredtoload="IS_COOP_CAMPAIGN_DEBUG_CODE_ENABLED">
            <Anchor side="Top" relative="$parent/DebugButton" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/DebugButton" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="ResetMasteryVersionButton" pos="Max" offset="20"/>
            <Width val="500"/>
            <Visible val="false"/>

            <Frame type="Image" name="Background">
                <Anchor relative="$parent"/>
                <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_MainContainer"/>
                <TextureType val="NineSlice"/>
            </Frame>
            
            <Frame type="EditBox" name="AdvanceTimeEditBox" template="StandardBattlenetTemplates/BattlenetEditBoxTemplate">
                <Anchor side="Top" relative="$parent/Background" pos="Min" offset="20"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
                <Height val="68"/>
                <Width val ="315"/>
            </Frame>

            <Frame type="Pulldown" name="AdvanceTimePulldown" template="StandardBattlenetTemplates/BattlenetPulldownTemplate">
                <Anchor side="Top" relative="$parent/AdvanceTimeEditBox" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
                <Width val ="315"/>
            </Frame>

            <Frame type="Button" name="AdvanceTimeButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
                <Anchor side="Top" relative="$parent/AdvanceTimePulldown" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
