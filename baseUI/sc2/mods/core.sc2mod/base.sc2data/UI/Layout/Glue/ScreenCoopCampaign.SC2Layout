                <Text val="Mastery Earned"/>

                <Animation name="Show">
                    <Event event="OnClick" action="Reset,Play" frame="$this"/>

                    <Controller type="Event" end="Pause" frame="$parent/$parent/MasteryUnlockPanel">
                        <Key type="Event" time="0" event="ShowMasteryWindow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Button" name="ResetNewUserButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
                <Anchor side="Top" relative="$parent/DebugMasteryButton" pos="Max" offset="20"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
                <Width val ="315"/>
                <Text val="Reset New User"/>
            </Frame>

            <Frame type="Button" name="ResetNewUserMutatorButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
                <Anchor side="Top" relative="$parent/ResetNewUserButton" pos="Max" offset="20"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
                <Width val ="315"/>
                <Text val="Reset New User Mutator"/>
            </Frame>

            <Frame type="Button" name="ResetNewUserMasteryButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
                <Anchor side="Top" relative="$parent/ResetNewUserMutatorButton" pos="Max" offset="20"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
                <Width val ="315"/>
                <Text val="Reset New User Mastery"/>
            </Frame>

            <Frame type="Button" name="ResetMasteryVersionButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
                <Anchor side="Top" relative="$parent/ResetNewUserMasteryButton" pos="Max" offset="20"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
                <Width val ="315"/>
                <Text val="Reset Mastery Version"/>
            </Frame>
        </Frame>

        <Frame type="CoopCampaignScorePanel" name="ScorePanel" template="CoopCampaignScorePanel/CoopCampaignScorePanelTemplate">
            <Handle val="ScreenCoopCampaign_ScorePanel"/>
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            <Alpha val="0"/>

            <Animation name="Show">
                <Event event="Show" action="DirectionForward"/>
                <Event event="Show" action="Play"/>
                <Event event="Hide" action="DirectionReverse"/>
                <Event event="Hide" action="Play"/>
                <Event event="Hide" action="Reset,Stop" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time=".175" value="255" in="Auto"/>
                </Controller>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".001" value="true"/>
                    <Key type="Bool" time=".175" value="true"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Frame" name="NewPlayerContainerFrame">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
            
            <StateGroup name="VisibleStateGroup">
                <State name="MasteryActiveState">
                    <When type="Property" frame="$parent/ScorePanel" Visible="false"/>
                    <When type="AnimationState" frame="$parent" ModeState="ModeStandard"/>
                    <Action type="SetProperty" frame="$this" Visible="true"/>
                </State>
            </StateGroup>
            
            <Frame type="Frame" name="NewPlayerPanel">
                <Anchor relative="$parent"/>
                <Visible val="false"/>
                <Alpha val="0"/>

                <!-- This animation is shown by the code based on the first time coming to this screen -->
                <!-- You may change the event to "OnShown" to debug it without a new account every time -->
                <Animation name="Show">
                    <Event event="ShowNewUser" action="Reset,Play" frame="$parent/$parent"/>
                    <Event event="OnClick" action="DirectionReverse,Play" frame="CloseButton"/>

                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Slow"/>
                        <Key type="Curve" time=".175" value="255" in="Auto"/>
                    </Controller>

                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="false"/>
                        <Key type="Bool" time=".001" value="true"/>
                        <Key type="Bool" time=".175" value="true"/>
                    </Controller>
                </Animation>
            
                <!-- This tooltip is just for blocking tooltips behind this panel -->
                <Frame type="StandardTooltip" name="CoopCampaignTooltip">
                    <Frame type="Label" name="Label">
                        <Anchor relative="$parent"/>
                    </Frame>
                </Frame>

                <Frame type="Image" name="FullscreenBlackoutImage">
                    <Anchor relative="$parent"/>
                    <Color val="0,0,0"/>
                    <Alpha val="180"/>
                    <AcceptsMouse val="true"/>
                    <TooltipFrame val="CoopCampaignTooltip"/>
