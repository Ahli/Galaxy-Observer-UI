                <Texture val="@UI/Glues_Battlenet_SubNav_ButtonDivider"/>
                <Alpha val="0"/>
                
                <Animation name="FadeNav">
                    <Event event="OnShown" action="Reset"/>
                    <Event event="OnShown" action="Play"/>

                    <Controller name="FadeIn" type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" in="Linear"/>
                        <Key type="Curve" time="0.5" value="255" out="Linear"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="ButtonDivider2">
                <Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
                <Anchor side="Right" relative="$parent/MutatorButton" pos="Max" offset="#ButtonOffset"/>
                <Texture val="@UI/Glues_Battlenet_SubNav_ButtonDivider"/>
                <Alpha val="0"/>
                
                <Animation name="FadeNav">
                    <Event event="OnShown" action="Reset"/>
                    <Event event="OnShown" action="Play"/>

                    <Controller name="FadeIn" type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" in="Linear"/>
                        <Key type="Curve" time="0.5" value="255" out="Linear"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Button" name="StandardButton" template="ScreenMultiplayer/ScreenMultiplayerButtonTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$Anchor_SubNav" pos="Max" offset="0"/>
                <Text val="@UI/ScreenCoopCampaign/StandardButton"/>
                <Alpha val="0"/>
                
                <Animation name="FadeNav">
                    <Event event="OnShown" action="Reset"/>
                    <Event event="OnShown" action="Play"/>

                    <Controller name="FadeIn" type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" in="Auto"/>
                        <Key type="Curve" time="0.3" value="0" in="Auto"/>
                        <Key type="Curve" time="0.45" value="255" out="Auto"/>
                    </Controller>
                    
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-40" out="Linear"/>
                        <Key type="Curve" time="0.3" value="-40" out="Linear"/>
                        <Key type="Curve" time="0.45" value="0" out="Linear"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Button" name="MutatorButton" template="ScreenMultiplayer/ScreenMultiplayerButtonTemplate">
                <Anchor side="Top" relative="$parent/StandardButton" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/StandardButton" pos="Max" offset="#ButtonOffsetLeft"/>
                <Text val="@UI/ScreenCoopCampaign/MutatorButton"/>
                <Alpha val="0"/>
                
                <Animation name="FadeNav">
                    <Event event="OnShown" action="Reset"/>
                    <Event event="OnShown" action="Play"/>

                    <Controller name="FadeIn" type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" in="Auto"/>
                        <Key type="Curve" time="0.3" value="0" in="Auto"/>
                        <Key type="Curve" time="0.47" value="255" out="Auto"/>
                    </Controller>
                    
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-10" out="Linear"/>
                        <Key type="Curve" time="0.3" value="-10" out="Linear"/>
                        <Key type="Curve" time="0.47" value="0" out="Linear"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Button" name="CustomButton" template="ScreenMultiplayer/ScreenMultiplayerButtonTemplate">
                <Anchor side="Top" relative="$parent/MutatorButton" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/MutatorButton" pos="Max" offset="#ButtonOffsetLeft"/>
                <Text val="@UI/ScreenCoopCampaign/CustomButton"/>
                <Alpha val="0"/>
                
                <Animation name="FadeNav">
                    <Event event="OnShown" action="Reset"/>
                    <Event event="OnShown" action="Play"/>

                    <Controller name="FadeIn" type="Fade" end="Pause">
                        <Key type="Curve" time="0.0" value="0" in="Auto"/>
                        <Key type="Curve" time="0.3" value="0" in="Auto"/>
                        <Key type="Curve" time="0.47" value="255" out="Auto"/>
                    </Controller>
                    
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-10" out="Linear"/>
                        <Key type="Curve" time="0.3" value="-10" out="Linear"/>
                        <Key type="Curve" time="0.47" value="0" out="Linear"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="NewContentFlagGlow">
                <Anchor side="Bottom" relative="$parent/NewMutatorIndicator" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/NewMutatorIndicator" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/NewMutatorIndicator" pos="Mid" offset="0"/>
                <Texture val="@UI/Glues_Battlenet_TopNav_NewContentGlow"/>
                <Width val="36"/>
                <Height val="36"/>
                <Visible val="False"/>

                <Frame type="Image" name="NewContentFlagPulse">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Texture val="@UI/Glues_Battlenet_TopNav_NewContentBg"/>
                    <Width val="36"/>
                    <Height val="36"/>
                    <Alpha val="0"/>
                    
                    <Animation name="FlagGlow">
                        <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                        <Event event="OnHidden" action="Stop" frame="$parent"/>

                        <Controller type="Rotation" relative="False" end="Loop">
                            <Key type="Curve" time="0" value="0" out="Linear"/>
                            <Key type="Curve" time="5" value="360" in="Linear"/>
                        </Controller>

                        <Controller type="Fade" end="Loop">
                            <Key type="Curve" time="0" value="255" out="Fast"/>
                            <Key type="Curve" time="2" value="0" in="Slow"/>
                            <Key type="Curve" time="4" value="255" out="Fast"/>
                        </Controller>
                    </Animation>
                </Frame>
                
                <Animation name="FlagGlow">
                    <Event event="OnShown" action="Reset,Play" frame="$parent/NewMutatorIndicator"/>
                    <Event event="OnHidden" action="Reset,Stop" frame="$parent/NewMutatorIndicator"/>

                    <Controller type="Rotation" relative="False" end="Loop">
                        <Key type="Curve" time="0" value="0" out="Linear"/>
                        <Key type="Curve" time="5" value="360" in="Linear"/>
                    </Controller>

                    <Controller name="Show" type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="true"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Frame" name="NewMutatorHelperFrame">
                <Anchor relative="$parent/NewMutatorIndicator"/>
                <Visible val="false"/>
                <Animation name="Show">
                    <Event event="Show" action="Reset,Play" frame="$parent/$parent/CommanderSelectionPanel/MutatorInfoPanel"/>
                    
                    <Controller name="Show" type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="true"/>
                        <Key type="Bool" time="0.1" value="false"/>
                    </Controller>
                </Animation>

                <StateGroup name="ShowMutatorIndicatorGroup">
                    <State name="ShowMutatorIndicatorState">
                        <When type="Property" frame="$this" Visible="true"/>
                        <When type="AnimationState" frame="$parent/$parent" ModeState="ModeStandard"/>
                        <Action type="SendEvent" frame="$parent/NewMutatorIndicator" event="Show"/>
                    </State>
                </StateGroup>
            </Frame>

            <Frame type="Frame" name="NewMutatorIndicator">
                <Anchor side="Bottom" relative="$parent/MutatorButton" pos="Max" offset="11"/>
                <Anchor side="Right" relative="$parent/MutatorButton/Label" pos="Max" offset="21"/>
                <Width val="36"/>
                <Height val="36"/>
                <Visible val="false"/>

                <Frame type="Image" name="IconImage">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@UI/Glues_Battlenet_TopNav_NewContent"/>
                    <Width val="36"/>
                    <Height val="36"/>
                </Frame>

                <Animation name="Show">
                    <Event event="Show" action="Reset,Play" frame="$this"/>
                    <Event event="OnClick" action="Reset,Stop" frame="$parent/MutatorButton"/>
                    <Event event="ModeMutator" action="Reset,Stop" frame="$parent/$parent"/>
                    
                    <Controller name="Show" type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="true"/>
                    </Controller>

                    <Controller type="Tooltip" end="Pause" frame="$parent/MutatorButton">
                        <Key type="Text" time="0" text="@UI/ScreenNavigation/NewMutation"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="VersionNotificationFrame" name="CustomNewContentIndicator">
                <Anchor side="Bottom" relative="$parent/CustomButton" pos="Max" offset="11"/>
                <Anchor side="Right" relative="$parent/CustomButton/Label" pos="Max" offset="21"/>
                <Width val="36"/>
                <Height val="36"/>
                <Visible val="False"/>
                <Version val="1"/>
                <SettingName val="CustomButton"/>
                
                <Animation name="VersionState">
                    <Event event="NewVersion" action="Reset,Play"/>
                    <Event event="OldVersion" action="Reset,Stop"/>
                    <Event event="InvalidVersion" action="Reset,Stop"/>
                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="True"/>
                    </Controller>
                    <Controller type="Tooltip" end="Pause" frame="$parent/CustomButton">
                        <Key type="Text" time="0" text="@UI/ScreenNavigation/NewMode"/>
                    </Controller>
                </Animation>
             
                <!-- The name of this animation must be "ClearNotification" -->
                <!-- When played, it will update the version of this notification  -->
                <Animation name="ClearNotification">
                    <Event event="OnClick" action="Reset,Play" frame="$parent/CustomButton"/>
                    <Event event="Toggled" action="Reset,Play" frame="$parent/CustomButton"/>
             
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="0.0" event="NoOp"/>
                        <Key type="Event" time="1.0" event="NoOp"/>
                    </Controller>

                    <Controller type="Tooltip" end="Pause" frame="$parent/CustomButton">
                        <Key type="Text" time="0" text=""/>
                    </Controller>
                </Animation>
                    
                <Frame type="Image" name="NewContentFlagBg">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@UI/Glues_Battlenet_TopNav_NewContentGlow"/>
                    <Width val="36"/>
                    <Height val="36"/>

                    <Animation name="FlagGlow">
                        <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                        <Event event="OnHidden" action="Stop" frame="$parent"/>

                        <Controller type="Rotation" relative="False" end="Loop">
                            <Key type="Curve" time="0" value="0" out="Linear"/>
                            <Key type="Curve" time="5" value="360" in="Linear"/>
                        </Controller>
                    </Animation>
                </Frame>

                <Frame type="Image" name="NewContentFlag">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@UI/Glues_Battlenet_TopNav_NewContent"/>
                    <Width val="36"/>
                    <Height val="36"/>
                </Frame>
                
                <Frame type="Image" name="NewContentFlagGlow">
                    <Anchor side="Bottom" relative="$parent/NewContentFlag" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent/NewContentFlag" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent/NewContentFlag" pos="Mid" offset="0"/>
                    <Texture val="@UI/Glues_Battlenet_TopNav_NewContentBg"/>
                    <Width val="36"/>
                    <Height val="36"/>
                    <Alpha val="0"/>
                    
                    <Animation name="FlagGlow">
                        <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                        <Event event="OnHidden" action="Stop" frame="$parent"/>

                        <Controller type="Rotation" relative="False" end="Loop">
                            <Key type="Curve" time="0" value="0" out="Linear"/>
                            <Key type="Curve" time="5" value="360" in="Linear"/>
                        </Controller>

                        <Controller type="Fade" end="Loop">
                            <Key type="Curve" time="0" value="255" out="Fast"/>
                            <Key type="Curve" time="2" value="0" in="Slow"/>
                            <Key type="Curve" time="4" value="255" out="Fast"/>
                        </Controller>
                    </Animation>
                </Frame>
            </Frame>
        </Frame>
    </Frame>
</Desc>
