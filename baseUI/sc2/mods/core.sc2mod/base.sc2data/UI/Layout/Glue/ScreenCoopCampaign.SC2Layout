            </Frame>

            <Frame type="Button" name="AdvanceRotationButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
                <Anchor side="Top" relative="$parent/AdvanceTimeButton" pos="Max" offset="20"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
                <Width val ="315"/>
                <Text val="Next Map Rotation"/>
            </Frame>
            
            <Frame type="Button" name="DebugScoreButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
                <Anchor side="Top" relative="$parent/AdvanceRotationButton" pos="Max" offset="20"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
                <Width val ="315"/>
                <Text val="Score Screen"/>
            </Frame>

            <Frame type="Button" name="DebugMasteryButton" template="StandardBattlenetTemplates/BattlenetMediumButtonTemplate">
                <Anchor side="Top" relative="$parent/DebugScoreButton" pos="Max" offset="20"/>
                <Anchor side="Left" relative="$parent/Background" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/Background" pos="Mid" offset="0"/>
