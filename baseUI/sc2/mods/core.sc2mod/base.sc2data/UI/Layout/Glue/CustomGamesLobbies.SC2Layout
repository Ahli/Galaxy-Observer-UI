                <StateGroup name="AspectRatioState">
                    <DefaultState val="Not16x9"/>
                    
                    <State name="16x9">
                        <When type="StateGroup" frame="$GlueUI" AspectRatioState="16x9"/>
                        <Action type="SetAnchor" frame="$this/Team3" side="Top" relative="$parent/Team1" pos="Min" offset="0"/>
                        <Action type="SetAnchor" frame="$this/Team3" side="Left" relative="$parent/Team2" pos="Max" offset="#CustomGamesLobbies_LobbyPreview_TeamList_Spacing_Horizontal"/>
                    </State>
                    
                    <State name="Not16x9">
                        <Action type="SetAnchor" frame="$this/Team3" side="Top" relative="$parent/Team1" pos="Max" offset="#CustomGamesLobbies_LobbyPreview_TeamList_Spacing_Vertical"/>
                        <Action type="SetAnchor" frame="$this/Team3" side="Left" relative="$parent/Team1" pos="Min" offset="0"/>
