    <!-- 
    ========================
    LOBBY PREVIEW TEMPLATES
    ========================
    -->
    
    <Frame type="LobbyPreviewTileListItem" name="LobbyPreviewPlayerSlotTemplate">
        <Anchor side="Left" relative="$parent" pos="Min" offset="7"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Height val="20"/>
        
        <Frame type="Label" name="Label">
            <Anchor relative="$parent"/>
            <Style val="@CustomGames_Lobby_Preview_PlayerSlot_Inactive"/>
            <Text val="{$parent/@PlayerName}"/>
        </Frame>

        <StateGroup name="LobbyActiveState">
            <DefaultState val="PlayerSlotNotOpen"/>
            
            <State name="PlayerSlotOccupied">
                <When type="Property" LobbySlotPreviewState="OCCUPIED"/>
                <Action type="SetProperty" frame="Label" Style="@CustomGames_Lobby_Preview_PlayerSlot_Inactive"/>
                <Action type="SetProperty" frame="Label" Text="{$parent/@PlayerName}"/>
            </State>
            <State name="PlayerSlotOpen">
                <When type="Property" LobbySlotPreviewState="OPEN"/>
                <Action type="SetProperty" frame="Label" Style="@CustomGames_Lobby_Preview_PlayerSlot_Active"/>
                <Action type="SetProperty" frame="Label" Text="@UI/Lobby/Open"/>
            </State>
            <State name="PlayerSlotNotOpen">
                <Action type="SetProperty" frame="Label" Style="@CustomGames_Lobby_Preview_PlayerSlot_Inactive"/>
                <Action type="SetProperty" frame="Label" Text="@UI/Lobby/Closed"/>
            </State>
        </StateGroup>
    </Frame>
    
    <Frame type="TileList" name="LobbyPreviewTeamTemplate">
