    <!-- 
    ===========================================================
    
    CUSTOM GAMES LOBBIES TEMPLATE
    
    ===========================================================
    -->
    <Frame type="LobbyListFrame" name="CustomGamesLobbies" template="CustomGamesTemplates/CustomGamesBaseTemplate">
        <Anchor relative="$parent"/>
        <HookupAlias original="LobbyListTileList" alias="ContentContainer/PrimaryPanel/LobbyList/LobbyListTileList"/>
        <HookupAlias original="LobbyListTileList/ScrollBar" alias="ContentContainer/PrimaryPanel/LobbyList/LobbyListTileList/ScrollBar"/>
        <HookupAlias original="LobbyListFilterPulldown" alias="ContentContainer/HeaderPanel/LobbyListFilterPulldown"/>
        
        <Frame type="Label" name="TitleLabel">
            <Text val="@UI/ScreenCustomGames/Lobbies/Title"/>
        </Frame>
        
        <Frame type="Label" name="SubtitleLabel">
            <Text val="@UI/ScreenCustomGames/Lobbies/Subtitle"/>
        </Frame>

        <Frame type="Frame" name="ContentContainer">

            <Frame type="Frame" name="HeaderPanel">
            
                <Frame type="Pulldown" name="LobbyListFilterPulldown" template="StandardSmallButtonTemplates/StandardSmallPulldownTemplate">
                
                    <Frame type="Frame" name="AnchorFrame">
                        <Anchor side="Top" relative="$parent/$parent/FilterEditBox" pos="Mid" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/$parent/FilterEditBox" pos="Mid" offset="0"/>
                        <Anchor side="Right" relative="$parent/$parent/RefreshButton/AnchorFrame" pos="Min" offset="-12"/>
                    </Frame>
                </Frame>
                
                <Frame type="EditBox" name="FilterEditBox" template="CustomGamesTemplates/SearchEditBoxTemplate">
    
                    <Frame type="Frame" name="AnchorFrame">
                        <Anchor side="Right" relative="$parent/$parent/LobbyListFilterPulldown/AnchorFrame" pos="Min" offset="-9"/>
                    </Frame>
                </Frame>

                <Frame type="Button" name="RefreshButton" template="StandardSmallButtonTemplates/StandardSmallIconButtonTemplate">
                
                    <Frame type="Frame" name="AnchorFrame">
                        <Anchor side="Top" relative="$parent/$parent/FilterEditBox" pos="Mid" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/$parent/FilterEditBox" pos="Mid" offset="0"/>
                        <Anchor side="Right" relative="$parent/$parent" pos="Max" offset="-12"/>
                    </Frame>

                    <Frame type="Image" name="Icon">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                        <Texture val="@UI/SC2_UI_Battlenet_Glue_Arcade_RefreshIcon"/>
                    </Frame>
        
                    <Animation name="RefreshLobbyList">
                        <Event event="OnClick" frame="$this" action="Reset,Play"/>

                        <Controller type="Event" end="Pause" frame="$ancestor[@type=LobbyListFrame]">
                            <Key type="Event" time="0.0" event="RequestRefreshOpenLobbyList"/>
                        </Controller>
                    </Animation>
                </Frame>
            </Frame>

            <Frame type="Frame" name="PrimaryPanel">
                <Frame type="Frame" name="LobbyList" template="CustomGamesLobbies/LobbyListTemplate"/>
            </Frame>

            <Frame type="Frame" name="SecondaryPanel">
                <Frame type="BattleMapFrame" name="BattleMapFrame">
                    <Anchor relative="$parent"/>
                    <MapHandle val="{$ancestor[@type=LobbyListFrame]/@SelectedMapHandle}"/>
                </Frame>

                <Frame type="BattleMapInfoPanel" name="BattleMapInfoPanel">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/ScreenShotTileList" pos="Min" offset="0"/>
                    <MapHandle val="{$ancestor[@type=LobbyListFrame]/@SelectedMapHandle}"/>

                    <Frame type="Label" name="MapNameLabel">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="18"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                        <Style val="@Arcade_Featured_Title"/>
                        <Text val="{$parent/$parent/BattleMapFrame/@Name}"/>
                    </Frame>

                    <Frame type="Label" name="AuthorLabel">
                        <Anchor side="Top" relative="$parent/MapNameLabel" pos="Max" offset="18"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    </Frame>

                    <Frame type="Label" name="ModeLabel">
                        <Anchor side="Top" relative="$parent/AuthorLabel" pos="Max" offset="18"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    </Frame>

                    <Frame type="Label" name="PlayerLabel">
                        <Anchor side="Top" relative="$parent/ModeLabel" pos="Max" offset="18"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    </Frame>

                    <Frame type="Label" name="DescriptionLabel">
                        <Anchor side="Top" relative="$parent/PlayerLabel" pos="Max" offset="18"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                        <Height val="70"/>
                        <Text val="{$parent/$parent/BattleMapFrame/@Description}"/>
                    </Frame>

                    <Frame type="Label" name="WebsiteLabel">
                        <Anchor side="Top" relative="$parent/DescriptionLabel" pos="Max" offset="18"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    </Frame>

                    <Frame Type="BattleUserContextMenu" name="UserContextMenu" template="BattleUserContextMenu/BattleUserContextMenu">
                        <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    </Frame>
                </Frame>
                
                <Frame type="BattleMapGameInfoScreenshotTileList" name="ScreenshotTileList">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="18"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Width val="372"/>
                    <Height val="305"/>
                    <MapHandle val="{$ancestor[@type=LobbyListFrame]/@SelectedMapHandle}"/>

                    <Frame type="Image" name="BattleMapInfoBg">
                        <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/Item0" pos="Max" offset="0"/>
                        <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
                        <Anchor side="Right" relative="$parent/Item0" pos="Max" offset="0"/>
                        <LayerColor val="02060b"/>
                        <Alpha val="190"/>
                    </Frame>

                    <!-- This shows if the map has no screenshots -->
                    <Frame type="BattleMapIcon" name="MapIcon">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                        <Height val="279"/>

                        <Frame type="Image" name="Image">
                            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                            <ScaleWidthToTexture val="true"/>
                            <TextureType val="Normal"/>
                            <AutoSizeClamps maxWidth="372"/>
                        </Frame>
                    </Frame>

                    <Frame type="BattleMapScreenshotTileListItem" name="Item0" template="BattleMapInfoPanel/BattleMapScreenshotTileListItemTemplate">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    </Frame>

                    <Frame type="PageSelectorButtonList" name="PageSelector" template="BattleMapInfoPanel/ScreenshotPageSelectorButtonListTemplate">
                        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                        <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="5"/>
                        <Anchor side="Right" relative="$parent/Item0" pos="Max" offset="0"/>

                        <Animation name="HideDisabled">
                            <Event event="OnEnabled" action="Reset,Stop"/>
                            <Event event="OnDisabled" action="Reset,Play"/>

                            <Controller type="Visibility" end="Pause">
                                <Key type="Bool" time="0" value="false"/>
                            </Controller>
                        </Animation>
                    </Frame>
                </Frame>
            </Frame>
            
            <Frame type="Frame" name="TertiaryPanel">
                <Frame type="LobbyPreviewFrame" name="LobbyPreview" template="CustomGamesLobbies/LobbyPreviewTemplate">
                    <AdvertHandleServerLabel val="{$ancestor[@type=LobbyListFrame]/@SelectedAdvertHandleServerLabel}"/>
                    <AdvertHandleServerEpoch val="{$ancestor[@type=LobbyListFrame]/@SelectedAdvertHandleServerEpoch}"/>
                    <AdvertHandleAdvertId val="{$ancestor[@type=LobbyListFrame]/@SelectedAdvertHandleAdvertId}"/>

                    <Frame type="Label" name="LobbyName">
                        <Text val="{$ancestor[@type=LobbyListFrame]/@SelectedLobbyName}"/>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
    </Frame>
</Desc>
