                    <Anchor side="Left" relative="$parent/Team0" pos="Max" offset="#CustomGamesLobbies_LobbyPreview_TeamList_Spacing_Horizontal"/>
                
                    <Frame type="Label" name="TeamID">
                        <Text val="@UI/ScreenCustomGames/Lobbies/Team2"/>
                    </Frame>
                </Frame>

                <Frame type="TileList" name="Team2" template="CustomGamesLobbies/LobbyPreviewTeamTemplate">
