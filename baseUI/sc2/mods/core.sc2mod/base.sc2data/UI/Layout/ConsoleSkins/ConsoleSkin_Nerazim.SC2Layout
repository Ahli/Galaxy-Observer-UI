<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    
    <!-- Console Skin Template -->
    <Frame type="Frame" name="ConsoleSkinTemplate" template="ConsoleSkin_Template/ConsoleSkinTemplate">
        
        <!-- Command Panel -->
        <Frame type="Frame" name="CommandPanelImageContainer">
            <Width val="800"/>
            
            <Frame type="Image" name="CommandPanelBackground">
                <Width val="800"/>
            </Frame>
        </Frame>
        
        <Frame type="Frame" name="EyeEffectLeft">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-30"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-438"/>
            <Width val="400"/>
            <Height val="400"/>
            
            <Frame type="Image" name="Smoke01">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-170"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Width val="18"/>
                <Height val="112"/>
                <LayerCount val="4"/>
                <Rotation val="-20"/>
                
                <Texture val="Assets/Textures/ui_console_flavor_eyeeffectgradient01_nerazim.dds" layer="0"/>
                <AlphaLayer val="True" layer="0"/>
                <Texture val="Assets/Textures/ui_console_flavor_eyeeffectgradient02_nerazim.dds" layer="1"/>
                <TextureCoords top="0" left="0" bottom="0.5" right="1" layer="1"/>
                <AlphaLayer val="True" layer="1"/>
                <WrapUV val="True" layer="1"/>
                <Texture val="Assets/Textures/white.dds" layer="2"/>
                <AlphaLayer val="True" layer="2"/>
                <WrapUV val="True" layer="2"/>
                <Texture val="Assets/Textures/white.dds" layer="3"/>
                <WrapUV val="True" layer="3"/>
                <LayerColor val="0,255,170" layer="3"/>
                
                <Animation name="SquiggleSlide">
                    <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                    <Event event="OnShown" action="Play" frame="$parent"/>
             
                    <Controller type="LayerUV" layer="1" side="Top" relative="False" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="-1" out="linear"/>
                        <Key type="Curve" time="25" value="1" in="linear"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="Smoke02">
