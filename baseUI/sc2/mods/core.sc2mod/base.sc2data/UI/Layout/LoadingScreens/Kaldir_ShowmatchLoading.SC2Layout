<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="Frame" name="Kaldir_CoopLoadingScreen" template="CoopLoadingShared/CoopLoadingTemplate">
        <Anchor relative="$parent"/>        
        
        <Frame type="Image" name="BackgroundImage">
            <Texture val="@UI/SC2_UI_Coop_Loading_Kaldir_Bg"/>
        </Frame>

        <Frame type="Label" name="Map_Title">
            <Text val="@UI/LoadingCoop/Kaldir/MapName"/>
        </Frame>
        
        <Frame type="Frame" name="GameplayItem1" template="CoopLoadingShared/CoopGameplayInfoTemplate">
                
            <Frame type="Image" name="GameplayIcon">
                <Texture val="@UI/SC2_UI_Coop_Loading_Kaldir_Gameplay1"/>
            </Frame>
            
            <Frame type="Label" name="GameplayText">
                <Text val="@UI/LoadingCoop/Kaldir/Gameplay1"/>
            </Frame>
        </Frame>
        
        <Frame type="Frame" name="GameplayItem2" template="CoopLoadingShared/CoopGameplayInfoTemplate">
                
            <Frame type="Image" name="GameplayIcon">
                <Texture val="@UI/SC2_UI_Coop_Loading_Kaldir_Gameplay2"/>
            </Frame>
            
            <Frame type="Label" name="GameplayText">
                <Text val="@UI/LoadingCoop/Kaldir/Gameplay2"/>
            </Frame>
        </Frame>
        
        <Frame type="LoadingCoopCampaignPanel" name="LoadingCoopPlayerPanel" template="LoadingCoopCampaignPanel/LoadingCoopCampaignPanelTemplate">
            <Anchor relative="$parent"/>
            <Frame type="LoadingCoopCampaignPlayerFrame" name="PlayerFrame" template="LoadingCoopCampaignPanel/LoadingCoopCampaignPlayerFrameTemplate">
                <Frame type="Image" name="CommanderImage">
                    <Texture val="Assets\Textures\ui_loading_coop_hero_alarak.dds"/>
                </Frame>
                <Frame type="Label" name="MasteryLevelLabel">
                    <Anchor side="Top" relative="$parent" pos="Max" offset="100"/>
                </Frame>   

                <Frame type="Image" name="CommanderNameplate">
                    <Visible val="False"/>
                </Frame>
                <Frame type="Image" name="CommanderNameplateHex">
                    <Visible val="False"/>
                </Frame>
                <Frame type="Image" name="CommanderNameplateBG">
                    <Visible val="False"/>
                </Frame>
                <Frame type="Image" name="LensFlare1">
                    <Visible val="False"/>
                </Frame>
                <Frame type="Image" name="LensFlare2">
                    <Visible val="False"/>
                </Frame>
            </Frame>
                        
            
            <Frame type="LoadingCoopCampaignPlayerFrame" name="PlayerFrameAlly" template="LoadingCoopCampaignPanel/LoadingCoopCampaignAllyFrameTemplate">
                <Frame type="Image" name="CommanderImage">
                    <Texture val="Assets\Textures\ui_loading_coop_hero_nova.dds"/>
                </Frame>
                <Frame type="Label" name="MasteryLevelLabel">
                    <Anchor side="Top" relative="$parent" pos="Max" offset="100"/>
                </Frame>
                <Frame type="Image" name="CommanderNameplate">
                    <Visible val="False"/>
                </Frame>
                <Frame type="Image" name="CommanderNameplateHex">
                    <Visible val="False"/>
                </Frame>
                <Frame type="Image" name="CommanderNameplateBG">
                    <Visible val="False"/>
                </Frame>
                <Frame type="Image" name="LensFlare1">
                    <Visible val="False"/>
                </Frame>
                <Frame type="Image" name="LensFlare2">
                    <Visible val="False"/>
                </Frame>
            </Frame>                        
        </Frame>
        
    </Frame>
</Desc>