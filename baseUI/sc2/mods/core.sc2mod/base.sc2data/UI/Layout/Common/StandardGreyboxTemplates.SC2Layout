        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <DesaturateOnDisable val="True"/>
        <Texture val="Assets/Textures/btn-ability-protoss-scattercannon.dds"/>
        
        <StateGroup name="IconState">
            <State name="Normal">
                <When type="Property" frame="$parent" toggled="False"/>
                <When type="Property" frame="$parent" enabled="True"/>
                <When type="Property" frame="$parent/NormalImage" visible="True"/>
                <Action type="SetProperty" frame="$this" alpha="200"/>
            </State>
            <State name="Hover">
                <When type="Property" frame="$parent" toggled="False"/>
                <When type="Property" frame="$parent" enabled="True"/>
                <When type="Property" frame="$parent/HoverImage" visible="True"/>
                <Action type="SetProperty" frame="$this" alpha="255"/>
            </State>
            <State name="Toggled">
                <When type="Property" frame="$parent" toggled="True"/>
                <When type="Property" frame="$parent" enabled="True"/>
                <Action type="SetProperty" frame="$this" alpha="255"/>
            </State>
            <State name="Disabled">
                <When type="Property" frame="$parent" enabled="False"/>
                <Action type="SetProperty" frame="$this" alpha="90"/>
            </State>
        </StateGroup>
    </Frame>
    
