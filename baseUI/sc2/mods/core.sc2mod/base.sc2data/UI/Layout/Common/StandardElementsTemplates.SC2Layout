            <TextureType val="NineSlice" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            
            <Texture val="@UI/Glues_Battlenet_PageAssets_StandardElements_ArtButton_Border_Highlight" layer="1"/>
            <LayerColor val="65,131,199" layer="1"/>
            <LayerAlpha val="64" layer="1"/>
            
            <BlendMode val="Add"/>
        
            <StateGroup name="DisplayState">
                <DefaultState val="Normal"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" ToggledState="Toggled"/>
                    <Action type="SetProperty" frame="$this" LayerColor="65,131,199" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="64" index="1"/>
                </State>
            
                <State name="Disabled">
                    <When type="StateGroup" frame="$parent" DisabledState="Disabled"/>
                    <Action type="SetProperty" frame="$this" LayerColor="65,131,199" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="0" index="1"/>
                </State>
            
                <State name="Hovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" LayerColor="65,131,199" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="64" index="1"/>
                </State>
            
                <State name="Normal">
                    <Action type="SetProperty" frame="$this" LayerColor="65,131,199" index="1"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="64" index="1"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- Button Label -->
        <Frame type="Label" name="Label">
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="-16"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="-4"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="4"/>
            <Style val="@StandardElements_ArtButton_Normal"/>
        
            <StateGroup name="SelectedState">
                <DefaultState val="NotSelected"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" ToggledState="Toggled"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardElements_ArtButton_Selected"/>
                </State>

                <State name="Disabled">
                    <When type="StateGroup" frame="$parent" DisabledState="Disabled"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardElements_ArtButton_Disabled"/>
                </State>
            
                <State name="Hovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardElements_ArtButton_Hovered"/>
                </State>

                <State name="NotSelected">
                    <Action type="SetProperty" frame="$this" Style="@StandardElements_ArtButton_Normal"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!--
        
        STANDARD STATEGROUPS
        
        -->
                
        <!-- ToggledState -->
        <StateGroup name="ToggledState">
            <DefaultState val="NotToggled"/>
            <State name="Toggled">
                <When type="Property" frame="$this" Selected="True"/>
            </State>
            <State name="NotToggled">
            </State>
        </StateGroup>
                
        <!-- HoveredState -->
        <StateGroup name="HoveredState">
            <DefaultState val="NotHovered"/>
            <State name="Hovered">
                <When type="Property" frame="$this" Hovering="True"/>
            </State>
            <State name="NotHovered"/>
        </StateGroup>
                
        <!-- PressedState -->
        <StateGroup name="PressedState">
            <DefaultState val="NotPressed"/>
            <State name="Pressed">
                <When type="Property" frame="$this" Pushed="True"/>
            </State>
            <State name="NotPressed">
            </State>
        </StateGroup>
                
        <!-- DisabledState -->
        <StateGroup name="DisabledState">
            <DefaultState val="NotDisabled"/>
            <State name="Disabled">
                <When type="Property" frame="$this" Enabled="False"/>
            </State>
            <State name="NotDisabled"/>
        </StateGroup>
