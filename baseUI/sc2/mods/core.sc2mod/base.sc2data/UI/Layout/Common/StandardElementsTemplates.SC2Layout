                    <Action type="SetProperty" frame="$this" Texture="@UI/Glues_Battlenet_PageAssets_StandardElements_ArtButton_Border_Disabled" index="0"/>
                    <Action type="SetProperty" frame="$this" LayerColor="78,153,199" index="2"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="0" index="2"/>
                </State>
            
                <State name="Hovered">
