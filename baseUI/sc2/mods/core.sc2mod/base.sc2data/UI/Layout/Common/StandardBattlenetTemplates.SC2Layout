        <Frame type="Image" name="Shimmer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Glues_BlueButtons_BackButton_ShimmerMask" layer="0"/>
            <TextureType val="Normal" layer="0"/>
            <AlphaLayer val="true" layer="0"/>
            
            <Texture val="@UI/Glues_BlueButtons_BackButton_Shimmer" layer="1"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
            <BlendMode val="Add"/>
            
            <Animation name="Shimmer">
                <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
         
                <Controller type="LayerUV" layer="1" side="Left" relative="False" sidelock="True" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="fast"/>
                    <Key type="Curve" time="0.7" value="-0.2" in="fast"/>
                    <Key type="Curve" time="1.1" value="1" in="fast"/>
                    <Key type="Curve" time="6.6" value="1" in="fast"/>
                </Controller>

                <Controller type="Fade" end="Loop" layer="1">
                    <Key type="Curve" time="0" value="54" out="Fast"/>
                    <Key type="Curve" time="0.7" value="255" out="Fast"/>
                    <Key type="Curve" time="1.1" value="54" out="Fast"/>
                    <Key type="Curve" time="6.6" value="0" out="Fast"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="BackArrow">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="-1"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="-1"/>
