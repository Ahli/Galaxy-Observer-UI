        <Frame type="Frame" name="ThrottlingIndicatorFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/StreamingIndicatorFrame" pos="Max" offset="0"/>
            <Height val="48"/>
            <Width val="48"/>
          
