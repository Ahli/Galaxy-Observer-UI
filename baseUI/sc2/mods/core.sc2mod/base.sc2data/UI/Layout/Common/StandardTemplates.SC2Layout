                <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
                <Tooltip val="@UI/ThrottlingIndicator"/>
                <TooltipAnchorPosition val="BottomRight"/>
                <AcceptsMouse val="true"/>
            </Frame>
            
            <Animation name="ShowAndFade">
                <Event event="Show" action="DirectionForward,Play"/>
                <Event event="Hide" action="DirectionReverse,Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="1" value="255"/>
                    <Key type="Curve" time="2" value="255"/>
                </Controller>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="0.01" value="True"/>
                    <Key type="Bool" time="2" value="True"/> <!-- Not having this extra line caused the indicataor to blink out briefly while animating in reverse -->
                </Controller>
                <Controller type="Event" end="Pause" frame="$parent">
                    <Key type="Event" time="2" event="ThrottlingShowComplete"/>
                </Controller>
            </Animation>          
        </Frame>
    </Frame>

