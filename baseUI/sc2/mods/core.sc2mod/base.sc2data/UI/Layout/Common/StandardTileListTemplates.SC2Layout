<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <!-- 
    ================================================================
    
    
    STANDARD TILE LIST TEMPLATES
    
    
    ================================================================
    
    These templates are for our new tile list items we created during the Collection UI update for patch 
    3.18 in August of 2017.  They have a particular focus on looking good in 16x9 resolutions when
    spaced properly.  More may be added in the future.
    
    Because there are situations where we use buttons to fake the appearance of a tilelist these are
    primarily built as controls first, and then branched into TileListItem and Button versions from
    there.  The only major difference between these is how the "SelectedState" gets set.  It is set by
    the "Toggled" property on buttons, and the "Selected" property on tilelistitems.
    
    There are multiple sizes and styles of tilelistitem produced for this update.
    -->
    
    <!-- 
    ================================================================
    
    
    CONTROL TEMPLATES
    
    
    ================================================================
    -->
    
    <!-- 
    =============================
    
    BASE CONTROL
    
    =============================
    -->
    <Frame type="Control" name="BaseControlTemplate">
        <Anchor side="Top" relative="$this/AnchorFrame" pos="Min" offset="-8"/>
        <Anchor side="Bottom" relative="$this/AnchorFrame" pos="Max" offset="8"/>
        <Anchor side="Left" relative="$this/AnchorFrame" pos="Min" offset="-8"/>
        <Anchor side="Right" relative="$this/AnchorFrame" pos="Max" offset="8"/>
        
        <!-- ANCHOR FRAME -->
        <Frame type="Frame" name="AnchorFrame">
            <Height val="76"/>
        </Frame>
            
        <!-- BACKGROUND -->
        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_StandardListItem_Background"/>
            <TextureType val="NineSlice"/>
            
            <StateGroup name="ColorState">
                <DefaultState val="Empty"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Color="11,45,83"/>
                    <Action type="SetProperty" frame="$this" Alpha="179"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Color="8,31,56"/>
                    <Action type="SetProperty" frame="$this" Alpha="179"/>
                </State>
                
                <State name="Locked">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" LockedState="Locked"/>
                    <Action type="SetProperty" frame="$this" Color="15,20,25"/>
                    <Action type="SetProperty" frame="$this" Alpha="140"/>
                </State>

                <State name="Normal">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <Action type="SetProperty" frame="$this" Color="6,15,25"/>
                    <Action type="SetProperty" frame="$this" Alpha="179"/>
                </State>

                <State name="Empty">
                    <Action type="SetProperty" frame="$this" Color="15,20,25"/>
                    <Action type="SetProperty" frame="$this" Alpha="140"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- BORDER GLOW -->
        <Frame type="Image" name="BorderGlow">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="-4"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="4"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="-4"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="4"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_StandardListItem_Border_Glow"/>
            <TextureType val="NineSlice"/>
            <Color val="0,102,255"/>
            <Visible val="False"/>
            <RenderPriority val="513"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!-- BORDER INNER -->
        <Frame type="Image" name="BorderInner">
            <Anchor relative="$parent/AnchorFrame"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_StandardListItem_Border_Inner"/>
            <TextureType val="NineSlice"/>
            <RenderPriority val="524"/>
            
            <StateGroup name="ColorState">
                <DefaultState val="Empty"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Color="154,206,255"/>
                    <Action type="SetProperty" frame="$this" Alpha="255"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Color="77,142,204"/>
                    <Action type="SetProperty" frame="$this" Alpha="255"/>
                </State>
                
                <State name="Locked">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" LockedState="Locked"/>
                    <Action type="SetProperty" frame="$this" Color="33,60,88"/>
                    <Action type="SetProperty" frame="$this" Alpha="255"/>
                </State>

                <State name="Normal">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <Action type="SetProperty" frame="$this" Color="23,70,110"/>
                    <Action type="SetProperty" frame="$this" Alpha="255"/>
                </State>

                <State name="Empty">
                    <Action type="SetProperty" frame="$this" Color="33,60,88"/>
                    <Action type="SetProperty" frame="$this" Alpha="255"/>
                </State>
            </StateGroup>
        </Frame>
            
        <!-- EQUIPPED ICON -->
        <Frame type="Image" name="EquippedIcon">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Collection_Equipped_Checkmark"/>
            <RenderPriority val="530"/>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" EquippedState="Equipped"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!--
        ===============
        LABELS
        ===============
        -->

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="17"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-17"/>
            <RenderPriority val="530"/>
            <Options val="NoWrapping|TooltipIfTruncated"/>
            <AcceptsMouseTooltip val="True"/>
            
            <StateGroup name="StyleState">
                <DefaultState val="Normal"/>
                
                <State name="Locked">
                    <When type="StateGroup" frame="$parent" HoveredState="NotHovered"/>
                    <When type="StateGroup" frame="$parent" SelectedState="NotSelected"/>
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" LockedState="Locked"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Locked_Normal"/>
                </State>
            
                <State name="EquippedSelected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <When type="StateGroup" frame="$parent" EquippedState="Equipped"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Selected"/>
                </State>
            
                <State name="PurchasableSelected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <When type="StateGroup" frame="$parent" PurchasableState="Purchasable"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Selected"/>
                </State>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Selected"/>
                </State>
                
                <State name="EquippedHovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <When type="StateGroup" frame="$parent" EquippedState="Equipped"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Hovered"/>
                </State>
            
                <State name="PurchasableHovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <When type="StateGroup" frame="$parent" PurchasableState="Purchasable"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Hovered"/>
                </State>
                
                <State name="Hovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Hovered"/>
                </State>

                <State name="EquippedNormal">
                    <When type="StateGroup" frame="$parent" EquippedState="Equipped"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Normal"/>
                </State>
            
                <State name="PurchasableNormal">
                    <When type="StateGroup" frame="$parent" PurchasableState="Purchasable"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Normal"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Large_Standard_Normal"/>
                </State>
            </StateGroup>
        </Frame>
        
        <!--
        ===============
        STATEGROUPS
        ===============
        -->
                
        <!-- EmptyState -->
        <StateGroup name="EmptyState">
            <DefaultState val="NotEmpty"/>
            <State name="Empty">
                <When type="Property" frame="$this" HasData="False"/>
            </State>
            <State name="NotEmpty"/>
        </StateGroup>
                
        <!-- SelectedState -->
        <StateGroup name="SelectedState">
            <DefaultState val="NotSelected"/>
            <State name="Selected">
                <Action type="SetProperty" frame="$this" RenderPriority="515"/>
            </State>
            <State name="NotSelected">
                <Action type="SetProperty" frame="$this" RenderPriority="512"/>
            </State>
        </StateGroup>
                
        <!-- HoveredState -->
        <StateGroup name="HoveredState">
            <DefaultState val="NotHovered"/>
            <State name="Hovered">
                <When type="Property" frame="$this" Hovering="True"/>
            </State>
            <State name="NotHovered"/>
        </StateGroup>
                
        <!-- LockedState -->
        <StateGroup name="LockedState">
            <DefaultState val="NotLocked"/>
            <State name="Locked">
            </State>
            <State name="NotLocked"/>
        </StateGroup>
                
        <!-- EquippedState -->
        <StateGroup name="EquippedState">
            <DefaultState val="NotEquipped"/>
            <State name="Equipped">
            </State>
            <State name="NotEquipped">
            </State>
        </StateGroup>
                
        <!-- PurchasableState -->
        <StateGroup name="PurchasableState">
            <DefaultState val="NotPurchasable"/>
            <State name="Purchasable">
            </State>
            <State name="NotPurchasable">
            </State>
        </StateGroup>
        
        <!-- ExternalState -->
        <StateGroup name="ExternalState">
            <DefaultState val="NotExternal"/>
            <State name="External"/>
            <State name="NotExternal"/>
        </StateGroup>
    </Frame>
    
    <!-- 
    =============================
    
    SMALL CONTROL
    
    =============================
    -->
    <Frame type="Control" name="SmallControlTemplate" template="StandardTileListTemplates/BaseControlTemplate">
        
        <Frame type="Frame" name="AnchorFrame">
            <Height val="38"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            
            <StateGroup name="StyleState">
                
                <State name="Locked">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Locked_Normal"/>
                </State>
            
                <State name="EquippedSelected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Selected"/>
                </State>
            
                <State name="PurchasableSelected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Selected"/>
                </State>
            
                <State name="Selected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Selected"/>
                </State>
                
                <State name="EquippedHovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Hovered"/>
                </State>
            
                <State name="PurchasableHovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Hovered"/>
                </State>
                
                <State name="Hovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Hovered"/>
                </State>

                <State name="EquippedNormal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Normal"/>
                </State>
            
                <State name="PurchasableNormal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Normal"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Small_Standard_Normal"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    <!-- 
    =============================
    
    MEDIUM CONTROL
    
    =============================
    -->
    <Frame type="Control" name="MediumControlTemplate" template="StandardTileListTemplates/BaseControlTemplate">
        
        <Frame type="Frame" name="AnchorFrame">
            <Height val="38"/>
        </Frame>
        
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Mid" offset="1"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Mid" offset="1"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="16"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-16"/>
            
            <StateGroup name="StyleState">
                
                <State name="Locked">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Locked_Normal"/>
                </State>
            
                <State name="EquippedSelected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Selected"/>
                </State>
            
                <State name="PurchasableSelected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Selected"/>
                </State>
            
                <State name="Selected">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Selected"/>
                </State>
                
                <State name="EquippedHovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Hovered"/>
                </State>
            
                <State name="PurchasableHovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Hovered"/>
                </State>
                
                <State name="Hovered">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Hovered"/>
                </State>

                <State name="EquippedNormal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Normal"/>
                </State>
            
                <State name="PurchasableNormal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Normal"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemTitle_Medium_Standard_Normal"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    <!-- 
    =============================
    
    LARGE CONTROL
    
    =============================
    -->
    <Frame type="Control" name="LargeControlTemplate" template="StandardTileListTemplates/BaseControlTemplate">
        
        <Frame type="Frame" name="AnchorFrame">
            <Height val="68"/>
        </Frame>
        
        <Frame type="Image" name="Icon">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="2"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="-2"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="2"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Min" offset="78"/>
            <Alpha val="255"/>
            <Visible val="False"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Glues_Battlenet_PageAssets_StandardListItem_IconMask" layer="0"/>
            <TextureType val="NineSlice" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            
            <Texture val="Assets/Textures/White.dds" layer="1"/>
            
            <StateGroup name="AlphaState">
                <DefaultState val="Empty"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="1"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="1"/>
                </State>
                
                <State name="Locked">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" LockedState="Locked"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="128" index="1"/>
                </State>

                <State name="Normal">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <Action type="SetProperty" frame="$this" LayerAlpha="255" index="1"/>
                </State>

                <State name="Empty">
                    <Action type="SetProperty" frame="$this" LayerAlpha="0" index="1"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Image" name="IconDarken">
            <Anchor relative="$parent/Icon"/>
            <Color val="0,0,0"/>
            <Alpha val="0"/>
            
            <StateGroup name="AlphaState">
                <DefaultState val="Empty"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Alpha="0"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Alpha="0"/>
                </State>
                
                <State name="Locked">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" LockedState="Locked"/>
                    <Action type="SetProperty" frame="$this" Alpha="77"/>
                </State>

                <State name="Normal">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <Action type="SetProperty" frame="$this" Alpha="0"/>
                </State>

                <State name="Empty">
                    <Action type="SetProperty" frame="$this" Alpha="0"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Label" name="TitleLabel">
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Mid" offset="2"/>
            <Anchor side="Left" relative="$parent/Icon" pos="Max" offset="15"/>
            <Anchor side="Right" relative="$parent//LockedIconContainer/LockIcon" pos="Min" offset="-15"/>
        </Frame>

        <Frame type="Label" name="SubtitleLabel">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Mid" offset="-2"/>
            <Anchor side="Left" relative="$parent/Icon" pos="Max" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <RenderPriority val="530"/>
            <Options val="Ellipsis | NoWrapping | TooltipIfTruncated"/>
            <AcceptsMouseTooltip val="True"/>
            <Style val="@StandardListItemSubtitle_Medium_Standard_Normal"/>
            
            <StateGroup name="StyleState">
                <DefaultState val="Normal"/>
            
                <State name="EquippedSelected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <When type="StateGroup" frame="$parent" EquippedState="Equipped"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Equipped_Selected"/>
                </State>
            
                <State name="PurchasableSelected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <When type="StateGroup" frame="$parent" PurchasableState="Purchasable"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Purchasable_Selected"/>
                </State>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Standard_Selected"/>
                </State>
                
                <State name="EquippedHovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <When type="StateGroup" frame="$parent" EquippedState="Equipped"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Equipped_Hovered"/>
                </State>
            
                <State name="PurchasableHovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <When type="StateGroup" frame="$parent" PurchasableState="Purchasable"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Purchasable_Hovered"/>
                </State>
                
                <State name="Hovered">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Standard_Hovered"/>
                </State>

                <State name="EquippedNormal">
                    <When type="StateGroup" frame="$parent" EquippedState="Equipped"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Equipped_Normal"/>
                </State>
            
                <State name="PurchasableNormal">
                    <When type="StateGroup" frame="$parent" PurchasableState="Purchasable"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Purchasable_Normal"/>
                </State>
                
                <State name="Locked">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" LockedState="Locked"/>
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Locked_Normal"/>
                </State>

                <State name="Normal">
                    <Action type="SetProperty" frame="$this" Style="@StandardListItemSubtitle_Medium_Standard_Normal"/>
                </State>
            </StateGroup>
        </Frame>
        
        <Frame type="Frame" name="LockedIconContainer">
            <Anchor relative="$parent/AnchorFrame"/>
            <RenderPriority val="519"/>
            <Visible val="False"/>
            
            <Frame type="Image" name="LockIcon">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-14"/>
                <Texture val="@UI/Glues_Battlenet_PageAssets_StandardListItem_Icon_Lock"/>
                <Alpha val="255"/>
                <CollapseLayout val="True"/>
            </Frame>
            
            <StateGroup name="VisibleState">
                <DefaultState val="NotVisible"/>
            
                <State name="Visible">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" LockedState="Locked"/>
                    <Action type="SetProperty" frame="$this" Visible="True"/>
                </State>
                
                <State name="NotVisible">
                    <Action type="SetProperty" frame="$this" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
    
    <!-- 
    =============================
    
    GRAPHIC CONTROL
    
    =============================
    -->
    <Frame type="Control" name="GraphicControlTemplate" template="StandardTileListTemplates/LargeControlTemplate">
        <Anchor side="Top" relative="$this/AnchorFrame" pos="Min" offset="-18"/>
        <Anchor side="Bottom" relative="$this/AnchorFrame" pos="Max" offset="18"/>
        <Anchor side="Left" relative="$this/AnchorFrame" pos="Min" offset="-18"/>
        <Anchor side="Right" relative="$this/AnchorFrame" pos="Max" offset="18"/>
        
        <Frame type="Frame" name="AnchorFrame">
            <Width val="200"/>
            <Height val="200"/>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor relative="$parent/AnchorFrame"/>
        </Frame>
            
        <!--
        ================
        Images
        ================
        -->
        <Frame type="Image" name="Background">
            <Texture val="Assets/Textures/White.dds"/>
            <TextureType val="NineSlice"/>
            <Color val="255,255,255"/>
            <Alpha val="178"/>
            <RenderPriority val="510"/>
        </Frame>
            
        <!-- BACKGROUND -->
        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent/AnchorFrame" pos="Min" offset="4"/>
            <Anchor side="Bottom" relative="$parent/AnchorFrame" pos="Max" offset="-4"/>
            <Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="4"/>
            <Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="-4"/>
            <Texture val="Assets/Textures/White.dds"/>
            <TextureType val="Normal"/>
            
            <StateGroup name="ColorState">
                <DefaultState val="Empty"/>
            
                <State name="Selected">
                    <When type="StateGroup" frame="$parent" SelectedState="Selected"/>
                    <Action type="SetProperty" frame="$this" Color="16,40,66"/>
                    <Action type="SetProperty" frame="$this" Alpha="179"/>
                </State>
                
                <State name="Hover">
                    <When type="StateGroup" frame="$parent" HoveredState="Hovered"/>
                    <Action type="SetProperty" frame="$this" Color="6,15,25"/>
                    <Action type="SetProperty" frame="$this" Alpha="179"/>
                </State>
                
                <State name="Locked">
                    <When type="StateGroup" frame="$parent" EmptyState="NotEmpty"/>
                    <When type="StateGroup" frame="$parent" LockedState="Locked"/>
                    <Action type="SetProperty" frame="$this" Color="12,18,25"/>
                    <Action type="SetProperty" frame="$this" Alpha="179"/>
                </State>

                <State name="Normal">
