    <Frame type="Image" name="StandardTileListScrollBarBackgroundTemplate">
        <Anchor side="Top" relative="$parent/ScrollBar" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent/ScrollBar" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent/ScrollBar" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent/ScrollBar" pos="Max" offset="0"/>
        <Texture val="@UI/Glues_Battlenet_PageAssets_StandardTileList_ScrollBar_Background"/>
        <TextureType val="NineSlice"/>
        <Alpha val="128"/>
            
        <StateGroup name="AlphaState">
            <DefaultState val="Visible"/>
        
            <State name="NotVisible">
                <When type="Property" frame="$parent/ScrollBar" Visible="True"/>
                <Action type="SetProperty" frame="$this" Visible="False"/>
            </State>
        
            <State name="Visible">
                <Action type="SetProperty" frame="$this" Visible="True"/>
            </State>
        </StateGroup>
    </Frame>
    
