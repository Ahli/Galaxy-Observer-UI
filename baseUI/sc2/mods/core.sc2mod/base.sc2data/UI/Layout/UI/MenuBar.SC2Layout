    <Frame type="Button" name="MenuBarMenuButtonTemplate">
        <Width val="70"/>
        <Height val="36"/>
        <DisplayType val="ImageLayer"/>
        <ClickSound val="@UI_ActionButtonSelect"/>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <LayerCount val="4"/>
            <Texture val="@@@UI/Game_Console_Buttons_MenuBar_Normal" layer="0"/>
            <Texture val="@@@UI/Game_Console_Buttons_MenuBar_Pressed" layer="1"/>
            <Texture val="@@@UI/Game_Console_Buttons_MenuBar_Selected" layer="2"/>
            <Texture val="@@@UI/Game_Console_Buttons_MenuBar_Disabled" layer="3"/>
            <TextureType val="NineSlice" layer="0"/>
            <TextureType val="NineSlice" layer="1"/>
            <TextureType val="NineSlice" layer="2"/>
            <TextureType val="NineSlice" layer="3"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <LayerCount val="4"/>
            <Texture val="@@@UI/Game_Console_Buttons_MenuBar_Hover" layer="0"/>
            <Texture val="@@@UI/Game_Console_Buttons_MenuBar_Pressed" layer="1"/>
            <Texture val="@@@UI/Game_Console_Buttons_MenuBar_Selected" layer="2"/>
            <Texture val="@@@UI/Game_Console_Buttons_MenuBar_Disabled" layer="3"/>
            <TextureType val="NineSlice" layer="0"/>
            <TextureType val="NineSlice" layer="1"/>
            <TextureType val="NineSlice" layer="2"/>
            <TextureType val="NineSlice" layer="3"/>
        </Frame>

        <Frame type="Label" name="MenuLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="5"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="2"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Style val="@@@MenuBarButton"/>
            <Text val="@UI/Menu"/>
        </Frame>
    </Frame>

    <Frame type="MenuBar" name="MenuBarTemplate">
        <Frame type="SystemInfoTooltip" name="SystemInfoTooltip" template="SystemInfoPanel/SystemInfoTooltipTemplate"/>

        <Frame type="Image" name="MenuBarBackgroundImage">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@@@UI/Game_Console_MenuBar_Background"/>
        </Frame>

        <Frame type="Button" name="HelpButton" template="MenuBar/MenuBarHelpButtonTemplate">
            <Anchor side="Left" relative="$parent/MenuBarBackgroundImage" pos="Min" offset="9"/>
            <Anchor side="Top" relative="$parent/MenuBarBackgroundImage" pos="Min" offset="9"/>
        </Frame>

        <Frame type="Button" name="ChatButton" template="MenuBar/MenuBarChatButtonTemplate">
            <Anchor side="Left" relative="$parent/MenuBarBackgroundImage" pos="Min" offset="59"/>
            <Anchor side="Top" relative="$parent/MenuBarBackgroundImage" pos="Min" offset="9"/>
        </Frame>

        <Frame type="SocialButton" name="SocialButton" template="MenuBar/MenuBarSocialButtonTemplate">
            <Anchor side="Left" relative="$parent/MenuBarBackgroundImage" pos="Min" offset="110"/>
            <Anchor side="Top" relative="$parent/MenuBarBackgroundImage" pos="Min" offset="-6"/>
        </Frame>

        <Frame type="Button" name="MenuButton" template="MenuBar/MenuBarMenuButtonTemplate">
            <Anchor side="Right" relative="$parent/MenuBarBackgroundImage" pos="Max" offset="-10"/>
            <Anchor side="Top" relative="$parent/MenuBarBackgroundImage" pos="Min" offset="9"/>
            <TooltipFrame val="SystemInfoTooltip"/>
        </Frame>

        <StateGroup name="TradeShowStateGroup">
            <DefaultState val="NotTradeShow"/>

            <State name="TradeShow">
                <When type="Property" frame="$GameUI" TradeShow="true"/>
                <Action type="SetProperty" frame="$this/ChatButton" Enabled="false"/>
                <Action type="SetProperty" frame="$this/SocialButton" Enabled="false"/>
                <Action type="SetProperty" frame="$this/ChatButton" UseTooltip="false"/>
                <Action type="SetProperty" frame="$this/SocialButton" UseTooltip="false"/>
            </State>

            <State name="NotTradeShow">
                <Action type="SetProperty" frame="$this/ChatButton" Enabled="true"/>
                <Action type="SetProperty" frame="$this/SocialButton" Enabled="true"/>
                <Action type="SetProperty" frame="$this/ChatButton" UseTooltip="true"/>
                <Action type="SetProperty" frame="$this/SocialButton" UseTooltip="true"/>
            </State>
        </StateGroup>
    </Frame>
</Desc>
