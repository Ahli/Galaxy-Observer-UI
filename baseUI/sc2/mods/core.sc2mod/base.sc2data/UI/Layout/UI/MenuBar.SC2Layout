            <Width val="28"/>
            <Height val="25"/>
            <Texture val="@UI/Game_Console_Buttons_MenuBar_Counter"/>
            <TextureType val="NineSlice"/>
            <Color val="###StandardButtonCounter"/>
        </Frame>

        <Frame type="Label" name="CountLabel">
            <Anchor side="Top" relative="$parent/CountBackground" pos="Mid" offset="1"/>
            <Anchor side="Bottom" relative="$parent/CountBackground" pos="Mid" offset="1"/>
            <Anchor side="Left" relative="$parent/CountBackground" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/CountBackground" pos="Mid" offset="0"/>
            <Width val="25"/>
            <RenderPriority val="520"/>
            <Style val="IdleButtonCharge"/>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="36"/>
            <Height val="36"/>
        </Frame>
