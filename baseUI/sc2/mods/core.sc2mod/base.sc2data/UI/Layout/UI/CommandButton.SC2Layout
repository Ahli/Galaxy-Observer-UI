        </Frame>

        <Frame type="Label" name="SupplyLabel" template="CommandButton/ResourceLabelTemplate">
            <Anchor side="Top" relative="$parent/SupplyIcon" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/SupplyIcon" pos="Mid" offset="0"/>
