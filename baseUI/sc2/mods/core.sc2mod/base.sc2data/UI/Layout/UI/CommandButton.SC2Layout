            <Anchor side="Bottom" relative="$parent/InventoryLabel" pos="Max" offset="15"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/HotkeyLabel" pos="Max" offset="15"/>
