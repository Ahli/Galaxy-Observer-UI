                <Frame type="Frame" name="FriendsPanel">
                    <Anchor relative="$parent"/>
                    <Visible val="false"/>

                    <Animation name="ToggleSocialPanelVisibility">
                        <Event event="OnClick" action="DirectionForward,Play" frame="Socialpanel/CloseButton"/>
                        <Event event="Toggle" action="ToggleDirection,Play" frame="$parent/MenuBar/SocialButton"/>
                        <Event event="OnHidden" action="DirectionForward,Play" frame="$parent/MenuBar/SocialButton"/>
                        <Event event="Hide" action="DirectionForward,Play"/>

                        <Controller type="Visibility" end="Pause">
                            <Key type="Bool" time="0" value="true"/>
                            <Key type="Bool" time=".01" value="false"/>
                        </Controller>
                    </Animation>

                    <StateGroup name="AnchorStateGroup">
                        <State name="AnchorTopLeftState">
                            <When type="AnimationState" frame="$parent/MenuBar/SocialButton" PositionState="TopLeft"/>
                            <When type="Property" frame="$this" Visible="true"/>
                            <Action type="SendEvent" frame="$this" event="TopLeft"/>
                        </State>
                        
                        <State name="AnchorTopRightState">
                            <When type="AnimationState" frame="$parent/MenuBar/SocialButton" PositionState="TopRight"/>
                            <When type="Property" frame="$this" Visible="true"/>
                            <Action type="SendEvent" frame="$this" event="TopRight"/>
                        </State>

                        <State name="AnchorBottomLeftState">
                            <When type="AnimationState" frame="$parent/MenuBar/SocialButton" PositionState="BottomLeft"/>
                            <When type="Property" frame="$this" Visible="true"/>
                            <Action type="SendEvent" frame="$this" event="BottomLeft"/>
                        </State>

                        <State name="AnchorBottomRightState">
                            <When type="AnimationState" frame="$parent/MenuBar/SocialButton" PositionState="BottomRight"/>
                            <When type="Property" frame="$this" Visible="true"/>
                            <Action type="SendEvent" frame="$this" event="BottomRight"/>
                        </State>
                    </StateGroup>

                    <Frame type="PartyInvitePanel" name="PartyInvitePanel" template="StandardBattlenetTemplates/PartyInvitePanel">
                        <Anchor side="Bottom" relative="$parent/SocialPanel" pos="Min" offset="-5"/>
                        <Anchor side="Right" relative="$parent/SocialPanel" pos="Max" offset="0"/>
                        <Width val="#SocialPanelWidth"/>

                        <Animation name="SlideIn">
                            <Controller name="Move" type="Anchor" side="Left" relative="true" end="Pause">
                                <Key type="Curve" time="0" value="0" out="Fast"/>
                                <Key type="Curve" time="0.3" value="0" in="Fast"/>
                            </Controller>
                        </Animation>

                        <!-- Animations to reanchor the panel when the social button position changes -->
                        <Animation name="AnchorTop">
                            <Event event="TopLeft" action="Reset,Play" frame="$parent"/>
                            <Event event="TopRight" action="Reset,Play" frame="$parent"/>

                            <Controller type="Anchor" side="Top" relative="True" relativeFrame="$parent/SocialPanel" pos="Max" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Bottom" relative="True" relativeFrame="$parent/SocialPanel" pos="Max" end="Pause">
                                <Key type="Curve" time="0.0" value="#PartyInvitePanelHeight" out="Auto"/>
                            </Controller>
                        </Animation>

                        <Animation name="AnchorBottom">
                            <Event event="BottomLeft" action="Reset,Play" frame="$parent"/>
                            <Event event="BottomRight" action="Reset,Stop" frame="$parent"/>

                            <Controller type="Anchor" side="Top" relative="True" relativeFrame="$parent/SocialPanel" pos="Min" end="Pause">
                                <Key type="Curve" time="0.0" value="#PartyInvitePanelHeightNegative" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Bottom" relative="True" relativeFrame="$parent/SocialPanel" pos="Min" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>
                        </Animation>
                    </Frame>

                    <Frame type="SocialPanel" name="SocialPanel" template="GameSocialPanel/SocialPanelTemplate">
                        <Anchor side="Bottom" relative="$parent/$parent/MenuBar" pos="Min" offset="7"/>
                        <Anchor side="Right" relative="$parent/$parent/MenuBar" pos="Max" offset="0"/>
                        <Height val="#SocialPanelHeight"/>
                        <Width val="#SocialPanelWidth"/>

                        <!-- Animations to reanchor the panel when the social button position changes -->
                        <Animation name="AnchorTopLeft">
                            <Event event="TopLeft" action="Reset,Play" frame="$parent"/>

                            <Controller type="Anchor" side="Top" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Max" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Bottom" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Max" end="Pause">
                                <Key type="Curve" time="0.0" value="#SocialPanelHeight" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Left" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Min" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Right" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Min" end="Pause">
                                <Key type="Curve" time="0.0" value="#SocialPanelWidth" out="Auto"/>
                            </Controller>
                        </Animation>

                        <Animation name="AnchorTopRight">
                            <Event event="TopRight" action="Reset,Play" frame="$parent"/>

                            <Controller type="Anchor" side="Top" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Max" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Bottom" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Max" end="Pause">
                                <Key type="Curve" time="0.0" value="#SocialPanelHeight" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Left" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Max" end="Pause">
                                <Key type="Curve" time="0.0" value="#SocialPanelWidthNegative" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Right" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Max" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>
                        </Animation>

                        <Animation name="AnchorBottomLeft">
                            <Event event="BottomLeft" action="Reset,Play" frame="$parent"/>

                            <Controller type="Anchor" side="Top" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Min" end="Pause">
                                <Key type="Curve" time="0.0" value="#SocialPanelHeightNegative" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Bottom" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Min" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Left" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Min" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Right" relative="True" relativeFrame="$parent/$parent/MenuBar/SocialButton" pos="Min" end="Pause">
                                <Key type="Curve" time="0.0" value="#SocialPanelWidth" out="Auto"/>
                            </Controller>
                        </Animation>

                        <Animation name="AnchorBottomRight">
                            <Event event="BottomRight" action="Reset,Stop" frame="$parent"/>

                            <Controller type="Anchor" side="Top" relative="True" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Bottom" relative="True" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Left" relative="True" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>

                            <Controller type="Anchor" side="Right" relative="True" end="Pause">
                                <Key type="Curve" time="0.0" value="0" out="Auto"/>
                            </Controller>
                        </Animation>
                    </Frame>
                </Frame>

