        ==================================================
        NEW ITEM STATE GROUP
        ==================================================
        -->
        
        <StateGroup name="NewItemStateGroup">
        
            <State name="NewItem">
                <When type="AnimationState" frame="$this" NewItemState="NewItem"/>
