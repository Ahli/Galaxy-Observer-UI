        <StateGroup name="ItemStatusStateGroup">
        
            <State name="AssignedOwned">
                <When type="AnimationState" frame="$this" AssignedState="Assigned"/>
                <When type="AnimationState" frame="$this" OwnedState="Owned"/>
                <When type="AnimationState" frame="$this" EmptyState="NotEmpty"/>
                <Action type="SetProperty" frame="$this" enabled="True"/>
                <Action type="SetProperty" frame="NormalImage" Alpha="0"/>
                <Action type="SetProperty" frame="HoverImage" Alpha="0"/>
                <Action type="SetProperty" frame="Icon" Alpha="255"/>
                <Action type="SetProperty" frame="ToggleImage" Visible="True"/>
                <Action type="SetProperty" frame="DisabledImage" Visible="False"/>
                <Action type="SetProperty" frame="LockImage" Visible="False"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="None"/>
            </State>
            
            <State name="UnassignedOwned">
                <When type="AnimationState" frame="$this" AssignedState="Unassigned"/>
                <When type="AnimationState" frame="$this" OwnedState="Owned"/>
                <When type="AnimationState" frame="$this" EmptyState="NotEmpty"/>
                <Action type="SetProperty" frame="$this" enabled="True"/>
                <Action type="SetProperty" frame="NormalImage" Alpha="255"/>
                <Action type="SetProperty" frame="HoverImage" Alpha="255"/>
                <Action type="SetProperty" frame="Icon" Alpha="255"/>
                <Action type="SetProperty" frame="ToggleImage" Visible="False"/>
                <Action type="SetProperty" frame="ToggleImageHighlightAnim" Visible="False"/>
                <Action type="SetProperty" frame="DisabledImage" Visible="False"/>
                <Action type="SetProperty" frame="LockImage" Visible="False"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
            </State>
            
            <State name="UnassignedUnowned">
                <When type="AnimationState" frame="$this" AssignedState="Unassigned"/>
                <When type="AnimationState" frame="$this" OwnedState="Unowned"/>
                <When type="AnimationState" frame="$this" EmptyState="NotEmpty"/>
                <Action type="SetProperty" frame="$this" enabled="False"/>
                <Action type="SetProperty" frame="NormalImage" Alpha="255"/>
                <Action type="SetProperty" frame="HoverImage" Alpha="255"/>
                <Action type="SetProperty" frame="Icon" Alpha="100"/>
                <Action type="SetProperty" frame="ToggleImage" Visible="False"/>
                <Action type="SetProperty" frame="ToggleImageHighlightAnim" Visible="False"/>
                <Action type="SetProperty" frame="DisabledImage" Visible="False"/>
                <Action type="SetProperty" frame="LockImage" Visible="True"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
            </State>
            
            <State name="Empty">
                <When type="AnimationState" frame="$this" EmptyState="Empty"/>
                <Action type="SetProperty" frame="$this" enabled="False"/>
                <Action type="SetProperty" frame="NormalImage" Alpha="0"/>
                <Action type="SetProperty" frame="HoverImage" Alpha="0"/>
                <Action type="SetProperty" frame="Icon" Alpha="0"/>
                <Action type="SetProperty" frame="ToggleImage" Visible="False"/>
                <Action type="SetProperty" frame="ToggleImageHighlightAnim" Visible="False"/>
                <Action type="SetProperty" frame="DisabledImage" Visible="True"/>
                <Action type="SetProperty" frame="LockImage" Visible="False"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
            </State>
        </StateGroup>
        
