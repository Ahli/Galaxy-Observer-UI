        <Width val="24"/>
        <Height val="24"/>
        <Visible val="False"/>
        <Alpha val="0"/>
        
        <!-- Phase 3 - Move Viewfinder Offscreen -->
        <Animation name="Show">
            <Event event="DisplayDot" action="Reset,Play"/>
            <Event event="MissionBriefing2_Phase1" action="Reset" frame="$MissionPreviewPanel"/>
            
             <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
            
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time=".3" value="255" in="Slow"/>
            </Controller>
            
            <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause">
                <Key type="Curve" time="0" value="50"/>
                <Key type="Curve" time=".5" value="130"/>
                <Key type="Curve" time=".7" value="100"/>
            </Controller>
            
            <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause">
                <Key type="Curve" time="0" value="50"/>
                <Key type="Curve" time=".5" value="130"/>
                <Key type="Curve" time=".7" value="100"/>
            </Controller>
            
            <Controller type="Event" end="Stop">
                <Key type="Event" time="2" event="Pulse"/>
            </Controller>
        </Animation>
        
        <Animation name="Pulse">
            <Event event="Pulse" action="Reset,Play"/>
            <Event event="DisplayDot" action="Reset,Stop"/>
            
            <Controller type="Fade" end="Loop">
                <Key type="Curve" time="0" value="255" out="Fast"/>
                <Key type="Curve" time="2" value="255" out="Fast"/>
                <Key type="Curve" time="2.6" value="140" in="Slow"/>
                <Key type="Curve" time="3.2" value="255" in="Slow"/>
            </Controller>
        </Animation>
    </Frame>
    
