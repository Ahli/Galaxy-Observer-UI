                <Event event="ResetBriefing" action="Reset,Stop" frame="$MissionPreviewPanel"/>
                
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="170" out="Fast"/>
                    <Key type="Curve" time="4" value="170" in="Slow"/>
                    <Key type="Curve" time="5" value="60" in="Slow"/>
                    <Key type="Curve" time="8" value="60" in="Slow"/>
                    <Key type="Curve" time="9" value="170" in="Slow"/>
                    <Key type="Curve" time="13" value="170" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="DropPointCircle2">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="170"/>
            <Height val="170"/>
            <Alpha val="0"/>
