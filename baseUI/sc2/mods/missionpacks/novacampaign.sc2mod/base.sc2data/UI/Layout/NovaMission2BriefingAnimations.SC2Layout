        <Height val="28"/>
    </Frame>
    
    <Frame type="Image" name="DominionBlipTemplate">
        <Texture val="Assets/Textures/ui_nova_storymode_briefing_02_map_dominionblips.dds"/>
        <Width val="24"/>
        <Height val="24"/>
        
        <Animation name="Show">
            <Event event="OnShown" action="Reset,Play"/>
            <Event event="MissionBriefing2_Phase1" action="Play" frame="$MissionPreviewPanel"/>
            <Event event="EndBriefingEarly" action="Play" frame="$MissionPreviewPanel"/>
            
            <Controller type="Fade" end="Loop">
                <Key type="Curve" time="0" value="255" out="Fast"/>
                <Key type="Curve" time="4" value="255" out="Fast"/>
                <Key type="Curve" time="4.5" value="100" in="Slow"/>
                <Key type="Curve" time="5" value="255" out="Fast"/>
                <Key type="Curve" time="7" value="255" out="Fast"/>
            </Controller>
        </Animation>
    </Frame>
    
