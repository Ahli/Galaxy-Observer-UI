<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    
    <!-- 
    
    
    =============================
    
    MISSION 2 BRIEFING ANIMATIONS
    
    =============================
    
    
    -->
    
    <!-- Phase 1 - Go to briefing start location -->
    <Animation name="MissionBriefing2_Phase1">
        <Event event="MissionBriefing2_Phase1" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="$MissionPreviewPanel">
            <Key type="Event" time="0" event="ResetBriefing"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="0" out="Fast"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="0" out="Fast"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="-300" out="Fast"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="-300" out="Fast"/>
        </Controller>
    </Animation>
    
    <!-- Phase 2 - Pan to Zerg base from Dominion Base -->
    <Animation name="MissionBriefing2_Phase2">
        <Event event="MissionBriefing2_Phase2" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="0" out="Fast"/>
            <Key type="Curve" time="0.3" value="-250" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="0" out="Fast"/>
            <Key type="Curve" time="0.3" value="-250" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="-300" out="Fast"/>
            <Key type="Curve" time="0.3" value="-300" out="Fast"/>
            <Key type="Curve" time="0.6" value="275" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="-300" out="Fast"/>
            <Key type="Curve" time="0.3" value="-300" out="Fast"/>
            <Key type="Curve" time="0.6" value="275" in="Slow"/>
        </Controller>
        
        <Controller type="Sound" end="Stop">
            <Key type="Sound" time="0" sound="@UI_Nova_BriefingMapMoveHorizontal"/>
            <Key type="Sound" time="0.3" sound="@UI_Nova_BriefingMapMoveVertical"/>
        </Controller>
    </Animation>
    
    <!-- Phase 3 - Play Zerg arrival animation -->
    <Animation name="MissionBriefing2_Phase3">
        <Event event="MissionBriefing2_Phase3" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="ZergContainer">
            <Key type="Event" time="0" event="ZergArrival"/>
        </Controller>
        
        <Controller type="Sound" end="Stop">
            <Key type="Sound" time="0" sound="@UI_Nova_Briefing02ZergStart"/>
            <Key type="Sound" time="0.5" sound="@UI_Nova_Briefing02ZergStart2"/>
        </Controller>
    </Animation>
    
    <!-- Phase 4 - Slide viewfinder on screen -->
    <Animation name="MissionBriefing2_Phase4">
        <Event event="MissionBriefing2_Phase4" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="$MissionBriefing_DiscoverBoxContainer">
            <Key type="Event" time="0" event="FadeIn"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="800" out="Fast"/>
            <Key type="Curve" time="0.3" value="200" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="800" out="Fast"/>
            <Key type="Curve" time="0.3" value="200" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="500" out="Fast"/>
            <Key type="Curve" time="0.3" value="500" out="Fast"/>
            <Key type="Curve" time="0.6" value="-100" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="500" out="Fast"/>
            <Key type="Curve" time="0.3" value="500" out="Fast"/>
            <Key type="Curve" time="0.6" value="-100" in="Slow"/>
        </Controller>
        
        <Controller type="Sound" end="Stop">
            <Key type="Sound" time="0" sound="@UI_Nova_BriefingReticleMoveHorizontal"/>
            <Key type="Sound" time="0.3" sound="@UI_Nova_BriefingReticleMoveVertical"/>
            <Key type="Sound" time="0.5" sound="@UI_Nova_BriefingReticleMoveVerticalEnd"/>
        </Controller>
    </Animation>
    
    <!-- Phase 5 - Expand Viewfinder -->
    <Animation name="MissionBriefing2_Phase5">
        <Event event="MissionBriefing2_Phase5" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="$MissionBriefing_DiscoverBox">
            <Key type="Event" time="0" event="Expand"/>
        </Controller>
    </Animation>
    
    <!-- Phase 6 - Collapse Viewfinder -->
    <Animation name="MissionBriefing2_Phase6">
        <Event event="MissionBriefing2_Phase6" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="$MissionBriefing_DiscoverBox">
            <Key type="Event" time="0" event="Collapse"/>
        </Controller>
    </Animation>
    
    <!-- Phase 7 - Move Viewfinder Offscreen -->
    <Animation name="MissionBriefing2_Phase7">
        <Event event="MissionBriefing2_Phase7" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
         
        <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="200" in="Slow"/>
            <Key type="Curve" time="0.3" value="800" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="200" in="Slow"/>
            <Key type="Curve" time="0.3" value="800" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="-100" in="Slow"/>
            <Key type="Curve" time="0.3" value="-100" in="Slow"/>
            <Key type="Curve" time="0.6" value="-500" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="-100" in="Slow"/>
            <Key type="Curve" time="0.3" value="-100" in="Slow"/>
            <Key type="Curve" time="0.6" value="-500" in="Slow"/>
        </Controller>
        
        <Controller type="Sound" end="Stop">
            <Key type="Sound" time="0" sound="@UI_Nova_BriefingReticleMoveHorizontal"/>
            <Key type="Sound" time="0.3" sound="@UI_Nova_BriefingReticleMoveVertical"/>
        </Controller>
    </Animation>
    
    
    <!-- Phase 8 - Pan to canyons -->
    <Animation name="MissionBriefing2_Phase8">
        <Event event="MissionBriefing2_Phase8" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="-250" out="Fast"/>
            <Key type="Curve" time="0.3" value="0" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="-250" out="Fast"/>
            <Key type="Curve" time="0.3" value="0" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="275" out="Fast"/>
            <Key type="Curve" time="0.3" value="275" out="Fast"/>
            <Key type="Curve" time="0.6" value="0" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="$Mission2_MapImage">
            <Key type="Curve" time="0" value="275" out="Fast"/>
            <Key type="Curve" time="0.3" value="275" out="Fast"/>
            <Key type="Curve" time="0.6" value="0" in="Slow"/>
        </Controller>
        
        <Controller type="Sound" end="Stop">
            <Key type="Sound" time="0" sound="@UI_Nova_BriefingMapMoveHorizontal"/>
            <Key type="Sound" time="0.3" sound="@UI_Nova_BriefingMapMoveVertical"/>
        </Controller>
    </Animation>
    
    <!-- Phase 9 - Play canyons animation -->
    <Animation name="MissionBriefing2_Phase9">
        <Event event="MissionBriefing2_Phase9" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="CanyonsContainer">
            <Key type="Event" time="0" event="CanyonsArrival"/>
        </Controller>
        
        <Controller type="Sound" end="Stop">
            <Key type="Sound" time="0" sound="@UI_Nova_Briefing02CanyonsStart"/>
        </Controller>
    </Animation>
    
    <!-- Phase 10 - Play drop zone animation -->
    <Animation name="MissionBriefing2_Phase10">
        <Event event="MissionBriefing2_Phase10" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="DropPointContainer">
            <Key type="Event" time="0" event="DropPointArrival"/>
        </Controller>
    </Animation>
    
    <!-- Phase 11 - Move viewfinder to reaper video location -->
    <Animation name="MissionBriefing2_Phase11">
        <Event event="MissionBriefing2_Phase11" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="-900" out="Fast"/>
            <Key type="Curve" time="0.3" value="160" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="-900" out="Fast"/>
            <Key type="Curve" time="0.3" value="160" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="500" out="Fast"/>
            <Key type="Curve" time="0.3" value="500" out="Fast"/>
            <Key type="Curve" time="0.6" value="80" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="500" out="Fast"/>
            <Key type="Curve" time="0.3" value="500" out="Fast"/>
            <Key type="Curve" time="0.6" value="80" in="Slow"/>
        </Controller>
        
        <Controller type="Sound" end="Stop">
            <Key type="Sound" time="0" sound="@UI_Nova_BriefingReticleMoveHorizontal"/>
            <Key type="Sound" time="0.3" sound="@UI_Nova_BriefingReticleMoveVertical"/>
            <Key type="Sound" time="0.6" sound="@UI_Nova_BriefingReticleMoveVerticalEnd"/>
        </Controller>
    </Animation>
    
    <!-- Phase 12 - Expand Viewfinder -->
    <Animation name="MissionBriefing2_Phase12">
        <Event event="MissionBriefing2_Phase12" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="$MissionBriefing_DiscoverBox">
            <Key type="Event" time="0" event="Expand"/>
        </Controller>
    </Animation>
    
    <!-- Phase 13 - Collapse Viewfinder -->
    <Animation name="MissionBriefing2_Phase13">
        <Event event="MissionBriefing2_Phase13" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
        <Controller type="Event" end="Stop" frame="$MissionBriefing_DiscoverBox">
            <Key type="Event" time="0" event="Collapse"/>
        </Controller>
    </Animation>
    
    <!-- Phase 14 - Move Viewfinder Offscreen and Hide it -->
    <Animation name="MissionBriefing2_Phase14">
        <Event event="MissionBriefing2_Phase14" action="Reset,Play" frame="$MissionPreviewPanel"/>
        <Event event="EndPreview" action="Reset,Stop" frame="$MissionPreviewPanel"/>
        
         
        <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="160" in="Slow"/>
            <Key type="Curve" time="0.3" value="900" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="160" in="Slow"/>
            <Key type="Curve" time="0.3" value="900" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="80" in="Slow"/>
            <Key type="Curve" time="0.3" value="80" in="Slow"/>
            <Key type="Curve" time="0.6" value="-500" in="Slow"/>
        </Controller>
        
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="$MissionBriefing_DiscoverBox">
            <Key type="Curve" time="0" value="80" in="Slow"/>
            <Key type="Curve" time="0.3" value="80" in="Slow"/>
            <Key type="Curve" time="0.6" value="-500" in="Slow"/>
        </Controller>
        
        <Controller type="Event" end="Stop" frame="$MissionBriefing_DiscoverBoxContainer">
            <Key type="Event" time="0.8" event="FadeOut"/>
        </Controller>
        
        <Controller type="Sound" end="Stop">
            <Key type="Sound" time="0" sound="@UI_Nova_BriefingReticleMoveHorizontal"/>
            <Key type="Sound" time="0.3" sound="@UI_Nova_BriefingReticleMoveVertical"/>
        </Controller>
    </Animation>
    
    
    <!-- 
    
    
    =============================
    
    MISSION 2 MAP FLAVOR TEXT TEMPLATE
    
    =============================
    
    
    -->
    
    <Frame type="Frame" name="MapFlavorTextContainerTemplate">
        <Anchor side="Bottom" relative="$this/FlavorText" pos="Max" offset="10"/>
        <Anchor side="Right" relative="$this/FlavorText" pos="Max" offset="20"/>
        
        <Frame type="BlurFrame" name="BackgroundBlur">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/Background" pos="Max" offset="0"/>
            <Visible val="True"/>
            
            <Animation name="Blur">
                <Event event="OnShown" action="Reset,Play"/>
                <Event event="MissionBriefing2_Phase1" action="Play" frame="$MissionPreviewPanel"/>
                <Event event="EndBriefingEarly" action="Play" frame="$MissionPreviewPanel"/>

                <Controller type="Blur" end="Pause">
                    <Key type="Curve" time="0" value=".2" out="Fast"/>
                </Controller>
            </Animation>
        </Frame>
    
        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Color val="0,0,0"/>
            <Alpha val="150"/>
        </Frame>
        
        <Frame type="Image" name="CapLeft">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/Background" pos="Min" offset="0"/>
            <Width val="3"/>
            <Color val="46,198,135"/>
        </Frame>
        
        <Frame type="Image" name="CapRight">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/Background" pos="Max" offset="0"/>
            <Width val="3"/>
            <Color val="46,198,135"/>
        </Frame>
        
        <Frame type="Label" name="FlavorText">
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <AutoSizeClamps minWidth="350"/>
            <Text val="@UI/NovaStory/MissionBriefing02/MapFlavorText"/>
        </Frame>
    </Frame>
    
    
    <!-- 
    
    
