                </Frame>
                
                <Frame type="Image" name="FrameBottomRight">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameBottomLeft"/>
                    <TextureCoords top="0" left="1" bottom="1" right="0"/>
                </Frame>
                
                <Frame type="Image" name="FrameMidVertical">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent/FrameTopLeft" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent/FrameTopRight" pos="Min" offset="0"/>
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameMiddle"/>
                    <Tiled val="True"/>
                </Frame>
                
                <Frame type="Frame" name="FrameLights">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <RenderPriority val="601"/>
                    
                    <Frame type="Image" name="FrameTopLightLeft">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="59"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="226"/>
                        <Texture val="@UI/NovaStory/ScoreScreen/FrameLights_Top"/>
                        <Height val="34"/>
                        <Width val="210"/>
                    </Frame>
                    
                    <Frame type="Image" name="FrameTopLightLeft_Pulse">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="59"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="226"/>
                        <Texture val="@UI/NovaStory/ScoreScreen/FrameLights_Top"/>
                        <Height val="34"/>
                        <Width val="210"/>
                        
                        <Animation name="Pulse">
                            <Event event="OnShown" action="Reset,Play"/>
                            <Controller type="Fade" end="Loop">
                                <Key type="Curve" time="0" value="0" out="Linear"/>
                                <Key type="Curve" time="1.0" value="0" out="Linear"/>
                                <Key type="Curve" time="1.35" value="255" out="Linear"/>
                                <Key type="Curve" time="1.85" value="0" in="Linear"/>
                                <Key type="Curve" time="2.85" value="0" in="Linear"/>
                                <Key type="Curve" time="3.55" value="200" in="Linear"/>
                                <Key type="Curve" time="3.95" value="0" in="Linear"/>
                                <Key type="Curve" time="4.15" value="150" in="Linear"/>
                                <Key type="Curve" time="4.35" value="0" in="Linear"/>
                                <Key type="Curve" time="4.65" value="80" in="Linear"/>
                                <Key type="Curve" time="4.95" value="0" in="Linear"/>
                                <Key type="Curve" time="5.95" value="0" in="Linear"/>
