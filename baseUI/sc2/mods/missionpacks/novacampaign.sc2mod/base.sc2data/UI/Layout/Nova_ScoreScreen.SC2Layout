    <Frame type="Frame" name="VictoryPanelTemplate" file="VictoryPanel">

        <Anchor relative="$parent"/>
    
        <Frame type="StandardTooltip" name="ScoreScreenTooltip" template="Nova_ScoreScreen/ScoreScreenTooltipTemplate"/>
		
		<Frame type="Frame" name="MainPanel">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            
            <Frame type="Image" name="BackgroundCover">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <LayerColor val="000000"/>
                <Alpha val="255"/>
                <RenderPriority val="10"/>
                <ScaleWidthToTexture val="true"/>
            </Frame>
            
            <Frame type="Image" name="BlurredLights">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Width val="1920"/>
                <Height val="1200"/>
                <Texture val="@UI/NovaStory/Login/BackgroundLights"/>
                <Alpha val="58"/>
                <RenderPriority val="10"/>
                <ScaleWidthToTexture val="true"/>
                
                <Animation name="BackLighting">
                    <Event event="OnShown" action="Reset, Play"/>
                    
                    <Controller type="Fade" end="Loop">
                        <Key type="Curve" time="0" value="58" out="Fast"/>
                        <Key type="Curve" time="2.5" value="0" in="Slow"/>
                        <Key type="Curve" time="4.5" value="58" out="Fast"/>
                    </Controller>
                    
                    <Controller type="Anchor" end="Loop" side="Left" relative="true">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" time="2" value="-100" in="Slow"/>
                        <Key type="Curve" time="4.5" value="0" out="Fast"/>
                    </Controller>
                    
                    <Controller type="Anchor" end="Loop" side="Top" relative="true">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" time="2" value="100" in="Slow"/>
                        <Key type="Curve" time="4.5" value="0" out="Fast"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Animation name="StartShimmer">
            <Event event="OnShown" action="Reset,Play"/>
     
                <Controller type="Event" end="Stop" frame="GridBG">
                    <Key type="Event" time="0" event="StartShimmer"/>
                </Controller>
            </Animation>
            
            <Frame type="Frame" name="GridBG" template="NovaMissionFlavorTemplates/GridBGTemplate">
                <Alpha val="150"/>
                <RenderPriority val="10"/>
            </Frame>
            
            <Frame type="Frame" name="MainPanelAnchorFrame">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Height val="799"/>
                <Width val="1441"/>
                <RenderPriority val="500"/>
                
                <Frame type="Image" name="FrameTopLeft">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameTopLeft"/>
                </Frame>
                
                <Frame type="Image" name="FrameMidLeft">
                    <Anchor side="Top" relative="$parent/FrameTopLeft" pos="Max" offset="0"/>
                    <Anchor side="Bottom" relative="$parent/FrameBottomLeft" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameMidLeft"/>
                    <Tiled val="True"/>
                </Frame>
                
                <Frame type="Image" name="FrameBottomLeft">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameBottomLeft"/>
                </Frame>
                
                <Frame type="Image" name="FrameBottomLeftHighlight">
                    <Anchor relative="$parent/FrameBottomLeft"/>
                    <LayerCount val="2"/>
                    
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameBottomLeftHighlightMask" layer="0"/>
                    <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
                    <AlphaLayer val="True" layer="0"/>
                    
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameBottomLeftHighlight" layer="1"/>
                    <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
                    
                    <Animation name="Shimmer">
                        <Event event="OnShown" action="Reset" frame="$parent"/>
                        <Event event="OnShown" action="Play" frame="$parent"/>
                 
                        <Controller type="LayerUV" layer="0" side="Left" relative="False" sidelock="True" end="Loop" >
                            <Key type="Curve" time="0" value="1" out="fast"/>
                            <Key type="Curve" time="1.6" value="1" out="fast"/>
                            <Key type="Curve" time="4.2" value="-1" in="fast"/>
                            <Key type="Curve" time="6.3" value="-1" in="fast"/>
                        </Controller>
                        
                        <Controller type="Fade" end="Loop">
                            <Key type="Curve" time="0" value="0" out="Fast"/>
                            <Key type="Curve" time="2.4" value="0" out="Fast"/>
                            <Key type="Curve" time="2.6" value="255" in="Slow"/>
                            <Key type="Curve" time="3.0" value="255" in="Slow"/>
                            <Key type="Curve" time="3.8" value="0" out="Fast"/>
                            <Key type="Curve" time="6.3" value="0" out="Fast"/>
                        </Controller>
                    </Animation>
                </Frame>
                
                <Frame type="Image" name="FrameBottomLeftScanLines">
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameBottomLeftScanlines"/>
                    <Alpha val="0"/>
                    
                    <Animation name="Shimmer">
                        <Event event="OnShown" action="Reset,Play" frame="$parent"/>
                 
                        <Controller type="Fade" end="Loop">
                            <Key type="Curve" time="0" value="0" out="Fast"/>
                            <Key type="Curve" time="2.4" value="0" out="Fast"/>
                            <Key type="Curve" time="2.8" value="255" in="Slow"/>
                            <Key type="Curve" time="3.0" value="255" in="Slow"/>
                            <Key type="Curve" time="3.8" value="0" out="Fast"/>
                            <Key type="Curve" time="6.3" value="0" out="Fast"/>
                        </Controller>
                    </Animation>
                </Frame>

                <Frame type="Image" name="FrameTopRight">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameTopLeft"/>
                    <TextureCoords top="0" left="1" bottom="1" right="0"/>
                </Frame>
                
                <Frame type="Image" name="FrameMidRight">
                    <Anchor side="Top" relative="$parent/FrameTopRight" pos="Max" offset="0"/>
                    <Anchor side="Bottom" relative="$parent/FrameBottomRight" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Texture val="@UI/NovaStory/ScoreScreen/FrameRightMiddle"/>
