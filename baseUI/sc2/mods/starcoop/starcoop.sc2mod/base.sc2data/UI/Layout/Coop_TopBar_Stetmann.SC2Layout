        <Frame type="Frame" name="StetelliteFlavorDisplay" template="Coop_TopBar_Stetmann/StetelliteFlavorDisplayTemplate"/>
        
        <StateGroup name="ChargesPossibleManager">
            <DefaultState val="0"/>
            
            <State name="7">
                <When type="AnimationState" frame="$this" ChargesPossible="7"/>
                <Action type="SetState" frame="$this" group="ChargesPossible3" state="True"/>
                <Action type="SetState" frame="$this" group="ChargesPossible5" state="True"/>
                <Action type="SetState" frame="$this" group="ChargesPossible7" state="True"/>
            </State>
            
            <State name="5">
                <When type="AnimationState" frame="$this" ChargesPossible="5"/>
                <Action type="SetState" frame="$this" group="ChargesPossible3" state="True"/>
                <Action type="SetState" frame="$this" group="ChargesPossible5" state="True"/>
                <Action type="SetState" frame="$this" group="ChargesPossible7" state="False"/>
            </State>
            
            <State name="3">
                <When type="AnimationState" frame="$this" ChargesPossible="3"/>
                <Action type="SetState" frame="$this" group="ChargesPossible3" state="True"/>
                <Action type="SetState" frame="$this" group="ChargesPossible5" state="False"/>
                <Action type="SetState" frame="$this" group="ChargesPossible7" state="False"/>
            </State>
            
            <State name="0">
                <Action type="SetState" frame="$this" group="ChargesPossible3" state="False"/>
                <Action type="SetState" frame="$this" group="ChargesPossible5" state="False"/>
                <Action type="SetState" frame="$this" group="ChargesPossible7" state="False"/>
            </State>
        </StateGroup>
        
        <StateGroup name="ChargesPossible3">
            <State name="True"/>
            <State name="False"/>
        </StateGroup>
        
        <StateGroup name="ChargesPossible5">
            <State name="True"/>
            <State name="False"/>
        </StateGroup>
        
        <StateGroup name="ChargesPossible7">
            <State name="True"/>
            <State name="False"/>
        </StateGroup>
        
        <StateGroup name="ChargeCount">
            <DefaultState val="Charge0"/>
            
            <State name="Charge1">
                <When type="AnimationState" frame="$this" ChargesAvailable="1"/>
                <Action type="SetState" frame="$this" group="ChargeState" state="Charge"/>
            </State>
            
            <State name="Charge2">
                <When type="AnimationState" frame="$this" ChargesAvailable="2"/>
                <Action type="SetState" frame="$this" group="ChargeState" state="Charge"/>
            </State>
            
            <State name="Charge3">
                <When type="AnimationState" frame="$this" ChargesAvailable="3"/>
                <Action type="SetState" frame="$this" group="ChargeState" state="Charge"/>
            </State>
            
            <State name="Charge4">
                <When type="AnimationState" frame="$this" ChargesAvailable="4"/>
                <Action type="SetState" frame="$this" group="ChargeState" state="Charge"/>
            </State>
            
            <State name="Charge5">
                <When type="AnimationState" frame="$this" ChargesAvailable="5"/>
                <Action type="SetState" frame="$this" group="ChargeState" state="Charge"/>
            </State>
            
            <State name="Charge6">
