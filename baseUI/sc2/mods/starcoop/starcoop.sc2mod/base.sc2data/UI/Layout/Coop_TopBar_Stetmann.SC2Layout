        <StateGroup name="ChargeState">
            <DefaultState val="Inactive"/>
            
            <State name="Available">
                <When type="AnimationState" ChargeState="Available"/>
                <Action type="SetProperty" frame="UsedIndicator" Visible="False"/>
                <Action type="SetProperty" frame="AvailableIndicatorGlow" Visible="True"/>
                <Action type="SetProperty" frame="AvailableIndicatorBase" Visible="True"/>
                <Action type="SetProperty" frame="AvailableIndicatorHighlight" Visible="True"/>
            </State>
            
            <State name="Used">
                <When type="AnimationState" ChargeState="Used"/>
                <Action type="SetProperty" frame="UsedIndicator" Visible="True"/>
                <Action type="SetProperty" frame="AvailableIndicatorGlow" Visible="False"/>
                <Action type="SetProperty" frame="AvailableIndicatorBase" Visible="False"/>
                <Action type="SetProperty" frame="AvailableIndicatorHighlight" Visible="False"/>
            </State>
            
            <State name="Inactive">
                <Action type="SetProperty" frame="UsedIndicator" Visible="False"/>
                <Action type="SetProperty" frame="AvailableIndicatorGlow" Visible="False"/>
                <Action type="SetProperty" frame="AvailableIndicatorBase" Visible="False"/>
                <Action type="SetProperty" frame="AvailableIndicatorHighlight" Visible="False"/>
            </State>
        </StateGroup>
    </Frame>

