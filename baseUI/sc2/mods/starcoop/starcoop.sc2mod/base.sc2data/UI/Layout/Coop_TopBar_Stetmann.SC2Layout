                <Action type="SetState" frame="$this" group="ChargeState" state="Charge"/>
            </State>
            
            <State name="Charge0">
                <Action type="SetState" frame="$this" group="ChargeState" state="NoCharge"/>
            </State>
        </StateGroup>
        
        <StateGroup name="ChargeState">
            <State name="Charge"/>
            <State name="NoCharge"/>
        </StateGroup>
        
        <StateGroup name="ActiveZone">
            <DefaultState val="NoActiveZone"/>
            
            <State name="Speed">
                <When type="AnimationState" frame="$this" ActiveZone="Speed"/>
                <Action type="SetProperty" frame="$StetmannTopBarPrimaryActiveColorBind" Color="#StetmannTopBar_Color_IndicatorSpeed"/>
                <Action type="SetProperty" frame="$StetmannTopBarHeatActiveColorBind" Color="#StetmannTopBar_Color_HeatSpeed"/>
                <Action type="SetState" frame="$this" group="LastActiveZone" state="Speed" on="Exit"/>
            </State>
            
            <State name="Health">
                <When type="AnimationState" frame="$this" ActiveZone="Health"/>
                <Action type="SetProperty" frame="$StetmannTopBarPrimaryActiveColorBind" Color="#StetmannTopBar_Color_IndicatorHealth"/>
                <Action type="SetProperty" frame="$StetmannTopBarHeatActiveColorBind" Color="#StetmannTopBar_Color_HeatHealth"/>
                <Action type="SetState" frame="$this" group="LastActiveZone" state="Health" on="Exit"/>
            </State>
            
            <State name="Energy">
                <When type="AnimationState" frame="$this" ActiveZone="Energy"/>
