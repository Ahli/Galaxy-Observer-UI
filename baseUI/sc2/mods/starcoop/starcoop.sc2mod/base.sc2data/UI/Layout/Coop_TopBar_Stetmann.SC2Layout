                <Action type="SetProperty" frame="$StetmannTopBarHeatActiveColorBind" Color="#StetmannTopBar_Color_HeatEnergy"/>
                <Action type="SetState" frame="$this" group="LastActiveZone" state="Energy" on="Exit"/>
            </State>
            
            <State name="NoActiveZone">
                <Action type="SetProperty" frame="$StetmannTopBarPrimaryActiveColorBind" Color="0,0,0"/>
            </State>
        </StateGroup>
            
        <StateGroup name="LastActiveZone">
            <State name="Speed"/>
            <State name="Health"/>
            <State name="Energy"/>
        </StateGroup>
        
        <StateGroup name="SliderEventHandler">
            <State name="SpeedToHealth">
                <When type="StateGroup" frame="$this" LastActiveZone="Speed"/>
                <When type="StateGroup" frame="$this" ActiveZone="Health"/>
                <Action type="SendEvent" frame="$this" event="SpeedToHealth" on="Enter"/>
            </State>
            <State name="SpeedToEnergy">
                <When type="StateGroup" frame="$this" LastActiveZone="Speed"/>
                <When type="StateGroup" frame="$this" ActiveZone="Energy"/>
                <Action type="SendEvent" frame="$this" event="SpeedToEnergy" on="Enter"/>
            </State>
            <State name="HealthToSpeed">
                <When type="StateGroup" frame="$this" LastActiveZone="Health"/>
                <When type="StateGroup" frame="$this" ActiveZone="Speed"/>
                <Action type="SendEvent" frame="$this" event="HealthToSpeed" on="Enter"/>
            </State>
            <State name="HealthToEnergy">
                <When type="StateGroup" frame="$this" LastActiveZone="Health"/>
                <When type="StateGroup" frame="$this" ActiveZone="Energy"/>
                <Action type="SendEvent" frame="$this" event="HealthToEnergy" on="Enter"/>
            </State>
            <State name="EnergyToSpeed">
                <When type="StateGroup" frame="$this" LastActiveZone="Energy"/>
                <When type="StateGroup" frame="$this" ActiveZone="Speed"/>
                <Action type="SendEvent" frame="$this" event="EnergyToSpeed" on="Enter"/>
            </State>
            <State name="EnergyToHealth">
                <When type="StateGroup" frame="$this" LastActiveZone="Energy"/>
                <When type="StateGroup" frame="$this" ActiveZone="Health"/>
                <Action type="SendEvent" frame="$this" event="EnergyToHealth" on="Enter"/>
            </State>
        </StateGroup>
    </Frame>
