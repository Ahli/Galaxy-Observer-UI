    <Frame type="Frame" name="ProphecyButtonContainerTemplate">
        <Anchor side="Top" relative="SelectPowerButton" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="SelectPowerButton" pos="Max" offset="0"/>
        <Anchor side="Left" relative="SelectPowerButton" pos="Min" offset="0"/>
