        <Frame type="Image" name="OuterPulse">
            <Anchor relative="$parent"/>
            <Visible val="{$parent/SelectPowerButton/@Visible}"/>
            <LayerCount val="2"/>
            <RenderPriority val="513"/>
            <Color val="150,200,255"/>
            
            <Texture val="Assets/Textures/ui_commandcard_commandbutton_mask_outline.dds" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            
            <Texture val="Assets/Textures/sc2_ui_global_genericeffect_mirroredanglegradient_256x256.dds" layer="1"/>
            
            <Animation name="Pulse">
                <Event event="OnShown" action="Reset,Play"/>
            
                <Controller type="Property" property="TextureCoordRotation" curve="True" index="1"  end="Loop" frame="$this">
                    <Key type="Property" time="0.000" value="0" inOut="Linear"/>
                    <Key type="Property" time="1.5" value="-360" inOut="Linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="Background">
            <Anchor relative="$parent/SelectPowerButton"/>
            <Texture val="Assets/Textures/ui_commandcard_protossframe_normal.dds"/>
            <Alpha val="0"/>
        </Frame>
        
        <Frame type="Button" name="SelectPowerButton">
            <Width val="68"/>
            <Height val="68"/>
            <Toggleable val="True"/>
            
            <Frame type="Image" name="NormalImage">
                <Anchor relative="$parent"/>
                <Texture val="Assets/Textures/ui_commandcard_protossframe_normal.dds"/>
            </Frame>
            
            <Frame type="Image" name="HoverImage">
                <Anchor relative="$parent"/>
                <Texture val="Assets/Textures/ui_commandcard_protossframe_highlighted.dds"/>
            </Frame>
            
            <Frame type="Image" name="Icon">
                <Anchor relative="$parent"/>
                <Texture val="Assets\Textures\btn-ability-zeratul-topbar-selectability.dds"/>
                <ColorAdjustMode val="Colorize"/>
                <AdjustmentColor val="100,100,255"/>
            </Frame>
            
            <Frame type="Image" name="IconPulse">
                <Anchor relative="$parent/Icon"/>
                <Texture val="{$parent/Icon/@Texture}"/>
                <ColorAdjustMode val="Colorize"/>
                <AdjustmentColor val="{$parent/Icon/@AdjustmentColor}"/>
                <BlendMode val="Add"/>
            </Frame>
        </Frame>
            
        <Animation name="Pulse">
            <Event event="OnShown" action="Reset,Play"/>
            
            <Controller type="Fade" frame="SelectPowerButton/IconPulse" end="PingPong">
                <Key type="Curve" time="0" value="0" inout="Slow"/>
                <Key type="Curve" time="1" value="255" inout="Slow"/>
            </Controller>
        </Animation>
        
        <StateGroup name="ArtifactState">
            <DefaultState val="NotCollected"/>
            
            <State name="Used">
                <When type="AnimationState" ArtifactState="Used"/>
                <Action type="SetProperty" frame="SelectPowerButton" Visible="False"/>
            </State>
            
            <State name="Collected">
                <When type="AnimationState" ArtifactState="Collected"/>
                <Action type="SetProperty" frame="SelectPowerButton" Visible="True"/>
            </State>
            
            <State name="NotCollected">
                <Action type="SetProperty" frame="SelectPowerButton" Visible="False"/>
            </State>
        </StateGroup>
    </Frame>
    
