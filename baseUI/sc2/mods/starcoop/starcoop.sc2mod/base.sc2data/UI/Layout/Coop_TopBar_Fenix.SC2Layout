        <Width val="4"/>
        <Height val="40"/>
        <Color val="#Coop_Fenix_Sparkle_Color"/>
        
        <Animation name="Show">
            <Event event="AnimationStart" action="Reset,Play"/>
             
            <Controller type="Anchor" end="Loop" side="Top" relative="true">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time="2" value="-150" in="Slow"/>
            </Controller>
            
            <Controller type="Fade" end="Loop">
                <Key type="Curve" time="0" value="60" out="Fast"/>
                <Key type="Curve" time="1.5" value="0" in="Slow"/>
                <Key type="Curve" time="2" value="0" in="Slow"/>
            </Controller>
        </Animation>
    </Frame>
    
