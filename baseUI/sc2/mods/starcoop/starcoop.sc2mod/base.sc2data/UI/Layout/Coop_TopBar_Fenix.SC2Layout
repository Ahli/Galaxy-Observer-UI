    <!-- UNIT STATUS (PROGRESS) BAR TEMPLATES -->
    <Frame type="ProgressBar" name="VitalsBarGenericTemplate">
        <Width val="#Coop_Fenix_UnitStatusBar_Width"/>
        <Height val="#Coop_Fenix_UnitStatusBar_Height"/>
        <UseFillContainer val="true"/>

        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="@UI/SC2_UI_Coop_Fenix_TopBar_Bar_Full_Underlay"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="#Coop_Fenix_UnitStatusBar_Underlay_Color"/>
        </Frame>

        <Frame type="Frame" name="FillImageContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Image" name="FillImage">
                <Anchor relative="$parent/$parent"/>
                <Texture val="@UI/SC2_UI_Coop_Fenix_TopBar_Bar_Full"/>
                <ColorAdjustMode val="Colorize"/>
                <DarkAdjustColor val="#Coop_Fenix_UnitStatusBar_Energy_Color_Dark"/>
                <LightAdjustColor val="#Coop_Fenix_UnitStatusBar_Energy_Color_Light"/>
            </Frame>
        </Frame>
    </Frame>

    <!-- Container -->
    <Frame type="UnitStatusFrame" name="FenixHeroUnitHeroMonitorStatusFrameTemplate">
        <Anchor side="Bottom" relative="$this/VitalsBar_Energy" pos="Max" offset="0"/>
        <Width val="#Coop_Fenix_UnitStatusBar_Container_Width"/>
        
        <Frame type="UnitFrame" name="UnitFrame">
            <Anchor relative="$parent"/>
            <UnitTag val="{$parent/@UnitTag}"/>
        </Frame>
        
        <!-- Health Bar -->
        <Frame type="ProgressBar" name="VitalsBar_Health" template="Coop_TopBar_Fenix/VitalsBarGenericTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Height val="4"/>
            <Width val="#Coop_Fenix_UnitStatusBar_Width_Half"/>
            <MinValue val="0"/>
            <MaxValue val="{$parent/UnitFrame/@MaxHealth}"/>
            <Value val="{$parent/UnitFrame/@CurrentHealth}"/>
            
            <Frame type="Image" name="Background">
                <Texture val="@UI/SC2_UI_Coop_Fenix_TopBar_Bar_Half_Underlay"/>
            </Frame>
            
            <Frame type="Frame" name="FillImageContainer">
                <Frame type="Image" name="FillImage">
                    <Texture val="@UI/SC2_UI_Coop_Fenix_TopBar_Bar_Half"/>
                    <DarkAdjustColor val="#Coop_Fenix_UnitStatusBar_Health_Color_Dark"/>
                    <LightAdjustColor val="#Coop_Fenix_UnitStatusBar_Health_Color_Light"/>
                </Frame>
            </Frame>
        </Frame>
        
        <!-- Shields Bar -->
        <Frame type="ProgressBar" name="VitalsBar_Shields" template="Coop_TopBar_Fenix/VitalsBarGenericTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="4"/>
            <Width val="#Coop_Fenix_UnitStatusBar_Width_Half"/>
            <MinValue val="0"/>
            <MaxValue val="{$parent/UnitFrame/@MaxShields}"/>
            <Value val="{$parent/UnitFrame/@CurrentShields}"/>
            
            <Frame type="Image" name="Background">
                <Texture val="@UI/SC2_UI_Coop_Fenix_TopBar_Bar_Half_Underlay"/>
                <TextureCoords top="0.0" left="1" bottom="1" right="0" layer="0"/>
            </Frame>
            
            <Frame type="Frame" name="FillImageContainer">
                <Frame type="Image" name="FillImage">
                    <Texture val="@UI/SC2_UI_Coop_Fenix_TopBar_Bar_Half"/>
                    <TextureCoords top="0.0" left="1" bottom="1" right="0" layer="0"/>
                    <DarkAdjustColor val="#Coop_Fenix_UnitStatusBar_Shields_Color_Dark"/>
                    <LightAdjustColor val="#Coop_Fenix_UnitStatusBar_Shields_Color_Light"/>
                </Frame>
            </Frame>
        </Frame>
        
        <!-- Energy Bar -->
        <Frame type="ProgressBar" name="VitalsBar_Energy" template="Coop_TopBar_Fenix/VitalsBarGenericTemplate">
            <Anchor side="Top" relative="$parent/VitalsBar_Health" pos="Max" offset="#Coop_Fenix_UnitStatusBar_Spacing"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <MinValue val="0"/>
            <MaxValue val="{$parent/UnitFrame/@MaxEnergy}"/>
            <Value val="{$parent/UnitFrame/@CurrentEnergy}"/>
            
            <Frame type="Frame" name="FillImageContainer">
                <Frame type="Image" name="FillImage">
                    <DarkAdjustColor val="#Coop_Fenix_UnitStatusBar_Energy_Color_Dark"/>
                    <LightAdjustColor val="#Coop_Fenix_UnitStatusBar_Energy_Color_Light"/>
                </Frame>
            </Frame>
        </Frame>
    </Frame>
    
    
