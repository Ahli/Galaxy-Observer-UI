            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Height val="52"/>
            <Width val="193"/>
            <Visible val="False"/>
            <RenderPriority val="511"/>
            <Alpha val="0"/>
            <Texture val="@UI/StormTalentAlertNameplateBG"/>

            <Animation name="StuckTextGlowAnim">
                <Event event="OnShown" action="Reset, DirectionForward, Play"/>

                <Controller type="Fade" end="Stop">
                    <Key type="Curve" time="0" value="0" inout="Slow"/>
