                                <Key type="Image" time="0" image="@UI/Storm_UI_HUD_Tracer_Blink_4of4"/>
                                <Key type="Image" time=".1" image="@UI/Storm_UI_HUD_Tracer_Blink_4of4"/>
                            </Controller>
                            <Controller type="Visibility" end="Pause" frame="$parent/$parent/LastCharge4Frame">
                                <Key type="Bool" time="0" value="True"/>
                            </Controller>
                            <Controller type="Visibility" end="Pause" frame="$parent/$parent/LastCharge3Frame">
                                <Key type="Bool" time="0" value="False"/>
                            </Controller>
                        </Animation>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
    </Frame>

    <!-- This is the frame used for the Stuck text when sticking Pulse Bomb to an enemy -->
    <Frame type="SelectionUnitStatusPanel" name="TracerPulseBombFrame">
        <Anchor side="Top" relative="$parent" pos="Mid" offset="-580"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Visible val="False"/>
        <Height val="800"/>
        <Width val="2000"/>
        <!--SpecificUnit val="HeroTracer"/-->

        <Frame type="Image" name="StuckTextImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-200"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-200"/>
