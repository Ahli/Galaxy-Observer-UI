                    <Key type="Curve" time=".85" value="255" inout="Linear,Slow"/>
                    <Key type="Curve" time="1.38" value="255"/>
                    <Key type="Curve" time="1.5" value="0" inout="Slow,Linear"/>
                    <Key type="Curve" time="1.6" value="0" inout="Slow"/>
