                    <Key type="Curve" time="1.65" value="255"/>
                    <Key type="Curve" time="1.73" value="0" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" end="Stop" percentage="True">
                    <Key type="Curve" time="0" value="200"/>
