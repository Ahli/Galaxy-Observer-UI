                    <Key type="Curve" time=".5" value="900" inout="Slow"/>
                    <Key type="Curve" time=".6" value="600" inout="Slow"/>
                    <Key type="Curve" time="1.25" value="600" inout="Linear,Slow"/>
                    <Key type="Curve" time="1.75" value="600 " inout="Slow"/>
