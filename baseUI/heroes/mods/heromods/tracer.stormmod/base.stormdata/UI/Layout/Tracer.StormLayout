                    <Key type="Curve" time="1.73" value="400" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" end="Stop" percentage="True">
                    <Key type="Curve" time="0" value="200"/>
                    <Key type="Curve" time=".15" value="100"/>
                    <Key type="Curve" time="1.65" value="100" inout="Linear,Slow"/>
