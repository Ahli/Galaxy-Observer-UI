            <Width val="20"/>
        </Frame>
        
        <Frame type="UnitStatusDurationBar" name="DurationBar">
            <Height val="4"/>
            <Width val="100"/>
            <VisibleToOwner val="true"/>
            <VisibleToEnemy val="true"/>
            <VisibleToAlly val="true"/>
            <Anchor side="Top" relative="$parent" pos="Max" offset="-12"/>
