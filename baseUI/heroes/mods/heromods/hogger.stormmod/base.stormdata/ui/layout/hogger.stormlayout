            <Frame type="UnitStatusBehaviorIcon" name="HoggerRageDecayCountdownFrame" template="StandardHeroUITemplates/CircularBehaviorCountdownBarTemplate">
                <Behavior val="HoggerGnollFrenzyDecayCoolup"/>
