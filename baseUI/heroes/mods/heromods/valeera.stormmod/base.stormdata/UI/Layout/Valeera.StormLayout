                    </Controller>
                    <Controller type="Dimension" dimension="Height" relative="False" percentage="false" unclip="true" end="pause">
                        <Key type="Curve" time="0" value="35"/>
                        <Key type="Curve" time=".23" value="35"/>
                        <Key type="Curve" time=".37" value="20"/>
                    </Controller>
