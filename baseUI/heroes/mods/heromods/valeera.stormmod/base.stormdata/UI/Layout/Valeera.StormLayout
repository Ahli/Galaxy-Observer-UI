            <Frame type="Image" name="ContainerSparkle2">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="-22"/>
                <Anchor side="Left" relative="$parent/$parent" pos="Min" offset="-65"/>
                <Texture val="@UI/Storm_UI_InGame_PocketWatchRing_Flare"/>
                <BlendMode val="Add"/>
                <ColorAdjustMode val="Colorize"/>
                <AdjustmentColor val="0000ff"/>
                <Unclipped val="True"/>
                <Alpha val="0"/>
                <RenderPriority val="620"/>

                <Animation name="SparkleAnimation2">
                    <Event event="OnShown" action="Reset, Play"/>
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time=".13" value="0" inout="Linear"/>
                        <Key type="Curve" time=".23" value="255" inout="Linear"/>
                        <Key type="Curve" time=".47" value="255" inout="Linear,Slow"/>
                        <Key type="Curve" time=".57" value="0" inout="Slow,Linear"/>
                    </Controller>
                    <Controller type="Anchor" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
