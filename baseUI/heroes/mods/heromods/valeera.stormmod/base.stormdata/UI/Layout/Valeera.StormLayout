        </Frame>

        <Frame type="Frame" name="ThreeStacksIndicatorFrame">
            <Anchor relative="$parent"/>
            <Visible val="false"/>

            <Frame type="Image" name="ThreeStacksIndicatorComboGemShineImage">
                <Anchor relative="$parent"/>
                <LayerCount val="2"/>
                <BlendMode val="Add"/>
                <Alpha val="0"/>
                <ColorAdjustMode val="Fill"/>
                <AdjustmentColor val="FFFFFF"/>
                
                <Texture val="@UI/Storm_UI_HeroUI_Greymane_ChromeShine_Mask" layer="0"/>
