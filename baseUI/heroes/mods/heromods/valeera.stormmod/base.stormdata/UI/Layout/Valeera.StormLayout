                    <Animation name="InnerGlowPulseAnimation">
                        <Event event="OnShown" action="Reset, Play"/>
                        <Controller type="Fade" end="pingpong">
                            <Key type="Curve" time="0" value="0" inout="Linear"/>
                            <Key type="Curve" time=".5" value="255" inout="Linear"/>
