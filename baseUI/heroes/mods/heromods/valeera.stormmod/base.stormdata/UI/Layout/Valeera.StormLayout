<!-- Start: Stealth Smoke Affect -->
    <Frame type="SelectionUnitStatusPanel" name="ValeeraSmokeFrame">
        <Anchor relative="$parent"/>
        <AcceptsMouse val="false"/>
        <SpecificUnit val="HeroValeera"/>
        <RenderType val="HDR"/>
        <RenderPriority val="400"/>

        <Frame type="UnitStatusFrame" name="UnitStatusFrame0">
            <Anchor relative="$parent"/>

            <Frame type="UnitStatusValidatorAnimElement" name="StealthValidators">
                <Anchor relative="$parent"/>
                <Validator val="ValeeraIsStealthedCombine"/>
                <Validator val="ValeeraNotStealthed"/>
                
                <Animation name="ValeeraIsStealthedCombine">
                    <Event event="Reset" action="Stop"/>
                    <Event event="ValeeraIsStealthedCombine" action="Restart, Play"/>

                    <Controller type="Visibility" end="Pause" frame="$parent/SmokeAnimationSmoke">
                        <Key type="Bool" time="0" value="True"/>
                    </Controller>
                    <Controller type="Visibility" end="Pause" frame="$parent/SmokeAnimationBackground">
                        <Key type="Bool" time="0" value="True"/>
                    </Controller>
                    <Controller type="Fade" end="pause" frame="$parent/SmokeAnimationSmoke">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time="1" value="255" inout="Linear"/>
                    </Controller>
                    <Controller type="Fade" end="pause" frame="$parent/SmokeAnimationBackground">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time="1" value="150" inout="Linear"/>
                    </Controller>

                </Animation>

                <Animation name="ValeeraNotStealthed">
                    <Event event="Reset" action="Stop"/>
                    <Event event="ValeeraNotStealthed" action="Restart, Play"/>

                    <Controller type="Visibility" end="Pause" frame="$parent/SmokeAnimationSmoke">
                        <Key type="Bool" time=".5" value="false"/>
                    </Controller>
                    <Controller type="Visibility" end="Pause" frame="$parent/SmokeAnimationBackground">
                        <Key type="Bool" time=".5" value="false"/>
                    </Controller>
                    <Controller type="Fade" end="pause" frame="$parent/SmokeAnimationSmoke">
                        <Key type="Curve" time="0" value="255" inout="Linear"/>
                        <Key type="Curve" time=".5" value="0" inout="Linear"/>
                    </Controller>
                    <Controller type="Fade" end="pause" frame="$parent/SmokeAnimationBackground">
                        <Key type="Curve" time="0" value="150" inout="Linear"/>
                        <Key type="Curve" time=".5" value="0" inout="Linear"/>
                    </Controller>

                </Animation>

            </Frame>

            <Frame type="Image" name="SmokeAnimationBackground">
                <Anchor relative="$parent"/>
                <RenderPriority val="513"/>
                <Visible val="false"/>
                <Alpha val="150"/>
                <Texture val="@UI/storm_ui_hud_fx_valeera_smoke_vignette_small"/>
                <RenderType val="HDR"/>
                <RenderPriority val="1"/>
            </Frame>

            <Frame type="Image" name="SmokeAnimationSmoke">
                <Anchor relative="$parent"/>
                <RenderPriority val="514"/>
                <Visible val="False"/>
                <Alpha val="255"/>
                <LayerCount val="3"/>
                <BlendMode val="Normal"/>
                <RenderType val="HDR"/>
                <SubpixelRendering val="True"/>
                <RenderPriority val="1"/>

                <Texture val="@UI/storm_ui_hud_fx_valeera_smoke" layer="2"/>
                <TextureCoords top="1.7" left="0" bottom="0" right="1.7" layer="2"/>
                <WrapUV val="True" layer="2"/>
                <AlphaLayer val="false" layer="2"/>
                <LayerColor val="2c0606" layer="2"/>

                <Texture val="@UI/storm_ui_hud_fx_valeera_smoke" layer="1"/>
                <TextureCoords top="1.7" left="0" bottom="0" right="1.7" layer="1"/>
                <WrapUV val="True" layer="1"/>
                <AlphaLayer val="true" layer="1"/>
                <LayerColor val="ffffff" layer="1"/>

                <Texture val="@UI/storm_ui_hud_fx_valeera_smoke_vignette_small" layer="0"/>
                <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
                <AlphaLayer val="true" layer="0"/>
                <LayerAlpha val="255" layer="0"/>
                <WrapUV val="True" layer="0"/>

                <Animation name="UVScroll">
                    <Event event="OnShown" action="Restart,DirectionForward,Play" frame="$parent"/>
                    <Controller type="LayerUV" layer="1" side="top" relative="True" sidelock="True" end="Loop">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time="12" value="-1" inout="Linear"/>
                    </Controller>
                    <Controller type="LayerUV" layer="1" side="right" relative="True" sidelock="True" end="Loop">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time="10" value="1" inout="Linear"/>
                    </Controller>
                    <Controller type="LayerUV" layer="2" side="top" relative="True" sidelock="True" end="Loop">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time="14" value="-1" inout="Linear"/>
                    </Controller>
                    <Controller type="LayerUV" layer="2" side="right" relative="True" sidelock="True" end="Loop">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
