<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <Frame type="SelectionUnitStatusPanel" name="ValeeraStacksFrameContainer">
        <Anchor relative="$parent"/>
        <SpecificUnit val="HeroValeera"/>

        <Frame type="UnitStatusUnitStackingDebuffElement" name="StacksFrame" template="UnitStatus/StackingBehaviorTemplate">
            <Anchor relative="$parent"/>
            <BuffBehavior val="ValeeraComboPointsToken"/>
            <StackBehavior val="ValeeraComboPointsToken"/>
            <ShowForBehaviorOwner val="True"/>
            <DebuffType val="TokenCounter"/>
            
            <Frame type="ProgressBar" name="StacksProgressBar">
                <Width val="50"/>
                <Height val="14"/>
            </Frame>

            <Frame type="Image" name="Stacking">
                <Texture val="@UI/Stacks/Valeera"/>
            </Frame>

            <Frame type="Image" name="StacksFull">
                <Texture val="@UI/Stacks/ValeeraFull"/>
            </Frame>
        </Frame>
    </Frame>
    
<!-- Start: Frame Overrides used for creating the glowing effect on the, Portrait Frame. Resource Fraem, and Ability Backplate -->

    <Frame type="CommandPanel" name="GameUI/UIContainer/ConsoleUIContainer/CommandPanel" file="GameUI">

        <StateGroup name="CurrentUnit">
            <State name="HeroValeera">
                <When type="AnimationState" CurrentUnit="HeroValeera"/>
                <Action type="SetProperty" frame="$AbilityBackPlate" LightAdjustColor="68002C"/>
                <Action type="SetProperty" frame="$AbilityBackPlate" DarkAdjustColor="350000"/>
                <Action type="SetProperty" frame="$PortraitBackPlate" LightAdjustColor="68002C"/>
                <Action type="SetProperty" frame="$PortraitBackPlate" DarkAdjustColor="350000"/>
                <Action type="SetProperty" frame="$ResourceBackPlate" LightAdjustColor="68002C"/>
                <Action type="SetProperty" frame="$ResourceBackPlate" DarkAdjustColor="350000"/>

                <Action type="SetProperty" frame="$AbilityBackPlate/$parent" LightAdjustColor="859BB8"/>
                <Action type="SetProperty" frame="$AbilityBackPlate/$parent" DarkAdjustColor="3B3F55"/>
                <Action type="SetProperty" frame="$PortraitBackPlate/$parent" LightAdjustColor="859BB8"/>
                <Action type="SetProperty" frame="$PortraitBackPlate/$parent" DarkAdjustColor="3B3F55"/>
                <Action type="SetProperty" frame="$ResourceBackPlate/$parent" LightAdjustColor="859BB8"/>
                <Action type="SetProperty" frame="$ResourceBackPlate/$parent" DarkAdjustColor="3B3F55"/>
                <Action type="SetProperty" frame="HeroValeeraCommandPanelAnimFrame" Visible="true"/>
            </State>
        </StateGroup>

        <Frame type="Frame" name="HeroValeeraCommandPanelAnimFrame">
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$layer" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$layer" pos="Max" offset="0"/>
            <Height val="4"/>
            <Visible val="False"/>

            <!-- An invisible frame will still update its animations, but not its children's.  This child frame
                is to prevent the animation from running when the parent is hidden. -->
            <Frame type="Frame" name="HeroValeera">
                <Anchor relative="$parent" offset="0"/>

                <Animation name="PulseFade" flags="NoEventsWhileHidden">
                    <Event event="OnHidden" action="Stop" frame="$parent"/>
                    <Event event="AbilBarHealthBarPortraitGlow" action="Restart,Play" frame="$AbilBarHealthBarPortraitGlow"/>
                    <Event event="NoState" action="Stop" frame="$AbilBarHealthBarPortraitGlow"/>

                    <Controller type="Fade" end="pause" frame="$AbilityBackPlate">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time="1" value="255" inout="Linear"/>
                    </Controller>
                    <Controller type="Fade" end="pause" frame="$PortraitBackPlate">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time="1" value="255" inout="Linear"/>
                    </Controller>
                    <Controller type="Fade" end="pause" frame="$ResourceBackPlate">
                        <Key type="Curve" time="0" value="0" inout="Linear"/>
                        <Key type="Curve" time="1" value="255" inout="Linear"/>
                    </Controller>

                    <Controller type="Property" property="ColorAdjustMode" end="Pause" frame="$AbilityBackPlate/$parent">
                        <Key type="Property" time="0" value="Colorize"/>
                    </Controller>
                    <Controller type="Property" property="ColorAdjustMode" end="Pause" frame="$PortraitBackPlate/$parent">
                        <Key type="Property" time="0" value="Colorize"/>
                    </Controller>
                    <Controller type="Property" property="ColorAdjustMode" end="Pause" frame="$ResourceBackPlate/$parent">
                        <Key type="Property" time="0" value="Colorize"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>
    </Frame>

<!-- End: Frame Overrides used for creating the glowing effect on the, Portrait Frame. Resource Fraem, and Ability Backplate -->

<!-- Start: ComboGem Templates -->

    <!-- Turn on Pulse Ring: This is seperate, because the ring pusles need to play on top of all the other gem and gem container elements -->
    <Frame type="Image" name="ContainerRingPulseTemplate">
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
        <Height val="55"/>
        <Width val="55"/>
        <ColorAdjustMode val="Colorize"/>
        <LightAdjustColor val="ffe68f"/>
        <DarkAdjustColor val="ff8260"/>
        <Texture val="@UI/Storm_UI_GenFX_RingGlow"/>
        <RenderPriority val="520"/>
        <Visible val="false"/>

        <Animation name="ContainerRingPulseAnimation">
            <Event event="OnShown" action="Reset, Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".23" value="255" inout="Linear"/>
                <Key type="Curve" time=".33" value="255" inout="Linear,Slow"/>
                <Key type="Curve" time=".57" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="False" percentage="false" unclip="true" end="pause">
                <Key type="Curve" time="0" value="126"/>
                <Key type="Curve" time=".23" value="55"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="False" percentage="false" unclip="true" end="pause">
                <Key type="Curve" time="0" value="126"/>
                <Key type="Curve" time=".23" value="55"/>
            </Controller>
        </Animation>
    </Frame>

    <!-- Red Pulse Three Stack Ring: This is seperate, because the ring pusles need to play on top of all the other gem and gem container elements -->
    <Frame type="Image" name="ThreeStacksIndicatorRingTemplate">
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
        <Height val="45"/>
        <Width val="45"/>
        <BlendMode val="Add"/>
        <ColorAdjustMode val="Colorize"/>
        <DarkAdjustColor val="c5006b"/>
        <LightAdjustColor val="ff0000"/>
        <Unclipped val="True"/>
        <Texture val="@UI/Storm_UI_GenFX_RingGlow"/>
        <RenderPriority val="620"/>
        <Visible val="false"/>

        <Animation name="ThreeStacksIndicatorRingAnimation">
            <Event event="OnShown" action="Reset, Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".16" value="0" inout="Linear"/>
                <Key type="Curve" time=".17" value="255" inout="Linear"/>
                <Key type="Curve" time=".97" value="0" inout="Linear,Slow"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="False" percentage="false" unclip="true" end="pause">
                <Key type="Curve" time="0" value="45"/>
                <Key type="Curve" time=".17" value="45"/>
                <Key type="Curve" time=".97" value="116"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="False" percentage="false" unclip="true" end="pause">
                <Key type="Curve" time="0" value="45"/>
                <Key type="Curve" time=".17" value="45"/>
                <Key type="Curve" time=".97" value="116"/>
            </Controller>
        </Animation>
    </Frame>

    <!-- Main Gem Images -->

    <Frame type="Image" name="ComboGemTemplate">
        <Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="-28"/>
        <Texture val="@UI/Storm_UI_HUD_Valeera_Socket"/>

        <!-- Series of Images that play when the Combo Gem 1 is turned on -->

        <Frame type="BehaviorIcon" name="ComboGemTooltipFrame">
