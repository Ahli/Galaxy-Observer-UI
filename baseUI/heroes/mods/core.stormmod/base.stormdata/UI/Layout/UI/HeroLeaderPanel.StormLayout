                            <Key type="Bool" time="0" value="False"/>
                        </Controller>
                        <Controller type="Visibility" end="Pause" frame="$parent/$parent/RedTeamStats">
                            <Key type="Bool" time="0" value="False"/>
                        </Controller>
                        <Controller type="Visibility" end="Pause" frame="$parent/$parent/VSLabel">
                            <Key type="Bool" time="0" value="False"/>
                        </Controller>
                    </Animation>
                </Frame>
            </Frame>

            <!-- BLUE TEAM -->
            <Frame type="Frame" name="BlueTeamStats" template="HeroLeaderPanel/BlueTeamStatsTemplate">
                <Anchor side="Top" relative="$parent/TabControl/PlayerFrame9" pos="Max" offset="5"/>
                <Anchor side="Bottom" relative="$parent/TabControl/ShowStatsButton" pos="Min" offset="8"/>
                <Anchor side="Left" relative="$parent/BackgroundImage" pos="Min" offset="5"/>
                <Anchor side="Right" relative="$parent/TabControl/ShowStatsButton" pos="Mid" offset="20"/>
            </Frame>

            <!-- VS label -->
            <Frame type="Label" name="VSLabel">
                <Anchor side="Top" relative="$parent/BlueTeamStats" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/BlueTeamStats" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
                <Text val="@UI/HeroDraftPanel/Versus"/>
                <Style val="Storm_ScoreScreen_VSLabel"/>
                <Alpha val="80"/>
            </Frame>
    
            <!-- RED TEAM -->
            <Frame type="Frame" name="RedTeamStats" template="HeroLeaderPanel/RedTeamStatsTemplate">
                <Anchor side="Top" relative="$parent/BlueTeamStats" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent/BlueTeamStats" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/TabControl/ShowDeathRecapButton" pos="Mid" offset="-28"/>
                <Anchor side="Right" relative="$parent/BackgroundImage" pos="Max" offset="-5"/>
            </Frame>

            <Animation name="ToggleVisibility">
                <Event event="Show" action="DirectionForward,Play" frame="$parent"/>
                <Event event="Hide" action="DirectionReverse,Play" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time=".1" value="255"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                    <Key type="Curve" time="0" value="-15" inout="Linear"/>
                    <Key type="Curve" time=".1" value="0" inout="Slow,Linear"/>
                </Controller>
            
                <Controller type="Anchor" end="Pause" side="Top" relative="true">
                    <Key type="Curve" time="0" value="-15" inout="Linear"/>
                    <Key type="Curve" time=".1" value="0" inout="Slow,Linear"/>
                </Controller>

                <Controller type="Sound" end="Pause">
                    <Key type="Sound" time="0.0" sound="@UI_Game_StatsContainerAppears" direction="Forward"/>
                    <Key type="Sound" time="0.175" sound="@UI_Game_StatsContainerHides" direction="Reverse"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
</Desc>
