<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <!--===============================================================================================
    <! Templates
    <!==============================================================================================-->

    <!--===============================================================================================
    <! Main Panel
    <!==============================================================================================-->
    <Frame type="Frame" name="TargetInfoPanelTemplate">
        <Frame type="Image" name="BackgroundFrame">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Storm_UI_TargetInfoPanel_Background"/>
            <TextureType val="Nineslice"/>
        </Frame>

        <Frame type="Image" name="TopBackground">
            <Anchor side="Top" relative="$parent" pos="Min" offset="8"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="8"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-8"/>
            <Height val="54"/>
            <Color val="251c3d"/>

            <Frame type="Image" name="BottomBorderImage">
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Height val="1"/>
                <Color val="3b315d"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="TargetPortraitFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="8"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="8"/>
            <Width val="150"/>
            <Height val="53"/>

            <Frame type="Image" name="TargetPortraitImage">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="-15"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            </Frame>
        </Frame>

        <Frame type="UnitTargetFrame" name="TargetHeroUnitFrame">
            <Anchor relative="$parent/TargetPortraitFrame"/>
            <AllowTargeting val="true"/>
            <AllowSelection val="false"/>
            <AllowCameraCentering val="true"/>
            <AllowSmartCommand val="false"/>
        </Frame>

        <Frame type="UnitStatusFrame" name="HealthAndManaUnitStatusFrame" template="UnitStatus/TargetPanelHealthAndManaUnitStatus">
            <Anchor relative="$parent"/>

            <Frame type="Frame" name="TargetStatsPanel">
                <Anchor side="Top" relative="$parent/$parent/TargetPortraitFrame" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

                <!-- Attack Stats -->
                <Frame type="Frame" name="AttackDamageFrame">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                    <AcceptsMouseTooltip val="true"/>
                    <TooltipAnchorPosition val="BottomCenter"/>
                    <Tooltip val="@UI/TargetInfoPanel/Tooltip/AttackDamage"/>
                    <Width val="90"/>
                    <Height val="30"/>

                    <Frame type="Image" name="AttackDamageIcon">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Texture val="@UI/Storm_UI_Attack_Damage_Icon"/>
                    </Frame>

                    <Frame type="UnitStatusDamageLabel" name="AttackDamageLabel">
                        <Anchor side="Top" relative="$parent/AttackDamageIcon" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent/AttackDamageIcon" pos="Max" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/AttackDamageIcon" pos="Mid" offset="0"/>
                        <FractionDigits val="1"/>
                        <Height val="30"/>
                        <Width val="60"/>

                        <Frame type="Label" name="StatusLabel">
                            <Anchor relative="$parent"/>
                            <Style val="Storm_TargetInfoPanel_Label"/>
                        </Frame>
                    </Frame>
                </Frame>

                <Frame type="Frame" name="AttackSpeedFrame">
                    <Anchor side="Top" relative="$parent/AttackDamageFrame" pos="Max" offset="-4"/>
                    <Anchor side="Left" relative="$parent/AttackDamageFrame" pos="Min" offset="0"/>
                    <AcceptsMouseTooltip val="true"/>
                    <TooltipAnchorPosition val="BottomCenter"/>
                    <Tooltip val="@UI/TargetInfoPanel/Tooltip/AttackSpeed"/>
                    <Width val="90"/>
                    <Height val="30"/>

                    <Frame type="Image" name="AttackSpeedIcon">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Texture val="@UI/Storm_UI_Attack_Speed_Icon"/>
                    </Frame>

                    <Frame type="UnitStatusAttackSpeedLabel" name="AttackSpeedLabel">
                        <Anchor side="Top" relative="$parent/AttackSpeedIcon" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent/AttackSpeedIcon" pos="Max" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/AttackSpeedIcon" pos="Mid" offset="0"/>
                        <RoundMode val="None"/>
                        <FractionDigits val="2"/>
                        <Height val="20"/>
                        <Width val="60"/>

                        <Frame type="Label" name="StatusLabel">
                            <Anchor relative="$parent"/>
                            <Style val="Storm_TargetInfoPanel_Label"/>
                        </Frame>
                    </Frame>
                </Frame>

                <Frame type="Frame" name="AttackRangeFrame">
                    <Anchor side="Top" relative="$parent/AttackSpeedFrame" pos="Max" offset="-4"/>
                    <Anchor side="Left" relative="$parent/AttackDamageFrame" pos="Min" offset="0"/>
                    <AcceptsMouseTooltip val="true"/>
                    <TooltipAnchorPosition val="BottomCenter"/>
                    <Tooltip val="@UI/TargetInfoPanel/Tooltip/AttackRange"/>
                    <Width val="90"/>
                    <Height val="30"/>

                    <Frame type="Image" name="AttackRangeIcon">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Texture val="@UI/Storm_UI_Attack_Range_Icon"/>
                    </Frame>

                    <Frame type="UnitStatusAttackRangeLabel" name="AttackRangeLabel">
                        <Anchor side="Top" relative="$parent/AttackRangeIcon" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent/AttackRangeIcon" pos="Max" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/AttackRangeIcon" pos="Mid" offset="0"/>
                        <RoundMode val="None"/>
                        <FractionDigits val="1"/>
                        <Height val="20"/>
                        <Width val="60"/>

                        <Frame type="Label" name="StatusLabel">
                            <Anchor relative="$parent"/>
                            <Style val="Storm_TargetInfoPanel_Label"/>
                        </Frame>
                    </Frame>
                </Frame>

                <!-- Armor Stats -->
                <Frame type="Frame" name="PhysicalArmorFrame">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="110"/>
                    <AcceptsMouseTooltip val="true"/>
                    <TooltipAnchorPosition val="BottomCenter"/>
                    <Tooltip val="@UI/TargetInfoPanel/Tooltip/PhysicalArmor"/>
                    <Width val="90"/>
                    <Height val="30"/>

                    <Frame type="Image" name="PhysicalArmorIcon">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Texture val="@UI/Storm_UI_Physical_Armor_Icon"/>
                    </Frame>

                    <Frame type="UnitStatusLabel" name="PhysicalArmorLabel">
                        <Anchor side="Top" relative="$parent/PhysicalArmorIcon" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent/PhysicalArmorIcon" pos="Max" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/PhysicalArmorIcon" pos="Mid" offset="0"/>
                        <Height val="20"/>
                        <Width val="60"/>

                        <CurrentValue val="{$this/ArmorValue/@CurrentPhysicalArmor}"/>
                        <MaxValue val="1000"/>
                        <HideMax val="True"/>

                        <Frame type="UnitStatusUnitArmor" name="ArmorValue">
                            <Anchor relative="$parent" offset="-5"/>
                            <UnitDamageType val="Hero"/>
                            <DamageKind val="Basic"/>
                        </Frame>

                        <Frame type="Label" name="StatusLabel">
                            <Anchor relative="$parent"/>
                            <Style val="Storm_TargetInfoPanel_Label"/>
                        </Frame>
                    </Frame>
                </Frame>

                <Frame type="Frame" name="AbilityArmorFrame">
                    <Anchor side="Top" relative="$parent/PhysicalArmorFrame" pos="Max" offset="-4"/>
                    <Anchor side="Left" relative="$parent/PhysicalArmorFrame" pos="Min" offset="0"/>
                    <AcceptsMouseTooltip val="true"/>
                    <TooltipAnchorPosition val="BottomCenter"/>
                    <Tooltip val="@UI/TargetInfoPanel/Tooltip/AbilityArmor"/>
                    <Width val="90"/>
                    <Height val="30"/>

                    <Frame type="Image" name="AbilityArmorIcon">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Texture val="@UI/Storm_UI_Ability_Armor_Icon"/>
                    </Frame>

                    <Frame type="UnitStatusLabel" name="AbilityArmorLabel">
                        <Anchor side="Top" relative="$parent/AbilityArmorIcon" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent/AbilityArmorIcon" pos="Max" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/AbilityArmorIcon" pos="Mid" offset="0"/>
                        <Height val="20"/>
                        <Width val="60"/>

                        <CurrentValue val="{$this/ArmorValue/@CurrentAbilityArmor}"/>
                        <MaxValue val="1000"/>
                        <HideMax val="True"/>

                        <Frame type="UnitStatusUnitArmor" name="ArmorValue">
                            <Anchor relative="$parent" offset="-5"/>
                            <UnitDamageType val="Hero"/>
                            <DamageKind val="Ability"/>
                        </Frame>

                        <Frame type="Label" name="StatusLabel">
                            <Anchor relative="$parent"/>
                            <Style val="Storm_TargetInfoPanel_Label"/>
                        </Frame>
                    </Frame>
                </Frame>

                <!-- Extra Stats -->
                <Frame type="Frame" name="AbilityPowerFrame">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="180"/>
                    <AcceptsMouseTooltip val="true"/>
                    <TooltipAnchorPosition val="BottomCenter"/>
                    <Tooltip val="@UI/TargetInfoPanel/Tooltip/AbilityPower"/>
                    <Width val="90"/>
                    <Height val="30"/>

                    <Frame type="Image" name="AbilityPowerIcon">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Texture val="@UI/Storm_UI_Spell_Power_Icon"/>
                    </Frame>

                    <Frame type="UnitStatusAbilityPowerLabel" name="AbilityPowerLabel">
                        <Anchor side="Top" relative="$parent/AbilityPowerIcon" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent/AbilityPowerIcon" pos="Max" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/AbilityPowerIcon" pos="Mid" offset="0"/>
                        <Height val="20"/>
                        <Width val="60"/>

                        <Frame type="Label" name="StatusLabel">
                            <Anchor relative="$parent"/>
                            <Style val="Storm_TargetInfoPanel_Label"/>
                        </Frame>
                    </Frame>
                </Frame>

                <Frame type="Frame" name="MoveSpeedFrame">
                    <Anchor side="Top" relative="$parent/AbilityPowerFrame" pos="Max" offset="-4"/>
                    <Anchor side="Left" relative="$parent/AbilityPowerFrame" pos="Min" offset="0"/>
                    <AcceptsMouseTooltip val="true"/>
                    <TooltipAnchorPosition val="BottomCenter"/>
                    <Tooltip val="@UI/TargetInfoPanel/Tooltip/MoveSpeed"/>
                    <Width val="90"/>
                    <Height val="30"/>

                    <Frame type="Image" name="MoveSpeedIcon">
                        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                        <Visible val="{$parent/UnitStatusMoveSpeedLabel/@Visible}"/>
                        <Texture val="@UI/Storm_UI_Move_Speed_Icon"/>
                    </Frame>

                    <Frame type="UnitStatusMoveSpeedLabel" name="UnitStatusMoveSpeedLabel">
                        <Anchor side="Top" relative="$parent/MoveSpeedIcon" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent/MoveSpeedIcon" pos="Max" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/MoveSpeedIcon" pos="Mid" offset="0"/>
                        <Width val="60"/>
                        <Height val="20"/>

                        <Frame type="Label" name="StatusLabel">
                            <Anchor relative="$parent"/>
                            <Style val="Storm_TargetInfoPanel_Label"/>
                        </Frame>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
    </Frame>
</Desc>
