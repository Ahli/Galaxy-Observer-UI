                <Frame type="Label" name="InstructionLabelWhite">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="-1"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="-1"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="-1"/>
                    <Text val="{$parent/@Text}"/>
                    <Style val="Storm_MapMechanic_OverwatchEvent_MVP_OpenBoxWhite"/>
                    <SubpixelRendering val="True"/>
                </Frame>
            </Frame>

            <Frame type="Label" name="OverwatchMVPTeamsTotal">
                <Anchor side="Bottom" relative="$parent/OverwatchMVPBoxesPercentOpened" pos="Max" offset="-5"/>
                <Anchor side="Left" relative="$parent/OverwatchMVPBoxesPercentOpened" pos="Max" offset="0"/>
                <Alpha val="0"/>
                <Text val="@UI/Event/OverwatchEvent/MapUI/MVPTeamTotal"/>
                <Style val="Storm_MapMechanic_OverwatchEvent_MVP_OpenBoxTextBlue"/>
                <SubpixelRendering val="True"/>
            </Frame>

        </Frame>
    </Frame>
<!-- End: Main Overwatch Event Template -->
           
    <Frame type="Frame" name="OverwatchEventTemplate">
        <Anchor relative="$parent"/>
        <Visible val="false"/>
        <AcceptsMouse val="False"/>

        <Frame type="Image" name="LootBoxImage">
            <Anchor side="Top" relative="$parent/PaneBlue" pos="Mid" offset="-50"/>
            <Anchor side="Bottom" relative="$parent/PaneBlue" pos="Mid" offset="-50"/>
            <Anchor side="Left" relative="$parent/PaneBlue" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/PaneBlue" pos="Mid" offset="0"/>
            <RenderPriority val="501"/>
            <Texture val="@UI/Storm_UI_splash_owevent_lootbox_small"/>
            <SubpixelRendering val="True"/>

            <Animation name="TurnOn">
                <Event event="OnShown" action="Restart, Play"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear"/>
                    <Key type="Curve" time="0.3" value="255" inout="Linear"/>
                </Controller>

                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause">
                    <Key type="Curve" time="0" value="50" inout="Linear"/>
                    <Key type="Curve" time="0.20" value="50" inout="Linear"/>
                    <Key type="Curve" time="0.25" value="110" inout="Linear"/>
                    <Key type="Curve" time="0.30" value="100" inout="Linear"/>
                </Controller>

                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause">
                    <Key type="Curve" time="0" value="50" inout="Linear"/>
                    <Key type="Curve" time="0.20" value="50" inout="Linear"/>
                    <Key type="Curve" time="0.25" value="110" inout="Linear"/>
                    <Key type="Curve" time="0.30" value="100" inout="Linear"/>
                </Controller>
            </Animation>

       </Frame>

        <Frame type="Label" name="OverwatchEventTimer">
            <Anchor side="Bottom" relative="$parent/PaneBlue" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/PaneBlue" pos="Min" offset="22"/>
            <Anchor side="Right" relative="$parent/PaneBlue" pos="Max" offset="-20"/>
            <RenderPriority val="501"/>
            <Style val="Storm_MapMechanic_OverwatchEvent_MechanicCounterCentered"/>
            <Text val="88:88"/>
            <SubpixelRendering val="True"/>
            <Visible val="True"/>

            <Animation name="TurnOn">
                <Event event="OnShown" action="Restart, Play"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time="0.30" value="0" inout="Linear"/>
                    <Key type="Curve" time="0.40" value="255" inout="Linear"/>
                </Controller>
            </Animation>

        </Frame>

        <Frame type="Image" name="PaneBlue">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-370"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-370"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Alpha val="255"/>
            <Texture val="@UI/Storm_UI_splash_owevent_pane_blue_b"/>
            <RenderPriority val="499"/>
            <SubpixelRendering val="True"/>

            <Animation name="TurnOn">
                <Event event="OnShown" action="Restart, Play"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time=".10" value="255" inout="Linear"/>
                </Controller>
            </Animation>

       </Frame>

       <Frame type="Frame" name="OverwatchEventOrder" template="OverwatchEvent/OverwatchEventFrameTemplate">
            <Anchor relative="$parent"/>
            <RenderPriority val="500"/>
            <Visible val="False"/>
        </Frame>


        <!-- The Chaos frame has the images and colors reveresed -->
        <Frame type="Frame" name="OverwatchEventChaos" template="OverwatchEvent/OverwatchEventFrameTemplate">
            <Anchor relative="$parent"/>
            <RenderPriority val="500"/>
            <Visible val="False"/>
            <Frame type="Frame" name="OverwatchEventMainFrame">

                <Frame type="Image" name="BackgroundContainer">
                    <TextureCoords top="0" left="1" bottom="1" right="0"/>
                </Frame>

                <Frame type="Label" name="LootBoxCounterLeft">
                    <Style val="Storm_MapMechanic_OverwatchEvent_Counter_Red"/>
                </Frame>

                <Frame type="Label" name="LootBoxCounterRight">
                    <Style val="Storm_MapMechanic_OverwatchEvent_Counter_Blue"/>
                </Frame>

                <Frame type="Image" name="RightPachimari">
                    <Texture val="@UI/Storm_UI_splash_owevent_doll_blue"/>
                </Frame>

                <Frame type="Image" name="RightX">
                    <Texture val="@UI/Storm_UI_splash_owevent_x_blue"/>
                </Frame>

                <Frame type="Image" name="LeftPachimari">
                    <Texture val="@UI/Storm_UI_splash_owevent_doll_red"/>
                </Frame>

                <Frame type="Image" name="LeftX">
                    <Texture val="@UI/Storm_UI_splash_owevent_x_red"/>
                </Frame>
            </Frame>

        </Frame>

    </Frame>

