<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

<!-- Start: Fail slash Template -->
    <Frame type="Frame" name="SlashAnimationTemplate">
        <Anchor relative="$parent"/>
        <Height val="110"/>
        <Width val="92"/>
        
        <!-- <AcceptsMouse val="True"/>
        <Animation name="Click">
            <Event event="OnMouseDown" action="Restart,Play"/>
            <Controller type="Event" end="Stop">
                <Key type="Event" time="0" event="NoGoldHere"/>
            </Controller>
        </Animation> -->
        
        <Frame type="Image" name="FailSlash1">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_splash_owevent_x_1"/>
            <Alpha val="0"/>
            <Animation name="NoGoldHere">
                <Event event="OnShown" action="Stop"/>
                <Event event="NoGoldHere" action="Restart,Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time=".03" value="255" inout="Linear"/>
                </Controller>
            </Animation>
        </Frame>
        <Frame type="Image" name="FailSlash2">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_splash_owevent_x_2"/>
            <Alpha val="0"/>
            <Animation name="NoGoldHere">
                <Event event="OnShown" action="Stop"/>
                <Event event="NoGoldHere" action="Restart,Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time=".1" value="0" inout="Linear"/>
                    <Key type="Curve" time=".13" value="255" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
<!-- End: Fail slash Template -->

<!-- Start: Golden Winner Pachimari Template -->
    <Frame type="Frame" name="GoldWinnerFrameTemplate">
        <Anchor relative="$parent"/>
        <Visible val="False"/>
        <Height val="600"/>
        <Unclipped val="true"/>

        <Animation name="TurnOn">
            <Event event="OnShown" action="Restart, Play"/>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="0.5" value="255" inout="Linear"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="GoldAnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_splash_owevent_gold_frame"/>
            <Alpha val="0"/>
       </Frame>

       <Frame type="Image" name="GoldFrame">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_splash_owevent_gold_frame"/>
            <Alpha val="255"/>
            <SubpixelRendering val="True"/>
            <Unclipped val="true"/>

            <Animation name="TurnOn">
                <Event event="OnShown" action="Restart, Play"/>

                <Controller type="Anchor" end="Pause" side="Top" relative="true">
                    <Key type="Curve" time="0" value="-200" inout="Linear"/>
                    <Key type="Curve" time="0.10" value="-200" inout="Linear"/>
                    <Key type="Curve" time="0.20" value="0" inout="Slow,Fast"/>
                    <Key type="Curve" time="0.30" value="-85" inout="Fast,Slow"/>
                    <Key type="Curve" time="0.38" value="0" inout="Slow,Fast"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                    <Key type="Curve" time="0" value="-200" inout="Linear"/>
                    <Key type="Curve" time="0.10" value="-200" inout="Linear"/>
                    <Key type="Curve" time="0.20" value="0" inout="Slow,Fast"/>
                    <Key type="Curve" time="0.30" value="-85" inout="Fast,Slow"/>
                    <Key type="Curve" time="0.38" value="0" inout="Slow,Fast"/>
                </Controller>
            </Animation>

       </Frame>

        <Frame type="Image" name="GoldLight">
            <Anchor side="Top" relative="$parent/GoldFrame" pos="Mid" offset="-60"/>
            <Anchor side="Bottom" relative="$parent/GoldFrame" pos="Mid" offset="-60"/>
            <Anchor side="Left" relative="$parent/GoldFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/GoldFrame" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_splash_owevent_light"/>
            <Alpha val="0"/>
            <Visible val="True"/>
            <Unclipped val="true"/>

            <Animation name="GoldLight">
                <Event event="GoldLight" action="Restart, Play"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time="1" value="204" inout="Linear"/>
                </Controller>

                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="PingPong">
                    <Key type="Curve" time="0" value="20" inout="Linear"/>
                    <Key type="Curve" time="1" value="25" inout="Linear"/>
                </Controller>

                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="PingPong">
                    <Key type="Curve" time="0" value="20" inout="Linear"/>
                    <Key type="Curve" time="1" value="25" inout="Linear"/>
                </Controller>
            </Animation>

            <SubpixelRendering val="True"/>
       </Frame>

       <Frame type="Image" name="GoldTurnip">
            <Anchor side="Top" relative="$parent/GoldFrame" pos="Mid" offset="-19"/>
            <Anchor side="Bottom" relative="$parent/GoldFrame" pos="Mid" offset="-19"/>
            <Anchor side="Left" relative="$parent/GoldFrame" pos="Mid" offset="-4"/>
            <Anchor side="Right" relative="$parent/GoldFrame" pos="Mid" offset="-4"/>
            <Texture val="@UI/Storm_UI_splash_owevent_doll_gold_small"/>
            <Alpha val="0"/>
            <SubpixelRendering val="True"/>
            <Unclipped val="true"/>

            <Animation name="TurnOn">
                <Event event="OnShown" action="Restart, Play"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time="0.25" value="0" inout="Linear"/>
                    <Key type="Curve" time="0.30" value="255" inout="Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Top" relative="true">
                    <Key type="Curve" time="0" value="-80" inout="Linear"/>
                    <Key type="Curve" time="0.30" value="-80" inout="Linear"/>
                    <Key type="Curve" time="0.35" value="0" inout="Slow,Fast"/>
                    <Key type="Curve" time="0.45" value="-45" inout="Fast,Slow"/>
                    <Key type="Curve" time="0.54" value="0" inout="Slow,Fast"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                    <Key type="Curve" time="0" value="-80" inout="Linear"/>
                    <Key type="Curve" time="0.30" value="-80" inout="Linear"/>
                    <Key type="Curve" time="0.35" value="0" inout="Slow,Fast"/>
                    <Key type="Curve" time="0.45" value="-45" inout="Fast,Slow"/>
                    <Key type="Curve" time="0.54" value="0" inout="Slow,Fast"/>
                </Controller>

                <Controller type="Animation" end="Pause" frame="$parent/GoldLight" animation="GoldLight">
                    <Key type="Animation" time="0.0" action="Reset"/>
                    <Key type="Animation" time="0.54" action="Play"/>
                </Controller>

                <Controller type="Animation" end="Pause" frame="$parent/GoldGlow" animation="GoldGlow">
                    <Key type="Animation" time="0.0" action="Reset"/>
                    <Key type="Animation" time="0.54" action="Play"/>
                </Controller>
            </Animation>
       </Frame>

       <Frame type="Image" name="GoldGlow">
            <Anchor side="Top" relative="$parent/GoldFrame" pos="Min" offset="-1"/>
            <Anchor side="Left" relative="$parent/GoldFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/GoldFrame" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_splash_owevent_gold_frame_glow"/>
            <Alpha val="0"/>
            <SubpixelRendering val="True"/>
            <Unclipped val="true"/>

            <Animation name="GoldGlow">
                <Event event="GoldGlow" action="Restart, Play"/>

                <Controller type="Fade" end="PingPong">
                    <Key type="Curve" time="0" value="102" inout="Linear"/>
                    <Key type="Curve" time="1" value="255" inout="Linear"/>
                </Controller>

            </Animation>
       </Frame>

       <Frame type="Label" name="GoldWinnerLabel">
            <Anchor side="Top" relative="$parent/GoldAnchorFrame" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/GoldAnchorFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/GoldAnchorFrame" pos="Mid" offset="0"/>
            <Text val="@UI/Event/OverwatchEvent/MapUI/GoldenVictory"/>
            <Alpha val="255"/>
            <Style val="Storm_MapMechanic_OverwatchEvent_Winner_Gold"/>
            <SubpixelRendering val="True"/>
            <RenderPriority val="521"/>
            <Width val="375"/>
            <Options val="ScaleToPreventTruncation"/>
            <Unclipped val="true"/>

            <Animation name="TurnOn">
                <Event event="OnShown" action="Restart, Play"/>

                <Controller type="Anchor" end="Pause" side="Left" relative="true">
                    <Key type="Curve" time="0" value="-300" inout="Linear"/>
                    <Key type="Curve" time=".2" value="-300" inout="Linear"/>
                    <Key type="Curve" time=".5" value="-20" inout="linear,Linear"/>
                    <Key type="Curve" time="2" value="0" inout="linear,Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Right" relative="true">
                    <Key type="Curve" time="0" value="-300" inout="Linear"/>
                    <Key type="Curve" time=".2" value="-300" inout="Linear"/>
                    <Key type="Curve" time=".5" value="-20" inout="linear,Linear"/>
                    <Key type="Curve" time="2" value="0" inout="linear,Linear"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
<!-- End: Golden Winner Pachimari Template -->

<!-- Start: Main Overwatch Event Template -->
    <Frame type="Frame" name="OverwatchEventFrameTemplate">
        <Anchor relative="$parent"/>
        <RenderPriority val="500"/>
        <Visible val="False"/>
        <AcceptsMouse val="False"/>
        <Frame type="Frame" name="OverwatchEventMainFrame">
            <Anchor relative="$parent"/>
            <AcceptsMouse val="False"/>

            <!-- <AcceptsMouse val="True"/>
            <Animation name="Click">
                <Event event="OnMouseDown" action="Restart,Play"/>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="None"/>
                </Controller>
            </Animation> -->


        <!-- Start: Animation to fade out the main parts of the UI so we can display the post event UI -->
            <Animation name="Fadeout">
                <Event event="Fadeout" action="Restart,Play"/>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/BackgroundContainer">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/LootBoxCounterLeft">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/LootBoxCounterRight">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/RightPachimari">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/LeftPachimari">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/GoldenPachimariWinnerRight">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/GoldenPachimariWinnerLeft">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/RightX">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/LeftX">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/RightHorizontalLineConnector">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/RightVerticleLineConnector">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/LeftHorizontalLineConnector">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/LeftVerticleLineConnector">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/$parent/$parent/SeasonalEventPanel/LootBoxImage">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/$parent/$parent/SeasonalEventPanel/OverwatchEventTimer">
                    <Key type="Curve" time="0" value="255" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                </Controller>
            </Animation>
        <!-- End: Animation to fade out the main parts of the UI so we can display the post event UI -->

        <!-- Start: Animation for showing the Golden Pachimari Success or Fail State -->
            <Animation name="TeamVictory">
                <Event event="TeamVictory" action="Restart,Play"/>

                <Controller type="Visibility" end="Pause" frame="$parent/OverwatchEventMainFrame/GoldenPachimariWinner">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="0" value="True"/>
                </Controller>
            </Animation>

            <Animation name="TeamLoss">
                <Event event="TeamLoss" action="Restart,Play"/>

                <Controller type="Visibility" end="Pause" frame="$parent/OverwatchEventMainFrame/GoldenPachimariLoser">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="0" value="True"/>
                </Controller>
            </Animation>
        <!-- End: Animation for showing the Golden Pachimari Success or Fail State -->

        <!-- Start: MVP Animation for the Winning Team -->
            <Animation name="WinningTeamMVPAnimation">
                <Event event="WinningTeamMVPAnimation" action="Restart,Play"/>

                <Controller type="Visibility" end="Pause" frame="$parent/OverwatchEventMainFrame/GoldenPachimariWinner">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time="0" value="False"/>
                </Controller>

                <Controller type="Visibility" end="Pause" frame="$parent/OverwatchEventMainFrame/GoldenPachimariLoser">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time="0" value="False"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$parent/OverwatchEventMainFrame/BackgroundContainer">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="160" inout="Linear,Linear"/>
                    <Key type="Curve" time="4" value="170" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$parent/OverwatchEventMainFrame/BackgroundContainer">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="160" inout="Linear,Linear"/>
                    <Key type="Curve" time="4" value="170" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$parent/OverwatchEventMainFrame/SlashLeftWinner">
                    <Key type="Curve" time="0" value="-50" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/SlashLeftWinner">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="255" inout="Linear,Linear"/>
                </Controller>
                
                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/LeftPachimari">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/LeftVerticleLineConnector">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/OverwatchMVPTitleLabel">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.4" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/OverwatchMVPHeroNameLabel">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.3" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.5" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/OverwatchMVPHeroTypeLabel">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.3" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.5" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/OverwatchMVPBoxesOpenedNumber">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.4" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.6" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/OverwatchMVPBoxesOpened">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.4" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.6" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/OverwatchMVPBoxesPercentOpened">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.4" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.6" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/OverwatchMVPTeamsTotal">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.4" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.6" value="255" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$parent/OverwatchEventMainFrame/OverwatchMVPTitleLabel">
                    <Key type="Curve" time="0" value="170" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="170" inout="Linear,Linear"/>
                    <Key type="Curve" time=".5" value="30" inout="Linear,Linear"/>
                    <Key type="Curve" time="3" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$parent/OverwatchEventMainFrame/OverwatchMVPHeroNameLabel">
                    <Key type="Curve" time="0" value="170" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="170" inout="Linear,Linear"/>
                    <Key type="Curve" time=".6" value="50" inout="Linear,Linear"/>
                    <Key type="Curve" time="3" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$parent/OverwatchEventMainFrame/OverwatchMVPBoxesOpenedNumber">
                    <Key type="Curve" time="0" value="170" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="170" inout="Linear,Linear"/>
                    <Key type="Curve" time=".7" value="60" inout="Linear,Linear"/>
                    <Key type="Curve" time="3" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Property" property="Texture" end="Pause" frame="$parent/OverwatchEventMainFrame/LeftPachimari">
                      <Key type="Property" time="0" value="@UI/Storm_UI_splash_owevent_doll_blue"/>
                </Controller>
            </Animation>
        <!-- End: MVP Animation for the Winning Team -->

        <!-- Start: MVP Animation for the Losing Team -->
            <Animation name="LosingTeamMVPAnimation">
                <Event event="LosingTeamMVPAnimation" action="Restart,Play"/>

                <Controller type="Visibility" end="Pause" frame="$parent/OverwatchEventMainFrame/GoldenPachimariWinner">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time="0" value="False"/>
                </Controller>

                <Controller type="Visibility" end="Pause" frame="$parent/OverwatchEventMainFrame/GoldenPachimariLoser">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time="0" value="False"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$parent/OverwatchEventMainFrame/BackgroundContainer">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="160" inout="Linear,Linear"/>
                    <Key type="Curve" time="4" value="170" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Right" relative="true" frame="$parent/OverwatchEventMainFrame/BackgroundContainer">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="160" inout="Linear,Linear"/>
                    <Key type="Curve" time="4" value="170" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$parent/OverwatchEventMainFrame/SlashLeftWinner">
                    <Key type="Curve" time="0" value="-50" inout="Linear,Linear"/>
                    <Key type="Curve" time=".2" value="0" inout="Linear,Linear"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/OverwatchEventMainFrame/SlashLeftWinner">
                    <Key type="Curve" time="0" value="0" inout="Linear,Linear"/>
                    <Key type="Curve" time="0.2" value="255" inout="Linear,Linear"/>
