    <!-- Blinking Energy Heroes (Probius) -->
    <Frame type="UnitStatusFrame" name="HeroUnitStatusConsoleFrameBlinkingEnergyTemplate" template="UnitStatus/HeroUnitStatusConsoleFrameTemplate">
    
        <Frame type="UnitStatusValidatorAnimElement" name="ChargingAnims">
            <Anchor relative="$parent"/>
            <Validator val="ProbiusNearPylonOrOverchargedPylon"/>
            <Validator val="ProbiusNotNearPylonAndNotNearOverchargedPylon"/>
                
            <Animation name="Recharging">
                <Event event="ProbiusNearPylonOrOverchargedPylon" action="Reset,Play"/>
                
                <Controller type="Visibility" end="Pause"  frame="$parent/EnergyBar/RechargeFlash">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>
            
             <Animation name="NotRecharging">
                <Event event="ProbiusNotNearPylonAndNotNearOverchargedPylon" action="Reset,Play"/>
                
                <Controller type="Visibility" end="Pause"  frame="$parent/EnergyBar/RechargeFlash">
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="UnitStatusValidatorAnimElement" name="FullManaAnims">
                <Anchor relative="$parent"/>
                <Validator val="EnergyFullCaster"/>
                    
                <Animation name="EnergyFull">
                    <Event event="EnergyFullCaster" action="Reset,Play"/>
                    
                    <Controller type="Visibility" end="Pause"  frame="$parent/EnergyBar/FullManaFlash">
