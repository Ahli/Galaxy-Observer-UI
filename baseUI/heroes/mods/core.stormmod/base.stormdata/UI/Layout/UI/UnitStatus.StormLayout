        <StateGroup name="HealingBonusIconController">
            <DefaultState val="Neutral"/>
            <State name="TemporaryBuff">
                <When type="Property" HealingModification="0" operator="GreaterThan"/>
                <When type="Property" DurationMax="0" operator="GreaterThan"/>
            </State>
            <State name="Buff">
                <When type="Property" HealingModification="0" operator="GreaterThan"/>
                <When type="Property" DurationMax="0"/>
            </State>
            <State name="Debuff">
                <When type="Property" HealingModification="0" operator="LessThan"/>
                <When type="Property" HealingModification="-100" operator="GreaterThan"/>
                <When type="Property" DurationMax="0" operator="GreaterThan"/>
                <When type="Option" colorblind="False"/>
                <Action type="SetProperty" frame="StatusLabel" Style="UnitStatusNegativeArmor"/>
                <Action type="SetProperty" frame="StatusLabelFormatSign" FormatText="%0%"/>
                <Action type="SetProperty" frame="StatusImage" Texture="@UI/Storm_UI_InGame_Status_HinderedHealing"/>
                <Action type="SetProperty" frame="DurationBar/FillImageContainer/FillImage" Texture="@UI/Storm_UI_InGame_Status_HinderedHealing_Negative"/>
            </State>
            <State name="DebuffMax">
                <When type="Property" HealingModification="-100"/>
                <When type="Property" DurationMax="0" operator="GreaterThan"/>
                <When type="Option" colorblind="False"/>
                <Action type="SetProperty" frame="StatusLabel" Style="UnitStatusNoHealing"/>
                <Action type="SetProperty" frame="StatusLabelFormatSign" FormatText="%0%"/>
                <Action type="SetProperty" frame="StatusImage" Texture="@UI/HealingDisplay/None/Icon"/>
                <Action type="SetProperty" frame="DurationBar/FillImageContainer/FillImage" Texture="@UI/HealingDisplay/None/DurationRing"/>
                <Action type="SetProperty" frame="TextFlareImage" Visible="true"/>
            </State>
            <State name="ColorblindDebuff">
                <When type="Property" HealingModification="0" operator="LessThan"/>
                <When type="Property" HealingModification="-100" operator="GreaterThan"/>
                <When type="Property" DurationMax="0" operator="GreaterThan"/>
                <When type="Option" colorblind="True"/>
                <Action type="SetProperty" frame="StatusLabel" Style="UnitStatusNegativeArmorColorblind"/>
                <Action type="SetProperty" frame="StatusLabelFormatSign" FormatText="%0%"/>
                <Action type="SetProperty" frame="StatusImage" Texture="@UI/Storm_UI_InGame_Status_HinderedHealing_CB"/>
                <Action type="SetProperty" frame="DurationBar/FillImageContainer/FillImage" Texture="@UI/Storm_UI_InGame_Status_HinderedHealing_Negative_CB"/>
            </State>
            <State name="ColorblindDebuffMax">
                <When type="Property" HealingModification="-100"/>
                <When type="Property" DurationMax="0" operator="GreaterThan"/>
                <When type="Option" colorblind="True"/>
                <Action type="SetProperty" frame="StatusLabel" Style="UnitStatusNoHealingColorblind"/>
                <Action type="SetProperty" frame="StatusLabelFormatSign" FormatText="%0%"/>
                <Action type="SetProperty" frame="TextFlareImage" Texture="@UI/HealingDisplay/None/TextFlareCB"/>
                <Action type="SetProperty" frame="StatusImage" Texture="@UI/HealingDisplay/None/IconCB"/>
                <Action type="SetProperty" frame="DurationBar/FillImageContainer/FillImage" Texture="@UI/HealingDisplay/None/DurationRingCB"/>
                <Action type="SetProperty" frame="TextFlareImage" Visible="true"/>
            </State>
            <State name="Neutral">
                <Action type="SetProperty" frame="StatusImage" Visible="false"/>
                <Action type="SetProperty" frame="StatusLabel" Visible="false"/>
                <Action type="SetProperty" frame="BackgroundImage" Visible="false"/>
                <Action type="SetProperty" frame="DurationBar" Visible="false"/>
            </State>
        </StateGroup>
    </Frame>
