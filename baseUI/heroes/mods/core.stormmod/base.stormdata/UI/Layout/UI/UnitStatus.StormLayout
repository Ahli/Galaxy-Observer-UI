        <DecreaseDuration val="#LifeLossDuration" index="1"/>
        <OwnerColor val="255, 255, 255" index="1"/>
        <AllyColor val="255, 255, 255" index="1"/>
        <EnemyColor val="255, 255, 255" index="1"/>
