            </Controller>
        </Animation>
        
        <Animation name="Progress3">
            <Event event="Progress3" action="Play"/>
            <Controller type="Anchor" end="Pause" side="Right" relative="true">
                <Key type="Curve" time="0.0" value="-120" inout="Linear,Auto"/>
                <Key type="Curve" time="1" value="-180" inout="Auto,Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="Progress4">
            <Event event="Progress4" action="Play"/>
            <Controller type="Anchor" end="Pause" side="Right" relative="true">
                <Key type="Curve" time="0.0" value="-180" inout="Linear,Auto"/>
