<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    
    <Frame type="Frame" name="RoosterPortraitTemplate">
		<Width val="96"/>
		<Height val="104"/>
        <UseAlternateTime val="true"/>
        <AcceptsMouse val="False"/>
        <Unclipped val="True"/>
        
        <Frame type="Image" name="PortraitImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="96"/>
            <Height val="104"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Rooster_Blue"/>
            <SubpixelRendering val="True"/>
        </Frame>
        
    </Frame>
    
    <Frame type="Image" name="FlowerTemplate">
            <Anchor side="Top" relative="$parent/CutsceneBorder" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/CutsceneBorder" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/CutsceneBorder" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/CutsceneBorder" pos="Mid" offset="0"/>
            <Unclipped val="True"/>
            <Visible val="False"/>
            <ColorAdjustMode val="AddSelf"/>
            <AdjustmentColor val="255,255,255,255"/>
            <RenderPriority val="750"/>
            
            <Animation name="FlowerBirth">
                <Event event="OnShown" action="Reset,Play"/>
                
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" inout="Linear,Auto"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause">
                    <Key type="Curve" time="0" value="40" inout="Linear,Auto"/>
                    <Key type="Curve" time=".25" value="100" inout="Auto,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause">
                    <Key type="Curve" time="0" value="40" inout="Linear,Auto"/>
                    <Key type="Curve" time=".25" value="100" inout="Auto,Linear"/>
                </Controller>
                <Controller type="AdjustmentColor" end="Pause">
                    <Key type="Curve" time="0" value="255,255,255,255" inout="Linear,Auto"/>
                    <Key type="Curve" time="0.75" value="0,255,255,255" inout="Auto"/>
                </Controller>
            </Animation>
            
            <Animation name="FlowerFade">
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" inout="Linear,Auto"/>
                    <Key type="Curve" time=".75" value="0" inout="Auto,Linear"/>
                </Controller>
            </Animation>
            
    </Frame>
    


    <!-- Rooster portrait frames for left timer bar -->
    <Frame type="Frame" name="RoosterPortraitLeftTemplate" template="LunarNewYearRooster/RoosterPortraitTemplate">
    
        <Animation name="RoosterBirth">
            <Event event="RoosterBirth" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time=".99" value="0" inout="Auto,Linear"/>
                <Key type="Curve" time="1" value="255" inout="Auto,Linear"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time="1" value="40" inout="auto,Auto"/>
                <Key type="Curve" time="1.5" value="0" inout="Auto,Linear"/>
            </Controller>
        </Animation>
    
        <Frame type="Image" name="PortraitImage">
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Rooster_Blue"/>
        </Frame>
        
        <Animation name="Progress1">
            <Event event="Progress1" action="Play"/>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0.0" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time="1" value="60" inout="Auto,Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="Progress2">
            <Event event="Progress2" action="Play"/>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0.0" value="60" inout="Linear,Auto"/>
                <Key type="Curve" time="1" value="120" inout="Auto,Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="Progress3">
            <Event event="Progress3" action="Play"/>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0.0" value="120" inout="Linear,Auto"/>
                <Key type="Curve" time="1" value="180" inout="Auto,Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="Progress4">
            <Event event="Progress4" action="Play"/>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0.0" value="180" inout="Linear,Auto"/>
                <Key type="Curve" time="1" value="240" inout="Auto,Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="RoosterBob">
            <Event event="RoosterBob" action="Reset,Play"/>
            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0.0" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time="0.125" value="-10" inout="Auto,Linear"/>
                <Key type="Curve" time="0.25" value="0" inout="Auto,Linear"/>
                <Key type="Curve" time="0.375" value="-10" inout="Auto,Linear"/>
                <Key type="Curve" time="0.5" value="0" inout="Auto,Linear"/>
                <Key type="Curve" time="0.625" value="-10" inout="Linear,Auto"/>
                <Key type="Curve" time="0.75" value="0" inout="Auto,Linear"/>
                <Key type="Curve" time="0.875" value="-10" inout="Auto,Linear"/>
                <Key type="Curve" time="1" value="0" inout="Auto,Linear"/>
            </Controller>
        </Animation>
        
    </Frame>
    
    <!-- Rooster portrait frames for right timer bar -->
    <Frame type="Frame" name="RoosterPortraitRightTemplate" template="LunarNewYearRooster/RoosterPortraitTemplate">
    
        <Animation name="RoosterBirth">
