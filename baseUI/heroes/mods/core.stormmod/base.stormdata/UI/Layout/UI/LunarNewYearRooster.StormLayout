            </Animation>
            
            <Animation name="SheenWipe">
                <Event event="SheenWipe" action="Reset, Play"/>
                
                <Controller type="LayerUV" end="Pause" side="Left" layer="1" relative="True" sidelock="True">
                    <Key type="Curve" time="0" value="1" inout="Linear,Auto"/>
                    <Key type="Curve" time="5" value="-1" inout="Auto,Linear"/>
                </Controller>
            </Animation>
            
        </Frame>
        
        <Frame type="Frame" name="RaceBarDividerFrame">
            <Anchor side="Top" relative="$parent/TimerBar" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/TimerBar" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/RoosterFrameRight" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/CutsceneBorder" pos="Mid" offset="0"/>
            
            <Animation name="ChangeAnchorsOnBirth">
                <Event event="OnShown" action="Play"/>
                <Controller type="Anchor" end="Pause" side="Right" relative="true">
                    <Key type="Curve" time="0" value="500" inout="Linear,Auto"/>
                    <Key type="Curve" time="1" value="500" inout="Auto"/>
                    <Key type="Curve" time="2" value="0" inout="Auto,Linear"/>
                </Controller>
            </Animation>
            
            <Frame type="Image" name="RaceBarDividers">
                <Anchor relative="$parent/$parent/TimerBar"/>
                <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Gold_Dividers"/>
                <TextureCoords top="0.0" left="1.0" bottom="1.0" right="0.0"/>
            </Frame>
        </Frame>
        
        
        <Frame type="Image" name="RaceBarScrollHandle">
            <Width val="44"/>
            <Height val="112"/>
            <Anchor side="Top" relative="$parent/TimerBar" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/TimerBar" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Max" offset="-32"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Handle_Red"/>
            <Unclipped val="True"/>
        </Frame>
        
        
    </Frame>
    
    
    <Frame type="Frame" name="LunarNewYearRoosterTemplate">
        <Anchor relative="$parent"/>
        <Visible val="false"/>
        
        <Animation name="PanelShown">
            <Event event="OnShown" action="Reset,Play"/>
            <Controller type="Visibility" end="Pause" frame="CutsceneBorderBackgroundFlare">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".15" value="True"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="OfferingLabel">
                <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time="0.75" value="0" inout="Auto"/>
                <Key type="Curve" time="1" value="255" inout="Auto,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="RaceTimeLeftLabel">
                <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time="0.75" value="0" inout="Auto"/>
                <Key type="Curve" time="1" value="255" inout="Auto,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="OfferingCutscene">
                <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
            </Controller>
            <Controller type="Event" end="Pause" frame="OfferingCutscene">
                <Key type="Event" time="0.75" event="FadeCutsceneIn"/>
            </Controller>
            
            <!--Controller type="Fade" end="Pause" frame="CutsceneBorderScrollFlare">
                <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time="0.5" value="0" inout="Auto"/>
                <Key type="Curve" time="1" value="255" inout="Auto,Linear"/>
            </Controller-->
            
            
            
            <Controller type="Event" end="Pause" frame="RoosterFrameLeft">
                <Key type="Event" time="0" event="RoosterBirth"/>
            </Controller>
            <Controller type="Event" end="Pause" frame="RoosterFrameLeftChaos">
                <Key type="Event" time="0" event="RoosterBirth"/>
            </Controller>
            <Controller type="Event" end="Pause" frame="RoosterFrameRight">
                <Key type="Event" time="0" event="RoosterBirth"/>
            </Controller>
            <Controller type="Event" end="Pause" frame="RoosterFrameRightChaos">
                <Key type="Event" time="0" event="RoosterBirth"/>
            </Controller>
        </Animation>
        
        <Animation name="ShowQuestCompletedLabel">
            <Event event="ShowQuestCompletedLabel" action="Reset,Play"/>
            <Controller type="Visibility" end="Stop" frame="QuestsCompletedLabelLeft">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".75" value="True"/>
            </Controller>
        </Animation>
        
        <Animation name="FadePanelOut">
            <Event event="FadePanelOut" action="Reset,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="255" inout="Linear,Auto"/>
                <Key type="Curve" time="1" value="0" inout="Auto,Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="ShowFlower">
            <Event event="OnShown" action="Reset,Play"/>
            <Event event="VictoryFlower" action="Reset,Play"/>
            <Controller type="Event" end="Stop">
                <Key type="Event" time="0" event="FlowerOn"/>
            </Controller>
        </Animation>
        
        <Animation name="FlowerOn">
            <Event event="FlowerOn" action="Reset,Play"/>
            
            
            <Controller type="Visibility" end="Pause" frame="Flower1">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".05" value="True"/>
                <Key type="Bool" time="3" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Flower2">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".1" value="True"/>
                <Key type="Bool" time="3" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Flower3">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".15" value="True"/>
                <Key type="Bool" time="3" value="False"/>
            </Controller>
            
            <Controller type="Animation" end="Pause" animation="FlowerFade" frame="Flower1">
                <Key type="Animation" time="1.05" action="Reset"/>
                <Key type="Animation" time="1.05" action="Play"/>
            </Controller>
            <Controller type="Animation" end="Pause" animation="FlowerFade" frame="Flower2">
                <Key type="Animation" time="1.1" action="Reset"/>
                     <Key type="Animation" time="1.1" action="Play"/>
            </Controller>
            <Controller type="Animation" end="Pause" animation="FlowerFade" frame="Flower3">
                <Key type="Animation" time="1.15" action="Reset"/>
                <Key type="Animation" time="1.15" action="Play"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="Flower1" template="LunarNewYearRooster/FlowerTemplate">
            <Height val="148"/>
            <Height val="148"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Flower_01"/>
            <RenderPriority val="601"/>
        </Frame>
        
        <Frame type="Image" name="Flower2" template="LunarNewYearRooster/FlowerTemplate">
            <Height val="168"/>
            <Height val="168"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Flower_02"/>
            <RenderPriority val="601"/>
        </Frame>
        
        <Frame type="Image" name="Flower3" template="LunarNewYearRooster/FlowerTemplate">
            <Height val="168"/>
            <Height val="168"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Flower_03"/>
            <RenderPriority val="601"/>
        </Frame>
        
        <Frame type="Label" name="QuestsCompletedLabelLeft">
            <Width val="100"/>
            <Height val="100"/>
            <RenderPriority val="700"/>
            <Anchor side="Left" relative="$parent/Team1RaceBar/TimerBar" pos="Min" offset="3"/>
            <Anchor side="Bottom" relative="$parent/Team1RaceBar/TimerBar" pos="Max" offset="5"/>
            <Style val="Storm_Events_LNYRooster_ReturnOffering"/>
            
            <Animation name="FadeIn">
                <Event event="OnShown" action="Reset,Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                    <Key type="Curve" time=".1" value="255" inout="Auto,Linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="QuestsCompletedLabelRight">
            <Width val="100"/>
            <Height val="100"/>
            <RenderPriority val="700"/>
            <Anchor side="Right" relative="$parent/Team2RaceBar/TimerBar" pos="Max" offset="3"/>
            <Anchor side="Bottom" relative="$parent/Team2RaceBar/TimerBar" pos="Max" offset="5"/>
            <Style val="Storm_Events_LNYRooster_ReturnOffering"/>
            
            <Animation name="FadeIn">
                <Event event="OnShown" action="Reset,Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                    <Key type="Curve" time=".1" value="255" inout="Auto,Linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="QuestCompleteImage">
            <Width val="84"/>
            <Height val="84"/>
