            <TextureCoords top="0.0" left="1.0" bottom="1.0" right="0.0" layer="1"/>
            <LayerCount val="2"/>
            
            <Animation name="PeriodicSheenWipe">
                <Event event="OnShown" action="Reset, Play"/>
