            </Controller>
        </Animation>
        
        <Animation name="RoosterBob">
            <Event event="RoosterBob" action="Reset,Play"/>
            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0.0" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time="0.125" value="-10" inout="Auto,Linear"/>
                <Key type="Curve" time="0.25" value="0" inout="Auto,Linear"/>
                <Key type="Curve" time="0.375" value="-10" inout="Auto,Linear"/>
                <Key type="Curve" time="0.5" value="0" inout="Auto,Linear"/>
                <Key type="Curve" time="0.625" value="-10" inout="Linear,Auto"/>
                <Key type="Curve" time="0.75" value="0" inout="Auto,Linear"/>
                <Key type="Curve" time="0.875" value="-10" inout="Auto,Linear"/>
                <Key type="Curve" time="1" value="0" inout="Auto,Linear"/>
            </Controller>
        </Animation>
       
    </Frame>
    
    <!-- Death Timer Frame master template -->
    <Frame type="Frame" name="RoosterRaceFrameTemplate">
        <Width val="410"/>
        <Height val="100"/>
        
        <!-- Basic timer bar elements -->

        <Frame type="Image" name="TimerBar"/>
    </Frame>
    
    <!-- Rooster Race Frame Left Side -->
    <Frame type="Frame" name="LeftSideTeamRoosterRaceTemplate" template="LunarNewYearRooster/RoosterRaceFrameTemplate">
    
        <Animation name="Show">
            <Event event="OnShown" action="Play,Reset"/>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
                <Key type="Curve" time="0.0" value="390" inout="Linear,Auto"/>
                <Key type="Curve" time="1" value="0" inout="Auto,Linear"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="TimerBar">
            <Width val="392"/>
            <Height val="88"/>
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <!-- the texture for this is set to the opposite color for team 2 in script -->
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Blue"/>
            <TextureType val="Nineslice"/>
  
        </Frame>
        
        <Frame type="Image" name="RaceBarGoldTrim">
            <Anchor relative="$parent/TimerBar"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Gold_Mask_Blue"/>
            <ImageMaskTexture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Gold_Sheen"/>
        </Frame>
        
        <Frame type="Image" name="RaceBarGoldSheen">
            <Anchor relative="$parent/TimerBar"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Gold_Mask_Blue" layer="0"/>
            <AlphaLayer val="true" layer="0"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Gold_Sheen" layer="1"/>
            <LayerCount val="2"/>
            
            <Animation name="PeriodicSheenWipe">
                <Event event="OnShown" action="Reset, Play"/>
                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0" event="SheenWipe"/>
                    <Key type="Event" time="7" event="SheenWipe"/>
                    <Key type="Event" time="14" event="SheenWipe"/>
                    <Key type="Event" time="21" event="SheenWipe"/>
                </Controller>
            </Animation>
           
            
            <Animation name="SheenWipe">
                <Event event="SheenWipe" action="Reset, Play"/>
                
                <Controller type="LayerUV" end="Pause" side="Left" layer="1" relative="False" sidelock="True">
                    <Key type="Curve" time="0" value="1" inout="Linear,Auto"/>
                    <Key type="Curve" time="5" value="-1" inout="Auto,Linear"/>
                </Controller>
            </Animation>
            
        </Frame>
        
        <Frame type="Frame" name="RaceBarDividerFrame">
            <Anchor side="Top" relative="$parent/TimerBar" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/TimerBar" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/RoosterFrameLeft" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/CutsceneBorder" pos="Mid" offset="0"/>
            
            <Animation name="ChangeAnchorsOnBirth">
                <Event event="OnShown" action="Play"/>
                <Controller type="Anchor" end="Pause" side="Left" relative="true">
                    <Key type="Curve" time="0" value="-500" inout="Linear,Auto"/>
                    <Key type="Curve" time="1" value="-500" inout="Auto"/>
                    <Key type="Curve" time="2" value="0" inout="Auto,Linear"/>
                </Controller>
            </Animation>
            
            
            <Frame type="Image" name="RaceBarDividers">
                <Anchor relative="$parent/$parent/TimerBar"/>
                <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Gold_Dividers"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="RaceBarScrollHandle">
            <Width val="44"/>
            <Height val="112"/>
            <Anchor side="Top" relative="$parent/TimerBar" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/TimerBar" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Min" offset="32"/>
            <Texture val="@UI/Storm_UI_HUD_Event_Lunar_2017_Scroll_Handle_Blue"/>
            <Unclipped val="True"/>
        </Frame>
        
    </Frame>
    
    <!-- Rooster Race Timer Frame Right Side -->
    <Frame type="Frame" name="RightSideTeamRoosterRaceTemplate" template="LunarNewYearRooster/RoosterRaceFrameTemplate">

        <Animation name="Show">
            <Event event="OnShown" action="Play,Reset"/>
            <Controller type="Anchor" end="Pause" side="Right" relative="true">
                <Key type="Curve" time="0.0" value="-390" inout="Linear,Auto"/>
