          <DefaultState val="Enemy" />
          <State name="Owner">
            <When type="Property" frame="$parent" CurrentRelation="Owner" />
            <Action type="SetProperty" frame="$this" Texture="@UI/Storm_UnitStatus_UnitContainer_Circle_Green" undo="false" />
