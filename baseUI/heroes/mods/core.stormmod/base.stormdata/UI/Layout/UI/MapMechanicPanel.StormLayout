                </Frame>

                <Frame type="Image" name="RavenIcon">
                    <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="25"/>
                    <Anchor side="Right" relative="$parent/Background" pos="Max" offset="20"/>
                    <Texture val="@UI/HUD_MapMechanic_CursedHollow_WingBlue"/>

                    <ColorAdjustMode val="Colorize"/>
                    <DarkAdjustColor val="{$GameUI/TeamChaosColors/@Color}"/>
                </Frame>

