<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    
    <!-- Basic Mechanic Template -->
    <Frame type="Frame" name="MechanicFrameTemplate">
        <Frame type="Frame" name="MinimapProxyFrame">
            <Anchor side="Top" relative="$parent/$parent/$parent/MinimapPanel/Minimap" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/$parent/$parent/MinimapPanel/Minimap" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/$parent/MinimapPanel/Minimap" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/MinimapPanel/Minimap" pos="Max" offset="0"/>
        </Frame>
    </Frame>
    
    <!-- Collection template. Mines Skull/Tributes/Coins -->
    <Frame type="Frame" name="TeamCollectionFrameTemplate">
        <Anchor relative="$parent"/>
        
        <Frame type="Frame" name="AnchorPointFrame">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/$parent/$parent/$parent/MinimapPanel/Minimap" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/$parent/$parent/$parent/MinimapPanel/Minimap" pos="Mid" offset="0"/>
        </Frame>

        <Frame type="Image" name="Team1Background">
            <Anchor side="Bottom" relative="$parent/AnchorPointFrame" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/AnchorPointFrame" pos="Mid" offset="1"/>
            <Texture val="@UI/HUD_MapMechanic_BG_TeamHighlight_Blue"/>

            <ColorAdjustMode val="Colorize"/>
            <DarkAdjustColor val="{$GameUI/TeamOrderColors/@Color}"/>
        </Frame>

        <Frame type="Image" name="Team2Background">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/AnchorPointFrame" pos="Mid" offset="-1"/>
            <Texture val="@UI/HUD_MapMechanic_BG_TeamHighlight_Blue"/>
            <TextureCoords top="0.000000" left="1.000000" bottom="1.000000" right="0.000000"/>

            <ColorAdjustMode val="Colorize"/>
            <DarkAdjustColor val="{$GameUI/TeamChaosColors/@Color}"/>
        </Frame>

        <Frame type="Image" name="Team1BackgroundBlue" template="StandardTemplates/RemoveThisFrameTemplate"/>
        <Frame type="Image" name="Team1BackgroundRed" template="StandardTemplates/RemoveThisFrameTemplate"/>
        <Frame type="Image" name="Team2BackgroundRed" template="StandardTemplates/RemoveThisFrameTemplate"/>
        <Frame type="Image" name="Team2BackgroundBlue" template="StandardTemplates/RemoveThisFrameTemplate"/>

        <Frame type="Image" name="Team1CollectionIcon">
            <Anchor side="Bottom" relative="$parent/Team1Background" pos="Max" offset="10"/>
            <Anchor side="Left" relative="$parent/Team1Background" pos="Min" offset="-5"/>
            <Texture val="@UI/HUD_MapMechanic_HauntedMines_Skull"/>
        </Frame>
        
        <Frame type="Image" name="Team1CollectionAltIcon">
            <Anchor relative="$parent/Team1CollectionIcon"/>
            <Texture val="@UI/HUD_MapMechanic_HauntedMines_Skull"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="Image" name="Team2CollectionIcon">
            <Anchor side="Bottom" relative="$parent/Team2Background" pos="Max" offset="10"/>
            <Anchor side="Right" relative="$parent/Team2Background" pos="Max" offset="5"/>
            <Texture val="@UI/HUD_MapMechanic_HauntedMines_Skull"/>
        </Frame>

        <Frame type="Image" name="Team2CollectionAltIcon">
            <Anchor relative="$parent/Team2CollectionIcon"/>
            <Texture val="@UI/HUD_MapMechanic_HauntedMines_Skull"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="Label" name="Team1CollectionCounter">
            <Anchor side="Bottom" relative="$parent/Team1Background" pos="Max" offset="-10"/>
            <Anchor side="Left" relative="$parent/Team1Background" pos="Min" offset="70"/>
            <Style val="Storm_MapMechanic_MechanicCounterLeft_Blue"/>
            <Text val="88"/>
        </Frame>

        <Frame type="Label" name="Team2CollectionCounter">
            <Anchor side="Bottom" relative="$parent/Team2Background" pos="Max" offset="-10"/>
            <Anchor side="Right" relative="$parent/Team2Background" pos="Max" offset="-72"/>
            <Style val="Storm_MapMechanic_MechanicCounterRight_Red"/>
            <Text val="88"/>
        </Frame>
        
        <Animation name="LeftTeamRed">
            <Event event="LeftTeamRed" action="DirectionForward,Play" frame="$parent"/>
            <Event event="LeftTeamBlue" action="Stop" frame="$parent"/>

            <Controller type="Visibility" end="Pause" frame="Team1CollectionIcon">
                <Key type="Bool" time="0" value="True"/>
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team1CollectionAltIcon">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".01" value="True"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team2CollectionIcon">
                <Key type="Bool" time="0" value="True"/>
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team2CollectionAltIcon">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".01" value="True"/>
            </Controller>
        </Animation>
    </Frame>
    
    <Frame type="Frame" name="NotificationFrameTemplate">
        <Anchor side="Top" relative="NotificationIcon" pos="Min" offset="-20"/>
        <Anchor side="Left" relative="BackgroundImage" pos="Min" offset="-100"/>
        <CollapseLayout val="True"/>
        <PreserveAnchorOffset val="False"/>
        
        <Visible val="False"/>
        
        <!-- Ingame blur -->
        <Frame type="BlurFrame" name="NotificationBackingBlur">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Min" offset="3"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Max" offset="-5"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Max" offset="-20"/>

            <BlurAmount val=".2"/>
            <Visible val="#IngameBlur"/>
            <RenderType val="HDR"/>
        </Frame>

        <Frame type="Image" name="BackgroundImage">
            <Anchor side="Top" relative="$parent/NotificationIconAnchor" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/NotificationIconAnchor" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
            <Anchor side="Left" relative="$parent/NotificationLabel" pos="Min" offset="-28"/>
            <Texture val="@UI/Storm_UI_Ingame_hud_notification_background"/>
            <TextureType val="Nineslice"/>
        </Frame>
        
        <Frame type="Image" name="BGShine">
            <Anchor relative="$parent/BackgroundImage"/>
            <LayerCount val="2"/>
            <BlendMode val="Add"/>
            <Color val="aaffff"/>
            <Alpha val="255"/>
            
            <Texture val="@UI/Storm_UI_Ingame_HealingWell_Active_BorderGlow_Mask" layer="0"/>
            <TextureCoords top="0" left="1" bottom="1" right="0" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <WrapUV val="False" layer="0"/>
            
            <Texture val="@UI/Storm_UI_Ingame_hud_notification_background" layer="1"/>
            <TextureType val="NineSlice" layer="1"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
            <WrapUV val="False" layer="1"/>
            
            <Animation name="UVScroll">
                <Event event="Intro" action="Restart,Play" frame="$parent"/>
                <Controller type="LayerUV" layer="0" side="Left" relative="False" sidelock="True" end="Pause" >
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time="1.4" value="2" inout="Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="BracketLeftImage">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Min" offset="-3"/>
            <Texture val="@UI/Storm_UI_Ingame_hud_notification_angle_large_left"/>
        </Frame>

        <Frame type="Image" name="BracketRightImage">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Max" offset="6"/>
            <Texture val="@UI/Storm_UI_Ingame_hud_notification_angle_large_right"/>
        </Frame>
        
        <Frame type="Image" name="IconFlareImage">
            <Anchor side="Top" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-27"/>
            <Anchor side="Bottom" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-27"/>
            <Anchor side="Left" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-40"/>
            <Anchor side="Right" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-40"/>
            <Texture val="@UI/Storm_UI_ChromeFlare"/>
            <TextureCoords top="1" left="0" bottom="0" right="1"/>
            <Width val="300"/>
            <Height val="300"/>
            <BlendMode val="Add"/>
            <Rotation val="-30"/>
            <Alpha val="0"/>
            <Visible val="False"/>
        </Frame>

        <Frame type="Label" name="NotificationLabel">
            <Anchor side="Top" relative="$parent/BackgroundImage" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/NotificationIconAnchor" pos="Min" offset="0"/>
            <Style val="Storm_MapMechanic_MechanicTimerCentered_Blue"/>
            <Width val="340"/>
        </Frame>
        
        <Frame type="Label" name="CountdownLabel">
            <Anchor side="Top" relative="$parent/NotificationLabel" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/NotificationLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/NotificationLabel" pos="Max" offset="0"/>
            <Style val="Storm_MapMechanic_MechanicTimerCentered_Blue"/>
        </Frame>
        
        <Frame type="Frame" name="NotificationIconAnchor">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Max" offset="-12"/>
            <Width val="112"/>
            <Height val="100"/>
        </Frame>
        
        <Frame type="Image" name="NotificationIcon">
            <Anchor side="Top" relative="$parent/NotificationIconAnchor" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/NotificationIconAnchor" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/NotificationIconAnchor" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/NotificationIconAnchor" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Ingame_MechanicIcon_Empty"/>
            
            <Frame type="Image" name="Shine">
                <Anchor relative="$parent"/>
                <LayerCount val="2"/>
                <BlendMode val="Add"/>
                <Color val="aaffff"/>
                
                <Texture val="@UI/Storm_UI_ProfileDialog_BorderGlow_Mask" layer="0"/>
                <TextureCoords top="1" left=".5" bottom="0" right="0" layer="0"/>
                <AlphaLayer val="True" layer="0"/>
                <WrapUV val="False" layer="0"/>
                
                <Texture val="@UI/Storm_UI_Ingame_MechanicIcon_Empty" layer="1"/>
                <TextureType val="Normal" layer="1"/>
                <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
                <WrapUV val="False" layer="1"/>
                
                <Animation name="UVScroll">
                    <Event event="Intro" action="Restart,Play" frame="$parent/$parent"/>
                    <Controller type="LayerUV" layer="0" side="Left" relative="False" sidelock="True" end="Pause" >
                        <Key type="Curve" time="0" value="-2" inout="Linear"/>
                        <Key type="Curve" time="1.5" value="2" inout="Linear"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>
        
        <!-- <AcceptsMouse val="True"/> -->
        <Animation name="Intro">
            <Event event="Show" action="DirectionForward,Play"/>
            <Event event="Hide" action="DirectionReverse,Play"/>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".01" value="True"/>
                <Key type="Bool" time=".4" value="True"/>
            </Controller>
            <Controller type="Event" end="Stop">
                <Key type="Event" time="0" event="Intro"/>
            </Controller>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".2" value="255" inout="Slow,Linear"/>
                <Key type="Curve" time=".4" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Right" relative="true">
                <Key type="Curve" time="0" value="50" inout="Linear,Smooth"/>
                <Key type="Curve" time=".1" value="0" inout="Smooth,Linear"/>
                <Key type="Curve" time=".4" value="0"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="BackgroundImage">
                <Key type="Curve" time="0" value="350" inout="Linear,Smooth"/>
                <Key type="Curve" time=".1" value="350" inout="Linear,Smooth"/>
                <Key type="Curve" time=".3" value="0" inout="Smooth,Linear"/>
                <Key type="Curve" time=".4" value="0"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="NotificationLabel">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time=".2" value="0" inout="Linear"/>
                <Key type="Curve" time=".4" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="CountdownLabel">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time=".2" value="0" inout="Linear"/>
                <Key type="Curve" time=".4" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause" frame="BracketLeftImage">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".2" value="100" inout="Slow,Linear"/>
                <Key type="Curve" time=".4" value="100"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause" frame="BracketLeftImage">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".2" value="100" inout="Slow,Linear"/>
                <Key type="Curve" time=".4" value="100"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause" frame="BracketRightImage">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".2" value="100" inout="Slow,Linear"/>
                <Key type="Curve" time=".4" value="100"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause" frame="BracketRightImage">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".2" value="100" inout="Slow,Linear"/>
                <Key type="Curve" time=".4" value="100"/>
            </Controller>
            <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Stop" frame="IconFlareImage">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".2" value="150" inout="Slow,Linear"/>
                <Key type="Curve" time=".4" value="100"/>
            </Controller>
            <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Stop" frame="IconFlareImage">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".2" value="150" inout="Slow,Linear"/>
                <Key type="Curve" time=".4" value="100"/>
            </Controller>
            <Controller type="Fade" end="Stop" frame="IconFlareImage">
                <Key type="Curve" time="0" value="0" />
                <Key type="Curve" time=".9" value="0" inout="Linear"/>
                <Key type="Curve" time="1" value="255" inout="Slow"/>
                <Key type="Curve" time="1.6" value="0" inout="Linear"/>
            </Controller>
        </Animation>
        
        <Animation name="TeamToggle">
            <Event event="BlueTeam" action="Stop"/>
            <Event event="RedTeam" action="Restart,Play"/>
            <Controller type="Style" end="Pause" style1="Storm_MapMechanic_MechanicTimerCentered_Blue" style2="Storm_MapMechanic_MechanicTimerCentered_Red" frame="NotificationLabel">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time=".01" value="1"/>
            </Controller>
            <Controller type="Style" end="Pause" style1="Storm_MapMechanic_MechanicTimerCentered_Blue" style2="Storm_MapMechanic_MechanicTimerCentered_Red" frame="CountdownLabel">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time=".01" value="1"/>
            </Controller>
        </Animation>
    </Frame>
    
    <Frame type="Frame" name="NotificationFrameCountdownTemplate" Template="MapMechanicPanel/NotificationFrameTemplate">
        <Frame type="Image" name="BackgroundImage">
            <Anchor side="Left" relative="$parent/NotificationLabel" pos="Min" offset="-70"/>
        </Frame>

        <Frame type="Label" name="NotificationLabel">
            <Anchor side="Left" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-250"/>
            <Anchor side="Right" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-250"/>
            <Width val="0"/>
        </Frame>
        
        <Frame type="Label" name="CountdownLabel">
            <Anchor side="Top" relative="$parent/NotificationLabel" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/NotificationLabel" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/NotificationLabel" pos="Max" offset="0"/>
            <Style val="Storm_MapMechanic_MechanicTimerCentered_Blue"/>
        </Frame>

    </Frame>
    
    <Frame type="Frame" name="NotificationFrameLeftTemplate" Template="MapMechanicPanel/NotificationFrameTemplate">
        <Frame type="Frame" name="NotificationIconAnchor">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/BackgroundImage" pos="Min" offset="16"/>
            <Anchor side="Right" relative="$this" pos="Min" offset="0"/>
        </Frame>

        <Frame type="Label" name="NotificationLabel">
            <Anchor side="Right" relative="$parent/BackgroundImage" pos="Max" offset="-25"/>
        </Frame>
        <Frame type="Image" name="BackgroundImage">
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
            <Anchor side="Left" relative="$parent/NotificationLabel" pos="Min" offset="-129"/>
        </Frame>
        
    </Frame>
    
    <Frame type="Frame" name="CannonShotFrame">
        <Width val="42"/>
        <Height val="42"/>
        
        <Frame type="Image" name="EmptyCannonBall">
            <Anchor relative="$parent"/>
            <Texture val="@UI/HUD_MapMechanic_BlackheartsBay_CannonBallEmpty"/>
        </Frame>
        
        <Frame type="Image" name="ActiveCannonBall">
            <Anchor relative="$parent"/>
            <Texture val="@UI/HUD_MapMechanic_BlackheartsBay_CannonBallBlue"/>
        </Frame>
    </Frame>
    
    <Frame type="UnitStatusFrame" name="MapMechanicHealthBarTemplate">
        <Width val="250"/>
        <Height val="100"/>
        <VisibilityControlledByUnitAlliances val="false"/>
        
        <Frame type="UnitStatusHealthBar" name="HealthBar" template="UnitStatus/UnitStatusSegmentedHealthBarTemplate">
            <Width val="250"/>
            <Height val="100"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            
            <ReductionColor val="200,0,0" index="0"/>
            <ReductionShown val="True" index="0"/>
            <IncreaseDuration val="1000" index="0"/>
            <DecreaseDuration val="3000" index="0"/>

            <MaxSegmentCount val="200"/>
            
            <Texture val="@UI/HUD_TopBar_CoreHealth_BarLeft"/>
            
            <RenderPriority val="600"/>
            
            <BackgroundShown val="false"/>
            <BarInsetRect top="0" left="0" bottom="0" right="0"/>
            <BackgroundInsetRect top="0" left="0" bottom="0" right="0"/>
            <AllyColor val="00b4ff" index="0"/>
            <EnemyColor val="ff2c59" index="0"/>

            <Tiled val="true" index="0"/>
            
            <Frame type="Label" name="StatusBarLabel">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="3"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="3"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Height val="0"/>
                <Width val="0"/>
            </Frame>
        </Frame>
        
        <Frame type="UnitStatusHealthLabel" name="HealthLabel">
            <Width val="50"/>
            <ShowPercent val="true"/>
            <Anchor side="Top" relative="$parent/HealthBar" pos="Mid" offset="-22"/>
            <Anchor side="Bottom" relative="$parent/HealthBar" pos="Mid" offset="0"/>
            
            <Frame type="Label" name="StatusLabel">
                <Anchor relative="$parent"/>
            </Frame>
        </Frame>
    </Frame>

    <Frame type="Frame" name="MapMechanicPanelTemplate">
        <!--**************************************************************-->
        <!-- Cursed Hollow                                                -->
        <!--**************************************************************-->
