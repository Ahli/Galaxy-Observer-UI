                        <RenderPriority val="1000"/>
                        
                        <Frame type="UnitStatusAbilCharge" name="TracerPulseBombChargeFrame">
                            <Anchor relative="$parent"/>
                            <AcceptsMouse val="False"/>
                            <AbilityLink val="TracerPulseBomb"/>
