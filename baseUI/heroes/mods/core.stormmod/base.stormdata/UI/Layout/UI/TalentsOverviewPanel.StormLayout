                    <Frame type="UnitStatusAbilIcon" name="UnitStatusAbil">
                        <Frame type="AbilIcon" name="AbilIcon">
                            <Frame type="Image" name="CooldownImage">
                                <Anchor relative="$parent/$parent/$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4"/>
                                <Alpha val="0"/>
                                <Visible val="false"/>
                            </Frame>
                            
                            <Frame type="Image" name="CooldownImage2">
                                <Anchor side="Top" relative="$parent/$parent/$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4" pos="Min" offset="-11"/>
                                <Anchor side="Left" relative="$parent/$parent/$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4" pos="Min" offset="0"/>
