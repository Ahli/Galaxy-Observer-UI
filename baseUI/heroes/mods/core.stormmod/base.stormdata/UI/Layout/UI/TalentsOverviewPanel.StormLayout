                                <Anchor side="Right" relative="$parent/$parent/$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Max" offset="6"/>
                                <AcceptsMouse val="False"/>

                                <Vertical val="True"/>
                                <MinValue val="0"/>

                                <Frame type="Frame" name="FillImageContainer">
                                    <Anchor relative="$parent"/>
                                
                                    <Frame type="Image" name="FillImage">
                                        <Anchor relative="$parent"/>
                                        <LayerCount val="1"/>

                                        <Texture val="@UI/obs_tracer_ult_meter_blue"/>
                                    </Frame>
                                </Frame>
                            </Frame>
                        </Frame>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
