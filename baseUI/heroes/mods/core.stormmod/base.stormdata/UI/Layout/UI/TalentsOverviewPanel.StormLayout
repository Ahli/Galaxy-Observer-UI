<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <!-- TalentsOverviewPanelTemplate is a template to create a panel displaying the talents of each player.
        2 templates are set up:
            - topbar: the talent icons displayed on the top bar
            - bottom panel: the toggle window at the bottom displaying full talent trees
    -->
    
    <!-- top bar -->
    <Constant name="TopBarSlotHeight" val="48"/>
    <Constant name="TopBarSlotWidth" val="160"/>
    <Constant name="TopBarSlotLeftOffsetFromTeamHeroOverviewBlue" val="82"/>
    <Constant name="TopBarSlotLeftOffsetFromTeamHeroOverviewRed" val="15"/>
    <Constant name="TopBarHorizTalentGapRed" val="-12"/>
    <Constant name="TopBarHorizTalentGapBlue" val="-10"/>
    <Constant name="TopBarTalentWidth" val="40"/>
    <Constant name="TopBarTalentHeight" val="20"/>
    
    <!-- bottom panel -->
    <Constant name="BottomPanelSlot5BottomOffset" val="4"/>
    <Constant name="BottomPanelSlotWidth" val="565"/>
    <Constant name="BottomPanelSlotHeight" val="60"/>
    <Constant name="BottomPanelTalentWidth" val="40"/>
    <Constant name="BottomPanelTalentHeight" val="40"/>
    <Constant name="BottomPanelHorizTalentGapBlue" val="16"/>
    <Constant name="BottomPanelHorizTalentGapRed" val="-16"/>
    <Constant name="BottomPanelHorizHeroicGapBlue" val="21"/>
    <Constant name="BottomPanelHorizHeroicGapRed" val="-21"/>
    <Constant name="BottomPanelTalentContainerVerticalOffset" val="-48"/>

    <Constant name="PlayerFrameVerticalGap" val="-12"/>

    <Constant name="RenderPriorityLevel_0" val="515"/>
    <Constant name="RenderPriorityLevel_1" val="520"/>
    <Constant name="RenderPriorityLevel_2" val="525"/>
    <Constant name="RenderPriorityLevel_3" val="530"/>
    <Constant name="RenderPriorityLevel_4" val="535"/>

    <Frame type="PlayerNameFrame" name="PlayerNameFrameTemplate">
        <Anchor relative="PlayerNameLabel"/>
        <UseAIDifficultyName val="true"/>

        <Frame type="Label" name="PlayerNameLabel">
            <Style val="Storm_LeaderPlayerName"/>
            <IgnoreParentColor val="true"/>
            <RenderPriority val="#RenderPriorityLevel_2"/>
        </Frame>
    </Frame>

    <Frame type="PlayerHeroNameFrame" name="PlayerHeroNameFrameTemplate">
        <Width val="230"/>
        <Height val="29"/>
        <Frame type="Label" name="HeroNameLabel">
            <Anchor relative="$parent"/>
            <Style val="Storm_LeaderHeroName"/>
            <IgnoreParentColor val="true"/>
            <RenderPriority val="#RenderPriorityLevel_2"/>
            <Options val="Ellipsis | TooltipIfTruncated"/>
            <AcceptsMouseTooltip val="True"/>
        </Frame>
    </Frame>

    <Frame type="PlayerDeathIndicatorFrame" name="PlayerDeathIndicatorFrameTemplate">
        <Width val="115"/>
        <Height val="45"/>

        <Frame type="Frame" name="ContainerFrame">
            <Anchor relative="$parent"/>

            <Frame type="Image" name="SkullImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="7"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="32"/>
                <IgnoreParentColor val="true"/>
                <Texture val="@UI/Storm_UI_InGame_Leader_DeathIcon"/>
                <RenderPriority val="#RenderPriorityLevel_3"/>
            </Frame>
            <Animation name="DeathToggle">
            <Event event="OnShown" action="DirectionForward,Play"/>
            <Event event="OnHidden" action="DirectionReverse,Play"/>
            <Controller type="Fade" end="Pause" frame="$parent/$parent/PlayerHeroIconFrame">
                <Key type="Curve" time="0" value="255" inout="Linear"/>
                <Key type="Curve" time=".3" value="80" inout="Slow,Linear"/>
            </Controller>
            </Animation>
        </Frame>
    </Frame>

    <Frame type="PlayerScoreValueFrame" name="PlayerScoreValueLabelFrameTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="4"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

        <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
            <Anchor relative="$parent"/>

                <Frame type="Label" name="Label">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Style val="Storm_ScoreScreenResultValueTeam1"/>
                <IgnoreParentColor val="true"/>
                <RenderPriority val="#RenderPriorityLevel_1"/>
            </Frame>

            <Animation name="AllyBestValue">
                <Event event="AllyBestValue" action="Reset,Play"/>
                <Controller type="Style" end="Pause" frame="Label" style1="Storm_ScoreScreenResultBestValueTeam1" style2="Storm_ScoreScreenResultBestValueTeam1">
                    <Key type="Curve" time="0.00" value="1.0" inout="Linear,Auto"/>
                </Controller>
            </Animation>

            <Animation name="AllyNotBestValue">
                <Event event="AllyNotBestValue" action="Reset,Play"/>
                <Controller type="Style" end="Pause" frame="Label" style1="Storm_ScoreScreenResultValueTeam1" style2="Storm_ScoreScreenResultValueTeam1">
                    <Key type="Curve" time="0.00" value="1.0" inout="Linear,Auto"/>
                </Controller>
            </Animation>

            <Animation name="EnemyBestValue">
                <Event event="EnemyBestValue" action="Reset,Play"/>
                <Controller type="Style" end="Pause" frame="Label" style1="Storm_ScoreScreenResultBestValueTeam2" style2="Storm_ScoreScreenResultBestValueTeam2">
                    <Key type="Curve" time="0.00" value="1.0" inout="Linear,Auto"/>
                </Controller>
            </Animation>

            <Animation name="EnemyNotBestValue">
                <Event event="EnemyNotBestValue" action="Reset,Play"/>
                <Controller type="Style" end="Pause" frame="Label" style1="Storm_ScoreScreenResultValueTeam2" style2="Storm_ScoreScreenResultValueTeam2">
                    <Key type="Curve" time="0.00" value="1.0" inout="Linear,Auto"/>
                </Controller>
            </Animation>

        </Frame>
    </Frame>

    <Frame type="TeamHeroLevelFrame" name="TeamHeroLevelFrameTemplate">
        <Anchor relative="HeroLevelLabel"/>
        <Frame type="Label" name="HeroLevelLabel">
            <RenderPriority val="#RenderPriorityLevel_1"/>
        </Frame>
    </Frame>

    <Frame type="TeamScoreValueFrame" name="TeamKillsFrameTemplate">
        <Anchor relative="TeamScoreValueLabel"/>
        <ScoreValueLink val="TeamTakedowns"/>
        <Frame type="Label" name="TeamScoreValueLabel">
            <RenderPriority val="#RenderPriorityLevel_1"/>
        </Frame>
    </Frame>

