            <Frame type="PlayerScoreValueFrame" name="DeathsScoreValueFrame">
                <Visible val="false"/>
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Visible val="false"/>
                </Frame>
            </Frame>
            
            <Frame type="PlayerScoreValueFrame" name="SiegeDamageScoreValueFrame">
                <Visible val="false"/>
                
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Visible val="false"/>
                </Frame>
            </Frame>
            
            <Frame type="PlayerScoreValueFrame" name="HeroDamageScoreValueFrame">
                <Visible val="false"/>
                
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Visible val="false"/>
                </Frame>
            </Frame>
            
            <Frame type="PlayerScoreValueFrame" name="HealingScoreValueFrame">
                <Visible val="false"/>
                
                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Visible val="false"/>
                </Frame>
                
                <Frame type="ScoreValueImageFrame" name="ScoreValueFrame1">
                    <Visible val="false"/>
                </Frame>
            </Frame>
        </Frame>
    </Frame>
    
    <!-- template to format individual talent icons -->
    <Frame type="UnitStatusTalentIcon" name="TalentIconObserverTemplate">
        <Frame type="Image" name="IconBorderImage">
            <Visible val="False"/>
        </Frame>
        
        <Frame type="Image" name="UnfilledProgressImage">
            <Visible val="False"/>
            
            <Animation name="KeepHidden">
                <Event event="OnShown" action="Reset,Play"/>
                
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.0" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="QuestProgressBar">
            <Visible val="False"/>
            
            <Animation name="KeepHidden">
                <Event event="OnShown" action="Reset,Play"/>
                
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.0" value="false"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="QuestCompleteImage">
            <Visible val="False"/>
            
            <Animation name="KeepHidden">
                <Event event="OnShown" action="Reset,Play"/>
                
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.0" value="false"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
    
