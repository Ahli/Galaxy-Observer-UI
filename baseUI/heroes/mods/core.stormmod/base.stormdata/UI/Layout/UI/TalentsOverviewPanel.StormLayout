    <Frame type="HeroTalentsPanelPlayerFrame" name="HeroTalentsPanelAllyFrameTemplate">
        <Width val="1294"/>
        <Height val="70"/>
        <Batch val="False"/>

        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-8"/>
            <Texture val="@UI/Storm_UI_ScoreScreen_Stats_PlayerBG"/>
            <TextureType val="NineSlice"/>
            <RenderPriority val="511"/>
        </Frame>

        <Frame type="Image" name="SelfBackground">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Storm_UI_ScoreScreen_Stats_PlayerBG_Gold"/>
            <RenderPriority val="511"/>
        </Frame>

        <Frame type="BattleUserContextMenu" name="PlayerContextMenu" template="BattleUserContextMenu/BattleUserContextMenu" >
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <RenderPriority val="#RenderPriorityLevel_4"/>
            <ReportsStats val="True"/>
            <!-- Handle provided in code -->
        </Frame>

        <Frame type="PlayerHeroIconFrame" name="PlayerHeroIconFrame">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="17"/>
            <Width val="100"/>
            <Height val="54"/>

            <Frame type="Image" name="HeroIconImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="-1"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Width val="100"/>
                <Height val="56"/>
                <ImageMaskTexture val="@UI/Storm_UI_InGame_Leader_ScoreInset_PortraitFade"/>
                <IgnoreParentColor val="true"/>
                <RenderPriority val="#RenderPriorityLevel_1"/>
            </Frame>

            <Frame type="Image" name="SilencedIcon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-6"/>
            <Width val="27"/>
            <Height val="24"/>
            <Visible val="False"/>
            <IgnoreParentColor val="true"/>
            <RenderPriority val="#RenderPriorityLevel_2"/>

            <Texture val="@UI/Storm_UI_PortraitBorder_SilencePenaltyIcon"/>
            <AcceptsMouseTooltip val="True"/>
            <Tooltip val="@UI/BattleUser/Silenced"/>

            <Animation name="SilencePenalty">
                <Event event="SilencedPenaltyOn" action="Play" frame="$parent"/>
                <Event event="SilencedPenaltyOff" action="Stop" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>
            </Frame>
        </Frame>

        <Frame type="PlayerDeathIndicatorFrame" name="PlayerDeathIndicatorFrame" template="TalentsOverviewPanel/PlayerDeathIndicatorFrameTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="7"/>
            <IgnoreParentColor val="True"/>
            <RenderPriority val="#RenderPriorityLevel_2"/>
        </Frame>

        <Frame type="PlayerHeroNameFrame" name="HeroNameFrame" template="TalentsOverviewPanel/PlayerHeroNameFrameTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="8"/>
            <Anchor side="Left" relative="$parent/PlayerHeroIconFrame" pos="Max" offset="-10"/>
            <RenderPriority val="#RenderPriorityLevel_2"/>
        </Frame>

        <Frame type="PlayerNameFrame" name="PlayerNameFrame" template="TalentsOverviewPanel/PlayerNameFrameTemplate">
            <Frame type="Label" name="PlayerNameLabel">
                <Anchor side="Top" relative="$parent/$parent/HeroNameFrame" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent/HeroNameFrame" pos="Min" offset="0"/>
            </Frame>
            <RenderPriority val="#RenderPriorityLevel_2"/>
        </Frame>

        <!-- Animation Events for Team Color -->
        <StateGroup name="PlayerTypeState">
            <State name="LocalPlayer">
                <When type="AnimationState" PlayerTypeState="LocalPlayer"/>
                <Action type="SetProperty" Style="Storm_ScoreScreenHeroNameSelf" frame="HeroNameFrame/HeroNameLabel"/>
                <Action type="SetProperty" Style="Storm_ScoreScreenPlayerNameSelf" frame="PlayerNameFrame/PlayerNameLabel"/>
                <Action type="SetProperty" Visible="True" frame="SelfBackground"/>
                <Action type="SetProperty" Alpha="255" frame="$this"/>
                <Action type="SetProperty" LightAdjustColor="{$GameUI/AllyColors/@AdditionalColor[0]}" frame="Background"/>
                <Action type="SetProperty" DarkAdjustColor="{$GameUI/AllyColors/@Color}" frame="Background"/>
            </State>
            <State name="AllyPlayer">
                <When type="AnimationState" PlayerTypeState="AllyPlayer"/>
                <Action type="SetProperty" Style="Storm_ScoreScreenHeroNameTeam1" frame="HeroNameFrame/HeroNameLabel"/>
                <Action type="SetProperty" Style="Storm_ScoreScreenPlayerNameTeam1" frame="PlayerNameFrame/PlayerNameLabel"/>
                <Action type="SetProperty" LightAdjustColor="{$GameUI/AllyColors/@AdditionalColor[0]}" frame="Background"/>
                <Action type="SetProperty" DarkAdjustColor="{$GameUI/AllyColors/@Color}" frame="Background"/>
            </State>
            <State name="EnemyPlayer">
                <When type="AnimationState" PlayerTypeState="EnemyPlayer"/>
                <Action type="SetProperty" Style="Storm_ScoreScreenHeroNameTeam2" frame="HeroNameFrame/HeroNameLabel"/>
                <Action type="SetProperty" Style="Storm_ScoreScreenPlayerNameTeam2" frame="PlayerNameFrame/PlayerNameLabel"/>
                <Action type="SetProperty" LightAdjustColor="{$GameUI/EnemyColors/@AdditionalColor[0]}" frame="Background"/>
                <Action type="SetProperty" DarkAdjustColor="{$GameUI/EnemyColors/@Color}" frame="Background"/>
            </State>
        </StateGroup>

        <Frame type="PlayerScoreValueFrame" name="CarriedMechanicScoreValueFrame" template="TalentsOverviewPanel/PlayerScoreValueLabelFrameTemplate">
            <Anchor side="Right" relative="$parent" pos="Min" offset="10"/>
            <Width val="110"/>

            <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                <ScoreValueLink val="CarriedMechanic"/>
                <HideZero val="true"/>
                <Frame type="Label" name="Label">
                    <Anchor side="Left" relative="$parent" pos="Min" offset="30"/>
                </Frame>
            </Frame>

            <Frame type="ScoreValueImageFrame" name="ScoreValueFrame1">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/ScoreValueFrame0/Label" pos="Min" offset="-10"/>
                <Width val="28"/>
                <Height val="28"/>
                <ScoreValueLink val="CarriedMechanic"/>
                <HideZero val="true"/>

                <Frame type="Image" name="Image">
                    <IgnoreParentColor val="True"/>
                    <Anchor relative="$parent"/>
                    <TextureType val="Normal"/>
                </Frame>
            </Frame>
        </Frame>

        <Frame type="Frame" name="TalentsContainerFrame">
            <Anchor relative="$parent"/>
            <Batch val="True"/>

            <Frame type="SelectionUnitStatusPanel" name="SelectionUnitStatusPanel">
                <Anchor relative="$parent"/>
                <HeroesOnly val="False"/>
                <AlwaysShowHero val="True"/>

                <Frame type="UnitStatusFrame" name="UnitStatusFrame0">
                    <Anchor relative="$parent"/>

                    <Frame type="PlayerHeroSelectedTalentTreeFrame" name="HeroSelectedTalentTreeFrame" template="TalentsOverviewPanel/PlayerHeroSelectedTalentTreeFrameTemplate">
                        <Anchor side="Top" relative="$parent/$parent/$parent" pos="Mid" offset="0"/>
                        <Anchor side="Bottom" relative="$parent/$parent/$parent" pos="Mid" offset="0"/>
                        <Anchor side="Left" relative="$parent/$parent/$parent/$parent/PlayerNameFrame" pos="Max" offset="0"/>
                    </Frame>

                    <Frame type="UnitStatusAbilIcon" name="UnitStatusAbil">
                        <Anchor side="Top" relative="$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Min" offset="0"/>
                        <Anchor side="Left" relative="$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Min" offset="-10"/>
                        <Anchor side="Right" relative="$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Max" offset="10"/>
                        <Anchor side="Bottom" relative="$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4/IconImage" pos="Max" offset="0"/>

                        <Frame type="AbilIcon" name="AbilIcon">
                            <Anchor relative="$parent"/>

                            <Frame type="Image" name="IconImage">
                                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                                <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
                                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                                <Alpha val="0"/>
                                <RenderPriority val="#RenderPriorityLevel_1"/>
                            </Frame>

                            <Frame type="Image" name="CooldownImage">
                                <Anchor relative="$parent/IconImage"/>
                                <Texture val="@UI/HUD_BTN_Ability_Cooldown_Anim"/>
                                <TextureCoords top="0.0" left="0.0" bottom="0.2" right="0.1"/>
                                <Animating val="True"/>
                                <ManagedAnim val="False"/>
                                <AnimColumns val="10"/>
                                <AnimCount val="50"/>
                                <Alpha val="192"/>
                                <RenderPriority val="#RenderPriorityLevel_3"/>
                            </Frame>

                            <Frame type="Label" name="CooldownLabel">
                                <Anchor relative="$parent"/>
                                <Style val="Storm_LeaderTalentHeader"/>
                                <RenderPriority val="#RenderPriorityLevel_4"/>
                            </Frame>
                        </Frame>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
    </Frame>

    <Frame type="HeroTalentsPanel" name="HeroTalentsPanelTemplate">
        <Batch val="False"/>

        <Frame type="Frame" name="HeroTalentsPanelContainer">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="1380"/>
            <Height val="863"/>
            <!--tabvis-->
