                                <Anchor side="Right" relative="$parent/$parent/$parent/HeroSelectedTalentTreeFrame/TalentTreeIcon4" pos="Max" offset="0"/>
                                <Texture val="@UI/obs_talents_cd"/>
                                <ImageMaskTexture val="@UI/obs_alphaMask_mini_talents"/>
                                <RenderPriority val="533"/>
                            </Frame>
                            
                            <StateGroup name="CDImageVisibility">
                                <DefaultState val="Hidden"/>
                                
                                <State name="Hidden">
                                    <When type="Property" frame="CooldownLabel" visible="False"/>
                                    <Action type="SetProperty" frame="CooldownImage2" visible="False"/>
                                </State>
                                
                                <State name="Showing">
                                    <When type="Property" frame="CooldownLabel" visible="True"/>
                                    <Action type="SetProperty" frame="CooldownImage2" visible="true"/>
                                </State>
                            </StateGroup>
