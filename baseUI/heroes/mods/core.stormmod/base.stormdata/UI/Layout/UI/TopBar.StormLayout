                <!-- the font style for this is set to the opposite color for team 2 in script -->
                <Style val="Storm_TopBar_CoreHealthPercentRightRed"/>
            </Frame>
        </Frame>
    </Frame>

    <!-- Main Top Bar Panel Template -->
    <Frame type="Frame" name="TopBarPanelTemplate">
        <Height val="150"/>
        
        <!-- Anim events sent to this frame from script
            Team1LevelUp (team 1 gains a level)
            Team2LevelUp (team 2 gains a level)
        -->

        <Animation name="ArenaModeTransition">            
            <Event event="ArenaMode" action="Play"/>

            <Controller type="Visibility" end="Pause" frame="Team1XPBackgroundBlue">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team1XPBackgroundRed">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team2XPBackgroundBlue">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team2XPBackgroundRed">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>   
        </Animation>        

        <Animation name="ArenaModeTransitionOrder">            
            <Event event="ArenaModeOrder" action="Play"/>

            <Controller type="Visibility" end="Pause" frame="Team1XPBackgroundBlueArena">
                <Key type="Bool" time=".01" value="True"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team1XPBackgroundRedArena">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team2XPBackgroundBlueArena">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team2XPBackgroundRedArena">
                <Key type="Bool" time=".01" value="True"/>
            </Controller>        
        </Animation>

        <Animation name="ArenaModeTransitionChaos">            
            <Event event="ArenaModeChaos" action="Play"/>

            <Controller type="Visibility" end="Pause" frame="Team1XPBackgroundBlueArena">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team1XPBackgroundRedArena">
                <Key type="Bool" time=".01" value="True"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team2XPBackgroundBlueArena">
                <Key type="Bool" time=".01" value="True"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="Team2XPBackgroundRedArena">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>        
        </Animation>

        <!-- Ingame blur -->
        <Frame type="BlurFrame" name="TopBarBackingBlur">
            <Anchor side="Top" relative="$parent/TopBarBacking" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/TopBarBacking" pos="Max" offset="-11"/>
            <Anchor side="Left" relative="$parent/TopBarBacking" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/TopBarBacking" pos="Max" offset="0"/>

            <AlphaMaskTexture val="@UI/HUD_TopBar_Backing"/>
            <BlurAmount val=".5"/>
            <Visible val="#IngameBlur"/>
        </Frame>
        
        <Frame type="Image" name="TopBarBacking">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/HUD_TopBar_Backing"/>
        </Frame>
        
        <Frame type="Label" name="GameTimerLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Style val="Storm_TopBar_GameTimer"/>
            <Text val="88:88"/>
            <Width val="120"/>
            <Height val="34"/>
        </Frame>
        
        <!-- XP Bars -->
        <Frame type="Image" name="Team1XPBackgroundBlue">
            <Width val="268"/>
            <Height val="91"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TopBarBacking" pos="Min" offset="55"/>
            <Texture val="@UI/HUD_TopBar_TeamXP_BackingBlue"/>
            <TooltipAnchorPosition val="BottomCenter"/>
