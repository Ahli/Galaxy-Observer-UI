    <Frame type="UnitStatusFrame" name="CoreUnitStatusLeftTemplate" template="TopBar/CoreUnitStatusBaseTemplate">
        <Frame type="UnitStatusElement" name="HealthBarBackingBlue">
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Frame type="Image" name="Image">
                <Texture val="@UI/HUD_TopBar_CoreHealth_BackingBlue"/>
            </Frame>
        </Frame>
        
        <Frame type="UnitStatusElement" name="HealthBarBackingRed">
