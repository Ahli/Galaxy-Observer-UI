            <Anchor side="Right" relative="$parent/TopBarBacking" pos="Max" offset="-55"/>
            <Texture val="@UI/HUD_TopBar_TeamXP_BackingBlue"/>
            <TextureCoords top="0.0" left="1.0" bottom="1.0" right="0.0"/>
            <TooltipAnchorPosition val="BottomCenter"/>
