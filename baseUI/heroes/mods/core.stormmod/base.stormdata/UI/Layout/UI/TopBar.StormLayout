        <!-- Team Level Labels -->
        <Frame type="Label" name="Team1LevelLabel">
            <Anchor side="Top" relative="$parent/TopBarBacking" pos="Min" offset="5"/>
            <Anchor side="Right" relative="$parent/TopBarBacking" pos="Mid" offset="-25"/>
            <Width val="90"/>
            <Style val="Storm_TopBar_TeamLevelLeft"/>
            <Text val="88"/>
            <TooltipAnchorPosition val="BottomCenter"/>
            
            <Animation name="LevelUpBlue">
                <Event event="Team1LevelUpBlue" action="Reset" frame="$parent"/>
                <Event event="Team1LevelUpBlue" action="Play" frame="$parent"/>
                <Controller type="Style" end="Stop" style1="Storm_TopBar_TeamLevelLeft" style2="Storm_TopBar_TeamLevelLeftLevelUpBlue" >
                    <Key type="Curve" time="0.00" value="0.0" out="Slow"/>
                    <Key type="Curve" time="0.25" value="1.0" in="Slow" out="Slow"/>
                    <Key type="Curve" time="2.75" value="1.0" in="Slow" out="Slow"/>
                    <Key type="Curve" time="3.00" value="0.0" in="Slow"/>
                </Controller>
            </Animation>

            <Animation name="LevelUpRed">
                <Event event="Team1LevelUpRed" action="Reset" frame="$parent"/>
                <Event event="Team1LevelUpRed" action="Play" frame="$parent"/>
                <Controller type="Style" end="Stop" style1="Storm_TopBar_TeamLevelLeft" style2="Storm_TopBar_TeamLevelLeftLevelUpRed" >
                    <Key type="Curve" time="0.00" value="0.0" out="Slow"/>
                    <Key type="Curve" time="0.25" value="1.0" in="Slow" out="Slow"/>
                    <Key type="Curve" time="2.75" value="1.0" in="Slow" out="Slow"/>
                    <Key type="Curve" time="3.00" value="0.0" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="Team1LevelUpLabel">
            <Anchor side="Top" relative="$parent/Team1XPUnitStatusFrame" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Team1XPUnitStatusFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/Team1XPUnitStatusFrame" pos="Mid" offset="0"/>
            <!-- the font style for this is set to the opposite color for team 2 in script -->
            <Style val="Storm_TopBar_TeamLevelUpLeftBlue"/>
            <Text val="@UI/LevelUp"/>
            
            <Animation name="LevelUp">
                <Event event="Team1LevelUpBlue" action="Reset" frame="$parent"/>
                <Event event="Team1LevelUpBlue" action="Play" frame="$parent"/>
                <Event event="Team1LevelUpRed" action="Reset" frame="$parent"/>
                <Event event="Team1LevelUpRed" action="Play" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time="3" value="false"/>
                </Controller>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Auto"/>
                    <Key type="Curve" time="0.5" value="255" in="Auto"/>
                    <Key type="Curve" time="2.5" value="255" out="Auto"/>
                    <Key type="Curve" time="3" value="0" in="Auto"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="TeamLevelVSLabel">
            <Anchor side="Top" relative="$parent" pos="Min" offset="40"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Style val="Storm_TopBar_TeamLevelVS"/>
            <Text val="@UI/VS"/>
        </Frame>
        
        <Frame type="Label" name="Team2LevelLabel">
            <Anchor side="Top" relative="$parent/Team1LevelLabel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TopBarBacking" pos="Mid" offset="30"/>
            <Width val="90"/>
            <Style val="Storm_TopBar_TeamLevelRight"/>
            <Text val="88"/>
            
            <Animation name="LevelUpBlue">
                <Event event="Team2LevelUpBlue" action="Reset" frame="$parent"/>
                <Event event="Team2LevelUpBlue" action="Play" frame="$parent"/>
                <Controller type="Style" end="Stop" style1="Storm_TopBar_TeamLevelRight" style2="Storm_TopBar_TeamLevelRightLevelUpBlue" >
                    <Key type="Curve" time="0.00" value="0.0" out="Slow"/>
                    <Key type="Curve" time="0.25" value="1.0" in="Slow" out="Slow"/>
                    <Key type="Curve" time="2.75" value="1.0" in="Slow" out="Slow"/>
                    <Key type="Curve" time="3.00" value="0.0" in="Slow"/>
                </Controller>
            </Animation>

            <Animation name="LevelUpRed">
                <Event event="Team2LevelUpRed" action="Reset" frame="$parent"/>
                <Event event="Team2LevelUpRed" action="Play" frame="$parent"/>
                <Controller type="Style" end="Stop" style1="Storm_TopBar_TeamLevelRight" style2="Storm_TopBar_TeamLevelRightLevelUpRed" >
                    <Key type="Curve" time="0.00" value="0.0" out="Slow"/>
                    <Key type="Curve" time="0.25" value="1.0" in="Slow" out="Slow"/>
                    <Key type="Curve" time="2.75" value="1.0" in="Slow" out="Slow"/>
                    <Key type="Curve" time="3.00" value="0.0" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="Team2LevelUpLabel">
            <Anchor side="Top" relative="$parent/Team2XPUnitStatusFrame" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Team2XPUnitStatusFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/Team2XPUnitStatusFrame" pos="Mid" offset="0"/>
            <!-- the font style for this is set to the opposite color for team 2 in script -->
            <Style val="Storm_TopBar_TeamLevelUpRightRed"/>
            <Text val="@UI/LevelUp"/>
            
            <Animation name="LevelUp">
                <Event event="Team2LevelUpBlue" action="Reset" frame="$parent"/>
                <Event event="Team2LevelUpBlue" action="Play" frame="$parent"/>
                <Event event="Team2LevelUpRed" action="Reset" frame="$parent"/>
                <Event event="Team2LevelUpRed" action="Play" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time="3" value="false"/>
                </Controller>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Auto"/>
                    <Key type="Curve" time="0.5" value="255" in="Auto"/>
                    <Key type="Curve" time="2.5" value="255" out="Auto"/>
                    <Key type="Curve" time="3" value="0" in="Auto"/>
                </Controller>
            </Animation>
        </Frame>
        
        <!-- Core Health -->
        <Frame type="UnitStatusFrame" name="Team1CoreUnitStatus" template="TopBar/CoreUnitStatusLeftTemplate">
            <Anchor side="Top" relative="$parent/Team1LevelLabel" pos="Max" offset="-15"/>
            <Anchor side="Right" relative="$parent/Team1LevelLabel" pos="Max" offset="-65"/>
        </Frame>
        
        <Frame type="UnitStatusFrame" name="Team2CoreUnitStatus" template="TopBar/CoreUnitStatusRightTemplate">
            <Anchor side="Top" relative="$parent/Team2LevelLabel" pos="Max" offset="-15"/>
            <Anchor side="Left" relative="$parent/Team2LevelLabel" pos="Max" offset="-25"/>
        </Frame>
        
        <Frame type="Label" name="CoreHealthLabel">
            <Anchor side="Bottom" relative="$parent/Team1CoreUnitStatus" pos="Max" offset="-7"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="230"/>
            <Text val="@UI/CoreHealth"/>
            <Style val="Storm_TopBar_CoreHealth"/>
            <ColorAdjustMode val="OutlineColor"/>
            <AdjustmentColor val="0a2351"/>
        </Frame>
    </Frame> 

