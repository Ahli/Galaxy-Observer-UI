        </Frame>
        
        <Frame type="Image" name="Team1XPBackgroundRedArena">
            <Width val="268"/>
            <Height val="91"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TopBarBacking" pos="Min" offset="55"/>
            <Texture val="@UI/hud_topbar_arena_backingred"/>
            <TextureCoords top="0.0" left="1.0" bottom="1.0" right="0.0"/>
