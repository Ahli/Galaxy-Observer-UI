<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <Frame type="Frame" name="OverwatchEventHelpPanelTemplate">
        <Anchor relative="$parent"/>
        <Visible val="false"/>
        <AcceptsMouse val="False"/>
        
        <Frame type="Image" name="DarkenScreenImage">
            <Anchor relative="$parent"/>
            <Color val="000000"/>
            <Alpha val="97"/>
        </Frame>

        <Animation name="TriggerShow">
            <Event event="OnShown" action="Restart,Play"/>
            <Event event="OnMouseDown" action="Restart,Play"/>
            <Controller type="Event" end="Stop">
                <Key type="Event" time="0" event="Show"/>
            </Controller>
        </Animation>

        <Animation name="Show">
            <Event event="Show" action="Restart,Play"/>

            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0" sound="@Event_OWEvent_Start_Countdown"/>
            </Controller>
            
            <Controller type="Fade" end="Pause" frame="$parent\UIContainer\FullscreenUpperContainer\ChatDisplay">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time="2.6" value="0" inout="Slow,Linear"/>
				<Key type="Curve" time="3.2" value="255" inout="Slow,Linear"/>
            </Controller>
			
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time=".2" value="255" inout="Slow,Linear"/>
                <Key type="Curve" time="2.6" value="255" inout="Linear,Fast"/>
                <Key type="Curve" time="3.0" value="255" inout="Linear,Fast"/>
                <Key type="Curve" time="3.2" value="0" inout="Slow,Linear"/>
            </Controller>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0" value="True"/>
                <Key type="Bool" time="3.3" value="True"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="CountdownLabel">
                <Key type="Event" time=".6" event="Countdown"/>
            </Controller>

            <Controller type="Rotation" relative="False" end="Pause" frame="DottedLineImage">
                <Key type="Curve" time="0" value="-720" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="linear"/>
                <Key type="Curve" time="3.2" value="20" inout="fast,Linear"/>
