    <Frame type="Frame" name="TutorialCardButtonTemplate">
        <Anchor side="Bottom" relative="Background" pos="Max" offset="0"/>
        <Anchor side="Right" relative="Background" pos="Max" offset="0"/>
        
        <Visible val="False"/>
        
        <Frame type="BlurFrame" name="BlurFrame" template="StandardTemplates/FullscreenBlurFrame">
            <Anchor relative="$parent/Background"/>
