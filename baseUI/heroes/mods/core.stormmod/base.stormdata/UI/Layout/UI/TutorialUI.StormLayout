            <Alpha val="150"/>
            <Visible val="#IngameBlur"/>
            <RenderType val="HDR"/>
        </Frame>

        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/RightArrow" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/RightArrow" pos="Max" offset="0"/>
            <Texture val="@UI/Storm_TutorialReferenceCardBG"/>
            <TextureType val="Nineslice"/>
            <Alpha val="180"/>
        </Frame>
        
        <Frame type="Button" name="RightArrow">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="RightArrowImage" pos="Max" offset="0"/>
            <Anchor side="Right" relative="RightArrowImage" pos="Max" offset="0"/>
            
            <Frame type="Label" name="ShowLabel">
                <Anchor side="Top" relative="$parent/RightArrowImage" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/RightArrowImage" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
                <Text val="@UI/ShowHelp"/>
                <Style val="Storm_TutorialReferenceCardItem"/>
            </Frame>
            
            <Frame type="Image" name="RightArrowImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/ShowLabel" pos="Max" offset="5"/>
                <Texture val="@UI/Storm_ArrowButtonRightNormal"/>
            </Frame>
        
            <Frame type="Image" name="RightArrowImageHover">
                <Anchor relative="$parent/RightArrowImage"/>
                <Texture val="@UI/Storm_ArrowButtonRightHover"/>
                <Alpha val="0"/>
                <Animation name="Hover">
                    <Event event="OnMouseEnter" action="DirectionForward" frame="$parent"/>
                    <Event event="OnMouseEnter" action="Play" frame="$parent"/>
                    <Event event="OnMouseExit" action="DirectionReverse" frame="$parent"/>
                    <Event event="OnMouseExit" action="Play" frame="$parent"/>
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" time=".2" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>
            <Animation name="Click">
                <Event event="OnMouseUp" action="Restart,Play"/>
                <Controller type="Visibility" end="Pause" frame="$parent">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time=".01" value="False"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$parent/$parent/TutorialReferenceCard">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time=".01" value="True"/>
                </Controller>
            </Animation>
        </Frame>

    </Frame>

