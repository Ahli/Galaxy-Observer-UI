                <Animation name="BlueFilter">
                    <Event event="BlueFilter" action="Reset,Play" frame="$this"/>
                    
                    <Controller type="Cutscene" end="Stop">
                        <Key type="Cutscene" time="0.0" action="ClearFilters"/>
