    <Frame type="Frame" name="SecondaryCutsceneTemplate">
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Width val="1650"/>
        <Height val="1200"/>
        
        <Frame type="HeroCutsceneFrame" name="HeroCutscene">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="50"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="1650"/>
            <Height val="1200"/>
            <UseAlternateTime val="true"/>
            <Unclipped val="True"/>
            <HeroCutsceneType val="EndOfMatch"/>
            <AutoPlay val="True"/>
            
            <!-- Set the starting bookmark on visible -->
            <Animation name="SlotReveal">
                <Event event="OnShown" action="Reset,Play"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" time="0.0" action="Bookmark" bookmark="Line Up Start"/>
                </Controller>
            </Animation>
            
            <!-- Apply filters and bookmarks on tier++;
                    Animation state is sent from triggers -->
            <StateGroup name="CutsceneTierFilterState">
                <State name="Tier1">
                    <When type="AnimationState" AwardTier="Tier1"/>
                    <Action type="SendEvent" event="Tier1EpicTransition"/>
                </State>
                <State name="Tier2">
                    <When type="AnimationState" AwardTier="Tier2"/>
                    <Action type="SendEvent" event="Tier2LegendaryTransition"/>
                </State>
            </StateGroup>
            
            <Animation name="Tier1EpicTransition">
                <Event event="Tier1EpicTransition" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" time="0.0" action="Bookmark" bookmark="Epic"/>
                    <Key type="Cutscene" time="0.0" action="AddFilter" filter="Epic"/>
                </Controller>
            </Animation>
            
            <Animation name="Tier2LegendaryTransition">
                <Event event="Tier2LegendaryTransition" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" time="0.0" action="Bookmark" bookmark="Legendary"/>
                    <Key type="Cutscene" time="0.0" action="AddFilter" filter="Legendary"/>
                </Controller>
            </Animation>
            
            <!-- team color set as an animation state ahead of time
                when the slot filter has been set after the cutscene loads, we can add the team filter -->
            <StateGroup name="CutsceneApplyTeamColor">
                <State name="Blue">
                    <When type="AnimationState" TeamColor="Blue"/>
                    <Action type="SendEvent" frame="$this" event="BlueTeamEffectFilter"/>
                </State>
                <State name="Red">
                    <When type="AnimationState" TeamColor="Red"/>
                    <Action type="SendEvent" frame="$this" event="RedTeamEffectFilter"/>
                </State>
            </StateGroup>
            
            <Animation name="BlueTeamEffectFilter">
                <Event event="BlueTeamEffectFilter" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" time="0.0" action="AddFilter" filter="Team0"/>
                </Controller>
            </Animation>
            
            <Animation name="RedTeamEffectFilter">
                <Event event="RedTeamEffectFilter" action="Reset,Play" frame="$this"/>
                
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" time="0.0" action="AddFilter" filter="Team1"/>
                </Controller>
            </Animation>
            
            <!-- state group waits for the cutscene to load if there is a need to update when secondaries are revealed (if votes have been cast due to earlier skip)-->
            <StateGroup name="SkipStatusFlags">
                <State name="Tier0">
                    <When type="AnimationState" SkipStatusFlags="Tier0"/>
                    <When type="Property" Visible="True"/>
                    <Action type="SendEvent" event="Tier0Sync"/>
                </State>
                
                <State name="Tier1">
                    <When type="AnimationState" SkipStatusFlags="Tier1"/>
                    <When type="Property" Visible="True"/>
                    <Action type="SendEvent" event="Tier0Sync"/>
                    <Action type="SendEvent" event="Tier1Sync"/>
                    <Action type="SendEvent" event="Tier1EpicTransition"/>
                </State>
                
                <State name="Tier2">
                    <When type="AnimationState" SkipStatusFlags="Tier2"/>
                    <When type="Property" Visible="True"/>
                    <Action type="SendEvent" event="Tier0Sync"/>
                    <Action type="SendEvent" event="Tier2Sync"/>
                    <Action type="SendEvent" event="Tier2LegendaryTransition"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>

    <Frame type="CountdownLabel" name="CountdownLabelTemplate">
