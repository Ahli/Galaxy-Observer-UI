<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <Frame type="Frame" name="SparkleImageTemplate">
        <Frame type="Image" name="SparkleImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Global_Sparkle_Orange"/>
            <Unclipped val="True"/>
            <BlendMode val="Add"/>
			<Alpha val="0"/>
        </Frame>
        <Width val="20"/>
        <Height val="20"/>
        <Animation name="Shine">
            <Event event="Shine" action="Restart,Play"/>
            <Controller type="Fade" end="Loop" frame="SparkleImage">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".1" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".5" value="255" inout="Linear,Slow"/>
                <Key type="Curve" time="1" value="0" inout="Linear"/>
            </Controller>
            <Controller type="Anchor" end="Loop" side="Top" relative="true">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time="1" value="-250" inout="Slow,Linear"/>
            </Controller>
        </Animation>
    </Frame>
    
        
    <Frame type="UnitStatusFrame" name="OnlyEnergyUnitStatusFrameTemplate" template="UnitStatus/OnlyEnergyUnitStatusFrameTemplate">
        
        <Frame type="UnitStatusEnergyBar" name="EnergyBar" template="UnitStatus/UnitStatusEnergyBarTemplate">
            <Texture val="@UI/Storm_UI_HUD_Bar_Nameplate_Yellow"/>
            <OwnerColor val="ffffff" index="0"/>
            <AllyColor val="ffffff" index="0"/>
            <EnemyColor val="ffffff" index="0"/>
            <OwnerColor val="ffffff" index="1"/>
            <AllyColor val="ffffff" index="1"/>
            <EnemyColor val="ffffff" index="1"/>
            <Width val="134"/>
            <Height val="14"/>
        </Frame>
        
        <Frame type="Label" name="PlateDescription">
            <Anchor side="Top" relative="$parent/BGImageContainer" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent/BGImageContainer" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/BGImageContainer" pos="Mid" offset="0"/>
            <Text val="@Param/Value/lib_GDHL_09438CE3"/>
            <Style val="MechanicHelperStyle_Urgent"/>
            <SubpixelRendering val="True"/>
            <Unclipped val="True"/>
        </Frame>
    </Frame>

    
    <Frame type="Frame" name="LunarNewYearTemplate">
        <Anchor relative="$parent"/>
        <Visible val="false"/>

        <Frame type="Frame" name="QuestCompleteNotificationFrame" template="MapMechanicPanel/NotificationFrameTemplate">
            <Anchor side="Bottom" relative="$parent/QuestNotificationFrame" pos="Min" offset="30"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

            <Animation name="Show">
                <Event event="OnShown" action="Restart,Play"/>
                
                <Controller type="Event" end="Stop" frame="Sparkle1Image">
                    <Key type="Event" time=".5" event="Shine"/>
                </Controller>
    
                <Controller type="Event" end="Stop" frame="Sparkle2Image">
                    <Key type="Event" time="1.5" event="Shine"/>
                </Controller>
    
                <Controller type="Event" end="Stop" frame="Sparkle3Image">
                    <Key type="Event" time=".2" event="Shine"/>
                </Controller>
    
                <Controller type="Event" end="Stop" frame="Sparkle4Image">
                    <Key type="Event" time="1.7" event="Shine"/>
                </Controller>
            </Animation>
            
            <Frame type="Image" name="NotificationIcon">
                <Visible val="False"/>
            </Frame>
            
            <Frame type="Image" name="IconBackgroundImage">
                <Anchor side="Top" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-26"/>
                <Anchor side="Bottom" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-26"/>
                <Anchor side="Left" relative="$parent/NotificationIconAnchor" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/NotificationIconAnchor" pos="Mid" offset="0"/>
                
                <Texture val="@UI/Storm_UI_MapMechanic_Notification_LunarMonkey_SuccessBG"/>
                <Unclipped val="True"/>
            </Frame>
            
            <Frame type="Frame" name="Sparkle1Image" template="LunarNewYear/SparkleImageTemplate">
                <Anchor side="Bottom" relative="$parent/IconBackgroundImage" pos="Max" offset="20"/>
                <Anchor side="Left" relative="$parent/IconBackgroundImage" pos="Mid" offset="-43"/>
                <Animation name="StartShine">
                    <Event event="Shine" action="Restart,Play"/>
                    <Controller type="Fade" end="Loop">
                        <Key type="Curve" time="0" value="0"/>
                        <Key type="Curve" time=".3" value="0" inout="Linear,Slow"/>
                        <Key type="Curve" time=".4" value="255" inout="Linear"/>
                        <Key type="Curve" time="1" value="255"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Frame" name="Sparkle2Image" template="LunarNewYear/SparkleImageTemplate">
                <Anchor side="Bottom" relative="$parent/IconBackgroundImage" pos="Max" offset="10"/>
                <Anchor side="Left" relative="$parent/IconBackgroundImage" pos="Mid" offset="-23"/>
            </Frame>
            
            <Frame type="Frame" name="Sparkle3Image" template="LunarNewYear/SparkleImageTemplate">
                <Anchor side="Bottom" relative="$parent/IconBackgroundImage" pos="Max" offset="-14"/>
                <Anchor side="Left" relative="$parent/IconBackgroundImage" pos="Mid" offset="-5"/>
            </Frame>

            <Frame type="Frame" name="Sparkle4Image" template="LunarNewYear/SparkleImageTemplate">
                <Anchor side="Bottom" relative="$parent/IconBackgroundImage" pos="Max" offset="8"/>
                <Anchor side="Left" relative="$parent/IconBackgroundImage" pos="Mid" offset="15"/>
                <Animation name="StartShine">
                    <Event event="Shine" action="Restart,Play"/>
                    <Controller type="Fade" end="Loop">
                        <Key type="Curve" time="0" value="0"/>
                        <Key type="Curve" time=".2" value="0" inout="Linear,Slow"/>
                        <Key type="Curve" time=".3" value="255" inout="Linear"/>
                        <Key type="Curve" time="1" value="255"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Frame" name="IconCardFrame">
                <Anchor side="Bottom" relative="$parent/IconBackgroundImage" pos="Max" offset="-1"/>
                <Anchor side="Left" relative="$parent/IconBackgroundImage" pos="Min" offset="26"/>
                <Width val="88"/>
                <Height val="92"/>
                
                <Frame type="Frame" name="ProgressBarMask">
                    <Anchor relative="$parent"/>
                    <AlphaMaskTexture val="@UI/Storm_UI_MapMechanic_Notification_LunarMonkey_Success_IconMask"/>
                    <AlphaMaskSiblings val="true"/>
                    <RenderPriority val="0"/>
                    <Visible val="true"/>
                </Frame>
                
                <Frame type="Image" name="IconCardImage">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
                    <Texture val="@UI/Storm_UI_MapMechanic_Notification_LunarMonkey_Success_Card"/>
                    <SubpixelRendering val="True"/>
                    
                    <Animation name="Enter">
                        <Event event="OnShown" action="Restart,Play"/>
                        <Controller type="Anchor" end="Pause" side="Top" relative="true">
                            <Key type="Curve" time="0" value="104" inout="Linear"/>
                            <Key type="Curve" time=".3" value="-10" inout="Slow"/>
                            <Key type="Curve" time=".5" value="0" inout="Linear"/>
                        </Controller>
                        <Controller type="Event" end="Stop">
                            <Key type="Event" time=".5" event="Loop"/>
                        </Controller>
                    </Animation>
                    <Animation name="Loop">
                        <Event event="Loop" action="Restart,Play"/>
                        <Controller type="Anchor" end="Loop" side="Top" relative="true">
                            <Key type="Curve" time="0" value="0" inout="Linear"/>
                            <Key type="Curve" time=".8" value="6" inout="Slow"/>
                            <Key type="Curve" time="1.6" value="-6" inout="Linear"/>
                            <Key type="Curve" time="2.3" value="0" inout="Linear"/>
                        </Controller>
                    </Animation>
                </Frame>
            </Frame>
            
            <Frame type="Image" name="BorderCoverImage">
                <Anchor side="Bottom" relative="$parent/IconBackgroundImage" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/IconBackgroundImage" pos="Min" offset="26"/>
                <Texture val="@UI/Storm_UI_MapMechanic_Notification_LunarMonkey_Success_BorderCover"/>
            </Frame>

            <Frame type="Image" name="TopGlowImage">
                <Anchor side="Bottom" relative="$parent/IconBackgroundImage" pos="Max" offset="-8"/>
                <Anchor side="Left" relative="$parent/IconBackgroundImage" pos="Min" offset="10"/>
                <Texture val="@UI/Storm_UI_MapMechanic_Notification_LunarMonkey_Success_TopGlow"/>
                <BlendMode val="Add"/>
                <Animation name="Flicker">
                    <Event event="OnShown" action="DirectionForward,Play"/>
                    <Controller type="Fade" end="PingPong">
                        <Key type="Curve" time="0" value="150" inout="Linear,Slow"/>
                        <Key type="Curve" time=".3" value="255" inOut="Fast,Slow"/>
                        <Key type="Curve" time=".6" value="169" inOut="Fast,Slow"/>
                        <Key type="Curve" time=".9" value="255" inout="Linear"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Label" name="NotificationLabel">
                <Text val="@UI/Event/LunarNewYear/MapUI/Captured"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="QuestNotificationFrame" template="MapMechanicPanel/NotificationFrameTemplate">
            <Anchor side="Bottom" relative="$parent/BackgroundContainer" pos="Min" offset="30"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            
            <Frame type="Image" name="NotificationIcon">
                <Anchor side="Top" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-1"/>
                <Anchor side="Bottom" relative="$parent/NotificationIconAnchor" pos="Mid" offset="-1"/>
                <Texture val="@UI/Storm_UI_MapMechanic_Notification_LunarMonkey_HelpIcon"/>
                
                <Frame type="Image" name="Shine">
                    <Texture val="@UI/Storm_UI_MapMechanic_Notification_LunarMonkey_HelpIcon" layer="1"/>
                </Frame>
            </Frame>
            
            <Frame type="Label" name="NotificationLabel">
                <Text val="@UI/Event/LunarNewYear/MapUI/HelpPrompt"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="BackgroundContainer" template="MapMechanicPanel/NotificationFrameCountdownTemplate">
            <Anchor side="Bottom" relative="$parent/$parent/MinimapPanel" pos="Min" offset="-25"/>
            <Anchor side="Right" relative="$parent/$parent/MinimapPanel" pos="Max" offset="0"/>
            <Visible val="True"/>

            <Frame type="Image" name="NotificationIcon">
                <Texture val="@UI/Storm_UI_MapMechanic_Notification_Elder_Monkey"/>
                <Frame type="Image" name="Shine">
                    <Texture val="@UI/Storm_UI_MapMechanic_Notification_Elder_Monkey" layer="1"/>
                </Frame>
            </Frame>

            <Frame type="Label" name="NotificationLabel">
                <Anchor side="Top" relative="$parent/BackgroundImage" pos="Min" offset="1"/>
                <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="10"/>
                <Style val="Storm_MapMechanic_MechanicCounterSmallLeft_Blue"/>
                <Text val="@UI/Event/LunarNewYear/MapUI/CaptureNotificationTitle"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="HealthBarBG">
            <Anchor side="Top" relative="$parent/BackgroundContainer/BackgroundImage" pos="Mid" offset="2"/>
            <Anchor side="Left" relative="$parent/BackgroundContainer/BackgroundImage" pos="Min" offset="13"/>
            <Anchor side="Right" relative="$parent/BackgroundContainer/NotificationIcon" pos="Min" offset="13"/>
            <TextureType val="Nineslice"/>
            <Texture val="@UI/HUD_MapMechanic_BG_HealthBar"/>
        </Frame>
        
        <Frame type="Label" name="LunarNewYearTimer">
            <Anchor side="Bottom" relative="$parent/BackgroundContainer/NotificationIcon" pos="Max" offset="-22"/>
            <Anchor side="Left" relative="$parent/BackgroundContainer/NotificationIcon" pos="Min" offset="22"/>
            <Anchor side="Right" relative="$parent/BackgroundContainer/NotificationIcon" pos="Max" offset="-20"/>
            <Style val="Storm_MapMechanic_MechanicCounterCentered"/>
            <Text val="88:88"/>
        </Frame>
        
        <Frame type="UnitStatusFrame" name="LunarNewYearHealthBar" template="MapMechanicPanel/MapMechanicHealthBarTemplate">
            <Anchor relative="$parent/BackgroundContainer" offset="0"/>
            
            <Frame type="UnitStatusHealthBar" name="HealthBar">
                <SegmentValue val="100"/>
                <Height val="9"/>
                <Width val="0"/>
                <Anchor side="Top" relative="$parent/$parent/HealthBarBG" pos="Min" offset="5"/>
                <Anchor side="Left" relative="$parent/$parent/HealthBarBG" pos="Min" offset="5"/>
                <Anchor side="Right" relative="$parent/$parent/HealthBarBG" pos="Max" offset="-5"/>
                <Texture val="@UI/Storm_UI_HUD_Bar_HealthBar_Yellow"/>
                <Expansion val="Left"/>
                <AllyColor val="ffffff" index="0"/>
                <EnemyColor val="ffffff" index="0"/>
            </Frame>
            
            <Frame type="UnitStatusHealthLabel" name="HealthLabel">
                <Anchor side="Top" relative="$parent/$parent/BackgroundContainer/NotificationLabel" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent/$parent/BackgroundContainer/NotificationLabel" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent/BackgroundContainer/NotificationLabel" pos="Max" offset="75"/>
                <Width val="58"/>
                <Frame type="Label" name="StatusLabel">
                    <Style val="Storm_MapMechanic_MechanicCounterSmallLeft_Blue"/>
                </Frame>
            </Frame>
        </Frame>

    </Frame>

</Desc>
