    <Frame type="RankBadge" name="RankBadgeDebugTemplate" template="RankBadgeTemplates/RankBadgeTemplate">
        <AcceptsMouse val="true"/>
        <Animation name="Trigger">
            <Event event="OnMouseUp" action="Restart,Play"/>
            <Controller type="Event" end="Pause" frame="$this">
                <Key type="Event" time="0" event="DebugSetup"/>
                <Key type="Event" time="0" event="DebugStart"/>
            </Controller>
        </Animation>

        <Animation name="Setup">
            <Event event="OnMouseUp" action="Restart,Play" frame="$ancestor[@name=DebugPanelRankBadge]/ResetButton"/>
            <Event event="OnMouseUp" action="Restart,Play" frame="$ancestor[@name=ScreenAssetDebug]/DebugTabs/DebugButtonRankBadge"/>
            <Controller type="Event" end="Pause">
                <Key type="Event" time="0" event="DebugSetup"/>
            </Controller>
        </Animation>
    </Frame>

