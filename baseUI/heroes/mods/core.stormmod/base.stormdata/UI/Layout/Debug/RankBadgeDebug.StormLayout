            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Text val="R"/>
        </Frame>

        <AcceptsMouse val="true"/>
    </Frame>

    <Frame type="Image" name="StartButtonTemplate" template="RankBadgeDebug/SmallButtonTemplate">
        <Anchor side="Left" relative="$parent" pos="Mid" offset="10"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="10"/>
        <Animation name="Trigger">
            <Event event="OnMouseUp" action="Restart,Play"/>
            <Controller type="Event" end="Pause" frame="$parent">
                <Key type="Event" time="0" event="DebugStart"/>
            </Controller>
        </Animation>

        <Frame type="Label" name="DebugPanelTempLabel">
            <Text val="S"/>
        </Frame>
    </Frame>

    <Frame type="Image" name="SetupButtonTemplate" template="RankBadgeDebug/SmallButtonTemplate">
        <Anchor side="Left" relative="$parent" pos="Mid" offset="-10"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="-10"/>
        <Animation name="Trigger">
            <Event event="OnMouseUp" action="Restart,Play"/>
