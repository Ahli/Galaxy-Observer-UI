                    <Key type="Bool" time=".2" value="False"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$parent/HuntBGImage/Bullet3Image">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time=".3" value="False"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$parent/HuntBGImage/Bullet2Image">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time=".4" value="False"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$parent/HuntBGImage/Bullet1Image">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time=".5" value="False"/>
                </Controller>
            </Animation>
            <Animation name="FlyAwayStep">
                <Event event="ResetEgg" action="Stop" frame="$parent"/>
                <Event event="FlyAwayStep" action="Play"/>
                <Controller type="Animation" end="Stop" animation="FlyAwaySteps">
                    <Key type="Animation" time=".1" action="Pause"/>
                </Controller>
            </Animation>
            <Animation name="FlyAwaySteps">
                <Event event="ResetEgg" action="Stop" frame="$parent"/>
                <Event event="FlyAwayStep" action="DirectionForward"/>
                <Event event="FlyAwayStep" action="Play"/>
                <Controller type="Event" end="Stop" frame="$parent/GullCutscene">
                    <Key type="Event" time=".5" event="FlyAway"/>
                </Controller>
            </Animation>
            <Animation name="Flash">
                <Event event="Flash" action="Restart"/>
                <Event event="Flash" action="Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".05" value="255" in="Slow"/>
                    <Key type="Curve" time=".1" value="0" in="Slow"/>
                </Controller>
                <Controller type="Fade" end="Pause" frame="$parent/KerpangWhiteImage">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".05" value="255" in="Slow"/>
                    <Key type="Curve" time=".1" value="0" in="Slow"/>
                </Controller>
                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0" sound="@UI_Home_HVH_DoveHunt_Gun_Fire"/>
                </Controller>
            </Animation>
            <Animation name="Miss">
                <Event event="OnMouseDown" action="Restart"/>
                <Event event="OnMouseDown" action="Play"/>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="FlyAwayStep"/>
                </Controller>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="BulletStep"/>
                </Controller>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="Flash"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="KerpangWhiteImage">
            <Anchor relative="$parent/GullCutscene"/>
            <RenderPriority val="550"/>
            <Color val="ffffff"/>
            <Alpha val="0"/>
            <AcceptsMouse val="True"/>
            <Visible val="False"/>
            <Animation name="Hit">
                <Event event="OnMouseDown" action="Restart"/>
                <Event event="OnMouseDown" action="Play"/>
                <Controller type="Event" end="Stop" frame="$parent/KerpangBlackImage">
                    <Key type="Event" time="0" event="Flash"/>
                </Controller>
                <Controller type="Event" end="Stop" frame="$parent/KerpangBlackImage">
                    <Key type="Event" time="0" event="BulletStep"/>
                </Controller>
                <Controller type="Event" end="Stop" frame="$parent/GullCutscene">
                    <Key type="Event" time="0" event="Shot"/>
                </Controller>
            </Animation>
            <Animation name="NoHitState">
                <Event event="Shot" action="Restart" frame="$parent/GullCutscene"/>
                <Event event="Shot" action="Play" frame="$parent/GullCutscene"/>
                <Event event="FlyAway" action="Restart" frame="$parent/GullCutscene"/>
                <Event event="FlyAway" action="Play" frame="$parent/GullCutscene"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.1" value="False"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$parent/KerpangBlackImage">
                    <Key type="Bool" time="0.1" value="False"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
    
