<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
  <Frame type="HeroContainerFrame" name="FenixFrameTemplate" template="HomeShared/NewContentFrameTemplate">
    <Hero val="Fenix" />
    <Frame type="Image" name="BackgroundImage">
      <Width val="2133" />
      <Height val="1200" />
      <RenderType val="HDR" />
      <RenderPriority val="500" />
    </Frame>
    <Frame type="HeroGlueButton" name="ShopButton" template="StandardTemplates/StandardButtonTemplate">
      <Handle val="ScreenHome_ShopButton_Fenix" />
      <Frame type="ABChoiceFrame" name="MoreInfoBuy">
        <Anchor relative="$parent" />
        <Visible val="true" />
        <NumABChoices val="10" />
        <Handle val="FenixAB" />
        <ReportsStats val="true" />
      </Frame>
      <StateGroup name="ForceUpdate">
        <DefaultState val="A" />
        <State name="B">
          <When type="Property" frame="MoreInfoBuy" ABChoice="9" />
          <Action type="SetProperty" frame="$this" Text="@UI/ScreenHome/ShopButtonB" undo="false" />
        </State>
        <State name="A">
          <Action type="SetProperty" frame="$this" Text="@UI/ScreenHome/ShopButton" undo="false" />
        </State>
      </StateGroup>
    </Frame>
    <Frame type="HeroGlueButton" name="TryButton" template="StandardTemplates/StandardButtonTemplate">
      <Handle val="ScreenHome_TryButton_Fenix" />
    </Frame>
  </Frame>
</Desc>