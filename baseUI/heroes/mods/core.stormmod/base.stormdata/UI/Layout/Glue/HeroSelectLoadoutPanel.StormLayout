        <Anchor side="Top" relative="$parent/LoadoutSelectorPulldown" pos="Mid" offset="0" />
        <Anchor side="Left" relative="$parent/LoadoutSelectorPulldown" pos="Max" offset="0" />
        <Anchor side="Bottom" relative="$parent/LoadoutSelectorPulldown" pos="Mid" offset="0" />
        <MaxCharacters val="28" />
        <LoseFocusOnEnterOrEsc val="true" />
        <Text val="{$parent/LoadoutCacheNameFrame/@Name}" />
        <Frame type="Image" name="Image">
          <Alpha val="0" />
        </Frame>
        <Frame type="Frame" name="LabelContainerFrame">
          <Frame type="Label" name="Label">
            <Style val="LoadoutNameLabel" />
          </Frame>
        </Frame>
        <Frame type="Label" name="SearchTextLabel">
          <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
          <Anchor side="Left" relative="$parent" pos="Min" offset="20" />
          <Anchor side="Right" relative="$this" pos="Min" offset="0" />
