          <Style val="LoaodoutEnterNameLabel" />
          <Text val="@UI/HeroSelectPanel/Loadout/EnterLoadoutName" />
        </Frame>
      </Frame>
      <Frame type="LoadoutCacheFrame" name="LoadoutCacheNameFrame">
        <Anchor relative="$parent" />
        <HeroLink val="{$parent/$parent/@HeroLink}" />
        <Name val="{$parent/LoadoutNameEditBox/@Text}" />
      </Frame>
      <Frame type="Image" name="EditTextImage">
        <Anchor side="Top" relative="$parent/LoadoutNameEditBox/LabelContainerFrame/Label" pos="Mid" offset="0" />
        <Anchor side="Left" relative="$parent/LoadoutNameEditBox/LabelContainerFrame/Label" pos="Max" offset="10" />
        <Anchor side="Bottom" relative="$parent/LoadoutNameEditBox/LabelContainerFrame/Label" pos="Mid" offset="0" />
        <Texture val="@UI/Storm_UI_Icon_Edit_Normal" />
        <StateGroup name="HoverStates">
          <State name="Hovering">
            <When type="Property" frame="$parent/LoadoutNameEditBox" Hovering="true" />
            <Action type="SetProperty" Texture="@UI/Storm_UI_Icon_Edit_Hover" />
          </State>
        </StateGroup>
        <StateGroup name="AnchorStates">
          <State name="NoLoadoutName">
            <When type="Property" frame="$parent/LoadoutNameEditBox/SearchTextLabel" Visible="true" />
            <Action type="SetAnchor" side="Left" relative="$parent/LoadoutNameEditBox/SearchTextLabel" pos="Max" offset="10" />
          </State>
        </StateGroup>
      </Frame>
      <StateGroup name="EditingStates">
        <State name="IsEditing">
          <When type="Property" frame="LoadoutNameEditBox" Focused="true" />
          <Action type="SendEvent" Event="StartEdit" />
        </State>
        <State name="IsNotEditing">
          <When type="Property" frame="LoadoutNameEditBox" Focused="false" />
          <Action type="SendEvent" Event="StopEdit" />
        </State>
      </StateGroup>
      <Animation name="EditAnims">
        <Event event="StartEdit" action="DirectionForward, Play" />
        <Event event="StopEdit" action="DirectionReverse, Play" />
        <Controller type="Fade" end="Pause" frame="EditTextImage">
          <Key type="Curve" time="0" value="255" />
          <Key type="Curve" time=".10" value="0" />
        </Controller>
        <Controller type="Fade" end="Pause" frame="LoadoutNameEditBox/Image">
          <Key type="Curve" time="0" value="0" />
          <Key type="Curve" time=".10" value="255" />
        </Controller>
      </Animation>
    </Frame>
