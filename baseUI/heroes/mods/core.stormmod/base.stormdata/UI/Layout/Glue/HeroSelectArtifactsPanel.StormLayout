                    <TooltipAnchorPosition val="TopCenter"/>
                </Frame>
            </Frame>
        </Frame>

        <Animation name="SlideInSound">
            <Event event="Show" action="DirectionForward,Play"/>
            <Event event="Hide" action="DirectionReverse,Play"/>
            <Controller type="Sound" end="Pause">
                <Key type="Sound" time="0.00" sound="@UI_BNet_ArtifactsPanelOpen" direction="Forward"/>
                <Key type="Sound" time="0.175" sound="@UI_BNet_ArtifactsPanelClose" direction="Reverse"/>
            </Controller>
        </Animation>
    </Frame>
</Desc>
