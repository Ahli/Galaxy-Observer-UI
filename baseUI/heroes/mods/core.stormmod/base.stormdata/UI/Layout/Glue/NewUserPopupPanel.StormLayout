                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="9" value="True"/>
                </Controller>
            </Animation>
            
                            
            <Animation name="AutoPlayFrames">
                <Event event="AutoPlayFrames" action="Restart,Play"/>
                <Event event="StopAutoPlay" action="Stop"/>
                <Event event="OnShown" action="Stop" frame="$parent"/>
                <Event event="OnHidden" action="Stop" frame="$parent"/>

                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="Page1"/>
                    <Key type="Event" time="3" event="Page2"/>
                    <Key type="Event" time="6" event="Page3"/>
                </Controller>
                <Controller type="Event" end="Stop" frame="PageTabs/PageTab1">
                    <Key type="Event" time="0.01" event="FillAnim"/>
                </Controller>
                <Controller type="Event" end="Stop" frame="PageTabs/PageTab2">
                    <Key type="Event" time="3.01" event="FillAnim"/>
                </Controller>
                <Controller type="Event" end="Stop" frame="PageTabs/PageTab3">
                    <Key type="Event" time="6.01" event="FillAnim"/>
                </Controller>
            </Animation>

            <Animation name="Page1">
                <Event event="CommercialBreak" action="Stop,Play" frame="$parent"/>
                <Event event="Page1" action="Restart,Play"/>

                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="PageTransition"/>
                </Controller>

                <Controller type="Event" end="Stop" frame="CommercialFrame1">
                    <Key type="Event" time="0" event="Begin"/>
                </Controller>
            </Animation>

            <Animation name="Page2">
                <Event event="CommercialBreak" action="Stop" frame="$parent"/>
                <Event event="Page2" action="Restart,Play"/>
                
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="PageTransition"/>
                </Controller>

                <Controller type="Event" end="Stop" frame="CommercialFrame2">
                    <Key type="Event" time="0" event="Begin"/>
                </Controller>
            </Animation>

            <Animation name="Page3">
                <Event event="CommercialBreak" action="Stop" frame="$parent"/>
                <Event event="Page3" action="Restart,Play"/>

                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="PageTransition"/>
                </Controller>

                <Controller type="Event" end="Stop" frame="CommercialFrame3">
                    <Key type="Event" time="0" event="Begin"/>
                </Controller>
            </Animation>
            
        </Frame>

        <StateGroup name="RewardState">
            <State name="RewardClaimed">
                <When type="Property" ChallengeCleared="true"/>
                <Action type="SetProperty" frame="ProgressFrame/Rewards/RewardLabel" style="StoreItemGoldLabel"/>
                <Action type="SetProperty" frame="ProgressFrame/Rewards/RewardCompletion" texture="@UI/Storm_UI_NewUser_IconComplete"/>
            </State>
            
            <State name="RewardedElsewhere">
                <When type="Property" RewardRemaining="0"/>
                <Action type="SetProperty" frame="ProgressFrame/Rewards/RewardCompletion" texture="@UI/Storm_UI_NewUser_IconComplete"/>
            </State>
            
            <State name="RewardAvailable">
                <When type="Property" ChallengeCleared="false"/>
                <Action type="SetProperty" frame="ProgressFrame/Rewards/RewardLabel" style="NewUserHomePageLocked"/>
                <Action type="SetProperty" frame="ProgressFrame/Rewards/RewardCompletion" texture="@UI/Storm_UI_NewUser_SmallLockIcon"/>
            </State>
        </StateGroup>

        <StateGroup name="CommercialState">
            <State name="On">
                <When type="Property" CommercialActive="true"/>
                <Action type="SetProperty" frame="CommercialBreakFrame" Visible="true"/>
                <Action type="SendEvent" frame="$this" event="CommercialBreak"/>
            </State>
            <State name="Off">
                <When type="Property" CommercialActive="false"/>
                <Action type="SetProperty" frame="CommercialBreakFrame" Visible="false"/>
                <Action type="SetProperty" frame="ProgressFrame" Visible="true"/>
                <Action type="SetProperty" frame="ProgressFrame" alpha="255"/>
            </State>
        </StateGroup>

        <StateGroup name="TutorialProgress">
            <State name="Welcome">
                <When type="Property" JustPlayed="false"/>
                <When type="Property" ChallengeCleared="false"/>
                <Action type="SetProperty" frame="TitleLabel" text="@UI/MapLoadingScreen/VeteranTutorialWelcome"/>
                <Action type="SetProperty" frame="ProgressFrame/PrimaryButton" text="@UI/Play"/>
                <Action type="SetProperty" frame="ProgressFrame/SecondaryButton" visible="false"/>
                <Action type="SetProperty" frame="ProgressFrame/DescriptionLabel" text="@UI/NewUser/VeteranIntroduction/WelcomeBody"/>
                <Action type="SetProperty" frame="ProgressFrame/ProgressLabel" text="@UI/NewUser/VeteranIntroduction/Title"/>
                <Action type="SetProperty" frame="ProgressFrame/ProgressLabel" style="NewUserHomePageTitle"/>
            </State>

            <State name="FinishedFirstTime">
                <When type="Property" CommercialActive="true"/>
                <Action type="SetProperty" frame="TitleLabel" text="@UI/NewUser/CommercialBreak/Title"/>
            </State>
            
            <State name="Completed">
                <When type="Property" ChallengeCleared="true"/>
                <Action type="SetProperty" frame="TitleLabel" text="@UI/NewUser/VeteranIntroduction/Title"/>
                <Action type="SetProperty" frame="ProgressFrame/PrimaryButton" text="@UI/NewUser/VeteranIntroduction/ConcludeButton"/>
                <Action type="SetProperty" frame="ProgressFrame/SecondaryButton" text="@UI/Restart"/>
                <Action type="SetProperty" frame="ProgressFrame/DescriptionLabel" text="@UI/NewUser/VeteranIntroduction/CompleteBody"/>
                <Action type="SetProperty" frame="ProgressFrame/ProgressLabel" text="@UI/NewUser/VeteranIntroduction/CompleteSubtitle"/>
                <Action type="SetProperty" frame="ProgressFrame/ProgressLabel" style="NewUserVeteranSubtitleComplete"/>
            </State>
            
            <State name="Incomplete">
                <When type="Property" ChallengeCleared="false"/>
                <Action type="SetProperty" frame="TitleLabel" text="@UI/NewUser/VeteranIntroduction/Title"/>
                <Action type="SetProperty" frame="ProgressFrame/PrimaryButton" text="@UI/NewUser/VeteranIntroduction/RestartIntroduction"/>
                <Action type="SetProperty" frame="ProgressFrame/SecondaryButton" text="@UI/NewUser/VeteranIntroduction/AbortButton"/>
                <Action type="SetProperty" frame="ProgressFrame/DescriptionLabel" text="@UI/NewUser/VeteranIntroduction/IncompleteBody"/>
                <Action type="SetProperty" frame="ProgressFrame/ProgressLabel" text="@UI/NewUser/VeteranIntroduction/IncompleteSubtitle"/>
                <Action type="SetProperty" frame="ProgressFrame/ProgressLabel" style="NewUserVeteranSubtitle"/>
            </State>
        </StateGroup>

        <StateGroup name="CutsceneFilterStates">
            <State name="Welcome">
                <When type="Property" JustPlayed="false"/>
                <Action type="SendEvent" frame="ProgressFrame/BackgroundContainer" event="Welcome"/>
            </State>

            <State name="NoReward">
                <When type="Property" RewardRemaining="0"/>
                <Action type="SendEvent" frame="ProgressFrame/BackgroundContainer" event="Welcome"/>
            </State>

            <State name="Completed">
                <When type="Property" ChallengeCleared="true"/>
                <Action type="SendEvent" frame="ProgressFrame/BackgroundContainer" event="Complete"/>
            </State>

            <State name="Incomplete">
                <When type="Property" ChallengeCleared="false"/>
                <Action type="SendEvent" frame="ProgressFrame/BackgroundContainer" event="Incomplete"/>
            </State>
        </StateGroup>
    </Frame>
