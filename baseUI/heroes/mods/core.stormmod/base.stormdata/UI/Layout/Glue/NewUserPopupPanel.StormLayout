    <!-- Commercial Break Page Tab Template -->
    <Frame type="Frame" name="PageTabTemplate">
        <Width val="272"/>
        <Height val="44"/>
        
        <Animation name="FillAnim">
            <Event event="FillAnim" action="Restart,Play"/>
            <Event event="StopAutoPlay" action="Pause" frame="$parent/$parent"/>
            <Controller type="Anchor" end="Pause" side="Right" relative="False" frame="Fill">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time="#PageStayDuration" value="240" in="Slow"/>
            </Controller>
        </Animation>
        
        <Animation name="Show">
            <Event event="PageTransition" action="DirectionReverse,Play" frame="$parent/$parent"/>
            <Event event="Show" action="DirectionForward,Play"/>
            <Event event="FillAnim" action="DirectionForward,Play"/>
            <Controller type="Fade" end="Pause" frame="Fill">
                <Key type="Curve" time="0" value="0" out="Slow"/>
                <Key type="Curve" time="#PageTransitionDuration" value="255" in="Fast"/>
            </Controller>
        </Animation>
        
        <Animation name="Click">
            <Event event="OnMouseDown" action="Restart,Play" frame="HitTest"/>
            <Controller type="Event" end="Stop" frame="$parent/$parent">
                <Key type="Event" time="0" event="StopAutoPlay"/>
            </Controller>
            <Controller type="Event" end="Stop">
                <Key type="Event" time=".04" event="Show"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="HitTest">
                <Key type="Bool" time=".01" value="False"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Right" relative="False" frame="Fill">
                <Key type="Curve" time="0" value="240"/>
            </Controller>
        </Animation>
        
        <Animation name="Visibility">
            <Event event="PageTransition" action="Reset" frame="$parent/$parent"/>
            <Controller type="Visibility" end="Pause" frame="HitTest">
                <Key type="Bool" time="0" value="True"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="Back">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="#PageTabWidth"/>
            <Height val="12"/>
            <Color val="05031d"/>
        </Frame>
        
        <Frame type="Image" name="HitTest">
            <Anchor relative="$parent/Back" offset="16"/>
            <AcceptsMouse val="True"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Image" name="Hover">
            <Anchor relative="$parent/Back"/>
            <Color val="17acff"/>
            <BlendMode val="Add"/>
            <Alpha val="0"/>
            <Animation name="Hover">
                <Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent/HitTest"/>
                <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent/HitTest"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".1" value="180" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="Fill">
            <Anchor side="Top" relative="$parent/Back" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Back" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Back" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/Back" pos="Min" offset="0"/>
            <Color val="17acff"/>
            <Alpha val="0"/>
            <Frame type="Image" name="Glow">
                <Anchor relative="$parent" offset="16"/>
                <Unclipped val="True"/>
                <Texture val="@UI/Storm_UI_NewUser_SellTabGlow"/>
                <TextureType val="NineSlice"/>
                <BlendMode val="Add"/>
