    <StateGroup name="CardState">
      <State name="FaceDown" />
      <State name="SlidingOut" />
      <State name="SlidingIn" />
      <State name="FaceUp" />
    </StateGroup>
    <Animation name="OnDrawCard">
      <Event event="ShowCard" action="Stop" />
      <Event event="HideCard" action="Stop" />
      <Event event="DrawCard" action="Reset,Play" />
      <Event event="DismissCard" action="Stop" />
      <Event event="OnHidden" action="Stop" />
      <Controller type="Sound" frame="FaceDownContent" end="Stop">
        <Key type="Sound" time="0" sound="*@UI_Toys18_Quest_Card_Fly_Out_Sound" />
        <Key type="Sound" time="0.75" sound="*@UI_Toys18_Quest_Card_Fly_In_Sound" />
      </Controller>
      <Controller type="State" stateGroup="CardState" end="Pause">
        <Key type="Identifier" time="0" value="FaceDown" />
        <Key type="Identifier" time="0.1" value="SlidingOut" />
        <Key type="Identifier" time=".75" value="SlidingIn" />
        <Key type="Identifier" time="1.5" value="FaceUp" />
      </Controller>
      <Controller type="Visibility" frame="FaceDownContent" end="Pause">
        <Key type="Bool" time="0" value="True" />
      </Controller>
      <Controller type="Anchor" frame="FaceDownContent" end="Pause" side="Right" relative="true">
        <Key type="Curve" time="0" value="0" inout="Fast" />
        <Key type="Curve" time="0.75" value="400" inout="Slow" />
      </Controller>
      <Controller type="Anchor" frame="FaceUpContent" end="Pause" side="Right" relative="true">
        <Key type="Curve" time="0.0" value="400" inout="Fast" />
        <Key type="Curve" time="0.75" value="400" inout="Fast" />
        <Key type="Curve" time="1.50" value="0" inout="Slow" />
      </Controller>
    </Animation>
    <Animation name="OnDismissCard">
      <Event event="ShowCard" action="Stop" />
      <Event event="HideCard" action="Stop" />
      <Event event="DrawCard" action="Stop" />
      <Event event="DismissCard" action="Play" />
      <Event event="OnHidden" action="Stop" />
      <Controller type="Visibility" frame="FaceUpContent" end="Pause">
        <Key type="Bool" time="0" value="True" />
        <Key type="Bool" time="0.75" value="False" />
      </Controller>
      <Controller type="Fade" frame="FaceUpContent" end="Pause">
        <Key type="Curve" time="0" value="255" inout="Fast" />
        <Key type="Curve" time=".35" value="0" inout="Slow" />
      </Controller>
      <Controller type="Anchor" frame="FaceUpContent" end="Pause" side="Bottom" relative="true">
        <Key type="Curve" time="0" value="0" inout="Fast" />
        <Key type="Curve" time="0.35" value="-50" inout="Slow" />
      </Controller>
      <Controller type="Visibility" frame="FaceDownContent" end="Pause">
        <Key type="Bool" time="0" value="False" />
      </Controller>
    </Animation>
    <Animation name="OnShowCard">
      <Event event="ShowCard" action="Reset,Play" />
      <Event event="HideCard" action="Stop" />
      <Event event="DrawCard" action="Stop" />
      <Event event="DismissCard" action="Stop" />
      <Event event="OnHidden" action="Stop" />
      <Controller type="Visibility" frame="FaceUpContent" end="Pause">
