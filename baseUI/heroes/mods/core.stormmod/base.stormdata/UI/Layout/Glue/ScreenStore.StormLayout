<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Constant name="StoreTileItemSpacing" val="12"/>
    <Constant name="StoreTileItemSpacingRight" val="-12"/>

    <Constant name="StoreSmallTileIconContainerWidth" val="55"/>
    <Constant name="StoreBigTileIconContainerWidth" val="100"/>

    <Frame type="Button" name="NavigationButtonTemplate" template="StandardTemplates/Top_Navigation">
        <ReportsStats val="true"/>

        <Animation name="Transition">
            <Event event="Show" action="Reset" frame="$parent"/>
            <Event event="Show" action="Play" frame="$parent"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0" out="Slow"/>
                <Key type="Curve" time="0.15" value="0" in="Slow"/>
                <Key type="Curve" time="0.4" value="255" in="Slow" out="Step"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0.0" value="-10" out="Slow"/>
                <Key type="Curve" time="0.15" value="-10" in="Slow"/>
                <Key type="Curve" time="0.4" value="0" in="Slow" out="Step"/>
            </Controller>
        </Animation>
    </Frame>

    <!--===========================================================================================
    <! StoreTileListItem
    <!=============================================================================================
    <! Events : ProductOwned / ProductNotOwned
    <! Events : NewProduct / NotNewProduct
    <! Events : NotSkin / Common / Master / Epic / Legendary
    <! Events : ShowBundle / HideBundle
    <! Events : Discount
    <!==========================================================================================-->
    <Frame type="StoreTileListItem" name="StoreTileListBaseTemplate">
        <Width val="268"/>
        <Height val="361"/>
        <SelectedSound val="@UI_BNet_ShopTileSelect"/>
        <HideWhenCleared val="false"/>
        <HoveredRenderPriority val="1024"/>
        <SelectedRenderPriority val="1023"/>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>

        <Frame type="Image" name="EmptyImage">
            <Anchor relative="$parent/NormalImage" />
            <Texture val="@UI/Storm_UI_Store_SmallFrame_Off"/>
            <Unclipped val="True"/>
            <RenderType val="HDR"/>
            <Alpha val="100"/>
        </Frame>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Store_SmallFrame_Off"/>
            <Unclipped val="true"/>
            <RenderType val="HDR"/>
        </Frame>
        
        <Frame type="Image" name="HoverImage">
            <Anchor relative="$parent/NormalImage" />
            <Texture val="@UI/Storm_UI_Store_SmallFrame_Off"/>
            <RenderType val="HDR"/>
        </Frame>

        <Frame type="Image" name="HoverFrameImage">
            <Anchor relative="$parent/NormalImage" />
            <Texture val="@UI/Storm_UI_Store_SmallFrame_Off"/>
            <BlendMode val="Add"/>
            <Unclipped val="true"/>
            <RenderType val="HDR"/>
            <Alpha val="0"/>
            <Animation name="Hover">
                <Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".2" value="255" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor relative="$parent"/>
            <Visible val="False"/>
            <Alpha val="0"/>
        </Frame>

        <Frame type="Image" name="PortraitImage">
            <Anchor relative="$parent/PortraitCutscene"/>
            <TextureType val="Normal"/>
        </Frame>

        <Frame type="Image" name="LockedIconImage">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-63"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_store_item_locked"/>
            <TextureType val="Normal"/>
            <Height val="151"/>
            <Width val="131"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="QualityGlow">
            <Anchor relative="$parent/QualityFrameImage" offset="-4"/>
            <LayerCount val="2"/>
            <BlendMode val="Add"/>
            <RenderType val="HDR"/>
            <Visible val="False"/>
            
            <Texture val="@UI/Storm_UI_Cloud_Mask" layer="0"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <WrapUV val="True" layer="0"/>
            
            <Texture val="@UI/Storm_UI_Store_QualityGlow_Legendary" layer="1"/>
               <TextureType val="Nineslice" layer="1"/>
            <WrapUV val="True" layer="1"/>
            
            <Animation name="ShowHide">
                <Event event="NotSkin" action="Stop" frame="$parent"/>
                <Event event="Normal" action="Stop" frame="$parent"/>
                <Event event="Epic" action="Stop" frame="$parent"/>
                <Event event="Legendary" action="Play" frame="$parent"/>
                <Event event="Master" action="Stop" frame="$parent"/>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".01" value="true"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$parent/QualityGlow2">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".01" value="true"/>
                </Controller>
            </Animation>
            
            <Animation name="UVScroll">
                <Event event="OnShown" action="Restart" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time="1" value="255" in="Slow"/>
                    <Key type="Curve" time="4" value="255" out="Fast"/>
                    <Key type="Curve" time="5" value="0" in="Slow"/>
                </Controller>
                <Controller type="LayerUV" layer="0" side="Left" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Right" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Top" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Bottom" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="QualityGlow2">
            <Anchor relative="$parent/QualityGlow" offset="0"/>
            <LayerCount val="2"/>
            <BlendMode val="Add"/>
            <Alpha val="0"/>
            <RenderType val="HDR"/>
            <Visible val="False"/>

            <Texture val="@UI/Storm_UI_Cloud_Mask" layer="0"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <WrapUV val="True" layer="0"/>
            
            <Texture val="@UI/Storm_UI_Store_QualityGlow_Legendary" layer="1"/>
               <TextureType val="Nineslice" layer="1"/>
            <WrapUV val="True" layer="1"/>
            
            <Animation name="Trigger">
                <Event event="OnShown" action="Restart,Play" frame="$parent"/>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="2.5" event="Trigger"/>
                </Controller>
            </Animation>
            
            <Animation name="UVScroll">
                <Event event="Trigger" action="Restart,Play"/>
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time="1" value="255" in="Slow"/>
                    <Key type="Curve" time="4" value="255" out="Fast"/>
                    <Key type="Curve" time="5" value="0" in="Slow"/>
                </Controller>
                <Controller type="LayerUV" layer="0" side="Left" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Right" relative="False" sidelock="False" end="Loop" >

                    <Key type="Curve" time="0" value="1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Top" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Bottom" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
            </Animation>
        </Frame>

        <!-- Filter is set based on the tile size. BigTile / SmallTile -->
        <Frame type="HeroCutsceneFrame" name="PortraitCutscene">
            <Anchor relative="$parent/CutsceneCoverImage"/>
            <Unclipped val="False"/>
            <RenderType val="HDR"/>
            <TimeScale val="0.5"/>
            <Visible val="True"/>
            <HeroCutsceneType val="ShopTileZoomed"/>
            <Animation name="ToggleHoverSpeed">
                <Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent"/>
                <Controller type="Cutscene" end="Pause">
                    <Key type="Cutscene" time="0.0" action="TimeScale" value=".5" out="Fast"/>
                    <Key type="Cutscene" time=".25" action="TimeScale" value="1.5" in="Slow"/>
                </Controller>
            </Animation>

            <Animation name="HoverLightOn">
                <Event event="OnMouseEnter" action="Play" frame="$parent"/>
                <Controller type="Animation" end="Stop" animation="HoverLightOff">
                    <Key type="Animation" time="0" action="Stop"/>
                </Controller>
                <Controller type="Cutscene" end="Stop" name="FilterOn">
                    <Key type="Cutscene" time="0.01" action="Bookmark" bookmark="Light On"/>
                    <Key type="Cutscene" time="0.01" action="Play"/>
                    <Key type="Cutscene" time="0.01" action="AddFilter" filter="LightOn"/>
                </Controller>
            </Animation>
            
            <Animation name="HoverLightOff">
                <Event event="OnMouseExit" action="Play" frame="$parent"/>
                <Controller type="Animation" end="Stop" animation="HoverLightOn">
                    <Key type="Animation" time="0" action="Stop"/>
                </Controller>
                <Controller type="Cutscene" end="Stop" name="FilterOff">
                    <Key type="Cutscene" time="0" action="Bookmark" bookmark="Light Off"/>
                    <Key type="Cutscene" time="0" action="Play"/>
                    <Key type="Cutscene" time="0.5" action="RemoveFilter" filter="LightOn"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="CutsceneCoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="8"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="8"/>
            <Texture val="@UI/Storm_UI_Store_SmallFrame_Top"/>
