    <Frame type="StoreTileListItem" name="StoreTileListItemTemplate" template="ScreenStore/StoreTileListBaseTemplate">
        <Frame type="Label" name="DescriptionLabel">
            <Animation name="NotSkin">
                <Event event="NotSkin" action="Play" frame="$parent"/>
                <Controller type="Style" end="Pause" style1="StoreItemDescriptionLabel" style2="StoreItemDescriptionLabel">
                    <Key type="Curve" time="0" value="1"/>
                </Controller>
            </Animation>
            <Animation name="Normal">
                <Event event="Normal" action="Play" frame="$parent"/>
                <Controller type="Style" end="Pause" style1="StoreItemDescriptionLabel" style2="StoreNormalDescriptionLabel">
                    <Key type="Curve" time="0" value="1"/>
                </Controller>
            </Animation>
            <Animation name="Epic">
                <Event event="Epic" action="Play" frame="$parent"/>
                <Controller type="Style" end="Pause" style1="StoreItemDescriptionLabel" style2="StoreEpicDescriptionLabel">
                    <Key type="Curve" time="0" value="1"/>
                </Controller>
            </Animation>
            <Animation name="Legendary">
                <Event event="Legendary" action="Play" frame="$parent"/>
