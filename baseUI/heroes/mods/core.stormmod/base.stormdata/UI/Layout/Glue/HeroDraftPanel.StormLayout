        <!-- Page selector -->
        <Frame type="PageSelectorButtonList" name="PageSelector" template="StandardTemplates/PageSelectorButtonListTemplate">
            <Anchor relative="$parent"/>

            <Frame type="Button" name="PageButton0" template="StandardTemplates/StandardPipButtonTemplate">
                <Anchor side="Top" relative="$parent/$parent/Item43" pos="Max" offset="-41"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="-34"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="-34"/>
                <CollapseLayout val="true"/>
            </Frame>
        </Frame>

    </Frame>

    <Frame type="HeroDraftPanel" name="HeroDraftPanel">
        <Anchor relative="$parent"/>

        <StateGroup name="HeroSelectButtonStates">
            <DefaultState val="AlternateButtonVisible"/>

            <State name="AlternateButtonHidden_ShowAlternate">
                <When type="AnimationState" frame="$this" HeroSelectType="ShowAlternateHeroSelect"/>
                <When type="AnimationState" frame="$this" DraftState="CoinTossBegin"/>

                <Action type="SetProperty" frame="ShowAlternateHeroSelectButton" visible="false"/>
                <Action type="SetProperty" frame="AlternateHeroTileListBlurFrame" visible="true"/>
                <Action type="SetProperty" frame="BannedHero/HeroNameLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="BannedHero/BannedLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="BanChooserLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="BanChooserPickingLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="RoundTypeFrame" renderType="HDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame0/NameLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame0/PickingLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame0/HeroLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame1/NameLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame1/PickingLabel" renderType="HDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame1/HeroLabel" renderType="HDR"/>
            </State>

            <State name="AlternateButtonVisible">
                <Action type="SetProperty" frame="ShowAlternateHeroSelectButton" visible="true"/>
                <Action type="SetProperty" frame="AlternateHeroTileListBlurFrame" visible="false"/>
                <Action type="SetProperty" frame="BannedHero/HeroNameLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="BannedHero/BannedLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="BanChooserLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="BanChooserPickingLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="RoundTypeFrame" renderType="LDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame0/NameLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame0/PickingLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame0/HeroLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame1/NameLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame1/PickingLabel" renderType="LDR"/>
                <Action type="SetProperty" frame="HeroDraftTeamReadyPanel/PickingSlotFrame1/HeroLabel" renderType="LDR"/>
            </State>
        </StateGroup>

        <Animation name="Transition">
            <Event event="Show" action="Reset,Play"/>
            <Event event="Hide" action="DirectionReverse,Play"/>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0" out="Slow"/>
                <Key type="Curve" time="0.4" value="255" in="Slow" out="Slow"/>
            </Controller>
        </Animation>

        <Animation name="PlayCoinTossSoundtrack">
            <Event event="OnBookmark[Red_CoinHit]" action="Reset,Play" frame="CoinTossFrame/CoinCutsceneFrame"/>
            <Event event="OnBookmark[Blue_CoinHit]" action="Reset,Play" frame="CoinTossFrame/CoinCutsceneFrame"/>

            <Controller type="Event" end="Stop">
                <Key type="Event" time="0" event="TriggerPlayCoinTossSoundtrack"/>
            </Controller>
        </Animation>

        <StateGroup name="DraftState">
            <DefaultState val="ResetState"/>

            <State name="CountDownBegin">
                <When type="AnimationState" DraftState="CountdownBegin"/>

                <Action type="SendEvent" event="Hide" frame="VersusPanel"/>
                <Action type="SetState" group="VisibilityState" state="Hidden" frame="ConfirmButton"/>
                <Action type="SetState" group="VisibilityState" state="Hidden" frame="ShowAlternateHeroSelectButton"/>
                <Action type="SetState" group="VisibilityState" state="Hidden" frame="HeroSelectPanel"/>
                <Action type="SendEvent" event="Hide" frame="Team0DraftBanLeft"/>
                <Action type="SendEvent" event="Hide" frame="Team0DraftBanRight"/>
                <Action type="SendEvent" event="Hide" frame="Team1DraftBanLeft"/>
                <Action type="SendEvent" event="Hide" frame="Team1DraftBanRight"/>

                <Action type="SendEvent" event="TriggerPlayCountdownSoundtrack"/>
            </State>

            <State name="DraftStartedObserver">
                <When type="AnimationState" DraftState="CoinTossBegin"/>
                <When type="Property" IsLocalParticipant="false"/>

                <Action type="SendEvent" event="Show" frame="VersusPanel"/>
                <Action type="SendEvent" event="Show" frame="CoinTossTransition"/>
                <Action type="SendEvent" event="Show" frame="RoundTypeFrame"/>
                <Action type="SendEvent" event="Show" frame="HeroDraftTeamReadyPanel"/>
                <Action type="SendEvent" event="Show" frame="Team0"/>
                <Action type="SendEvent" event="Show" frame="Team1"/>

                <!-- Bans -->
                <Action type="SendEvent" event="Show" frame="Team0DraftBanLeft"/>
                <Action type="SendEvent" event="Show" frame="Team0DraftBanRight"/>
                <Action type="SendEvent" event="Show" frame="Team1DraftBanLeft"/>
                <Action type="SendEvent" event="Show" frame="Team1DraftBanRight"/>
                <Action type="SendEvent" event="Show" frame="BannedHero"/>
                <Action type="SendEvent" event="Show" frame="DraftBanEmptyCutscene"/>
                <Action type="SendEvent" event="Show" frame="BanChooserLabel"/>
                <Action type="SendEvent" event="Show" frame="BanChooserPickingLabel"/>

                <!-- Bottom Controls : Hide for observer -->
                <Action type="SetState" group="VisibilityState" state="Hidden" frame="HeroSelectPanel"/>
                <Action type="SetState" group="VisibilityState" state="Hidden" frame="ShowAlternateHeroSelectButton"/>
                <Action type="SetState" group="VisibilityState" state="Hidden" frame="ConfirmButton"/>
            </State>

            <State name="DraftStartedParticipant">
                <When type="AnimationState" DraftState="CoinTossBegin"/>

                <Action type="SendEvent" event="Show" frame="VersusPanel"/>
                <Action type="SendEvent" event="Show" frame="CoinTossTransition"/>
                <Action type="SendEvent" event="Show" frame="RoundTypeFrame"/>
                <Action type="SendEvent" event="Show" frame="HeroDraftTeamReadyPanel"/>
                <Action type="SendEvent" event="Show" frame="Team0"/>
                <Action type="SendEvent" event="Show" frame="Team1"/>

                <!-- Bans -->
                <Action type="SendEvent" event="Show" frame="Team0DraftBanLeft"/>
                <Action type="SendEvent" event="Show" frame="Team0DraftBanRight"/>
                <Action type="SendEvent" event="Show" frame="Team1DraftBanLeft"/>
                <Action type="SendEvent" event="Show" frame="Team1DraftBanRight"/>
                <Action type="SendEvent" event="Show" frame="BannedHero"/>
                <Action type="SendEvent" event="Show" frame="DraftBanEmptyCutscene"/>
                <Action type="SendEvent" event="Show" frame="BanChooserLabel"/>
                <Action type="SendEvent" event="Show" frame="BanChooserPickingLabel"/>

                <!-- Bottom Controls : Show for participant -->
                <Action type="SetState" group="VisibilityState" state="Visible" frame="HeroSelectPanel"/>
                <Action type="SetState" group="VisibilityState" state="Visible" frame="ShowAlternateHeroSelectButton"/>
                <Action type="SetState" group="VisibilityState" state="Visible" frame="ConfirmButton"/>
                <Action type="SetState" group="VisibilityState" state="Shown" frame="HeroSelectPanel/HeroAttributesGroup/ChangeHeroPanel"/>
            </State>

            <!-- Intro OR NotActive -->
            <State name="ResetState" >
                <When type="AnimationState" DraftState="NotActive"/>
                
                <Action type="SendEvent" event="Reset" frame="VersusPanel"/>
