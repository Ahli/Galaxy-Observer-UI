<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <!--DRAFT HERO SELECT ANIMATION START-->
    <Frame type="HeroSelectTileListItem" name="DraftHeroSelectTileBaseTemplate" template="HeroSelectTileList/HeroSelectTileListItemTemplate">
        <Frame type="HeroContainerFrame" name="HeroContainerFrame">
            <StateGroup name="HeroStates">
                <State name="AlwaysAllowed">
                    <When type="AnimationState" frame="$parent/$parent/$parent/$parent/$parent/$parent" DraftRoundState="YourBan"/>
                    <When type="Property" frame="HeroState" isAllowed="false"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>

    <Frame type="HeroSelectTileListItem" name="DraftHeroSelectTileFirstRowTemplate" template="HeroDraftPanel/DraftHeroSelectTileBaseTemplate">
        <TooltipFrame val="DraftHeroTooltip"/>

        <Animation name="Intro">
            <Event event="StartShow" action="DirectionForward,Play" frame="$parent"/>
            <Event event="StartHide" action="Stop" frame="$parent"/>

            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0" value="25" out="Slow"/>
                <Key type="Curve" time=".3" value="0" in="Fast"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="HeroSelectTileListItem" name="DraftHeroSelectTileTemplate" template="HeroDraftPanel/DraftHeroSelectTileBaseTemplate">
        <TooltipFrame val="DraftHeroTooltip"/>

        <DisabledAlpha val="0"/>
        <Enabled val="false"/>

        <!-- Prevent tooltip from showing when this was disabled and hidden -->
        <UseTooltip val="false"/>

        <Animation name="Intro">
            <Event event="StartShow" action="DirectionForward,Play" frame="$parent"/>
            <Event event="StartHide" action="Stop" frame="$parent"/>

            <Controller type="Property" property="UseTooltip" end="Pause">
                <Key type="Property" time="0" value="True"/>
            </Controller>
            <Controller type="Enabled" end="Pause">
                <Key type="Bool" time="0.000" value="false"/>
                <Key type="Bool" time="0.001" value="true"/>
                <Key type="Bool" time="0.3" value="true"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0" value="25" out="Slow"/>
                <Key type="Curve" time=".3" value="0" in="Fast"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="HeroSelectTooltip" name="DraftHeroTooltip" template="StandardTooltip/HeroSelectTooltipTemplate">
        <!-- Cut off everything below rolelabel -->
        <Frame type="Frame" name="BottomFrame">
            <Anchor side="Top" relative="$parent/DescriptionLabel" pos="Min" offset="-15"/>
            <Anchor side="Left" relative="$parent/NameLabel" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/NameLabel" pos="Max" offset="0"/>
        </Frame>

        <!-- Hide everything else -->
        <Frame type="Label" name="DescriptionLabel">
            <Alpha val="0"/>
        </Frame>

        <Frame type="Image" name="BorderImage">
            <Alpha val="0"/>
        </Frame>

        <Frame type="Frame" name="QuestNotEligibleFrame">
            <Alpha val="0"/>
        </Frame>

        <Frame type="Label" name="QuestsLabel">
            <Alpha val="0"/>
        </Frame>

        <Frame type="TileList" name="QuestsTileList">
            <Alpha val="0"/>
        </Frame>

        <Frame type="Frame" name="FreeToPlayFrame">
            <Alpha val="0"/>
        </Frame>

        <Frame type="Frame" name="IGRBenefitFrame">
            <Alpha val="0"/>
        </Frame>

        <Frame type="Frame" name="AvailabilityWarningFrame">
            <Alpha val="0"/>
        </Frame>
    </Frame>

    <!--DRAFT HERO SELECT ANIMATION END-->

