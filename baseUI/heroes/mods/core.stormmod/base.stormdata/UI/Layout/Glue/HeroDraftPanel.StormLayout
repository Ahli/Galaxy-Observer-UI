                    <Anchor relative="$parent"/>
                    <TextureType val="Normal"/>
                </Frame>
            </Frame>

            <Frame type="Frame" name="MapImageContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="200"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-200"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <RenderType val="HDR"/>

                <Frame type="Image" name="SplashScreenMapImage">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <TextureType val="Normal"/>
                    <ScaleHeightToTexture val="true"/>

                    <Animation name="ScaleAnim">
                        <Event event="OnShown" action="Reset,Play" frame="$parent"/>

                        <Controller type="LayerUV" layer="0" side="Left" sidelock="False" end="Pause">
                            <Key type="Curve" time="0" value="0.05" out="linear"/>
                            <Key type="Curve" time="7.0" value="0.0" in="linear"/>
                        </Controller>
                        <Controller type="LayerUV" layer="0" side="Top" sidelock="False" end="Pause">
                            <Key type="Curve" time="0" value="0.05" out="linear"/>
                            <Key type="Curve" time="7.0" value="0.0" in="linear"/>
                        </Controller>
                        <Controller type="LayerUV" layer="0" side="Right" sidelock="False" end="Pause">
                            <Key type="Curve" time="0" value="0.95" out="linear"/>
                            <Key type="Curve" time="7.0" value="1.0" in="linear"/>
                        </Controller>
                        <Controller type="LayerUV" layer="0" side="Bottom" sidelock="False" end="Pause">
                            <Key type="Curve" time="0" value="0.95" out="linear"/>
                            <Key type="Curve" time="7.0" value="1.0" in="linear"/>
                        </Controller>
                        <Controller type="Fade" end="Pause">
                            <Key type="Curve" time="0" value="255"/>
                            <Key type="Curve" time="4.5" value="255"/>
                            <Key type="Curve" time="5.0" value="0"/>
                        </Controller>
                    </Animation>
                </Frame>
            </Frame>

            <Frame type="Label" name="MapNameLabel">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="-55"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="-800"/>
                <Width val="1600"/>
                <Style val="HeroDraftIntro_MapTitle"/>
                <SubpixelRendering val="True"/>
                <RenderType val="LDR"/>

                <Animation name="MapLabelAnim" template="HeroDraftTemplates/DraftIntroLabelAnimBaseTemplate">
                    <Event event="OnShown" action="Restart,DirectionForward,Play" frame="$parent"/>

                    <Controller type="Anchor" name="Sliding" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="350" out="Auto"/>
                        <Key type="Curve" time="1.0" value="350" out="Auto"/>
                        <Key type="Curve" time="1.2" value="50" in="Auto" out="Linear"/>
                        <Key type="Curve" time="4.4" value="-50" in="Linear" out="Auto"/>
                        <Key type="Curve" time="4.6" value="-350" in="Auto"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Label" name="WelcomeLabel">
                <Anchor side="Top" relative="$parent/MapNameLabel" pos="Min" offset="-65"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="800"/>
                <Width val="1600"/>
                <Style val="HeroDraftIntro_WelcomeTitle"/>
                <Text val="@UI/HeroDraft/IntroScreen/MapNameWelcome"/>
                <SubpixelRendering val="True"/>
                <RenderType val="LDR"/>

                <Animation name="WelcomeLabelAnim" template="HeroDraftTemplates/DraftIntroLabelAnimBaseTemplate">
                    <Event event="OnShown" action="Restart,DirectionForward,Play" frame="$parent"/>

                    <Controller type="Anchor" name="Sliding" end="Pause" side="Left" relative="true">
                        <Key type="Curve" time="0" value="-400" out="Auto"/>
                        <Key type="Curve" time="1.0" value="-400" out="Auto"/>
                        <Key type="Curve" time="1.2" value="-100" in="Auto" out="Linear"/>
                        <Key type="Curve" time="4.4" value="0" in="Linear" out="Auto"/>
                        <Key type="Curve" time="4.6" value="300" in="Auto"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Frame" name="IntroBorderTop" template="HeroDraftIntroScreen/SplashScreenContainerTemplate">
                <Anchor relative="$parent"/>
                <RenderType val="HDR"/>

                <Frame type="BlurFrame" name="BlurFrame">
                    <Anchor side="Top" relative="$parent\BorderBg" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent\BorderBgLine" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent\BorderBg" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent\BorderBg" pos="Max" offset="0"/>
                </Frame>

            </Frame>

            <Frame type="Frame" name="IntroBorderBottom" template="HeroDraftIntroScreen/SplashScreenContainerTemplate">
                <Anchor relative="$parent"/>
                <RenderType val="HDR"/>

                <Frame type="Image" name="BorderBg">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="400"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                </Frame>

                <Frame type="Image" name="BorderHexGrid">
                    <Anchor side="Top" relative="$parent/BorderBg" pos="Min" offset="2"/>
                    <Anchor side="Left" relative="$parent/BorderBg" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/BorderBg" pos="Max" offset="0"/>
                    <TextureCoords top="1" left="0" bottom="0" right="1"/>
                </Frame>

                <Frame type="Image" name="BorderHexGridAnim">
                    <Anchor side="Top" relative="$parent/BorderBg" pos="Min" offset="1"/>
                    <Anchor side="Left" relative="$parent/BorderBg" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/BorderBg" pos="Max" offset="0"/>
                    <TextureCoords top="1" left="0" bottom="0" right="1"/>
                    <LayerCount val="2"/>

                    <Texture val="@UI/storm_ui_draft_intro_hex_grid_overlay" layer="0"/>
                    <AlphaLayer val="True" layer="0"/>

                    <Texture val="@UI/storm_ui_draft_intro_hex_grid_glow" layer="1"/>
                    <TextureCoords top="1" left="0" bottom="0" right="1" layer="1"/>
                    <Alpha val="200" layer="1"/>
                    <WrapUV val="False" layer="1"/>

                    <Animation name="UVScrollGlowBottom" template="HeroDraftIntroScreen/UVScrollGlowTemplate">
                        <Controller type="LayerUV" layer="1" side="Left" sidelock="true" end="Loop" >
                            <Key type="Curve" time="0" value="-1" out="fast"/>
                            <Key type="Curve" time="5" value="1" in="fast"/>
                        </Controller>
                    </Animation>
                </Frame>

                <Frame type="Image" name="BorderBgLine">
                    <Anchor side="Bottom" relative="$parent/BorderBg" pos="Min" offset="4"/>
                    <Anchor side="Left" relative="$parent/BorderBg" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/BorderBg" pos="Max" offset="0"/>
                </Frame>

                <Frame type="Image" name="BorderFlare">
                    <Animation name="UVScrollFlareBottom" template="HeroDraftIntroScreen/UVScrollFlareAnimTemplate">
                        <Controller type="LayerUV" side="Left" sidelock="true" end="Loop">
                            <Key type="Curve" time="0" value="-1" out="fast"/>
                            <Key type="Curve" time="5" value="1" in="fast"/>
                        </Controller>
                    </Animation>
                </Frame>
            </Frame>
        </Frame>

        <Frame type="Image" name="CoinTossTransition">
            <Anchor relative="$parent"/>
            <Texture val="@UI/storm_ui_draft_intro_blackscreen_transition"/>
            <RenderType val="HDR"/>
            <Alpha val="0"/>

            <Animation name="BackgroundTransition">
                <Event event="Show" action="Reset,Play"/>
                <Event event="Reset" action="Stop"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time="0.3" value="0" out="Slow"/>
                    <Key type="Curve" time="0.5" value="255" out="Slow"/>
                    <Key type="Curve" time="1.2" value="0" out="Slow"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="CoinTossFrame" name="CoinTossFrame" template="CoinTossFrame/CoinTossFrameTemplate">
            <Anchor relative="$parent"/>
        </Frame>

        <Frame type="Label" name="WaitingForPlayersFrame">
            <Anchor relative="$parent"/>
            <Text val="@UI/WaitingForPlayers"/>
            <Style val="DialogHeaderLabel"/>
            <Alpha val="0"/>

            <Animation name="WaitingFadeInOut">
                <Event event="CoinTossEnd" frame="$parent" action="Restart,Play" />
                <Event event="DraftPickBegin" frame="$parent" action="DirectionReverse,Play" />

                <Controller type="Fade" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="0" out="Fast"/>
                    <Key type="Curve" time="0.5" value="0" in="Fast"/>
                    <Key type="Curve" time="1.0" value="255" in="Fast"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Frame" name="DisconnectedInfo">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-120"/>
            <Width val="500"/>
            <Height val="185"/>
            <Visible val="false"/>

            <StateGroup name="ConnectionState">
                <State name="Disconnected">
                    <When type="Property" frame="$parent" IsConnected="false"/>
                    <Action type="SetProperty" frame="$this" Visible="true" />
                </State>
            </StateGroup>

            <Frame type="Label" name="TitleLabel">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Height val="25"/>
                <Text val="@UI/DisconnectedDialogTitle"/>
            </Frame>

            <Frame type="Label" name="MessageLabel">
                <Anchor side="Top" relative="$parent/TitleLabel" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent/TitleLabel" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/TitleLabel" pos="Mid" offset="0"/>
                <Width val="500"/>
                <Text val="@UI/DisconnectedDialogMessage"/>
            </Frame>

            <Frame type="Image" name="WaitingCycleImage">
                <Height val="15"/>
                <Anchor side="Top" relative="$parent/MessageLabel" pos="Max" offset="5"/>
                <Anchor side="Right" relative="$parent/MessageLabel" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/MessageLabel" pos="Mid" offset="0"/>
                <Texture val="@UI/Storm_HeroSelect_Icon_Pending0"/>

                <Animation name="WaitingShow">
                    <Event event="OnShown" action="Play"/>
                    <Event event="OnShown" action="Reset"/>
                    <Controller type="Texture" end="Loop">
                        <Key type="Image" time="0.0" image="@UI/Storm_HeroSelect_Icon_Pending0"/>
                        <Key type="Image" time="0.5" image ="@UI/Storm_HeroSelect_Icon_Pending1"/>
                        <Key type="Image" time="1.0" image ="@UI/Storm_HeroSelect_Icon_Pending2"/>
                        <Key type="Image" time="1.5" image ="@UI/Storm_HeroSelect_Icon_Pending3"/>
                        <Key type="Image" time="2.0" image ="@UI/Storm_HeroSelect_Icon_Pending3"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Button" name="DisconnectedInfoLogoutButton" template="StandardTemplates/StandardButtonSmallTemplate">
                <Anchor side="Top" relative="$parent/WaitingCycleImage" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent/WaitingCycleImage" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/WaitingCycleImage" pos="Mid" offset="0"/>
                <Text val="@UI/MenuPanelLogout"/>
            </Frame>

            <Frame type="Button" name="Button">
                <Alpha val="0"/>
                <Visible val="false"/>
            </Frame>
        </Frame>
    </Frame>
</Desc>
