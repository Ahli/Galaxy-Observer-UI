      <Anchor side="Left" relative="$parent/ContentAnchorFrame" pos="Mid" offset="0" />
      <Anchor side="Bottom" relative="$parent/ContentAnchorFrame" pos="Mid" offset="0" />
      <Anchor side="Right" relative="$parent/ContentAnchorFrame" pos="Mid" offset="0" />
      <RenderPriority val="802" />
      <Animation name="ShowDialog">
        <Event event="DiceRollFailed" frame="$parent" action="Reset,Play" />
        <Controller type="Visibility" end="Pause">
          <Key type="Bool" time="0" value="False" />
          <Key type="Bool" time=".1" value="True" />
        </Controller>
      </Animation>
    </Frame>
  </Frame>
  <Frame type="ToyEventQuestPopupPanel" name="ToyEventDialog">
    <Anchor relative="$parent" />
    <Handle val="Toy2019EventPopup" />
    <Frame type="BlurFrame" name="FullscreenBlurFrame">
      <Anchor relative="$parent" />
      <Frame type="Image" name="LowImage">
        <Anchor relative="$parent" />
        <Color val="17,25,50" />
        <Alpha val="220" />
      </Frame>
      <Animation name="DialogAnimation">
        <Event event="OnShown" action="DirectionForward,Play" frame="$parent" />
        <Event event="DialogDismissed" action="DirectionReverse,Play" frame="$parent" />
        <Controller type="Blur" end="Pause">
          <Key type="Curve" time="0" value="0.0" inout="Linear,Auto" />
          <Key type="Curve" time="0.2" value="1.0" inout="Auto,Linear" />
        </Controller>
      </Animation>
    </Frame>
    <Frame type="PopupPanel" name="PopupPanel" template="Toy2019EventQuestPopupPanel/PopupPanelTemplate">
      <Anchor side="Top" relative="$parent" pos="Mid" offset="0" />
      <Anchor side="Left" relative="$parent" pos="Mid" offset="0" />
      <Anchor side="Right" relative="$parent" pos="Mid" offset="0" />
      <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0" />
    </Frame>
  </Frame>
  <Frame type="Button" name="OpenToy2019EventPopupButtonTemplate" template="StandardTemplates/OpenToyEventPopupButtonTemplate">
    <Enabled val="{$QuestUI/@IsQuestSelectable}" />
    <Frame type="Image" name="NormalImage">
      <Texture val="@UI/Storm_UI_Toy2019Event_SpecialEvent_Button_Normal" />
    </Frame>
    <Frame type="Image" name="HoverImage">
      <Texture val="@UI/Storm_UI_Toy2019Event_SpecialEvent_Button_Hover" />
    </Frame>
    <Frame type="Label" name="Label">
      <Style val="ToyEvent_CustomButtonLabelSmall" />
      <Text val="@UI/QuestLine/Event/Toys2019/QuestLogButton" />
    </Frame>
    <StateGroup name="Tooltip">
      <State name="Disabled">
        <When type="Property" enabled="False" />
        <Action type="SetProperty" tooltip="@UI/QuestLine/Event/QuestButtonTooltip/UnavailableReasonInQueue" />
      </State>
      <DefaultState val="Enabled" />
      <State name="Enabled" />
    </StateGroup>
    <Animation name="OpenPopup">
      <Event event="OnClick" action="Restart,Play" />
      <Controller type="Event" frame="$QuestUI" end="Pause">
        <Key type="Event" time="0" event="OpenActiveEventDialog" />
      </Controller>
    </Animation>
  </Frame>
</Desc>