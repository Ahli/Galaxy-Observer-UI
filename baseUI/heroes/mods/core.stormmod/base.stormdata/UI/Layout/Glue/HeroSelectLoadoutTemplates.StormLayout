    <Animation name="Show">
      <Event event="OnShown" action="Reset, Play" />
      <Event event="OnHidden" action="DirectionReverse, Play" />
      <Controller type="Fade" end="Pause">
        <Key type="Curve" time="0" value="0" inout="Linear" />
        <Key type="Curve" time=".3" value="255" inout="Slow,Linear" />
      </Controller>
    </Animation>
  </Frame>
  <Frame type="Frame" name="HeroSelectLoadoutItemPanelTemplate">
    <Anchor relative="$parent" />
    <Visible val="false" />
    <Frame type="Label" name="SelectedContentLabel" template="HeroSelectLoadoutTemplates/SelectedItemNameLabelTemplate">
      <Text val="{$parent/ContentTileList/@SelectedGameContent[Name]}" />
    </Frame>
    <Frame type="GameContentOwnershipFilterPulldown" name="OwnershipFilterPulldown" template="HeroSelectLoadoutTemplates/LoadoutOwnershipFilterPulldownTemplate">
      <GameContentType val="{$parent/ContentTileList/@GameContentType}" />
    </Frame>
    <Frame type="EditBox" name="SearchEditBox" template="HeroSelectLoadoutTemplates/LoadoutSearchBoxTemplate">
      <Anchor side="Right" relative="$parent/ContentTileList/ScrollBar" pos="Max" offset="0" />
      <Anchor side="Bottom" relative="$parent/ContentTileList/Item0" pos="Min" offset="16" />
      <InputDelayTimeout val="#EditBoxInputTimeout" />
      <StateGroup name="SearchHideContent">
        <State name="SearchInactive">
          <When type="Property" frame="$this" Text="" />
          <Action type="SetProperty" frame="$parent/ContentTileList" SearchFlags="" />
        </State>
        <State name="SearchActive">
          <When type="Property" frame="$this" Text="" operator="NotEqual" />
          <Action type="SetProperty" frame="$parent/CategoryTileList" Visible="false" />
          <Action type="SetProperty" frame="$parent/ContentTileList" MaxItemPerPage="18" />
          <Action type="SetProperty" frame="$parent/ContentTileList" CollectionCategoryFilter="" undo="false" />
          <Action type="SetProperty" frame="$parent/ContentTileList" ProductAttribute="" index="0" />
          <Action type="SetProperty" frame="$parent/ContentTileList" CollectionCategoryFilter="{$parent/CategoryTileList/@SelectedCollectionCategory}" on="Exit" />
        </State>
      </StateGroup>
    </Frame>
    <Frame type="CollectionCategoryTileList" name="CategoryTileList" template="HeroSelectLoadoutTemplates/LoadoutCategoryTileListTemplate">
      <FilterFlags val="{$parent/ContentTileList/@FilterFlags}" />
      <UpdateWhenLicensesChange val="{$parent/ContentTileList/@UpdateWhenLicensesChange}" />
    </Frame>
    <Frame type="LoadoutTileList" name="ContentTileList">
      <FilterString val="{$parent/SearchEditBox/@Text}" />
      <CollectionCategoryFilter val="{$parent/CategoryTileList/@SelectedCollectionCategory}" />
      <StateGroup name="OwnershipFilter">
        <State name="AllContent">
          <When type="Property" frame="$parent/OwnershipFilterPulldown" SelectedFilter="AllContent" />
          <Action type="SetProperty" FilterFlag_Owned="true" />
          <Action type="SetProperty" FilterFlag_NotOwned="true" />
        </State>
        <State name="OwnedContent">
          <When type="Property" frame="$parent/OwnershipFilterPulldown" SelectedFilter="OwnedContent" />
          <Action type="SetProperty" FilterFlag_Owned="true" />
          <Action type="SetProperty" FilterFlag_NotOwned="false" />
        </State>
        <State name="UnownedContent">
          <When type="Property" frame="$parent/OwnershipFilterPulldown" SelectedFilter="UnownedContent" />
          <Action type="SetProperty" FilterFlag_Owned="false" />
          <Action type="SetProperty" FilterFlag_NotOwned="true" />
        </State>
      </StateGroup>
    </Frame>
  </Frame>
</Desc>