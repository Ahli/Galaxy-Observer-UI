<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="HeroRankedPanel" name="HeroRankedPanel">
        <Anchor relative="$parent"/>

        <Animation name="Transition">
            <Event event="Show" action="Reset"/>
            <Event event="Show" action="Play"/>
            <Event event="Hide" action="DirectionReverse"/>
            <Event event="Hide" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0" out="Slow"/>
                <Key type="Curve" time="0.4" value="255" in="Slow" out="Slow"/>
            </Controller>
        </Animation>

        <Animation name="TransitionSound">
            <Event event="Show" action="Reset"/>
            <Event event="Show" action="Play"/>

            <Controller type="Sound" end="Pause">
                <Key type="Sound" time="0.0" sound="@UI_Gluescreen_Transition_HeroLeague"/> <!-- TODO: Use correct sound? -->
            </Controller>
        </Animation>
        
        <!-- 
            Under the level requirement for Ranked Play -or- Over level req. but doesn't own enough heroes
        -->
         <Animation name="QualifiedForRankedPlay">
            <Event event="QualifiedForRankedPlay" action="DirectionReverse"/>
            <Event event="QualifiedForRankedPlay" action="Play"/>
            <Event event="DisqualifiedForRankedPlay" action="Restart"/>
            <Event event="DisqualifiedForRankedPlay" action="Play"/>
            <Controller type="Visibility" end="Pause" frame="RequirementsLabel">
                <Key type="Bool" time="0.00" value="false"/>
                <Key type="Bool" time="0.00" value="true"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="LockedImage">
                <Key type="Bool" time="0.00" value="false"/>
                <Key type="Bool" time="0.00" value="true"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="LockedGlowImage">
                <Key type="Bool" time="0.00" value="false"/>
                <Key type="Bool" time="0.00" value="true"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="SeasonProgressBackgroundImage">
                <Key type="Bool" time="0.00" value="true"/>
                <Key type="Bool" time="0.00" value="false"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="LeagueTypes">
                <Key type="Bool" time="0.00" value="true"/>
                <Key type="Bool" time="0.00" value="false"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="ReadyButton">
                <Key type="Bool" time="0.00" value="True"/>
                <Key type="Bool" time="0.00" value="False"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="TitleBackgroundImage">
                <Key type="Bool" time="0.00" value="False"/>
                <Key type="Bool" time="0.00" value="True"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="LockedTitleLabel">
                <Key type="Bool" time="0.00" value="False"/>
                <Key type="Bool" time="0.00" value="True"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="LockedDescriptionLabel">
                <Key type="Bool" time="0.00" value="False"/>
                <Key type="Bool" time="0.00" value="True"/>
            </Controller>
        </Animation>
        
        <!-- 
            Hero League or Team League 
        -->
        <Animation name="ToggleGameMode">
            <Event event="RankedPlayTeamLeagueActive" action="Restart"/>
            <Event event="RankedPlayTeamLeagueActive" action="Play"/>
            <Event event="RankedPlayHeroLeagueActive" action="DirectionReverse"/>
            <Event event="RankedPlayHeroLeagueActive" action="Play"/>
            
            <Controller type="Fade" end="Pause" frame="$this/LeagueTypes/HeroLeague">
                <Key type="Curve" time="0" value="255" out="Auto"/>
                <Key type="Curve" time=".4" value="225" out="Auto"/>
            </Controller>

            <Controller type="Fade" end="Pause" frame="$this/LeagueTypes/TeamLeague">
                <Key type="Curve" time="0" value="225" out="Auto"/>
                <Key type="Curve" time=".4" value="255" out="Auto"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="$this/LeagueTypes/HeroLeague">
                <Key type="Event" time="0" event="Selected"/>
                <Key type="Event" time=".01" event="Deselected"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="$this/LeagueTypes/TeamLeague">
                <Key type="Event" time="0" event="Deselected"/>
                <Key type="Event" time=".01" event="Selected"/>
            </Controller>
        </Animation>

        <Animation name="NoGameModeActive">
            <Event event="RankedPlayNoActiveQueue" action="Restart"/>
            <Event event="RankedPlayNoActiveQueue" action="Play"/>
            
            <Controller type="Fade" end="Pause" frame="$this/LeagueTypes/HeroLeague">
                <Key type="Curve" time="0" value="225" out="Auto"/>
            </Controller>

            <Controller type="Fade" end="Pause" frame="$this/LeagueTypes/TeamLeague">
                <Key type="Curve" time="0" value="225" out="Auto"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="$this/LeagueTypes/HeroLeague">
                <Key type="Event" time="0" event="Deselected"/>
            </Controller>
            
            <Controller type="Event" end="Stop" frame="$this/LeagueTypes/TeamLeague">
                <Key type="Event" time="0" event="Deselected"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="TitleBackgroundImage">
            <Anchor side="Top" relative="$parent/LockedTitleLabel" pos="Mid" offset="20"/>
            <Anchor side="Bottom" relative="$parent/LockedTitleLabel" pos="Mid" offset="20"/>
            <Anchor side="Left" relative="$parent/LockedTitleLabel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/LockedTitleLabel" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_Lobby_Ranked_HeroLeague_RankBG_Glow"/>
            <Width val="1250"/>
            <Height val="208"/>
            <BlendMode val="Add"/>
            <Alpha val="175"/>
        </Frame>

        <Frame type="Label" name="LockedTitleLabel">
            <Anchor side="Bottom" relative="$parent/LockedDescriptionLabel" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/LockedDescriptionLabel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/LockedDescriptionLabel" pos="Mid" offset="0"/>
            <Text val="@UI/RankedPlay/LockedTitle"/>
            <Style val="HeroLobbyRankedTitle"/>
        </Frame>

        <Frame type="Label" name="LockedDescriptionLabel">
            <Anchor side="Bottom" relative="$parent/LockedImage" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Text val="@UI/RankedPlay/LockedDescription"/>
            <Style val="HeroLobbyRankedTitleDescription"/>
        </Frame>

        <Frame type="Image" name="LockedImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="30"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="30"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_TeamLeague_Locked"/>
        </Frame>
        
        <Frame type="Image" name="LockedGlowImage">
            <Anchor relative="$parent/LockedImage"/>
            <LayerCount val="2"/>
            <BlendMode val="Add"/>
            
            <Texture val="@UI/Storm_UI_ProfileDialog_BorderGlow_Mask" layer="0"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <WrapUV val="True" layer="0"/>
            
            <Texture val="@UI/Storm_UI_TeamLeague_Locked_Glow" layer="1"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
            <WrapUV val="True" layer="1"/>
            
            <Animation name="UVScroll">
                <Event event="OnShown" action="Restart" frame="$parent"/>
                <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
                <Controller type="LayerUV" layer="0" side="Left" relative="True" sidelock="True" end="Loop" >
                    <Key type="Curve" time="0" value="1" out="fast"/>
                    <Key type="Curve" time="9" value="0" in="fast"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="RequirementsLabel">
            <Anchor side="Top" relative="$parent/LockedImage" pos="Max" offset="-76" />
            <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
            <Anchor side="Right" relative="$parent" pos="Max" offset="0" />
            <Style val="HeroLobbyRankedRequirements"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Frame" name="LeagueTypes">
            <Anchor side="Top" relative="$parent" pos="Min" offset="200" />
            <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0" />
            <Anchor side="Right" relative="$parent" pos="Max" offset="0" />
            
            <Frame type="Image" name="FloorBackground">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Width val="0"/>
                <Texture val="@UI/Storm_UI_Ranked_BackgroundFloor"/>
                <RenderType val="HDR"/>
            </Frame>

            <Frame type="HeroRankedLeagueContainer" name="HeroLeague" template="HeroRankedLeagueContainer/HeroRankedLeagueContainerTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
                <Anchor side="Right" relative="$parent" pos="Mid" offset="-5" />
                <QueueTag val="RnkS"/>
                <Animation name="LeagueType">
                    <Event event="OnShown" action="Play"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="0" event="HeroLeague"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="HeroRankedLeagueContainer" name="TeamLeague" template="HeroRankedLeagueContainer/TeamRankedLeagueContainerTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
                <Anchor side="Left" relative="$parent" pos="Mid" offset="5" />
                <QueueTag val="RnkP"/>
                <Animation name="LeagueType">
                    <Event event="OnShown" action="Play"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="0" event="TeamLeague"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="Clouds1Image">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Height val="936"/>
                <Width val="4000"/>
                <LayerCount val="3"/>
                <SubpixelRendering val="True"/>
                
                <Texture val="@UI/Storm_UI_Plasma_Clouds2_Mask" layer="0"/>
                <TextureCoords top="0" left="0" bottom="1" right=".5" layer="0"/>
                <AlphaLayer val="True" layer="0"/>
                <WrapUV val="True" layer="0"/>
                
                <Texture val="@UI/Storm_UI_Clouds_RepeatX" layer="1"/>
                <TextureCoords top="0" left="-.2" bottom="1" right=".8" layer="1"/>
                <AlphaLayer val="True" layer="1"/>
                <WrapUV val="True" layer="1"/>
                
                <Texture val="@UI/Storm_UI_Clouds_RepeatX" layer="2"/>
                <TextureCoords top="0" left="0" bottom="1" right=".5" layer="2"/>
                <WrapUV val="True" layer="2"/>
                
                <Animation name="UVScroll">
                    <Event event="OnShown" action="Restart" frame="$parent"/>
                    <Event event="OnShown" action="Play" frame="$parent"/>
                    <Controller type="LayerUV" layer="0" side="Left" relative="True" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="0" out="fast"/>
                        <Key type="Curve" time="91.9" value="1" in="fast"/>
                    </Controller>
                    <Controller type="LayerUV" layer="1" side="Left" relative="True" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="0" out="fast"/>
                        <Key type="Curve" time="31.9" value="1" in="fast"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Image" name="Clouds2Image">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Height val="936"/>
                <Width val="4000"/>
                <LayerCount val="3"/>
                <SubpixelRendering val="True"/>
                <Alpha val="255"/>
                <Visible val="True"/>
                
                <Texture val="@UI/Storm_UI_Clouds_RepeatX" layer="0"/>
                <TextureCoords top="0" left="0" bottom="1" right=".5" layer="0"/>
                <AlphaLayer val="True" layer="0"/>
                <WrapUV val="True" layer="0"/>
                
                <Texture val="@UI/Storm_UI_Clouds_RepeatX" layer="1"/>
                <TextureCoords top="0" left="-.2" bottom="1" right=".8" layer="1"/>
                <AlphaLayer val="True" layer="1"/>
                <WrapUV val="True" layer="1"/>
                
                <Texture val="@UI/Storm_UI_Clouds_RepeatX" layer="2"/>
                <TextureCoords top="0" left="1" bottom="1" right="0" layer="2"/>
                <WrapUV val="True" layer="2"/>
                
                <Animation name="UVScroll">
                    <Event event="OnShown" action="Restart" frame="$parent"/>
                    <Event event="OnShown" action="Play" frame="$parent"/>
                    <Controller type="LayerUV" layer="0" side="Left" relative="True" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="0" out="fast"/>
                        <Key type="Curve" time="121.9" value="1" in="fast"/>
                    </Controller>
                    <Controller type="LayerUV" layer="1" side="Left" relative="True" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="0" out="fast"/>
                        <Key type="Curve" time="91.9" value="1" in="fast"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="CutsceneFrame" name="FogCutscene">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="100"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                <Height val="1500"/>
                <RenderType val="HDR"/>
                <AutoPlay val="true"/>
                <Alpha val="70"/>
                
                <Animation name="OnShown">
                    <Event event="OnShown" action="Reset"/>
                    <Event event="OnShown" action="Play"/>
                    <Controller type="Cutscene" end="Stop">
                        <Key type="Cutscene" time="0.0" action="SetCutscene" file="Cutscenes/UI_GroundFog_Blue.StormCutscene"/>
                        <Key type="Cutscene" time="0.0" action="Bookmark" bookmark="Stand Start"/>
                        <Key type="Cutscene" time="0.0" action="Play"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>
        
        <Frame type="Image" name="SeasonProgressBackgroundImage">
            <Anchor side="Top" relative="SeasonLabel" pos="Mid" offset="4"/>
            <Anchor side="Bottom" relative="SeasonLabel" pos="Mid" offset="4"/>
            <Anchor side="Left" relative="SeasonLabel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="SeasonLabel" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Ranked_SeasonProgressBackground"/>
            <Width val="892"/>
            <TextureType val="Nineslice"/>

            <Frame type="Frame" name="SeasonProgressContainer">
                <Anchor relative="$parent" offset="-4"/>
            
                <Frame type="Frame" name="ProgressBarMask">
                    <Anchor relative="$parent"/>
                    <AlphaMaskTexture val="@UI/Storm_UI_Ranked_SeasonProgressBackground"/>
                    <AlphaMaskSiblings val="true"/>
                    <RenderPriority val="0"/>
                    <Visible val="true"/>
                </Frame>

                <Frame type="ProgressBar" name="SeasonProgressBar">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

                    <UseFillContainer val="true" />

                    <Texture val="@UI/Storm_UI_Ranked_SeasonProgressBar"/>
                    <MinValue val="0.00"/>
                    <MaxValue val="1.00"/>

                    <Frame type="Frame" name="FillImageContainer">
                        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    </Frame>
                </Frame>
            </Frame>

            <Frame type="Image" name="SeasonProgressBarSubImage">
                <Anchor side="Top" relative="$parent/SeasonProgressContainer" pos="Max" offset="-16"/>
                <Anchor side="Left" relative="$parent/SeasonProgressContainer" pos="Min" offset="6"/>
                <Anchor side="Right" relative="$parent/SeasonProgressContainer/SeasonProgressBar/FillImageContainer" pos="Max" offset="-12"/>
                <Texture val="@UI/Storm_UI_Ranked_SeasonProgressBar_Sub"/>
                <TextureType val="Nineslice"/>
            </Frame>

            <Frame type="Image" name="SeasonProgressBarSubGlowImage">
                <Anchor side="Top" relative="$parent/SeasonProgressBarSubImage" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/SeasonProgressBarSubImage" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/SeasonProgressBarSubImage" pos="Max" offset="10"/>
                <Texture val="@UI/Storm_UI_Ranked_SeasonProgressBar_SubGlow"/>
                <BlendMode val="Add"/>
                <Animation name="Pulse">
                    <Event event="OnShown" action="Play"/>
                    <Controller type="Fade" end="PingPong">
                        <Key type="Curve" time="0" value="200" out="Slow"/>
                        <Key type="Curve" time=".4" value="255" in="Slow"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Label" name="SeasonLabel">
                <Anchor side="Bottom" relative="$parent/$parent/LeagueTypes/HeroLeague" pos="Max" offset="-25" />
                <Anchor side="Right" relative="$parent/$parent/LeagueTypes" pos="Mid" offset="0" />
                <Anchor side="Left" relative="$parent/$parent/LeagueTypes" pos="Mid" offset="0" />
                <Style val="HeroLobbyRankedHeroLeagueSeason" />
                <Text val="@UI/RankedPlay/CurrentSeasonDates" />
            </Frame>
        </Frame>

        <Frame type="Button" name="ReadyButton" template="StandardTemplates/ReadyButtonTemplate_Middle">
            <Handle val="HeroRankedPanel_ReadyButton"/>
            <ClickSound val="@UI_BNet_Ready"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-20"/>
            <Text val="@UI/ScreenHeroMatchmaking/ReadyButton"/>
            <TooltipAnchorPosition val="TopCenter"/>
        </Frame>
    </Frame>
</Desc>
