            <ReportsStats val="true"/>
            <Handle val="ScreenArtifact_NextItemButton"/>
            <Shortcut val="@UI/ScreenNavigationRight_Hotkey"/>
        </Frame>

        <Frame type="Button" name="AboutButton" template="StandardTemplates/Top_Navigation">
            <Anchor side="Top" relative="$parent" pos="Min" offset="120"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="100"/>
            <Text val="@UI/ScreenArtifact/AboutButton"/>
            <Shortcut val="@UI/ScreenNavigation1_Hotkey"/>
            <ReportsStats val="true"/>
            <Handle val="ScreenArtifact_AboutButton"/>
        </Frame>
    </Frame>
</Desc>
