<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="StoreTileListItem" name="EmoticonPackStoreTileListItemTemplate" template="StoreTemplates/StoreTileListItemTemplate">
        <Frame type="HeroCutsceneFrame" name="PortraitCutscene">
            <Anchor side="Top" relative="$parent" pos="Min" offset="-40"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <HeroCutsceneType val="LootChestReward"/>
            <GameContent val="{$parent/@GameContent}"/>
        </Frame>
        <Frame type="Image" name="FreePlayImage">
            <Visible val="false"/>
        </Frame>
    </Frame>

    <!--===============================================================================================
    <! Main Frame
    <!==============================================================================================-->
    <Frame type="Frame" name="EmoticonPackCollectionPanelTemplate" template="CollectionTemplates/GameContentCollectionPanelTemplate">
        <Frame type="Frame" name="BigSectionHeader" template="CollectionTemplates/SectionHeaderTemplate">
            <Frame type="Image" name="TitleIcon">
                <Texture val="@UI/Storm_UI_Store_TitleIcon_Emojis"/>
            </Frame>
            <Frame type="Label" name="TitleLabel">
                <Text val="@UI/ScreenStore/BigSectionTitle/EmoticonPacks"/>
            </Frame>
        </Frame>

        <Frame type="EditBox" name="SearchEditBox" template="CollectionTemplates/SearchEditBoxTemplate">
            <Frame type="Label" name="SearchTextLabel">
                <Text val="@UI/ScreenStore/SearchEditBox/EmoticonPacks"/>
            </Frame>
        </Frame>

        <Frame type="CollectionCategoryTileList" name="CategoryTileList" template="CollectionTemplates/NoImageCategoryTileListTemplate">
            <Handle val="ScreenCollection_EmoticonPack_CategoryTileList"/>
            <GameContentType val="EmoticonPack"/>

            <Frame type="Button" name="FeaturedItem" template="StoreTemplates/FeaturedCategoryTemplate">
                <Visible val="false"/>
            </Frame>
        </Frame>

        <Frame type="GameContentTileList" name="ContentTileList" template="CollectionTemplates/ContentTileListTemplate">
            <Handle val="ScreenCollection_EmoticonPack_ContentTileList"/>
            <GameContentType val="EmoticonPack"/>

            <Frame type="StoreTileListItem" name="Item0" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item1" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item2" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item3" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item4" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item5" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item6" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item7" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item8" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item9" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item10" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
            <Frame type="StoreTileListItem" name="Item11" template="EmoticonPackCollectionPanel/EmoticonPackStoreTileListItemTemplate"/>
        </Frame>
    </Frame>
</Desc>
