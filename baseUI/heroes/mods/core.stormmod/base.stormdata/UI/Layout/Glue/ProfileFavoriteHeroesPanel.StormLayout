<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Constant name="FavoriteHeroesVerticalSpace" val="-79"/>
    <Constant name="FavoriteHeroesHorizontalSpace" val="-113"/>
    <Constant name="Padding" val="15"/>
    <Constant name="PaddingNeg" val="-15"/>

    <Frame type="Frame" name="SparkleContainerTemplate">
        <AcceptsMouse val="false"/>

        <Animation name="SparkleController">
            <Event event="OnShown" action="Restart,Play"/>

            <!-- Reset Sparkles whenever data is updated due to paging -->
            <Event event="HeroItemUpdated" action="Restart,Play" frame="$ancestor[@type=HeroCollectionTileListItem]"/>

            <Controller type="Event" end="Loop">
                <Key type="Event" time="0" event="ResetSparkle"/>
                <Key type="Event" time="3" event="BeginFlare"/>
                <Key type="Event" time="3.3" event="BeginShine"/>
                <Key type="Event" time="7" event="Null"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="CoinFlashImage">
            <Anchor relative="$parent/$parent/HeroContainerFrame/HeroExperienceBar/FillImageContainer/FillImage"/>
            <LayerCount val="2"/>
            <BlendMode val="Add"/>
            <Alpha val="0"/>
            
            <Texture val="@UI/Storm_UI_Play_HeroPortrait_ProgressBar_Mask" layer="0"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <WrapUV val="True" layer="0"/>
            
            <Texture val="@UI/storm_ui_glues_draft_headerBar_AngleGlow_Glow" layer="1"/>
            <TextureCoords top="-1" left=".4" bottom="2" right=".6" layer="1"/>
            <LayerColor val="dfc482" layer="1"/>
            <WrapUV val="False" layer="1"/>
            
            <Animation name="UVScroll">
                <Event event="BeginShine" action="Restart,Play" frame="$parent"/>
                <Event event="ResetSparkle" action="Stop" frame="$parent"/>

                <Controller type="LayerUV" layer="1" side="Left" relative="False" sidelock="True" end="Stop" >
                    <Key type="Curve" time="0" value="2" inout="Linear"/>
                    <Key type="Curve" time="1" value="2" inout="Linear"/>
                    <Key type="Curve" time="3" value="-2" inout="Linear"/>
                    <Key type="Curve" time="7" value="-2" inout="Linear"/>
                </Controller>
                <Controller type="Fade" end="Stop">
                    <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                    <Key type="Curve" time="2.5" value="255" inout="Linear,Auto"/>
                    <Key type="Curve" time="3" value="255" inout="Auto,Linear"/>
                    <Key type="Curve" time="7" value="0" inout="Auto,Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Frame" name="LensFlare1Anchor">
            <Anchor side="Top" relative="$parent" pos="Min" offset="84"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="84"/>
            <Width val="1"/>
            <Height val="1"/>
        </Frame>
        
        <Frame type="Image" name="LensFlareImage">
            <Anchor side="Top" relative="$parent/LensFlare1Anchor" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LensFlare1Anchor" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/LensFlare1Anchor" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/LensFlare1Anchor" pos="Mid" offset="0"/>
            
            <Alpha val="0"/>
            <BlendMode val="Add"/>
            <Texture val="@UI/Storm_UI_GoldCoin_Flare"/>
            <Unclipped val="true"/>

            <Animation name="Flare">
                <Event event="BeginFlare" action="Restart,Play" frame="$parent"/>
                <Event event="ResetSparkle" action="Stop" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                    <Key type="Curve" time=".4" value="255" inout="Slow,Linear"/>
                    <Key type="Curve" time="1.6" value="255" inout="Slow,Linear"/>
                    <Key type="Curve" time="2" value="0" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Rotation" relative="False" end="Loop" >
                    <Key type="Curve" time="0" value="-45" inout="Linear,linear"/>
                    <Key type="Curve" time="2" value="55" inout="linear,Linear"/>
                </Controller>
                <Controller type="Anchor" end="Loop" side="Left" relative="true" frame="$parent/LensFlare1Anchor">
                    <Key type="Curve" time="0" value="-10" inout="Linear"/>
                    <Key type="Curve" time="2" value="14" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Anchor" end="Loop" side="Top" relative="true" frame="$parent/LensFlare1Anchor">
                    <Key type="Curve" time="0" value="10" inout="Linear"/>
                    <Key type="Curve" time="2" value="-5" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Loop" >
                    <Key type="Curve" time="0" value="40" inout="Linear,Slow"/>
                    <Key type="Curve" time=".5" value="80" inout="Slow,Linear"/>
                    <Key type="Curve" time="1.4" value="104" inout="Slow,Linear"/>
                    <Key type="Curve" time="2" value="8" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Loop" >
                    <Key type="Curve" time="0" value="40" inout="Linear,Slow"/>
                    <Key type="Curve" time=".5" value="80" inout="Slow,Linear"/>
                    <Key type="Curve" time="1.4" value="104" inout="Slow,Linear"/>
                    <Key type="Curve" time="2" value="8" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Frame" name="LensFlare2Anchor">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-82"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-79"/>
            <Width val="5"/>
            <Height val="5"/>
        </Frame>
        
        <Frame type="Image" name="LensFlare2Image">
            <Anchor side="Top" relative="$parent/LensFlare2Anchor" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/LensFlare2Anchor" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/LensFlare2Anchor" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/LensFlare2Anchor" pos="Mid" offset="0"/>
            
            <Texture val="@UI/Storm_UI_GoldCoin_Flare"/>
            <BlendMode val="Add"/>
            <Alpha val="0"/>

            <Animation name="Flare">
                <Event event="BeginFlare" action="Restart,Play" frame="$parent"/>
                <Event event="ResetSparkle" action="Stop" frame="$parent"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                    <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
                    <Key type="Curve" time="1.4" value="255" inout="Slow,Linear"/>
                    <Key type="Curve" time="2" value="0" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Rotation" relative="False" end="Pause" >
                    <Key type="Curve" time="0" value="180" inout="Linear,linear"/>
                    <Key type="Curve" time="2" value="210" inout="linear,Linear"/>
                </Controller>
                <Controller type="Anchor" end="Pause" side="Left" relative="true" frame="$parent/LensFlare2Anchor">
                    <Key type="Curve" time="0" value="8" inout="Linear"/>
                    <Key type="Curve" time="2" value="-10" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$parent/LensFlare2Anchor">
                    <Key type="Curve" time="0" value="-5" inout="Linear"/>
                    <Key type="Curve" time="2" value="7" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Pause" >
                    <Key type="Curve" time="0" value="40" inout="Linear,Slow"/>
                    <Key type="Curve" time=".5" value="80" inout="Slow,Linear"/>
                    <Key type="Curve" time="1.4" value="80" inout="Slow,Linear"/>
                    <Key type="Curve" time="2" value="8" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Pause" >
                    <Key type="Curve" time="0" value="40" inout="Linear,Slow"/>
                    <Key type="Curve" time=".5" value="80" inout="Slow,Linear"/>
                    <Key type="Curve" time="1.4" value="80" inout="Slow,Linear"/>
                    <Key type="Curve" time="2" value="8" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <!--=======================================================================================
    <! Favorite TileListItem Template
    <!======================================================================================-->
    <Frame type="HeroCollectionTileListItem" name="FavoriteBaseTileListItemTemplate" template="HeroStandardTemplate/HeroTileListItemTemplate">
        <Frame type="HeroContainerFrame" name="HeroContainerFrame">
            <Anchor relative="$parent" offset="-60"/>

            <StateGroup name="HeroStates">
                <State name="AlwaysAllowed">
                    <Action type="ApplyTemplate" template="HeroStandardTemplate/HeroItemAllowedTemplate"/>
                    <Action type="SetProperty" frame="HeroLevelLabel" style="HeroSelectHeroLevelLabel_Small"/>
                </State>

                <State name="LockedGold">
                    <Action type="ApplyTemplate" template="HeroStandardTemplate/HeroItemLockedGoldTemplate"/>
                    <Action type="SetProperty" frame="HeroLevelLabel" style="HeroSelectHeroLevelUnownedCompleteLabel_Small"/>
                </State>

                <State name="LockedMax">
                    <Action type="ApplyTemplate" template="HeroStandardTemplate/HeroItemNotAllowedMaxTemplate"/>
                    <Action type="SetProperty" frame="HeroLevelLabel" style="HeroSelectHeroLevelUnownedCompleteLabel_Small"/>
                </State>
                
                <State name="Locked">
                    <Action type="ApplyTemplate" template="HeroStandardTemplate/HeroItemNotAllowedTemplate"/>
                    <Action type="SetProperty" frame="HeroLevelLabel" style="HeroSelectHeroLevelUnownedLabel_Small"/>
                </State>

                <State name="AllowedNormal">
                    <Action type="ApplyTemplate" template="HeroStandardTemplate/HeroItemAllowedTemplate"/>
                    <Action type="SetProperty" frame="HeroLevelLabel" style="HeroSelectHeroLevelLabel_Small"/>
                </State>

                <State name="Gold">
                    <Action type="ApplyTemplate" template="HeroStandardTemplate/HeroItemAllowedGoldTemplate"/>
                    <Action type="SetProperty" frame="HeroLevelLabel" style="HeroSelectHeroLevelCompleteLabel_Small"/>
                </State>

                <State name="Max">
                    <Action type="ApplyTemplate" template="HeroStandardTemplate/HeroItemAllowedMaxTemplate"/>
                    <Action type="SetProperty" frame="HeroLevelLabel" style="HeroSelectHeroLevelCompleteLabel_Small"/>
                </State>
            </StateGroup>

            <Frame type="HeroLabel" name="HeroNameLabel">
                <Anchor side="Top" relative="$parent" pos="Max" offset="4"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

                <Unclipped val="true"/>
                <HeroLabelType val="Name"/>
                <Style val="PlayerProfile_HeroRoster_HeroName"/>
                <Options val="ScaleToPreventTruncation"/>
            </Frame>
        </Frame>


        <Frame type="Image" name="HoverBackgroundImage">
            <RenderPriority val="500"/>
            <Anchor relative="$parent/HeroContainerFrame"/>
            <SubpixelRendering val="True"/>
            <Texture val="@UI/Storm_UI_Play_HeroPortrait_Hover"/>
            <Alpha val="0"/>

            <Animation name="Hover">
                <Event event="Hover" action="DirectionForward,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent"/>
                <Event event="Death" action="DirectionReverse,Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                    <Key type="Curve" time=".1" value="255" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <StateGroup name="LocalUserState">
            <DefaultState val="LocalUser"/>
            
            <State name="NonLocalUser">
                <When type="AnimationState" frame="$parent/$parent" localUserState="NonLocalUser"/>
                <Action type="SetProperty" dragMoveSlop="10000"/>
            </State>

            <State name="LocalUser">
                <Action type="SetProperty" dragMoveSlop="12"/>
            </State>
        </StateGroup>

        <!-- HOVERBACKGROUND IMAGE  -->
        <StateGroup name="HoverBackgroundImageState">
            <DefaultState val="Gold"/>
            
            <State name="Locked">
                <When type="Property" frame="HeroContainerFrame/HeroState" isAllowed="false"/>
                <Action type="SetProperty" frame="HoverBackgroundImage" texture="@UI/Storm_UI_Play_HeroPortrait_Dimmed_Hover"/>
            </State>

            <State name="AllowedNormal">
                <!-- Allowed == false -->
                <When type="Property" frame="HeroContainerFrame/HeroState" levelType="Normal"/>
                <Action type="SetProperty" frame="HoverBackgroundImage" texture="@UI/Storm_UI_Play_HeroPortrait_Hover"/>
            </State>

            <State name="Gold">
                <!-- Allowed == false -->
                <!-- LevelType == Gold || Max -->
                <Action type="SetProperty" frame="HoverBackgroundImage" texture="@UI/Storm_UI_Play_HeroPortrait_Gold_Hover"/>
            </State>
        </StateGroup>

        <!-- HERO NAME LABEL -->
        <StateGroup name="HeroNameStyle">
            <State name="Normal">
                <When type="Property" frame="HeroContainerFrame/HeroState" levelType="Normal"/>
                <Action type="SetProperty" frame="HeroContainerFrame/HeroNameLabel" style="PlayerProfile_HeroRoster_HeroName"/>
            </State>
            <State name="GoldLocked">
                <When type="Property" frame="HeroContainerFrame/HeroState" LevelType="Gold"/>
                <When type="Property" frame="HeroContainerFrame/HeroState" isAllowed="false"/>
                <Action type="SetProperty" frame="HeroContainerFrame/HeroNameLabel" style="PlayerProfile_HeroRoster_HeroNameMasterLocked"/>
            </State>
            <State name="Gold">
                <When type="Property" frame="HeroContainerFrame/HeroState" LevelType="Gold"/>
                <Action type="SetProperty" frame="HeroContainerFrame/HeroNameLabel" style="PlayerProfile_HeroRoster_HeroNameMaster"/>
            </State>
            <State name="Max">
                <When type="Property" frame="HeroContainerFrame/HeroState" LevelType="Max"/>
                <Action type="SetProperty" frame="HeroContainerFrame/HeroNameLabel" style="PlayerProfile_HeroRoster_HeroNameMaster"/>
            </State>
        </StateGroup>

        <StateGroup name="HeroNameOpacity">
            <State name="Locked">
                <When type="Property" frame="HeroContainerFrame/HeroState" isAllowed="false"/>
                <Action type="SetProperty" frame="HeroContainerFrame/HeroNameLabel" alpha="128" undo="true"/>
            </State>
        </StateGroup>

        <Animation name="StartUp">
            <Event event="StartUp" action="Reset,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
            </Controller>
            <Controller type="Animation" end="Pause" animation="Intro">
                <Key type="Animation" time="0" action="Reset"/>
                <Key type="Animation" time="0" action="Stop"/>
            </Controller>
        </Animation>
        
        <Animation name="Intro">
            <Event event="Intro" action="Restart,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".4" value="255" inout="Slow,Linear"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="HeroCollectionTileListItem" name="FavoriteTileListItemTemplate" template="ProfileFavoriteHeroesPanel/FavoriteBaseTileListItemTemplate">
        <Frame type="Frame" name="SparkleContainer" template="ProfileFavoriteHeroesPanel/SparkleContainerTemplate">
            <Anchor relative="$parent" />
            <Visible val="false"/>
            <StateGroup name="ShowSparkle">
                <State name="Show">
                    <When type="Property" frame="$parent/HeroContainerFrame/HeroState" levelType="Max"/>
                    <When type="Property" frame="$parent/HeroContainerFrame/HeroState" isAllowed="true"/>
                    <Action type="SetProperty" visible="true"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>

    <!--=======================================================================================
    <! FavoriteTargetTileListItemTemplate
    <!======================================================================================-->
    <Frame type="HeroCollectionTileListItem" name="FavoriteTargetTileListItemTemplate" template="ProfileFavoriteHeroesPanel/FavoriteBaseTileListItemTemplate">
        <Frame type="Frame" name="SparkleContainer" template="ProfileFavoriteHeroesPanel/SparkleContainerTemplate">
            <Anchor relative="$parent" />
            <Visible val="false"/>
            <StateGroup name="ShowSparkle">
                <State name="Show">
                    <When type="Property" frame="$parent/HeroContainerFrame/HeroState" levelType="Max"/>
                    <When type="Property" frame="$parent/HeroContainerFrame/HeroState" isAllowed="true"/>
                    <Action type="SetProperty" visible="true"/>
                </State>
            </StateGroup>
        </Frame>

        <StateGroup name="DragIndicator">
            <DefaultState val="Empty"/>
            
            <State name="DraggingHovered">
                <When type="AnimationState" frame="$parent/$parent" DragResult="DraggingNonFavorite"/>
                <Action type="SetProperty" frame="EmptyImage" texture="@UI/Storm_UI_Play_HeroPortrait_Empty_Active"/>
            </State>

            <State name="DraggingAddHovered">
                <When type="AnimationState" frame="$parent/$parent" DragResult="DraggingAdd"/>
                <Action type="SetProperty" frame="EmptyImage" texture="@UI/Storm_UI_Play_HeroPortrait_Empty_Hover"/>
            </State>

            <State name="Empty">
                <Action type="SetProperty" frame="EmptyImage" texture="@UI/Storm_UI_Play_HeroPortrait_Empty"/>
            </State>
        </StateGroup>
    </Frame>

