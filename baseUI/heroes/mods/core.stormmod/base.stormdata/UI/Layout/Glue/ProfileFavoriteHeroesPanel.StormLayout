    <!--=======================================================================================
    <! Panel
    <!======================================================================================-->
    <Frame type="ProfileFavoriteHeroesPanel" name="ProfileFavoriteHeroesPanelTemplate">

        <StateGroup name="LocalUserState">
            <DefaultState val="LocalUser"/>

            <State name="NonLocalUser">
                <When type="AnimationState" localUserState="NonLocalUser"/>
                <Action type="SetProperty" frame="FavoriteHeroTileList" visible="false" />
                <Action type="SetProperty" frame="FavoritesHeaderLabel" visible="false" />
                <Action type="SetProperty" frame="DropArea" visible="false"/>
            </State>

            <State name="LocalUser">
                <Action type="SetProperty" frame="FavoriteHeroTileList" visible="true" />
                <Action type="SetProperty" frame="FavoritesHeaderLabel" visible="true" />
                <Action type="SetProperty" frame="DropArea" visible="true"/>
            </State>
        </StateGroup>

        <Frame Type="HeroContextMenu" name="ContextMenu" template="StandardTemplates/StandardContextMenuTemplate" >
            <Anchor side="Top" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>

        <Frame type="Frame" name="DropArea">
            <Anchor relative="$parent/FavoriteHeroTileList"/>
        </Frame>

        <!--===========================
        <! Favorite Header
        <!===========================-->
        <Frame type="Frame" name="FavoritesHeaderLabel" template="ProfileSummaryPanel/SummarySubHeaderTemplate">
            <Anchor side="Top" relative="$parent/HeroFilterPanel" pos="Max" offset="-38"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-53"/>
            <Width val="413"/>

            <Frame type="Label" name="HeaderLabel">
                <Text val="@UI/ProfileDialog/FavoritesHeader"/>
            </Frame>
            <CollapseLayout val="true"/>
        </Frame>

        <!--===========================
        <! Favorite Tile List
        <!===========================-->
        <Frame type="TileList" name="FavoriteHeroTileList" template="ProfileFavoriteHeroesPanel/FavoriteTileListTemplate">
            <Anchor side="Top" relative="$parent/FavoritesHeaderLabel" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="300"/>
            <Width val="510"/>
            <CollapseLayout val="true"/>
        </Frame>

        <!--===========================
        <! General Stats
        <!===========================-->
        <Frame type="Frame" name="GeneralStatsHeaderLabel" template="ProfileSummaryPanel/SummarySubHeaderTemplate">
            <Anchor side="Top" relative="$parent/FavoriteHeroTileList" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-53"/>
            <Width val="413"/>

            <Frame type="Label" name="HeaderLabel">
                <Text val="@UI/ProfileDialog/GeneralStatsHeader"/>
            </Frame>
        </Frame>

        <Frame type="ListBox" name="GeneralStatListBox" template="ProfileSummaryPanel/ProfileGeneralStatListBoxTemplate">
            <Anchor side="Top" relative="$parent/GeneralStatsHeaderLabel" pos="Max" offset="15"/>
            <Anchor side="Left" relative="$parent/GeneralStatsHeaderLabel" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent/GeneralStatsHeaderLabel" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <ItemHeight val="35"/>
        </Frame>

        <!--===========================
        <! Header
        <!===========================-->
        <Frame type="Frame" name="HeroRosterHeaderLabel" template="ProfileSummaryPanel/SummarySubHeaderTemplate">
            <Anchor side="Bottom" relative="$parent/HeroTileList" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="48"/>
            <Width val="888"/>

            <Frame type="Label" name="HeaderLabel">
                <Text val="@UI/ProfileDialog/HeroRosterHeader"/>
            </Frame>
        </Frame>

        <!--===========================
        <! Hero TileList
        <!===========================-->
        <Frame type="TileList" name="HeroTileList" template="ProfileFavoriteHeroesPanel/HeroTileListTemplate">
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent/HeroFilterPanel" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

            <!-- Set fixed size so that scroll bar anchored to tilelist can be at fixed position. -->
            <Width val="940"/>
        </Frame>

        <!--===========================
        <! Filter Panel
        <!===========================-->
        <Frame type="HeroFilterPanel" name="HeroFilterPanel" template="HeroSelectChangeHeroPanel/HeroFilterPanelTemplate">
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="138"/>
            <FilterTarget val="$parent"/>

            <Frame type="Label" name="RoleFilterLabel">
                <Anchor side="Bottom" relative="$parent/FilterWarriorButton" pos="Mid" offset="0"/>
                <Anchor side="Top" relative="$parent/FilterWarriorButton" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/FilterWarriorButton" pos="Min" offset="5"/>
                <Style val="HeroSelectFilterLabel"/>
                <Text val="@UI/HeroSelect/Role"/>
            </Frame>

            <!-- Hide Blur frame that creates an artifact -->
            <Frame type="HeroRoleButton" name="FilterWarriorButton">
                <Anchor side="Top" relative="$parent" pos="Min" offset="20"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="95"/>
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="HeroRoleButton" name="FilterDamageButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="HeroRoleButton" name="FilterSupportButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="HeroRoleButton" name="FilterSpecialistButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="HeroRoleButton" name="FilterMulticlassButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Label" name="UniverseFilterLabel">
                <Anchor side="Bottom" relative="$parent/FilterWarcraftButton" pos="Mid" offset="0"/>
                <Anchor side="Top" relative="$parent/FilterWarcraftButton" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/FilterWarcraftButton" pos="Min" offset="5"/>
                <Style val="HeroSelectFilterLabel"/>
                <Text val="@UI/HeroSelect/Universe"/>
            </Frame>

            
            <Frame type="Button" name="FilterWarcraftButton">
                <Anchor side="Left" relative="$parent/FilterMulticlassButton" pos="Max" offset="97"/>
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterStarCraftButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterDiabloButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterOverwatchButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Button" name="FilterRetroButton">
                <Frame type="BlurFrame" name="BlurFrame">
                    <Visible val="false"/>
                </Frame>
            </Frame>

            <Frame type="Label" name="SortLabel">
                <Anchor side="Bottom" relative="$parent/SortButtonGroups/SortByAplhabetButton" pos="Mid" offset="0"/>
                <Anchor side="Top" relative="$parent/SortButtonGroups/SortByAplhabetButton" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/SortButtonGroups/SortByAplhabetButton" pos="Min" offset="5"/>
                <Style val="HeroSelectFilterLabel"/>
                <Text val="@UI/HeroSelect/Sort"/>
            </Frame>

            <Frame type="Frame" name="SortButtonGroups">
                <Anchor side="Top" relative="$parent/FilterOverwatchButton" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/FilterOverwatchButton" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="SortByFavoriteButton" pos="Min" offset="0"/>
                <Anchor side="Right" relative="SortByCloserToLevelingButton" pos="Max" offset="0"/>

                <Frame type="Button" name="SortByFavoriteButton">
                    <Visible val="false"/>
                </Frame>

                <Frame type="Image" name="SortDividerImage">
                    <Visible val="false"/>
                </Frame>

                <Frame type="Button" name="SortByAplhabetButton">
                    <Frame type="BlurFrame" name="BlurFrame">
                        <Visible val="false"/>
                    </Frame>
                </Frame>

                <Frame type="Button" name="SortByLevelButton">
                    <Frame type="BlurFrame" name="BlurFrame">
                        <Visible val="false"/>
                    </Frame>
                </Frame>

                <Frame type="Button" name="SortByCloserToLevelingButton">
                    <Anchor side="Right" relative="$parent/$parent/FilterEditBox" pos="Min" offset="-42"/>
                    <Frame type="BlurFrame" name="BlurFrame">
                        <Visible val="false"/>
                    </Frame>
                </Frame>
            </Frame>

            <Frame type="EditBox" name="FilterEditBox">
                <Anchor side="Right" relative="$parent" pos="Max" offset="-45"/>
                <Width val="420"/>
            </Frame>
        </Frame>

        <StateGroup name="DragState">
            <DefaultState val="None"/>

            <State name="Dragging">
                <When type="AnimationState" DragResult="Dragging"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Invalid"/>
            </State>

            <State name="DraggingNonFavorite">
                <When type="AnimationState" DragResult="DraggingNonFavorite"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Invalid"/>
            </State>

            <State name="DraggingAdd">
                <When type="AnimationState" DragResult="DraggingAdd"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="DropArea" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Add"/>
            </State>

            <State name="DraggingSwap">
                <When type="AnimationState" DragResult="DraggingSwap"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="DropArea" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Move"/>
            </State>

            <State name="DraggingRemove">
                <When type="AnimationState" DragResult="DraggingRemove"/>
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" visible="true"/>
                <Action type="SetProperty" frame="CursorAnchorFrame/CursorIcon" texture="@UI/Storm_UI_Cursor_Favorite_Remove"/>
                <Action type="SetProperty" frame="DropArea" visible="true"/>
            </State>

            <State name="None">
                <Action type="SetProperty" frame="CursorAnchorFrame" visible="false"/>
                <Action type="SetProperty" frame="DropArea" visible="false"/>
            </State>
        </StateGroup>

        <!-- This should be last so it always renders on top of all other inventory frames. -->
        <Frame type="Frame" name="CursorAnchorFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="32"/>
            <Height  val="32"/>
            <RenderPriority val="1024"/>

            <Frame type="Image" name="DragCursorImage">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="-10"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-10"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Width val="84"/>
                <Height val="84"/>
                <LayerCount val="1"/>
                <TextureType val="Normal" layer="0"/>
                <Alpha val="200"/>
                <Unclipped val="true"/>
            </Frame>

            <Frame type="Image" name="DragBorderImage">
                <Anchor side="Top" relative="$parent/DragCursorImage" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/DragCursorImage" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/DragCursorImage" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/DragCursorImage" pos="Mid" offset="0"/>
                <TextureType val="Normal"/>
                <Texture val="@UI/Storm_UI_Play_HeroPortrait_Drag"/>
                <Alpha val="200"/>
                <Unclipped val="true"/>
            </Frame>

            <Frame type="Image" name="CursorIcon">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="20"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="20"/>
                <Width val="22"/>
                <Height val="22"/>
                <RenderPriority val="1024"/>
                <LayerCount val="1"/>
                <TextureType val="Normal" layer="0"/>
                <Unclipped val="true"/>
            </Frame>
        </Frame>
    </Frame>
