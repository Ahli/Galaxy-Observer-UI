                    <Event event="OnShown" action="DirectionForward" frame="$parent/$parent"/>
                    <Event event="OnShown" action="Play" frame="$parent/$parent"/>
                    <Controller type="LayerUV" layer="1" side="Top" relative="True" sidelock="True" end="Loop" >
