            </Frame>
            
            <Frame type="Image" name="ExperienceBackground1">
                <Anchor side="Top" relative="$parent/PlayerExperienceBar" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayerExperienceBar" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/PlayerExperienceBar" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/PlayerExperienceBar" pos="Mid" offset="0"/>
                <Width val="632"/>
                <Height val="632"/>
                <LayerCount val="2"/>
                <Alpha val="180"/>
                
                <Texture val="@UI/Storm_UI_ProfileDialog_Collection_HeroMask" layer="0"/>
                <TextureCoords top=".045" left=".06" bottom=".92" right=".94" layer="0"/>
                <AlphaLayer val="True" layer="0"/>
                <WrapUV val="True" layer="0"/>
                
                <Texture val="@UI/Storm_UI_ProfileDialog_PlayerLevel_Background" layer="1"/>
                <TextureCoords top=".7" left=".3" bottom="1.6" right=".9" layer="1"/>
                <WrapUV val="True" layer="1"/>
                
                <Animation name="UVScroll">
                    <Event event="OnShown" action="Restart" frame="$parent/$parent"/>
                    <Event event="OnShown" action="DirectionForward" frame="$parent/$parent"/>
                    <Event event="OnShown" action="Play" frame="$parent/$parent"/>
                    <Controller type="LayerUV" layer="1" side="Top" relative="True" sidelock="True" end="Loop" >
                        <Key type="Curve" time="0" value="0" out="fast"/>
                        <Key type="Curve" time="45" value="1" in="fast"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="ExperienceBackground2">
                <Anchor side="Top" relative="$parent/PlayerExperienceBar" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/PlayerExperienceBar" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/PlayerExperienceBar" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/PlayerExperienceBar" pos="Mid" offset="0"/>
                <Width val="632"/>
                <Height val="632"/>
                <LayerCount val="2"/>
                <BlendMode val="Add"/>
                
