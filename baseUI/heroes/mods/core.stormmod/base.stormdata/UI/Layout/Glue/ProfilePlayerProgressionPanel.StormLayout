    <Frame type="PlayerRewardTileListItem" name="ProfilePlayerRewardTileListItemTemplate">
        <Height val="44"/>
        <Width val="400"/>
        <TooltipAnchorPosition val="MiddleLeft"/>
        
        <Animation name="OnShown">
            <Event event="OnShown" action="Reset"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Slow"/>
            </Controller>
        </Animation>
        
        <Frame type="Image" name="RewardIcon">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="40"/>
            <Height val="40"/>
            <Width val="40"/>
            <DesaturateOnDisable val="true"/>
            <DesaturationColor val="9c85bf"/>
            <OnDisableDesaturationColor val="9c85bf"/>
            
            <Animation name="RankLabelAwarded">
                <Event event="NotAwarded" action="DirectionForward" frame="$parent"/>
                <Event event="NotAwarded" action="Play" frame="$parent"/>
                <Event event="Awarded" action="DirectionReverse" frame="$parent"/>
                <Event event="Awarded" action="Play" frame="$parent"/>
                <Controller type="Desaturation" end="Pause">
                    <Key type="Bool" time="0" value="false"/>
                    <Key type="Bool" time=".01" value="true"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Label" name="LevelLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Style val="PlayerProfileRewardLocked"/>
            
            <Animation name="RewardLabelAwarded">
                <Event event="Awarded" action="DirectionForward" frame="$parent"/>
                <Event event="Awarded" action="Play" frame="$parent"/>
                <Event event="NotAwarded" action="DirectionReverse" frame="$parent"/>
                <Event event="NotAwarded" action="Play" frame="$parent"/>
                <Controller type="Style" end="Pause" style1="PlayerProfileRewardLocked" style2="PlayerProfileReward">
                    <Key type="Curve" time="0" value="0" out="Step"/>
                    <Key type="Curve" time=".01" value="1.0" in="Step"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Label" name="RewardLabel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/RewardIcon" pos="Max" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
