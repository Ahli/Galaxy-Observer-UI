                <Key type="Curve" time=".4" value="0" in="Slow"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="PlayerLevelFrame/PlayerLevelLabel">
                <Key type="Curve" time="0.0" value="60" out="Fast"/>
                <Key type="Curve" time=".4" value="0" in="Slow"/>
            </Controller>
        </Animation>
        
