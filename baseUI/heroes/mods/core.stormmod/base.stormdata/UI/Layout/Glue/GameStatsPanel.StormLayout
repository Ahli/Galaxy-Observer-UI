                <Style val="@Storm_ScoreScreenGameTime"/>
            </Frame>
        </Frame>

        <!-- BLUE TEAM -->
        <Frame type="Frame" name="BlueTeamStats" template="GameStatsPanel/BlueTeamStatsTemplate">
