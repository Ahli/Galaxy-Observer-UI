        </Frame>

        <!-- RED TEAM -->
        <Frame type="Frame" name="RedTeamStats" template="GameStatsPanel/RedTeamStatsTemplate">
            <Anchor side="Top" relative="$parent/GameTimeContainer" pos="Min" offset="0"/>
