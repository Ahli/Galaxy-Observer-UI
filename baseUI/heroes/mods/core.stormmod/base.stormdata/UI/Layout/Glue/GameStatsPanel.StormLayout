    <Frame type="UnitStatusBaselineQuestIcon" name="UnitStatusBaselineQuestIconTemplate" template="GameStatsPanel/QuestIconImageTemplate">
        <TooltipFrame val="HeroBaselineQuestTooltip"/>

        <StateGroup name="QuestState">
            
            <State name="QuestSelected">
                <Action type="SetProperty" frame="$this/IconBorderImage" texture="@UI/Storm_UI_InGame_Leader_Talent_Available"/>
                <Action type="SetProperty" frame="$this/IconBackgroundImage" alpha="255"/>
            </State>

            <State name="QuestAvailable">
                <Action type="SetProperty" frame="$this/IconImage" texture="@UI/Storm_UI_InGame_Leader_Talent_Unselected"/>
                <Action type="SetProperty" frame="$this/IconBorderImage" texture="@UI/Storm_UI_InGame_Leader_Talent_Available"/>
                <Action type="SetProperty" frame="$this/IconBackgroundImage" alpha="255"/>
            </State>

            <State name="QuestNotAvailable">
                <Action type="SetProperty" frame="$this/IconImage" visible="false"/>
                <Action type="SetProperty" frame="$this/IconBorderImage" texture="@UI/Storm_UI_InGame_Leader_Talent_Unavailable"/>
                <Action type="SetProperty" frame="$this/IconBackgroundImage" alpha="100"/>
            </State>
        </StateGroup>

        <StateGroup name="BaselineQuestProgress">
            <DefaultState val="NoQuest"/>

            <State name="HasQuest">
                <When type="Property" frame="$this" QuestProgress="QuestInProgress"/>
                <Action type="SetProperty" frame="QuestProgressBar" visible="True"/>
                <Action type="SetProperty" frame="UnfilledProgressImage" visible="True"/>
                <Action type="SetProperty" frame="QuestCompleteImage" visible="False"/>
                <Action type="SetProperty" frame="QuestProgressStackLabel" visible="True"/>
            </State>

            <State name="HasUntrackedQuest">
                <When type="Property" frame="$this" QuestProgress="ProgressUntracked"/>
                <Action type="SetProperty" frame="QuestProgressBar" visible="false"/>
                <Action type="SetProperty" frame="UnfilledProgressImage" visible="True"/>
                <Action type="SetProperty" frame="QuestCompleteImage" visible="False"/>
                <Action type="SetProperty" frame="QuestProgressStackLabel" visible="False"/>
            </State>

            <State name="FinishedQuest">
                <When type="Property" frame="$this" QuestProgress="FinishedQuest"/>
                <Action type="SetProperty" frame="QuestProgressBar" visible="False"/>
                <Action type="SetProperty" frame="UnfilledProgressImage" visible="True"/>
                <Action type="SetProperty" frame="QuestCompleteImage" visible="True"/>
                <Action type="SetProperty" frame="QuestProgressStackLabel" visible="False"/>
            </State>

            <State name="NoQuest">
                <Action type="SetProperty" frame="QuestProgressBar" visible="False"/>
                <Action type="SetProperty" frame="UnfilledProgressImage" visible="False"/>
                <Action type="SetProperty" frame="QuestCompleteImage" visible="False"/>
                <Action type="SetProperty" frame="QuestProgressStackLabel" visible="False"/>
            </State>
        </StateGroup>
    </Frame>
    
