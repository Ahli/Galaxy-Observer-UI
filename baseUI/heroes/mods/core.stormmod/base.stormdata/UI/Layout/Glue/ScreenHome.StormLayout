<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="ScreenHome" name="ScreenHome" template="StandardTemplates/GlueScreen">
        <Anchor relative="$parent"/>

        <Animation name="Transition">
            <Event event="Show" action="Reset"/>
            <Event event="Show" action="Play"/>
            <Event event="Hide" action="DirectionReverse"/>
            <Event event="Hide" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0" out="Slow"/>
                <Key type="Curve" time="0.4" value="255" in="Slow" out="Slow"/>
            </Controller>
        </Animation>

        <Animation name="PreTransition">
            <Event event="Begin" action="Reset"/>
            <Event event="Begin" action="Play"/>

            <Controller type="Sound" end="Pause">
                <Key type="Sound" time="0.0" sound="@UI_Gluescreen_Transition_Home"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="GenericBackground">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <ScaleWidthToTexture val="true"/>
            <Texture val="@UI/Storm_background_generic"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="HeroHomeInfoPanel" name="HeroHomeInfoPanel">
            <Anchor relative="$parent"/>
            <Frame type="HeroContainerFrame" name="PTRFrame" template="HomePTR/PTRFrameTemplate">
                <Animation name="ChangeHomeScreenHero">
                    <Event event="ResetHero" action="Stop" frame="$parent"/>
                    <Event event="PTR" action="Restart" frame="$parent"/>
                    <Event event="PTR" action="Play" frame="$parent"/>
                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0.00" value="false"/>
                        <Key type="Bool" time="0.01" value="true"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="HeroContainerFrame" name="TracerFrame" template="Home17mid/TracerFrameTemplate">
                <Animation name="ChangeHomeScreenHero">
                    <Event event="ResetHero" action="Stop" frame="$parent"/>
                    <Event event="Tracer" action="Restart,Play" frame="$parent"/>
                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="false"/>
                        <Key type="Bool" time=".01" value="true"/>
                    </Controller>
                    <Controller type="State" stateGroup="SoundtrackState" end="Pause" frame="$layer/GlueUI">
                        <Key type="Identifier" time="0" value="Tracer"/>
                    </Controller>
                </Animation>
                <Visible val="False"/>
            </Frame>
        
            <Frame type="HeroContainerFrame" name="DehakaFrame" template="Home17/DehakaFrameTemplate">
                <Animation name="ChangeHomeScreenHero">
                    <Event event="ResetHero" action="Stop" frame="$parent"/>
                    <Event event="Dehaka" action="Restart,Play" frame="$parent"/>
                    <Controller type="Visibility" end="Pause">
                        <Key type="Bool" time="0" value="false"/>
                        <Key type="Bool" time=".01" value="true"/>
                    </Controller>
                    <Controller type="State" stateGroup="SoundtrackState" end="Pause" frame="$layer/GlueUI">
                        <Key type="Identifier" time="0" value="Dehaka"/>
                    </Controller>
                </Animation>
                <Visible val="False"/>
            </Frame>
        
            <Frame type="HeroGlueButton" name="HomeDebugButton" template="StandardTemplates/StandardButtonMiniTemplate" requiredtoload="IS_SCREEN_MAIN_MENU_ENABLED">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-112"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="-12"/>
                <!-- <Tooltip val="Homescreen Debug"/>
                <TooltipAnchorPosition val="TopLeft"/> -->
                <Visible val="True"/>
                <Width val="90"/>
                <Height val="87"/>
                <Frame type="Label" name="TextLabel">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent/ConfigImage" pos="Max" offset="0"/>
                    <Text val=""/>
                    <Style val="StandardGlueButtonSmallLeft"/>
                </Frame>
                <Frame type="Image" name="ConfigImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="1"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="1"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="34"/>
                    <Texture val="@UI/Storm_Lobby_Icon_Settings"/>
                </Frame>
                <Animation name="Click">
                    <Event event="OnMouseDown" action="ToggleDirection,Play"/>
                    <Controller type="Fade" end="Pause" frame="$parent/HomeDebugContainer">
                        <Key type="Curve" time="0" value="255" in="Slow"/>
                        <Key type="Curve" time=".2" value="0" out="Slow"/>
                    </Controller>
                    <Controller type="Event" end="Pause" frame="$parent/HomeDebugContainer">
                        <Key type="Event" time="0" event="Begin"/>
                        <Key type="Event" time=".01" event="End"/>
                    </Controller>
                </Animation>
            </Frame>
        
