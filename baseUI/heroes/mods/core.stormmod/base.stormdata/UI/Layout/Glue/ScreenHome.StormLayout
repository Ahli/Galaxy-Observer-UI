<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <Frame type="Button" name="HomeDebugButtonTemplate" template="StandardTemplates/StandardButtonMiniTemplate">
        <CollapseLayout val="True"/>
        <Height val="70"/>
        <Animation name="Begin">
            <Event event="Begin" action="DirectionForward,Play" frame="$parent"/>
            <Event event="End" action="DirectionReverse,Play" frame="$parent"/>
            <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                <Key type="Curve" time="0" value="100" inout="Linear"/>
                <Key type="Curve" time=".2" value="0" inout="Slow,Linear"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="ScreenHome" name="ScreenHome" template="StandardTemplates/GlueScreen">
        <Handle val="ScreenHome_Main" />
        <Anchor relative="$parent"/>
        <AutoCycleContent val="true"/>
        <AutoShowContent val="true"/>
        <ContentCycleDelay val="30000"/>

        <Animation name="Transition">
            <Event event="Show" action="Reset,Play"/>
            <Event event="Hide" action="DirectionReverse,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time="0.4" value="255" inout="Slow"/>
            </Controller>
        </Animation>

        <Animation name="PreTransition">
            <Event event="BeginPageTransition" action="Reset,Play"/>
            <Controller type="Sound" end="Pause">
                <Key type="Sound" time="0.0" sound="@UI_Gluescreen_Transition_Home"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="GenericBackground">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <ScaleWidthToTexture val="true"/>
            <Texture val="@UI/Storm_UI_Null"/>
            <Visible val="false"/>
        </Frame>

        <StateGroup name="ContentTakeoverState">
            <State name="WaitingForData">
                <When type="Property" WaitingForData="True"/>
                <Action type="SetState" frame="HeroHomeInfoPanel" group="ShowingHero" state="Hidden"/>
            </State>

            <State name="ShowContent">
                <When type="Property" WaitingForData="False"/>
                <When type="Property" ShowContent="True"/>
                <Action type="SendEvent" event="ShowContent"/>
                <Action type="SetState" frame="HeroHomeInfoPanel" group="ShowingHero" state="Hidden"/>
            </State>

            <State name="ShowHero">
                <When type="Property" WaitingForData="False"/>
                <When type="Property" ShowContent="False"/>
                <Action type="SendEvent" event="HideContent"/>
                <Action type="SetState" frame="HeroHomeInfoPanel" group="ShowingHero" state="Shown"/>
            </State>
        </StateGroup>

        <Frame type="HeroHomeInfoPanel" name="HeroHomeInfoPanel">
            <Anchor relative="$parent"/>
            <Visible val="False"/>

            <StateGroup name="ShowingHero">
                <State name="Shown">
                    <Action type="SetProperty" Visible="True" undo="false"/>
                    <Action type="SendEvent" event="Reset"/>
                    <Action type="SendEvent" event="Hide" on="Exit"/>
                </State>

                <State name="Hidden"/>
            </StateGroup>

            <Animation name="HideHero">
                <Event event="Hide" action="Play"/>
                <Event event="Reset" action="Pause, Restart"/>

                <Controller type="Visibility" end="Stop">
                    <Key type="Bool" time="0.00" value="true"/>
                    <Key type="Bool" time="0.99" value="true"/>
                    <Key type="Bool" time="1" value="false"/>
                </Controller>
            </Animation>
            
