                        <Event event="Begin" action="Play" frame="$parent"/>
                        <Event event="End" action="DirectionReverse" frame="$parent"/>
                        <Event event="End" action="Play" frame="$parent"/>
                        <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
                            <Key type="Curve" time="0" value="100" out="Fast"/>
                            <Key type="Curve" time=".2" value="0" in="Slow"/>
                        </Controller>
                    </Animation>
