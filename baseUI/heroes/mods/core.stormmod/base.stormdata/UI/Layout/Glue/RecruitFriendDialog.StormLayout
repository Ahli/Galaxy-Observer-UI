<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>
    <Constant name="RAFShareButtonSpacing" val="-25"/>
    
    <Frame type="AddFriendButton" name="IconButtonTemplate">
        <Width val="80"/>
        <Height val="80"/>
        <CollapseLayout val="true"/>
        <PreserveAnchorOffset val="True"/>
        <ClickSound val="@UI_BNet_Generic_01"/>
        <DisplayType val="ImageLayer"/>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <LayerCount val="4"/>
            <Texture val="@UI/Storm_standardButtonMini_Blue_Normal" layer="0"/>
            <Texture val="@UI/Storm_standardButtonMini_Blue_Down" layer="1"/>
            <Texture val="@UI/Storm_standardButtonMini_Blue_Down" layer="2"/>
            <Texture val="@UI/Storm_standardButtonMini_Blue_Disabled" layer="3"/>
            <TextureType val="Nineslice" layer="0"/>
            <TextureType val="Nineslice" layer="1"/>
            <TextureType val="Nineslice" layer="2"/>
            <TextureType val="Nineslice" layer="3"/>
            <Tiled val="true"/>
            <StateCount val="1"/>
            <DesaturateOnDisable val="false"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <LayerCount val="4"/>
            <Texture val="@UI/Storm_standardButtonMini_Blue_Hover" layer="0"/>
            <Texture val="@UI/Storm_standardButtonMini_Blue_Down" layer="1"/>
            <Texture val="@UI/Storm_standardButtonMini_Blue_Down" layer="2"/>
            <Texture val="@UI/Storm_standardButtonMini_Blue_Disabled" layer="3"/>
            <TextureType val="Nineslice" layer="0"/>
            <TextureType val="Nineslice" layer="1"/>
            <TextureType val="Nineslice" layer="2"/>
            <TextureType val="Nineslice" layer="3"/>
            <Tiled val="true"/>
            <StateCount val="1"/>
            <DesaturateOnDisable val="false"/>
        </Frame>

        <Frame type="Image" name="IconImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Social_Add"/>
            <DisabledAlpha val="100"/>
            <DesaturateOnDisable val="False"/>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="22"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-22"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="22"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-22"/>
        </Frame>
    </Frame>

    <Frame type="Button" name="RafButtonTemplate" template="StandardTemplates/StandardButtonMiniTemplate">
        <Height val="94"/>
        <Frame type="Label" name="Label">
            <Style val="RafButtonText"/>
        </Frame>
    </Frame>

    <!--===========================================================================================
    <! ButtonTemplates
    <!==========================================================================================-->
    <Frame type="Button" name="BreakLinkButtonTemplate">
        <Width val="40"/>
        <Height val="40"/>
        <ClickOnContextMenu val="true"/>
        <AcceptsMouse val="true"/>

        <Frame type="Image" name="IconImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="40"/>
            <Height val="40"/>
            <Texture val="@UI/Storm_Social_SocialPanel_Invite_Decline"/>
        </Frame>
        
        <Frame type="Image" name="IconImageGlow">
            <Anchor relative="$parent/IconImage"/>
            <BlendMode val="Add"/>
            <Alpha val="0"/>
            <Texture val="@UI/Storm_Social_SocialPanel_Invite_Decline"/>
            <Animation name="Hover">
                <Event event="OnMouseEnter" action="DirectionForward" frame="$parent"/>
                <Event event="OnMouseEnter" action="Play" frame="$parent"/>
                <Event event="OnMouseExit" action="DirectionReverse" frame="$parent"/>
                <Event event="OnMouseExit" action="Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time=".1" value="150" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>


    <!-- RecruitFriend TileList Background -->
    <Frame type="Image" name="RecruitTileBackground">
        <Texture val="@UI/Storm_UI_RAF_ProgressBackground"/>
        <TextureType val="Normal"/>
        <Alpha val="50"/>
    </Frame>

    <!--===========================================================================================
    <! RecruitFriendTileListItemTemplate
    <!=============================================================================================
    <! Events : Complete / Incomplete
    <!==========================================================================================-->
    <Frame type="RecruitFriendTileListItem" name="RecruitFriendTileListItemTemplate">
        <Anchor side="Right" relative="RecruitFrame" pos="Max" offset="0"/>
        <Anchor side="Bottom" relative="RecruitFrame" pos="Max" offset="0"/>
        
        <SelectedSound val="@UI_BNet_Generic_01" direction="Forward"/>
        <HideWhenCleared val="True"/>
        <AcceptsMouse val="true"/>
        <TooltipAnchorPosition val="BottomCenter"/>
        <CollapseLayout val="false"/>

        <Frame type="Frame" name="BackgroundAnchor">
            <Anchor side="Top" relative="$parent" pos="Min" offset="45"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>

        <!--=======================================================================================
        <! RECRUITFRAME - Will be visible through Show animation event
        <!======================================================================================-->
        <Frame type="Frame" name="RecruitFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
            <Anchor side="Right" relative="ProgressBar" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="ProgressBar" pos="Max" offset="10"/>

            <!-- Will be shown by Show animation event -->
            <Visible val="false"/>

            <Frame type="ProgressBar" name="ProgressBar">
                <Anchor side="Bottom" relative="$parent/AvatarFrame/AnchorFrame" pos="Max" offset="10"/>
                <Anchor side="Left" relative="$parent/AvatarFrame/AnchorFrame" pos="Max" offset="-13"/>
                <AcceptsMouse val="False"/>
                <Height val="52"/>
                <Width val="556"/>
                <Texture val="@UI/Storm_UI_RAF_IndividualProgressBar"/>
                <MinValue val="0"/>
                <MaxValue val="1"/>
                <FillInset val=".0359"/>
            </Frame>

            <Frame type="AvatarFrame" name="AvatarFrame" template="AvatarFrameTemplates/AvatarStateTemplate_Party">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <OverrideOverlayType val="HideRank"/>
            </Frame>

            <Frame type="AddFriendButton" name="FriendInviteButton" template="RecruitFriendDialog/IconButtonTemplate">
                <Anchor side="Bottom" relative="$parent/AvatarFrame" pos="Max" offset="10"/>
                <Anchor side="Left" relative="$parent/AvatarFrame" pos="Min" offset="-10"/>
            </Frame>

            <Frame type="PartyInviteButton" name="PartyInviteButton" template="RecruitFriendDialog/IconButtonTemplate">
                <Anchor side="Bottom" relative="$parent/AvatarFrame" pos="Max" offset="10"/>
                <Anchor side="Left" relative="$parent/AvatarFrame" pos="Min" offset="-10"/>
                <Frame type="Image" name="IconImage">
                    <Texture val="@UI/Storm_UI_Social_Invite"/>
                </Frame>
            </Frame>

            <Frame type="Label" name="NameLabel">
                <Anchor side="Bottom" relative="$parent/ProgressBar" pos="Min" offset="10"/>
                <Anchor side="Left" relative="$parent/ProgressBar" pos="Min" offset="30"/>
                <Options val="NoWrapping"/>
                <Style val="RafRecruitName"/>
                <Text val="LoremIpsum"/>
            </Frame>

            <Frame type="Label" name="ProgressLabel">
                <Anchor side="Bottom" relative="$parent/NameLabel" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent/ProgressBar" pos="Max" offset="-15"/>
                <Options val="NoWrapping"/>
                <Style val="RafRecruitName"/>
                <Text val="LoremIpsum"/>
                <Animation name="Complete">
                    <Event event="Complete" action="Restart,Play" frame="$parent/$parent"/>
                    <Event event="Incomplete" action="Stop" frame="$parent/$parent"/>
                    <Controller type="Style" end="Pause" style1="RafRecruitName" style2="RafRecruitComplete">
                        <Key type="Curve" time="0" value="0"/>
                        <Key type="Curve" time=".01" value="1"/>
                    </Controller>
                </Animation>
            </Frame>

        </Frame>

        <Frame type="Frame" name="EmptyRecruitFrame">
            <Anchor relative="$parent/BackgroundAnchor"/>
          
            <Frame type="Label" name="MoreRecruitLabel">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Style val="RafRecruitListInfo"/>
                <Text val="@UI/RecruitFriendDialog/MoreRecruits"/>
            </Frame>
            
            <Frame type="Image" name="InfoTooltip">
                <Anchor side="Top" relative="$parent/MoreRecruitLabel" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/MoreRecruitLabel" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/MoreRecruitLabel" pos="Max" offset="10"/>
                <AcceptsMouseTooltip val="true"/>
                <Tooltip val="@UI/RecruitFriendDialog/TooManyRecruits/InfoTooltip"/>
                <Texture val="@UI/TooltipInfoIcon"/>
                <Visible val="false"/>
            </Frame>

        </Frame>

        <!--=======================================================================================
        <! LAYOUT ANIMATIONS - Toggle recruits instruction / list of recruits
        <!======================================================================================-->
        <Animation name="LayoutAnimation">
            <Event event="Reset" action="Stop"/>
            <Event event="Show" action="Reset"/>
            <Event event="Show" action="Play"/>
            <Controller type="Visibility" end="Pause" frame="RecruitFrame">
                <Key type="Bool" time="0.0" value="true"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="EmptyRecruitFrame">
                <Key type="Bool" time="0.0" value="false"/>
            </Controller>
        </Animation>
    </Frame>

    <Constant name="RecruitFriendItemVSpacing" val="0"/>
    <Constant name="RecruitFriendItemHSpacing" val="20"/>
    
    <!--===========================================================================================
    <! RewardTileList templates
    <!=============================================================================================
    <! Events: RewardEarned / RewardUnearned
    <!==========================================================================================-->
    <Frame type="RecruitRewardTileListItem" name="RewardItemTemplate">
        <Anchor side="Bottom" relative="RewardIconImage" pos="Max" offset="10"/>
        <Anchor side="Right" relative="RewardNameLabel" pos="Max" offset="10"/>
        <CollapseLayout val="true"/>

        <Animation name="LayoutAnimation">
            <Event event="RewardUnearned" action="Restart,Pause"/>
            <Event event="RewardEarned" action="Restart,Play"/>
            <Controller type="Style" end="Pause" style1="RafRecruitName" style2="RafRecruitComplete" frame="RewardNameLabel">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time=".01" value="1"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="RewardIconImage">
                <Key type="Curve" time="0" value="165"/>
                <Key type="Curve" time=".01" value="255"/>
            </Controller>
        </Animation>
        <Animation name="DisableAnimation">
            <Event event="RewardUnearned" action="Restart,Play"/>
            <Controller type="Desaturation" end="Pause" frame="RewardIconImage">
                <Key type="Bool" time="0" value="False"/>
                <Key type="Bool" time=".1" value="True"/>
            </Controller>
        </Animation>
        
        <Frame type="Image" name="RewardIconImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
            <Height val="160"/>
            <Width val="160"/>
            <Alpha val="165"/>
            <DesaturationColor val="a78cf5"/>
            <OnDisableDesaturationColor val="a78cf5"/>
        </Frame>
        
        <Frame type="Label" name="RewardNameLabel">
            <Anchor side="Top" relative="$parent/RewardIconImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/RewardIconImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/RewardIconImage" pos="Max" offset="0"/>
            <Width val="190"/>
            <Height val="130"/>
            <Style val="RafRecruitName"/>
        </Frame>

    </Frame>

    <!-- Reward list for the "Recruited by" tooltip -->
    <Frame type="TileList" name="RewardTileListTemplate">
        <Anchor side="Bottom" relative="Item1" pos="Max" offset="10"/>
        <Anchor side="Right" relative="Item1" pos="Max" offset="10"/>

        <Frame type="RecruitRewardTileListItem" name="Item0" template="RecruitFriendDialog/RewardItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        </Frame>
      
        <Frame type="RecruitRewardTileListItem" name="Item1" template="RecruitFriendDialog/RewardItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="0"/>
        </Frame>
    </Frame>

    <Frame type="RecruitRewardTileListItem" name="VeteranRewardItemTemplate">
        <Width val="350"/>
        <Height val="100"/>
        
        <Animation name="Complete">
            <Event event="RewardEarned" action="Restart,Play"/>
            <Event event="RewardUnearned" action="Stop"/>
            <Controller type="Style" end="Pause" style1="RafRecruitName" style2="RafRecruitComplete" frame="RewardNameLabel">
                <Key type="Curve" time="0" value="0"/>
                <Key type="Curve" time=".01" value="1"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="CompletedIcon">
                <Key type="Bool" time="0.0" value="false"/>
                <Key type="Bool" time="0.01" value="true"/>
            </Controller>
            <Controller type="Fade" end="PingPong" frame="CompletedIcon">
                <Key type="Curve" time="0" value="245" out="Fast"/>
                <Key type="Curve" time=".05" value="255" in="Slow"/>
            </Controller>
        </Animation>

        <Animation name="EmptyReward">
            <Event event="Hide" action="Reset,Play"/>
            <Event event="Show" action="Stop"/>
            <Controller type="Visibility" end="Pause" frame="RewardIconImage">
                <Key type="Bool" time="0.0" value="false"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="RewardBackgroundImage">
                <Key type="Bool" time="0.0" value="false"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="RewardNameLabel">
                <Key type="Bool" time="0.0" value="false"/>
            </Controller>
            <Controller type="Visibility" end="Pause" frame="CompletedIcon">
                <Key type="Bool" time="0.0" value="false"/>
            </Controller>
        </Animation>
            
        <Frame type="Image" name="RewardBackgroundImage">
            <Anchor side="Top" relative="$parent/RewardIconImage" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/RewardIconImage" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/RewardIconImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/RewardIconImage" pos="Mid" offset="0"/>
            <Width val="60"/>
            <Height val="60"/>
            <Texture val="@UI/Storm_UI_ProfileDialog_RecentGame_Frame_Hover"/>
            <Rotation val="45"/>
        </Frame>

        <Frame type="Image" name="RewardIconImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-7"/>
            <Height val="100"/>
            <Width val="100"/>
            <Visible val="true"/>
        </Frame>

        <Frame type="Label" name="RewardNameLabel">
            <Anchor side="Top" relative="$parent/RewardIconImage" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent/RewardIconImage" pos="Max" offset="-5"/>
            <Style val="RafRecruitName"/>
            <Unclipped val="True"/>
        </Frame>
        
        <Frame type="Image" name="CompletedIcon">
            <Anchor side="Right" relative="$parent/RewardIconImage" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/RewardIconImage" pos="Max" offset="0"/>
            <Width val="0"/>
            <Height  val="0"/>
            <Visible val="false"/>
            <Texture val="@UI/Storm_UI_IconComplete_Small"/>
        </Frame>
    </Frame>

    <Frame type="TileList" name="RewardTileListVeteranRewardTemplate">
        <Anchor side="Right" relative="Item3" pos="Max" offset="10"/>
        <Height val="200"/>

        <Frame type="RecruitRewardTileListItem" name="Item0" template="RecruitFriendDialog/VeteranRewardItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="-5"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-5"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="94"/>
        </Frame>
      
        <Frame type="RecruitRewardTileListItem" name="Item1" template="RecruitFriendDialog/VeteranRewardItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="0"/>
        </Frame>

        <Frame type="RecruitRewardTileListItem" name="Item2" template="RecruitFriendDialog/VeteranRewardItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="0"/>
        </Frame>

        <Frame type="RecruitRewardTileListItem" name="Item3" template="RecruitFriendDialog/VeteranRewardItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item2" pos="Max" offset="-55"/>
            <Frame type="Label" name="RewardNameLabel">
                <Anchor side="Left" relative="$this" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent/RewardIconImage" pos="Min" offset="10"/>
            </Frame>
        </Frame>
    </Frame>

    <!--===========================================================================================
    <! RecruitTileList
    <!==========================================================================================-->
    <Frame type="TileList" name="RecruitFriendTileList">
        <AcceptsMouse val="true"/>
        
        <Animation name="Init">
            <Event event="OnShown" action="Reset"/>    
            <Event event="OnShown" action="Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time=".2" value="255" in="Slow"/>
            </Controller>
        </Animation>

        <Frame type="RecruitFriendTileListItem" name="Item0" template="RecruitFriendDialog/RecruitFriendTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
        </Frame>

        <Frame type="RecruitFriendTileListItem" name="Item1" template="RecruitFriendDialog/RecruitFriendTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="#RecruitFriendItemHSpacing"/>
        </Frame>

        <Frame type="RecruitFriendTileListItem" name="Item2" template="RecruitFriendDialog/RecruitFriendTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Max" offset="#RecruitFriendItemVSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="RecruitFriendTileListItem" name="Item3" template="RecruitFriendDialog/RecruitFriendTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item1" pos="Max" offset="#RecruitFriendItemVSpacing"/>
            <Anchor side="Left" relative="$parent/Item1" pos="Min" offset="0"/>
        </Frame>
        
        <Frame type="RecruitFriendTileListItem" name="Item4" template="RecruitFriendDialog/RecruitFriendTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item2" pos="Max" offset="#RecruitFriendItemVSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>
        
        <Frame type="RecruitFriendTileListItem" name="Item5" template="RecruitFriendDialog/RecruitFriendTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item2" pos="Max" offset="#RecruitFriendItemVSpacing"/>
            <Anchor side="Left" relative="$parent/Item1" pos="Min" offset="0"/>

            <Frame type="Frame" name="EmptyRecruitFrame">
                <Frame type="Image" name="InfoTooltip">
                    <Visible val="true"/>
                </Frame>
            </Frame>
        </Frame>

        <!-- Declar background outside of TileListItem so that it's visible all time -->
        <Frame type="Image" name="Background0" template="RecruitFriendDialog/RecruitTileBackground">
            <Anchor relative="$parent/Item0/BackgroundAnchor"/>
        </Frame>

        <Frame type="Image" name="Background1" template="RecruitFriendDialog/RecruitTileBackground">
            <Anchor relative="$parent/Item1/BackgroundAnchor"/>
        </Frame>

        <Frame type="Image" name="Background2" template="RecruitFriendDialog/RecruitTileBackground">
            <Anchor relative="$parent/Item2/BackgroundAnchor"/>
        </Frame>

        <Frame type="Image" name="Background3" template="RecruitFriendDialog/RecruitTileBackground">
            <Anchor relative="$parent/Item3/BackgroundAnchor"/>
        </Frame>

        <Frame type="Image" name="Background4" template="RecruitFriendDialog/RecruitTileBackground">
            <Anchor relative="$parent/Item4/BackgroundAnchor"/>
        </Frame>

        <Frame type="Image" name="Background5" template="RecruitFriendDialog/RecruitTileBackground">
            <Anchor relative="$parent/Item5/BackgroundAnchor"/>
        </Frame>
    </Frame>

    <Frame type="Image" name="DescriptionBackgroundTemplate">
        <Width val="635"/>
        <Height val="80"/>
        <RenderType val="LDR"/>
        <Color val="10031b"/>
        <Alpha val="84"/>
        <ImageMaskTexture val="@UI/Storm_UI_Horizontal_Mask"/>
    </Frame>
    
    <!--===========================================================================================
    <! IntroductionTemplate
    <!==========================================================================================-->
    <Frame type="Frame" name="IntroductionTemplate">
       
        <Frame type="Image" name="IntroBGImage">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="33"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="22"/>
            <Texture val="@UI/Storm_UI_RAF_IntroBG"/>
            <Alpha val="180"/>
        </Frame>

        
        <Frame type="ScrollableFrame" name="ScrollableFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Width val="618"/>
            <Height val="610"/>
            <RenderPriority val="513"/>
        
            <AcceptsMouse val="true"/>
        
            <Frame type="Frame" name="ContainerFrame">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="RafIntroBodyLabel" pos="Max" offset="40"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="-10"/>

                <Frame type="Label" name="RafIntroBodyLabel">
                    <Anchor side="Top" relative="$parent" pos="Min" offset="26"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Text val="@UI/RecruitFriendDialog/NewUserIntroduction"/>
                    <Style val="RafTextBody"/>
                    <AcceptsMouse val="true"/>
                </Frame>
            </Frame>
            
            <Frame type="Image" name="BottomTextFadeImage">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
                <Color val="100c29"/>
                <Height val="45"/>
                <Alpha val="255"/>
                <ImageMaskTexture val="@UI/storm_ui_glues_draft_pickingBanner_GlowyBits_Mask_Blue"/>
            </Frame>

            <Frame type="Image" name="TopTextFadeImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/ScrollBar" pos="Min" offset="0"/>
                <Color val="18123a"/>
                <Height val="45"/>
                <Alpha val="255"/>
                <ImageMaskTexture val="@UI/storm_ui_glues_draft_pickingBanner_GlowyBits_Mask_Blue"/>
                <TextureCoords top="1" left="0" bottom="0" right="1"/>
            </Frame>

            <Frame type="ScrollBar" name="ScrollBar" template="StandardTemplates/ScrollableBarTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-7"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="RecruitRewardContainer">
            <Anchor relative="$parent"/>
            <Alpha val="0"/>
            
            <Animation name="Show">
                <Event event="Show" action="Restart,Play"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" out="Slow"/>
                    <Key type="Curve" time=".4" value="255" in="Slow"/>
