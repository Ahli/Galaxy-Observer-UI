<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
  <DescFlags val="Locked" />
  <Frame type="RewardTileListItem" name="UnlockRewardTileListItemTemplate">
    <Height val="260" />
    <Width val="539" />
    <HideWhenCleared val="true" />
    <Preload val="false" />
    <ShowAsFanfare val="true" />
    <StateGroup name="SoundPlayer">
      <DefaultState val="Hidden" />
      <State name="Hidden">
        <When type="Property" visible="false" />
      </State>
      <State name="BrawlGoldReward">
        <When type="Property" Reward="GoldReward1000" />
        <Action type="PlaySound" sound="*@UI_BNet_Progression_Levelup_HeroGold" />
      </State>
      <State name="IsNotProgressionreward">
        <When type="Property" ProgressionRewardType="Unknown" />
      </State>
      <State name="AccountGem">
        <When type="Property" ProgressionRewardType="AccountGem" />
        <Action type="PlaySound" sound="*@UI_BNet_Progression_Levelup_Gems" />
      </State>
      <State name="HeroGold">
        <When type="Property" ProgressionRewardType="HeroGold" />
        <Action type="PlaySound" sound="*@UI_BNet_Progression_Levelup_HeroGold" />
      </State>
      <State name="AllOtherProgressionRewards">
        <When type="Property" operator="NotEqual" ProgressionRewardType="Unknown" />
        <Action type="PlaySound" sound="*@UI_BNet_Progression_Bonus01" />
      </State>
    </StateGroup>
    <Animation name="QuestCompletedSound">
      <Event event="QuestDaily" action="Reset,Play" />
      <Event event="QuestSpecialEvent" action="Reset,Play" />
      <Controller type="Sound" end="Stop">
        <Key type="Sound" time="0.0" sound="*@UI_BNet_Progression_Quest_Complete" />
      </Controller>
    </Animation>
    <Animation name="SpecialEvent">
      <Event event="QuestSpecialEvent" action="Play" />
      <Event event="EternalConflict" action="Play" />
      <Event event="NotQuest" action="Stop" />
      <Event event="QuestDaily" action="Stop" />
      <Event event="ProgressionPlayerRank" action="Stop" />
      <Event event="ProgressionHeroRank" action="Stop" />
    </Animation>
    <Animation name="QuestSpecialEvent">
      <Event event="Reset" action="Stop" />
      <Event event="QuestSpecialEvent" action="Restart,Play" />
