        <Key type="Image" time=".01" image="@UI/Storm_UI_Scorescreen_QuestComplete_Background_Special" />
      </Controller>
      <Controller type="Texture" end="Pause" frame="QuestIconImage">
        <Key type="Image" time="0" image="@UI/Storm_UI_Scorescreen_QuestComplete_SpecialIcon" />
      </Controller>
      <Controller type="Style" end="Pause" style1="ScoreRewardPanelItemLabel" style2="ScoreRewardPanelItemSpecialLabel" frame="RewardNameLabel">
        <Key type="Curve" time=".01" value="1" />
      </Controller>
      <Controller type="Style" end="Pause" style1="ScoreRewardSourceLabel" style2="ScoreRewardSourceSpecialLabel" frame="SourceLabel">
        <Key type="Curve" time=".01" value="1" />
      </Controller>
    </Animation>
