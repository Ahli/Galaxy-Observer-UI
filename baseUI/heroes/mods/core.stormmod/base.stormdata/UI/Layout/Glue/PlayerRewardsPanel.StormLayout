                <Key type="Curve" time="0" value="200" inout="Linear"/>
                <Key type="Curve" time=".2" value="0" inout="Slow"/>
                <Key type="Curve" time=".25" value="8"  inout="Linear"/>
                <Key type="Curve" time=".3" value="0" inout="Linear,Auto"/>
                <Key type="Curve" time=".8" value="0" inout="Slow"/>
                <Key type="Curve" time="1" value="-100" inout="Linear"/>
            </Controller>
        </Animation>
