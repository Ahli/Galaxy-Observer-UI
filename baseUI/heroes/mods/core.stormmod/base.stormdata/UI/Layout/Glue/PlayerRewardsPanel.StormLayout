    <! MAIN LAYOUT
    <!=============================================================================================
    <! Events (Recieve): ShowDailyQuest
    <! Events (Send)   : InitialDelay
    <!==========================================================================================-->
    <Frame type="PlayerRewardsPanel" name="PlayerRewardsPanelTemplate">
        <AscensionLevelProgressDurationMS val="300"/>
        <Visible val="true"/>
        <Anchor relative="$parent"/>

        <!--=======================================================================================
        <! GLOBAL ANIMATIONS
        <!======================================================================================-->
        <Animation name="InitialDelay">
            <Event event="InitialDelay" action="Reset, Play"/>

            <!-- Add completed daily quests to RewardNotificationFrame -->
            <Controller type="Event" end="Stop">
                <Key type="Event" time="0.55" event="ShowDailyQuest"/>
                <Key type="Event" time="0.70" event="ShowDailyQuest"/>
                <Key type="Event" time="0.85" event="ShowDailyQuest"/>
                <Key type="Event" time="1.00" event="ShowDailyQuest"/>
                <Key type="Event" time="1.15" event="ShowDailyQuest"/>
                <Key type="Event" time="1.30" event="ShowDailyQuest"/>
                <Key type="Event" time="2.00" event="Null"/>
            </Controller>
        </Animation>

        <Animation name="ScoreScreenIntro">
            <Event event="ShowIntro" action="Reset, Play"/>

            <Controller type="Fade" end="Stop">
                <Key type="Curve" time="0.0" value="255" inout="Linear,Slow"/>
                <Key type="Curve" time="2.0" value="255" inout="Slow,Linear"/>
            </Controller>

            <!-- Player -->
            <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="PlayerScoreFrame">
                <Key type="Curve" time="0" value="30" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="PlayerScoreFrame">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="NextUnlockTileList">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".3" value="0" inout="Linear"/>
                <Key type="Curve" time=".6" value="255" inout="Slow,Linear"/>
            </Controller>

            <!-- Ranked -->
            <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="RankFrameAnchor">
                <Key type="Curve" time="0" value="50" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="RankScoreFrame">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".9" value="255" inout="Slow,Linear"/>
            </Controller>

            <!-- Hero -->
            <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="HeroScoreFrame">
                <Key type="Curve" time="0" value="30" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="NextHeroUnlockTileList">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".3" value="0" inout="Linear"/>
                <Key type="Curve" time=".6" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="HeroScoreFrame">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
            </Controller>

            <!-- Hero Rounds -->
            <Controller type="Event" end="Pause" frame="RoundScoreFrame0">
                <Key type="Event" time="0.1" event="Intro"/>
            </Controller>
            <Controller type="Event" end="Pause" frame="RoundScoreFrame1">
                <Key type="Event" time="0.2" event="Intro"/>
            </Controller>
            <Controller type="Event" end="Pause" frame="RoundScoreFrame2">
                <Key type="Event" time="0.3" event="Intro"/>
            </Controller>

            <!-- Counting Labels -->
            <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="XPCountingFrame">
                <Key type="Curve" time="0" value="-30" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="XPCountingFrame">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="RankCountingFrame">
                <Key type="Curve" time="0" value="-30" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="RankCountingFrame">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="GoldCountingFrame">
                <Key type="Curve" time="0" value="-30" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="GoldCountingFrame">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
            </Controller>

            <!-- Brawl -->
            <Controller name="RewardSlide" type="Anchor" end="Pause" side="Bottom" relative="true" frame="BrawlRewardScoreFrame">
                <Key type="Curve" time="0" value="30" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller name="RewardFade" type="Fade" end="Pause" frame="BrawlRewardScoreFrame">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
            </Controller>
        </Animation>

        <StateGroup name="RewardType">
            <DefaultState val="Regular"/>

            <State name="RankedGame">
                <When type="AnimationState" RewardType="RankedGame"/>

                <Action type="SetProperty" frame="RankScoreFrame" visible="true"/>
                <Action type="SetProperty" frame="RankCountingFrame" visible="true"/>
                <Action type="SetProperty" frame="RankRewardTileList" visible="true"/>
                <Action type="SetProperty" frame="RankEarnedToolTipHitTestFrame" visible="true"/>

                <Action type="SendEvent" frame="PlayerScoreFrame" event="SmallSize"/>
                <Action type="SendEvent" frame="HeroScoreFrame" event="SmallSize"/>
                <Action type="SetProperty" frame="XPScoreFrameAnchor" width="650"/>

                <Action type="SendEvent" frame="$this" event="LayoutThreeScores"/>
            </State>

            <State name="Brawl">
                <When type="AnimationState" RewardType="Brawl"/>
                
                <Action type="SetProperty" frame="BrawlRewardScoreFrame" visible="True"/>

                <Action type="SendEvent" frame="PlayerScoreFrame" event="SmallSize"/>
                <Action type="SendEvent" frame="HeroScoreFrame" event="SmallSize"/>
                <Action type="SetProperty" frame="XPScoreFrameAnchor" width="650"/>

                <Action type="SendEvent" frame="$this" event="LayoutThreeScores"/>
            </State>

            <State name="Regular">
                <Action type="SendEvent" frame="PlayerScoreFrame" event="NormalSize"/>
                <Action type="SendEvent" frame="HeroScoreFrame" event="NormalSize"/>
                <Action type="SetProperty" frame="XPScoreFrameAnchor" width="405"/>

                <Action type="SendEvent" frame="$this" event="LayoutTwoScores"/>
            </State>
        </StateGroup>

        <!--=======================================================================================
        <! TOP ROW - PROGRESS BARS
        <!======================================================================================-->

        <!-- SCORE FRAME ANCHOR -->
        <Frame type="Frame" name="ScoreFramesAnchor">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="50"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="635"/>
        </Frame>

        <!--=======================================================================================
        <! PLAYER SCORE FRAME
        <!======================================================================================-->
        <Frame type="ScoreScreenPointReward" name="PlayerScoreFrame" template="PlayerRewardsPanel/ScoreFrameTemplate">
            <Anchor side="Bottom" relative="$parent/ScoreFramesAnchor" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/XPScoreFrameAnchor" pos="Min" offset="232"/>

            <!-- Updating size for Draft/Brawl -->
            <Animation name="ScoreLayout">
                <Controller type="State" stateGroup="PlayerScoreFrameSize" end="Pause">
                    <Key type="Identifier" time="0" value="NormalSize"/>
                    <Key type="Identifier" time="0" value="SmallSize"/>
                </Controller>
            </Animation>
            <StateGroup name="PlayerScoreFrameSize">
                <DefaultState val="NormalSize"/>
                <State name="NormalSize">
                    <Action type="SendEvent" frame="AscensionFrame" event="RefreshBaseValue"/>
                </State>
                <State name="SmallSize">
                    <Action type="SetProperty" frame="AscensionFrame/FontStyle" FormatText="AscensionBadge_ScoreRanked_T%0%" undo="true"/>
                    <Action type="SetProperty" frame="AscensionFrame" Width="224" undo="true"/>
                    <Action type="SetProperty" frame="AscensionFrame" Height="224" undo="true"/>
                    <Action type="SendEvent" frame="AscensionFrame" event="RefreshBaseValue"/>
                </State>
            </StateGroup>

            <!-- PLAYER SCORE FRAME ANIMATION -->
            <Animation name="PlayerLevelUp1">
                <Event event="PlayerLevelUp1" action="Reset,Play"/>
                <Event event="PlayerPostLevelUp1" action="Stop"/>
                <Event event="OnShown" action="Stop"/>
                <Event event="OnHidden" action="Stop"/>

                <!-- Level logo fade -->
                <Controller type="Fade" frame="AscensionFrame/BadgeContainer/LogoImage" end="Pause">
                    <Key type="Curve" time="0.0" value="255"/>
                    <Key type="Curve" time="0.25" value="0"/>
                </Controller>

                <!-- Level text fade -->
                <Controller type="Fade" frame="AscensionFrame/ProgressionBadgeText" end="Pause">
                    <Key type="Curve" time="0.0" value="255"/>
                    <Key type="Curve" time="0.25" value="0"/>
                </Controller>

                <!-- Play sound early so that pounding sound is synced with zoom in animation in PlayerPostLevelUp1 -->
                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0.40" sound="*@UI_BNet_Progression_Levelup01"/>
                </Controller>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0.45" event="Complete"/>
                </Controller>
            </Animation>

            <Animation name="PlayerPostLevelUp1">
                <Event event="PlayerPostLevelUp1" action="Reset,Play"/>

                <!-- Level label scale -->
                <Controller type="Dimension" frame="AscensionFrame/ProgressionBadgeText" dimension="Width" percentage="True" end="Pause">
                    <Key type="Curve" time="0.0" value="133" inout="Linear"/>
                    <Key type="Curve" time="0.17" value="100" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" frame="AscensionFrame/ProgressionBadgeText" dimension="Height" percentage="True" end="Pause">
                    <Key type="Curve" time="0.0" value="133" inout="Linear"/>
                    <Key type="Curve" time="0.17" value="100" inout="Slow,Linear"/>
                </Controller>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0.5" event="IntroComplete"/>
                </Controller>
            </Animation>

            <Animation name="PlayerLevelUp25">
                <Event event="PlayerLevelUp25" action="Reset,Play"/>
                <Event event="OnHidden" action="Stop"/>

                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0.001" sound="*@UI_BNet_Progression_Levelup_Rank25"/>
                </Controller>
                <Controller type="Event" end="Pause" frame="AscensionFrame">
                    <Key type="Event" time="0.0" event="BadgeTransitionFlash"/>
                </Controller>
                <Controller type="Event" end="Pause">
                    <Key type="Event" time="1.75" event="Complete"/>
                </Controller>
            </Animation>

            <Animation name="PlayerPostLevelUp25">
                <Event event="PlayerPostLevelUp25" action="Reset,Play"/>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="1.3" event="IntroComplete"/>
                </Controller>
            </Animation>

            <Animation name="PlayerLevelUp100">
                <Event event="PlayerLevelUp100" action="Reset,Play"/>
                <Event event="OnHidden" action="Stop"/>

                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0.001" sound="*@UI_BNet_Progression_Levelup_Rank100"/>
                </Controller>
                <Controller type="Event" end="Pause" frame="AscensionFrame">
                    <Key type="Event" time="0.0" event="BadgeTransitionFlash"/>
                </Controller>
                <Controller type="Event" end="Pause">
                    <Key type="Event" time="1.75" event="Complete"/>
                </Controller>
            </Animation>

            <Animation name="PlayerPostLevelUp100">
                <Event event="PlayerPostLevelUp100" action="Reset,Play"/>

                <Controller type="Fade" frame="AscensionFrame/BadgeContainer/LogoImage" end="Pause">
                    <Key type="Curve" time="0" value="255"/>
                </Controller>

                <Controller type="Event" end="Pause">
                    <Key type="Event" time="1.3" event="IntroComplete"/>
                </Controller>
            </Animation>

            <Animation name="AnimationCascade">
                <Event event="OnShown" action="DirectionForward,Play"/>
                <Event event="OnMouseUp" action="Play"/>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="Shine"/>
                </Controller>
                <Controller type="Event" end="Stop" frame="$parent/RankScoreFrame">
                    <Key type="Event" time="0.5" event="StartShine"/>
                </Controller>
                <Controller type="Event" end="Stop" frame="$parent/HeroScoreFrame">
                    <Key type="Event" time="1" event="Shine"/>
                </Controller>
                <Controller name="RewardCascade" type="Event" end="Stop" frame="$parent/BrawlRewardScoreFrame">
                    <Key type="Event" time="1" event="Shine"/>
                </Controller>
            </Animation>
            
            <Frame type="Image" name="ProgressBackground">
                <ColorAdjustMode val="Colorize"/>
                <LightAdjustColor val="{$ancestor[@type=ScoreScreenPointReward]/AscensionFrame/ProgressBarColorizationProvider/@LightAdjustColor}"/>
                <DarkAdjustColor val="{$ancestor[@type=ScoreScreenPointReward]/AscensionFrame/ProgressBarColorizationProvider/@DarkAdjustColor}"/>
            </Frame>

            <Frame type="Label" name="LevelLabel">
                <Visible val="False"/>
            </Frame>

            <Frame type="ProgressBar" name="ProgressBar">
                <Frame type="Frame" name="FillImageContainer">
                    <Frame type="Image" name="FillImage">
                        <ColorAdjustMode val="Colorize"/>
                        <LightAdjustColor val="{$ancestor[@type=ScoreScreenPointReward]/AscensionFrame/ProgressBarColorizationProvider/@LightAdjustColor}"/>
                        <DarkAdjustColor val="{$ancestor[@type=ScoreScreenPointReward]/AscensionFrame/ProgressBarColorizationProvider/@DarkAdjustColor}"/>
                    </Frame>
                </Frame>
            </Frame>

            <Frame type="ProgressBar" name="BoostProgressBar">
                <Visible val="False"/>
            </Frame>

            <Frame type="AscensionFrame" name="AscensionFrame" template="AscensionBadge/AscensionScoreBadgeTemplate">
                <Anchor side="Left" relative="$parent/ProgressBackground" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/ProgressBackground" pos="Mid" offset="0"/>
                <Anchor side="Top" relative="$parent/ProgressBackground" pos="Mid" offset="-1"/>
                <Anchor side="Bottom" relative="$parent/ProgressBackground" pos="Mid" offset="-1"/>
                <AccountLevel val="{$ancestor[@type=PlayerRewardsPanel]/@DisplayedAccountLevel}"/>
            </Frame>
        </Frame>

        <!--=======================================================================================
        <! HERO SCORE FRAME
        <!======================================================================================-->
        <Frame type="ScoreScreenPointReward" name="HeroScoreFrame" template="PlayerRewardsPanel/HeroScoreFrameTemplate">
            <Anchor side="Bottom" relative="$parent/ScoreFramesAnchor" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/XPScoreFrameAnchor" pos="Max" offset="-235"/>

            <Frame type="Image" name="ProgressBackground" template="PlayerRewardsPanel/ScoreNonRankBackgroundTemplate">
                <Texture val="@UI/Storm_UI_Scorescreen_HeroXP_Background"/>
            </Frame>
            
            <Frame type="Image" name="ProgressPattern">
                <Texture val="@UI/Storm_UI_Scorescreen_HeroXP_Pattern" layer="1"/>
            </Frame>
            
            <Frame type="ProgressBar" name="BoostProgressBar" template="StandardTemplates/ScoreProgressBarTemplate">
                <Frame type="Frame" name="FillImageContainer">
                    <Frame type="Image" name="FillImage">
                        <Texture val="@UI/Storm_UI_Scorescreen_HeroXP_BoostBar"/>
                    </Frame>
                </Frame>
            </Frame>
            
            <Frame type="ProgressBar" name="ProgressBar" template="StandardTemplates/ScoreProgressBarTemplate">
                <Frame type="Frame" name="FillImageContainer">
                    <Frame type="Image" name="FillImage">
                        <Texture val="@UI/Storm_UI_Scorescreen_HeroXP_Bar"/>
                    </Frame>
                </Frame>
            </Frame>

            <Frame type="Image" name="FillImageForAnimation" template="PlayerRewardsPanel/BrightnessTemplate">
                <Anchor relative="$parent/BoostProgressBar"/>
                <Alpha val="0"/>
                <Texture val="@UI/Storm_UI_Scorescreen_HeroXP_BoostBar"/>

                <Animation name="BrightnessAnimation">
                    <Event event="HeroLevelUpIntro" action="Restart,Play" frame="$ancestor[@type=ScoreScreenPointReward]"/>
                    <Event event="IntroComplete" action="Stop" frame="$ancestor[@type=ScoreScreenPointReward]"/>
                </Animation>
            </Frame>

            <Frame type="Label" name="LevelLabel">
                <Style val="ScoreHeroLevel"/>
                <Animation name="ShakeAnimation" template="StandardAnimationTemplates/ShakeAnimation"/>
            </Frame>
            
            <Frame type="Frame" name="LevelUpContainer" template="PlayerRewardsPanel/LevelUpContainerTemplate">
                <Frame type="Image" name="BottomShadowImage">
                    <Texture val="@UI/Storm_UI_Scorescreen_LevelUp_BottomShadow_Hero"/>
                </Frame>
                <Frame type="Image" name="ArrowsImage">
                    <Texture val="@UI/Storm_UI_Scorescreen_LevelUp_Arrows_Hero" layer="1"/>
                </Frame>
            </Frame>

            <Animation name="ScoreLayout">
                <Controller type="Visibility" end="Pause" frame="LensFlareImage">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="LensFlare2Image">
                    <Key type="Bool" time="0" value="true"/>
                    <Key type="Bool" time="0" value="false"/>
                </Controller>
            </Animation>

            <!-- HERO SCORE FRAME ANIMATION -->
            <Animation name="LevelUpTransition">
                <Event event="LevelUp" action="Reset,Play"/>

                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0.01" sound="@UI_BNet_Progression_Levelup02"/>
                </Controller>
                <Controller type="Event" end="Pause">
                    <Key type="Event" time="0.0" event="Complete"/>
                </Controller>
            </Animation>

            <!-- "HeroLevelUpIntro" Animation Name/Event is referenced from the code -->
            <Animation name="HeroLevelUpIntro">
                <Event event="HeroLevelUpIntro" action="Reset,Play"/>

                <Controller type="Event" name="CompleteEvent" end="Pause">
                    <Key type="Event" time="0.55" event="IntroComplete"/>
                </Controller>
            </Animation>

            <!-- "LevelUpMasteryUpgrade" Animation Name/Event is referenced from the code -->
            <Animation name="LevelUpMasteryUpgrade">
                <Event event="LevelUpMasteryUpgrade" action="Reset,Play"/>
                <Event event="HeroLevelUpIntroMasteryUpgrade" action="Stop"/>
                <Event event="OnShown" action="Stop"/>
                <Event event="OnHidden" action="Stop"/>

                <Controller type="Event" end="Stop" frame="LevelLabel">
                    <Key type="Event" time="0.1" event="StartShaking" />
                    <Key type="Event" time="2.2" event="StopShaking"/>
                </Controller>
                <Controller type="Event" end="Stop" frame="MasteryIconFrame/MasteryIcon">
                    <Key type="Event" time="0.2" event="StartShaking" />
                    <Key type="Event" time="2.2" event="StopShaking"/>
                </Controller>
                <Controller type="Event" name="CompleteEvent" end="Stop">
                    <Key type="Event" time="2.3" event="Complete"/>
                </Controller>

                <Controller type="Fade" frame="GlowValueProvider" end="Pause">
                    <Key type="Curve" time="0.0" value="0"/>
                    <Key type="Curve" time="2.2" value="255"/>
                </Controller>
                <Controller type="AdjustmentColor" frame="GlowValueProvider" end="Pause">
                    <Key type="Curve" time="0.0" value="00ffffff"/>
                    <Key type="Curve" time="2.3" value="ffffffff"/>
                </Controller>
                <Controller type="Sound" end="Stop">
                    <Key type="Sound" time="0.0" sound="*@UI_BNet_Progression_Levelup_Mastery"/>
                </Controller>
            </Animation>

            <!-- "HeroLevelUpIntroMasteryUpgrade" Animation Name/Event is referenced from the code -->
            <Animation name="HeroLevelUpIntroMasteryUpgrade">
                <Event event="HeroLevelUpIntroMasteryUpgrade" action="Reset,Play"/>

                <Controller type="Event" name="CompleteEvent" end="Pause">
                    <Key type="Event" time="1.0" event="IntroComplete"/>
                </Controller>
                <Controller type="Fade" frame="GlowValueProvider" end="Pause">
                    <Key type="Curve" time="0.0" value="255"/>
                    <Key type="Curve" time="1.0" value="0"/>
                </Controller>
                <Controller type="AdjustmentColor" frame="GlowValueProvider" end="Pause">
                    <Key type="Curve" time="0" value="ffffffff"/>
                    <Key type="Curve" time="1.0" value="00ffffff"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="ScoreScreenPointReward" name="RoundScoreFrame0" template="PlayerRewardsPanel/RoundScoreFrameTemplate">
            <Anchor side="Top" relative="$parent/ScoreFramesAnchor" pos="Max" offset="-22"/>
