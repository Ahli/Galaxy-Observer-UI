                    <Key type="Curve" time="0" value="255" inout="Linear,Auto"/>
                </Controller>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0" event="Start"/>
                </Controller>
            </Animation>

            <Animation name="FinishAnimation">
                <Event event="FinishAnimation" action="Reset,Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                </Controller>
            </Animation>
        </Frame>

        <!--=======================================================================================
        <! RANK REWARD ITEMS
        <!======================================================================================-->
        <Frame type="TileList" name="RankRewardTileList">
            <Anchor side="Top" relative="$parent/RankCountingFrame/EarnedLabelAnchor" pos="Max" offset="-130"/>
            <Anchor side="Left" relative="$parent/RankCountingFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/RankCountingFrame" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-100"/>
            <Width val="510"/>
            <Visible val="false"/>

            <Frame type="ScoreRewardTileListItem" name="Item0" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="0" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="ScoreRewardTileListItem" name="Item1" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time=".8" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="ScoreRewardTileListItem" name="Item2" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent/Item1" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="1.6" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>

            <Animation name="Trigger">
                <Event event="Begin" action="Reset,Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" inout="Linear,Auto"/>
                </Controller>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0.2" event="Start"/>
                </Controller>
            </Animation>

            <Animation name="FinishAnimation">
                <Event event="FinishAnimation" action="Reset,Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                </Controller>
            </Animation>
        </Frame>

        <!--=======================================================================================
        <! GOLD REWARD ITEMS
        <!======================================================================================-->
        <Frame type="TileList" name="GoldRewardTileList">
            <Anchor side="Top" relative="$parent/GoldCountingFrame/EarnedLabelAnchor" pos="Max" offset="-130"/>
            <Anchor side="Left" relative="$parent/GoldCountingFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/GoldCountingFrame" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-100"/>
            <Width val="510"/>
            <Visible val="true"/>

            <Frame type="ScoreRewardTileListItem" name="Item0" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="0" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>
          
            <Frame type="ScoreRewardTileListItem" name="Item1" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time=".8" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>
          
            <Frame type="ScoreRewardTileListItem" name="Item2" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent/Item1" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="1.6" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>
          
            <Frame type="ScoreRewardTileListItem" name="Item3" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent/Item2" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="2.4" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>
          
            <Frame type="ScoreRewardTileListItem" name="Item4" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent/Item3" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="3.2" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="ScoreRewardTileListItem" name="Item5" template="PlayerRewardsPanel/EarnedRewardTileListItemTemplate">
                <Anchor side="Top" relative="$parent" pos="Min" offset="130"/>
                <Anchor side="Left" relative="$parent/Item4" pos="Min" offset="0"/>
                <Animation name="Trigger">
                    <Event event="Start" action="Play" frame="$parent"/>
                    <Controller type="Event" end="Stop">
                        <Key type="Event" time="4.0" event="Intro"/>
                    </Controller>
                </Animation>
            </Frame>

            <Animation name="Trigger">
                <Event event="Begin" action="Reset,Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" inout="Linear,Auto"/>
                </Controller>
                <Controller type="Event" end="Stop">
                    <Key type="Event" time="0.3" event="Start"/>
                </Controller>
            </Animation>

            <Animation name="FinishAnimation">
                <Event event="FinishAnimation" action="Reset,Play" frame="$parent"/>
