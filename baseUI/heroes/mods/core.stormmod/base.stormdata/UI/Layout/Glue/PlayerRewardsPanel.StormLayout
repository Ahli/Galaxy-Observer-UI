            <Anchor side="Left" relative="$parent/RankFrameAnchor" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/RankFrameAnchor" pos="Mid" offset="0"/>
            <Visible val="false"/>
            <AcceptsMouse val="True"/>
            <Width val="650"/>
            <Handle val="PlayerRewardPanel_RankScoreFrame"/>

            <StateGroup name="GrandMasterAnchor">
                <DefaultState val="Normal"/>

                <State name="GrandMaster">
                    <When type="Property" RankType="GrandMaster"/>
                    <Action type="SendEvent" event="RaiseRankBadgeAnchor"/>
                </State>
                <State name="Normal">
                    <Action type="SendEvent" event="ResetRankBadgeAnchor"/>
                </State>
            </StateGroup>

