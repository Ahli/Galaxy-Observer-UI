<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Constant name="EarnedRewardTileSpacing" val="-5"/>
    <Constant name="EarnedRewardTooltipHorizontalLeftPadding" val="12"/>
    <Constant name="EarnedRewardTooltipHorizontalRightPadding" val="-5"/>
    <Constant name="RenderPriorityScoreScreenTooltip" val="600"/>
    <Constant name="QuestIneligibleColor" val="FF888888"/>

    <!-- Glow VFX template for hero progress bar -->
    <Frame type="Image" name="AddGlowOverride" template="StandardAnimationTemplates/AddGlowOverride">
        <Frame type="Image" name="FillImage">
            <Alpha val="{$ancestor[@type=ScoreScreenPointReward]/GlowValueProvider/@Alpha}"/>
        </Frame>
    </Frame>

    <!-- Mastery icon for hero progress bar -->
    <Frame type="Frame" name="MasteryIconFrameTemplate">
        <Anchor relative="MasteryIcon"/>
        <Visible val="False"/>
        <Unclipped val="True"/>

        <StateGroup name="IsUpgraded">
            <DefaultState val="Default"/>
            <State name="Default"/>
            <State name="Upgraded">
                <Action type="SetProperty" frame="MasteryIconRays" visible="True"/>
                <Action type="SetProperty" frame="MasteryIconGlow" visible="True"/>
                <Action type="SetProperty" frame="GlitterVfx" visible="True"/>
            </State>
        </StateGroup>

        <StateGroup name="MasteryIconSize">
            <DefaultState val="NormalSize"/>
            <State name="NormalSize">
                <Action type="SendEvent" frame="MasteryIcon" event="RefreshBaseValue"/>
                <Action type="SendEvent" frame="ProgressBarFx" event="RefreshBaseValue"/>
            </State>
            <State name="SmallSize">
                <Action type="SetProperty" frame="MasteryIcon" Height="106"/>
                <Action type="SetProperty" frame="MasteryIcon" Width="106"/>
                <Action type="SetProperty" frame="ProgressBarFx" Height="249"/>
                <Action type="SetProperty" frame="ProgressBarFx" Width="249"/>
                <Action type="SendEvent" frame="MasteryIcon" event="RefreshBaseValue"/>
                <Action type="SendEvent" frame="ProgressBarFx" event="RefreshBaseValue"/>
            </State>
        </StateGroup>

        <!-- Mastery Icon and VFX -->
        <Frame type="Image" name="ProgressBarFx">
            <Anchor relative="$ancestor[@type=ScoreScreenPointReward]/ProgressBar"/>
            <Texture val="@UI/Storm_UI_Scorescreen_PlayerXP_BoostBar"/>
            <Color val="AAAAAA"/>
            <Visible val="False"/>

            <Animation name="FadeOut">
                <Event event="OnShown" action="Restart,Play"/>
                <Event event="OnHidden" action="Stop"/>
                <Event event="RefreshBaseValue" action="RefreshBaseValue"/>

                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0.0" value="255" inout="Linear"/>
                    <Key type="Curve" time="0.35" value="0" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0.0" value="True"/>
                    <Key type="Bool" time="0.35" value="False"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" percentage="True" end="Pause" unclip="True">
                    <Key type="Curve" time="0.0" value="100" inout="Linear"/>
                    <Key type="Curve" time="0.35" value="110" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" percentage="True" end="Pause" unclip="True">
                    <Key type="Curve" time="0.0" value="100" inout="Linear"/>
                    <Key type="Curve" time="0.35" value="110" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="MasteryIconRays">
            <Anchor relative="$parent" offset="6"/>
            <Texture val="Assets\Textures\Storm_UI_Master_Taunt_Effect_Flare.dds"/>
            <BlendMode val="Add"/>
            <Unclipped val="True"/>
            <Visible val="False"/>

            <Animation name="Rotation">
                <Event event="OnShown" action="Restart,Play"/>
            
                <Controller type="Rotation" end="Loop">
                    <Key type="Curve" time="0.0" value="0"/>
                    <Key type="Curve" time="12.0" value="360"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="MasteryIconGlow">
            <Anchor relative="$parent/MasteryIcon"/>
            <Texture val="{TextureFormatter/@Text}"/>
            <Visible val="False"/>

            <Frame type="TextFormatFrame" name="TextureFormatter">
                <Anchor relative="$parent"/>
                <FormatText val="Assets\Textures\Storm_UI_Icon_Master_Taunt_%0%_Lrg_Bright.dds"/>
                <Replacement val="{$ancestor[@type=PlayerRewardsPanel]/@MasteryIndex}" index="0"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="MasteryIcon">
            <Texture val="{TextureFormatter/@Text}"/>
            <Height val="154"/>
            <Width val="154"/>

            <Frame type="TextFormatFrame" name="TextureFormatter">
                <Anchor relative="$parent"/>
                <FormatText val="Assets\Textures\Storm_UI_Icon_Master_Taunt_%0%_Lrg.dds"/>
                <Replacement val="{$ancestor[@type=PlayerRewardsPanel]/@MasteryIndex}" index="0"/>
            </Frame>
 
            <Animation name="FadeIn">
                <Event event="HeroLevelUpIntroMasteryUpgrade" action="Reset,Play" frame="$ancestor[@type=ScoreScreenPointReward]"/>
                <Event event="RefreshBaseValue" action="RefreshBaseValue"/>
                <Controller type="State" frame="$parent" stateGroup="IsUpgraded" end="Pause">
                    <Key type="Identifier" time="0" value="Upgraded"/>
                </Controller>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                    <Key type="Curve" time="0.15" value="32" inout="Slow"/>
                    <Key type="Curve" time="0.25" value="255" inout="Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" percentage="True" end="Pause" unclip="True">
                    <Key type="Curve" time="0.0" value="350" inout="Linear,Slow"/>
                    <Key type="Curve" time="0.25" value="100" inout="Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" percentage="True" end="Pause" unclip="True">
                    <Key type="Curve" time="0.0" value="350" inout="Linear,Slow"/>
                    <Key type="Curve" time="0.25" value="100" inout="Linear"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$parent/ProgressBarFx">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="0.25" value="True"/>
                </Controller>
            </Animation>

            <Animation name="ResetAnim">
                <Event event="OnShown" action="Reset,Play"/>
                <Controller type="State" frame="$parent" stateGroup="IsUpgraded" end="Pause">
                    <Key type="Identifier" time="0" value="Default"/>
                </Controller>
            </Animation>

            <Animation name="ShakeAnimation" template="StandardAnimationTemplates/ShakeAnimation"/>
        </Frame>

        <!-- Glitter VFX -->
        <Frame type="Frame" name="GlitterVfx">
            <Anchor relative="$parent/MasteryIcon"/>
            <Visible val="False"/>

            <Frame type="Image" name="Sparkle1">
                <Anchor side="Top" relative="$parent" pos="0.33" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="0.33" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="0.445" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="0.445" offset="0"/>
                <Width val="85"/>
                <ScaleHeightToTexture val="True"/>
                <Texture val="@UI/Storm_UI_Scorescreen_Lens_Flare_Gold"/>
                <BlendMode val="Add"/>
            </Frame>
            <Frame type="Image" name="Sparkle2" template="PlayerRewardsPanel/MasteryIconFrameTemplate/GlitterVfx/Sparkle1">
                <Anchor side="Top" relative="$parent" pos="0.59" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="0.59" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="0.525" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="0.525" offset="0"/>
            </Frame>
            <Frame type="Image" name="Sparkle3" template="PlayerRewardsPanel/MasteryIconFrameTemplate/GlitterVfx/Sparkle1">
                <Anchor side="Top" relative="$parent" pos="0.355" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="0.355" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="0.75" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="0.75" offset="0"/>
            </Frame>
            <Frame type="Image" name="Sparkle4" template="PlayerRewardsPanel/MasteryIconFrameTemplate/GlitterVfx/Sparkle1">
                <Anchor side="Top" relative="$parent" pos="0.42" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="0.42" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="0.29" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="0.29" offset="0"/>
            </Frame>
            <Frame type="Image" name="Sparkle5" template="PlayerRewardsPanel/MasteryIconFrameTemplate/GlitterVfx/Sparkle1">
                <Anchor side="Top" relative="$parent" pos="0.29" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="0.29" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="0.525" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="0.525" offset="0"/>
            </Frame>
            <Frame type="Image" name="Sparkle6" template="PlayerRewardsPanel/MasteryIconFrameTemplate/GlitterVfx/Sparkle1">
                <Anchor side="Top" relative="$parent" pos="0.562" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="0.562" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="0.588" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="0.588" offset="0"/>
            </Frame>

            <Animation name="Translate">
                <Event event="OnShown" action="Reset,Play"/>

                <Controller type="Anchor" frame="Sparkle1" side="Left" relative="True" end="Loop">
                    <Key type="Curve" time="0" value="-10"/>
                    <Key type="Curve" time="0.2" value="-10"/>
                    <Key type="Curve" time="1.7" value="10"/>
                    <Key type="Curve" time="8.0" value="10"/>
                </Controller>
                <Controller type="Anchor" frame="Sparkle1" side="Right" relative="True" end="Loop">
                    <Key type="Curve" time="0" value="-10"/>
                    <Key type="Curve" time="0.2" value="-10"/>
                    <Key type="Curve" time="1.7" value="10"/>
                    <Key type="Curve" time="8.0" value="10"/>
                </Controller>
                <Controller type="Anchor" frame="Sparkle1" side="Top" relative="True" end="Loop">
                    <Key type="Curve" time="0" value="7"/>
                    <Key type="Curve" time="0.2" value="7"/>
                    <Key type="Curve" time="1.7" value="-7"/>
                    <Key type="Curve" time="8.0" value="-7"/>
                </Controller>
                <Controller type="Anchor" frame="Sparkle1" side="Bottom" relative="True" end="Loop">
                    <Key type="Curve" time="0" value="7"/>
                    <Key type="Curve" time="0.2" value="7"/>
                    <Key type="Curve" time="1.7" value="-7"/>
                    <Key type="Curve" time="8.0" value="-7"/>
                </Controller>

                <Controller type="Anchor" frame="Sparkle6" side="Left" relative="True" end="Loop">
                    <Key type="Curve" time="0" value="-10"/>
                    <Key type="Curve" time="5.3" value="-10"/>
                    <Key type="Curve" time="6.8" value="10"/>
                    <Key type="Curve" time="8.0" value="10"/>
                </Controller>
                <Controller type="Anchor" frame="Sparkle6" side="Right" relative="True" end="Loop">
                    <Key type="Curve" time="0" value="-10"/>
                    <Key type="Curve" time="5.3" value="-10"/>
                    <Key type="Curve" time="6.8" value="10"/>
                    <Key type="Curve" time="8.0" value="10"/>
                </Controller>
                <Controller type="Anchor" frame="Sparkle6" side="Top" relative="True" end="Loop">
                    <Key type="Curve" time="0" value="6"/>
                    <Key type="Curve" time="5.3" value="6"/>
                    <Key type="Curve" time="6.8" value="-6"/>
                    <Key type="Curve" time="8.0" value="-6"/>
                </Controller>
                <Controller type="Anchor" frame="Sparkle6" side="Bottom" relative="True" end="Loop">
                    <Key type="Curve" time="0" value="6"/>
                    <Key type="Curve" time="5.3" value="6"/>
                    <Key type="Curve" time="6.8" value="-6"/>
                    <Key type="Curve" time="8.0" value="-6"/>
                </Controller>
            </Animation>

            <Animation name="Blink">
                <Event event="OnShown" action="Reset,Play"/>
                <Controller type="Fade" frame="Sparkle1" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="0.2" value="255"/>
                    <Key type="Curve" time="1.1" value="255"/>
                    <Key type="Curve" time="1.7" value="0"/>
                    <Key type="Curve" time="8.0" value="0"/>
                </Controller>
                <Controller type="Fade" frame="Sparkle2" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="1.7" value="0"/>
                    <Key type="Curve" time="2.1" value="255"/>
                    <Key type="Curve" time="2.5" value="0"/>
                    <Key type="Curve" time="8.0" value="0"/>
                </Controller>
                <Controller type="Fade" frame="Sparkle3" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="2.3" value="0"/>
                    <Key type="Curve" time="2.7" value="255"/>
                    <Key type="Curve" time="3.1" value="0"/>
                    <Key type="Curve" time="8.0" value="0"/>
                </Controller>
                <Controller type="Fade" frame="Sparkle4" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="2.9" value="0"/>
                    <Key type="Curve" time="3.3" value="255"/>
                    <Key type="Curve" time="3.7" value="0"/>
                    <Key type="Curve" time="8.0" value="0"/>
                </Controller>
                <Controller type="Fade" frame="Sparkle5" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="4.4" value="0"/>
                    <Key type="Curve" time="4.8" value="255"/>
                    <Key type="Curve" time="5.2" value="0"/>
                    <Key type="Curve" time="8.0" value="0"/>
                </Controller>
                <Controller type="Fade" frame="Sparkle6" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="5.1" value="0"/>
                    <Key type="Curve" time="5.3" value="255"/>
                    <Key type="Curve" time="6.2" value="255"/>
                    <Key type="Curve" time="6.8" value="0"/>
                    <Key type="Curve" time="8.0" value="0"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <Frame type="ScoreScreenPointReward" name="RoundScoreFrameTemplate">
        <Width val="130"/>
        <Height val="130"/>
        <AcceptsMouseTooltip val="true"/>
        <TooltipAnchorPosition val="BottomCenter"/>

        <!-- Intro Animation sets this -->
        <Alpha val="0"/>
        
        <HookupAlias original="LevelLabel" alias="HeroContainerFrame/HeroLevelLabel"/>
        <HookupAlias original="BoostProgressBar" alias="HeroContainerFrame/HeroExperienceBar"/>

        <StateGroup name="ShowLevelUpAnimation">
            <State name="ShowLevelUpAnimation">
                <When type="AnimationState" LevelUpAnimation="NotSuppressed"/>
                <Action type="SetProperty" frame="LevelUpArrow" visible="true"/>
                <Action type="SetProperty" frame="Flare" visible="true"/>
            </State>
        </StateGroup>

        <Frame type="Image" name="LevelUpArrow">
            <Anchor side="Bottom" relative="$parent/HeroContainerFrame" pos="Max" offset="-5"/>
            <Anchor side="Left" relative="$parent/HeroContainerFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/HeroContainerFrame" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Scorescreen_Brawl_Level_Up_Arrow"/>
            <Unclipped val="true"/>
            <Alpha val="0"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="Flare">
            <Anchor side="Top" relative="$parent/LevelUpArrow" pos="Mid" offset="-12"/>
            <Anchor side="Bottom" relative="$parent/LevelUpArrow" pos="Mid" offset="-12"/>
            <Anchor side="Left" relative="$parent/LevelUpArrow" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/LevelUpArrow" pos="Mid" offset="0"/>
            <Texture val="@UI/Storm_UI_Centered_Lens_Flare"/>
            <RenderPriority val="513"/>
            <BlendMode val="Add"/>
            <Unclipped val="True"/>
            <Alpha val="0"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="SelectedBackgroundImage">
            <Anchor side="Top" relative="$parent/HeroContainerFrame" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/HeroContainerFrame" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/HeroContainerFrame" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/HeroContainerFrame" pos="Mid" offset="0"/>

            <Texture val="@UI/Storm_UI_Play_HeroPortrait_Selected"/>
            <Width val="130"/>
            <Height val="130"/>
            <BlendMode val="Add"/>
            <SubpixelRendering val="True"/>
            <Unclipped val="true"/>

            <Animation name="OnShown" template="HeroSelectTileList/SelectedBackgroundImageSecondaryOnShownTemplate"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="SelectedBackgroundImage2" template="PlayerRewardsPanel/RoundScoreFrameTemplate/SelectedBackgroundImage">
            <TextureCoords top="1" left="1" bottom="0" right="0"/>
            <Unclipped val="true"/>
        </Frame>

        <Frame type="HeroContainerFrame" name="HeroContainerFrame" template="HeroStandardTemplate/HeroItemBaseTemplate">
            <Hero val="{$ancestor[@type=PlayerRewardsPanel]/@Hero[0]}"/>

            <!-- Width, Height: 130 - 32*2 = 66 -->
            <Anchor relative="$parent" offset="-32"/>

            <Frame type="HeroLabel" name="HeroLevelLabel">
                <ScaleText val="True"/>
            </Frame>

            <Animation name="HeroLabelScale">
                <Event event="OnShown" action="Play"/>
                <Controller type="Dimension" frame="HeroLevelLabel" dimension="Height" relative="False" percentage="True" end="Pause" >
                    <Key type="Curve" time="0" value="80" inout="Linear,Smooth"/>
                </Controller>
                <Controller type="Dimension" frame="HeroLevelLabel" dimension="Width" relative="False" percentage="True" end="Pause" >
                    <Key type="Curve" time="0" value="80" inout="Linear,Smooth"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="SelectedImage">
            <Anchor relative="$parent/HeroContainerFrame" />
            <SubpixelRendering val="True"/>
            <Texture val="@UI/Storm_UI_Play_HeroPortrait_ProgressBar_Selected_Gold"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <RenderPriority val="520"/>
            <Anchor relative="$parent/HeroContainerFrame" />
            <SubpixelRendering val="True"/>
            <Texture val="@UI/Storm_UI_Play_HeroPortrait_ProgressBar_Hover_On"/>
            <Alpha val="0"/>
            
            <Animation name="Hover">
                <Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                    <Key type="Curve" time=".1" value="255" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>

        <Animation name="Intro">
            <Event event="Intro" action="Play"/>
            <Event event="OnHidden" action="Reset"/>
            <Controller type="Anchor" end="Pause" side="Top" relative="true">
                <Key type="Curve" time="0" value="30" inout="Linear"/>
                <Key type="Curve" time=".5" value="0" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Linear,Slow"/>
                <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
            </Controller>
        </Animation>

        <!-- HERO SCORE FRAME ANIMATION -->
        <Animation name="LevelUpTransition">
            <Event event="LevelUp" action="Reset,Play"/>
            <Event event="ShowIntro" action="Stop" frame="$ancestor[@type=PlayerRewardsPanel]"/>

            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0.01" sound="@UI_BNet_Progression_Levelup02"/>
            </Controller>
            <Controller type="Event" end="Pause">
                <Key type="Event" time="0.0" event="Complete"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="LevelUpArrow">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".5" value="255" inout="Slow,Linear"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Bottom" relative="true" frame="LevelUpArrow">
                <Key type="Curve" time="0" value="0" inout="Linear"/>
                <Key type="Curve" time=".5" value="-30" inout="Slow"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="Flare">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.5" value="192" inout="Linear,Smooth"/>
                <Key type="Curve" time="1.0" value="0" inout="Linear"/>
            </Controller>
            <Controller type="Dimension" frame="Flare" dimension="Width" relative="False" percentage="True" end="Pause" >
                <Key type="Curve" time="0" value="200" inout="Linear,Smooth"/>
                <Key type="Curve" time="1.0" value="0"/>
            </Controller>
            <Controller type="Dimension" frame="Flare" dimension="Height" relative="False" percentage="True" end="Pause" >
                <Key type="Curve" time="0" value="200" inout="Linear,Smooth"/>
                <Key type="Curve" time="1.0" value="0"/>
            </Controller>
        </Animation>

        <!-- "LevelUpIntro" Animation Name is referenced from the code -->
        <Animation name="LevelUpIntro">
            <Event event="HeroLevelUpIntro" action="Reset,Play"/>

            <Controller type="Event" end="Pause">
                <Key type="Event" time="0.55" event="IntroComplete"/>
            </Controller>
        </Animation>

        <!-- For small round frames, do not show mastery animation -->
        <Animation name="HeroLevelUpIntroMasteryUpgrade">
            <Event event="HeroLevelUpIntroMasteryUpgrade" action="Reset,Play"/>

            <Controller type="Event" end="Pause">
                <Key type="Event" time="0.55" event="IntroComplete"/>
            </Controller>
        </Animation>

        <!-- For small round frames, do not show mastery animation -->
        <Animation name="LevelUpMasteryUpgrade">
            <Event event="LevelUpMasteryUpgrade" action="Reset,Play"/>
            <Event event="HeroLevelUpIntroMasteryUpgrade" action="Stop"/>
            <Event event="OnShown" action="Stop"/>
            <Event event="OnHidden" action="Stop"/>

            <Controller type="Event" end="Pause">
                <Key type="Event" time="0.0" event="Complete"/>
            </Controller>
        </Animation>

        <Frame type="ProgressBar" name="ProgressBar">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Label" name="NameLabel">
            <Anchor relative="$parent"/>
            <Visible val="false"/>
        </Frame>

        <Animation name="Click">
            <Event event="Selected" action="Restart,Play"/>
            <Controller type="Sound" end="Pause">
                <Key type="Sound" time="0.0" sound="@UI_BNet_HeroChoose"/>
            </Controller>
        </Animation>

        <!--============================================================================
        <! STATEGROUP
        <!===========================================================================-->
        <StateGroup name="Selected">
            <State name="Selected">
                <When type="AnimationState" Selected="Selected"/>
                <Action type="SetProperty" frame="SelectedImage" visible="true"/>
                <Action type="SetProperty" frame="SelectedBackgroundImage" visible="true"/>
                <Action type="SetProperty" frame="SelectedBackgroundImage2" visible="true"/>
            </State>
        </StateGroup>

        <StateGroup name="HeroStates">
            <DefaultState val="Normal"/>

            <State name="Max">
                <When type="AnimationState" frame="$this" Theme="ThemeMaxLevel"/>
                <Action type="SetProperty" frame="HoverImage" texture="@UI/Storm_UI_Play_HeroPortrait_ProgressBar_Hover_Gold"/>
                <Action type="ApplyTemplate" frame="HeroContainerFrame" template="HeroStandardTemplate/HeroItemAllowedMaxTemplate"/>
                <Action type="SetProperty" frame="SelectedImage" texture="@UI/Storm_UI_Play_HeroPortrait_ProgressBar_Selected_Gold"/>
                <Action type="SetProperty" frame="SelectedBackgroundImage" texture="@UI/Storm_UI_Play_HeroPortrait_Selected_Gold"/>
                <Action type="SetProperty" frame="SelectedBackgroundImage2" texture="@UI/Storm_UI_Play_HeroPortrait_Selected_Gold"/>
                <Action type="SetProperty" frame="LevelUpArrow" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="LevelUpArrow" AdjustmentColor="fed600"/>
            </State>

            <State name="Gold">
                <When type="AnimationState" frame="$this" Theme="ThemeGold"/>
                <Action type="SetProperty" frame="HoverImage" texture="@UI/Storm_UI_Play_HeroPortrait_ProgressBar_Hover_Gold"/>
                <Action type="ApplyTemplate" frame="HeroContainerFrame" template="HeroStandardTemplate/HeroItemAllowedGoldTemplate"/>
                <Action type="SetProperty" frame="SelectedImage" texture="@UI/Storm_UI_Play_HeroPortrait_ProgressBar_Selected_Gold"/>
                <Action type="SetProperty" frame="SelectedBackgroundImage" texture="@UI/Storm_UI_Play_HeroPortrait_Selected_Gold"/>
