                </Controller>
            </Animation>

            <Animation name="HideCutscene">
                <Event event="FinishAnimation" action="Reset,Play" frame="$ancestor[@type=PlayerRewardsPanel]"/>
                <Event event="OnHidden" action="Reset,Play" frame="$ancestor[@type=PlayerRewardsPanel]"/>

                <Controller type="Visibility" end="Pause" frame="AscensionTransitionCutscene">
                    <Key type="Bool" time="0" value="False"/>
                </Controller>
            </Animation>

            <Frame type="CutsceneFrame" name="AscensionTransitionCutscene">
                <Anchor side="Left" relative="$parent" pos="Mid" offset="25"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="25"/>
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Width val="665"/>
                <Height val="620"/>

                <RenderPriority val="2000"/>
                <File val="Cutscenes/UI/UI_ScoreScreen_Lightning.StormCutscene"/>
                <Unclipped val="True"/>
                <Visible val="False"/>

                <Animation name="Ranked">
                    <Event event="Ranked" action="Reset,Play"/>
                    <Controller type="Cutscene" end="Stop">
                        <Key type="Cutscene" time="0.00" action="ClearFilters"/>
                        <Key type="Cutscene" time="0.00" action="AddFilter" filter="Ranked"/>
                    </Controller>
                </Animation>

                <Animation name="Unranked">
                    <Event event="Unranked" action="Reset,Play"/>
                    <Controller type="Cutscene" end="Stop">
                        <Key type="Cutscene" time="0.00" action="ClearFilters"/>
                        <Key type="Cutscene" time="0.00" action="AddFilter" filter="Unranked"/>
                    </Controller>
                </Animation>

                <Animation name="RankedGold">
                    <Event event="RankedGold" action="Reset,Play"/>
                    <Controller type="Cutscene" end="Stop">
                        <Key type="Cutscene" time="0" action="ClearFilters"/>
                        <Key type="Cutscene" time="0" action="AddFilter" filter="Ranked"/>
                        <Key type="Cutscene" time="0" action="AddFilter" filter="Gold"/>
                    </Controller>
                </Animation>

                <Animation name="UnrankedGold">
                    <Event event="UnrankedGold" action="Reset,Play"/>
                    <Controller type="Cutscene" end="Stop">
                        <Key type="Cutscene" time="0" action="ClearFilters"/>
                        <Key type="Cutscene" time="0" action="AddFilter" filter="Unranked"/>
                        <Key type="Cutscene" time="0" action="AddFilter" filter="Gold"/>
                    </Controller>
                </Animation>

                <StateGroup name="CutsceneFilter">
                    <DefaultState val="Ranked"/>
                    <!-- Wait until Cutscene is loaded, so that filter can be applied after creation -->
                    <State name="Invisible">
                        <When type="Property" inheritedVisible="false"/>
                    </State>
                    <State name="Unloaded">
                        <When type="AnimationState" CutsceneLoad="Unloaded"/>
                    </State>
                    <State name="Loading">
                        <When type="AnimationState" CutsceneLoad="Loading"/>
                    </State>
                    <State name="UnrankedGold">
                        <When type="StateGroup" frame="$ancestor[@type=PlayerRewardsPanel]" RewardType="Regular"/>
                        <When type="StateGroup" frame="$ancestor[@type=PlayerRewardsPanel]/HeroScoreFrame" HeroMastery="Gold"/>
                        <Action type="SendEvent" frame="$this" event="UnrankedGold"/>
                    </State>
                    <State name="Unranked">
                        <When type="StateGroup" frame="$ancestor[@type=PlayerRewardsPanel]" RewardType="Regular"/>
                        <Action type="SendEvent" frame="$this" event="Unranked"/>
                    </State>
                    <State name="RankedGold">
                        <When type="StateGroup" frame="$ancestor[@type=PlayerRewardsPanel]/HeroScoreFrame" HeroMastery="Gold"/>
                        <Action type="SendEvent" frame="$this" event="RankedGold"/>
                    </State>
                    <State name="Ranked">
                        <Action type="SendEvent" frame="$this" event="Ranked"/>
                    </State>
                </StateGroup>
            </Frame>
        </Frame>

