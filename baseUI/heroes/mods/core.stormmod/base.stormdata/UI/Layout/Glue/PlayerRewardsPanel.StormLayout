                    <Key type="Curve" time="0" value="0" inout="Linear,Auto"/>
                </Controller>
            </Animation>
        </Frame>

        <!--=======================================================================================
        <! TOOLTIP FOR EARNED FRAME
        <!======================================================================================-->

        <Frame type="Frame" name="XPEarnedToolTipHitTestFrame" template="PlayerRewardsPanel/EarnedToolTipHitTestFrameTemplate">
            <Anchor side="Top" relative="$parent/XPCountingFrame/EarnedLabelAnchor" pos="Mid" offset="-100"/>
            <Anchor side="Bottom" relative="$parent/XPCountingFrame/EarnedLabelAnchor" pos="Mid" offset="100"/>
            <Anchor side="Left" relative="$parent/XPCountingFrame/EarnedLabelAnchor" pos="Mid" offset="-180"/>
            <Anchor side="Right" relative="$parent/XPCountingFrame/EarnedLabelAnchor" pos="Mid" offset="180"/>

            <Frame type="TileList" name="EarnedToolTipFrame">
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Frame type="Label" name="ToolTipHeader">
                    <Text val="@UI/PlayerRewardsPanel/XPEarnedToolTipHeader"/>
                </Frame>
            </Frame>
        </Frame>

        <Frame type="Frame" name="GoldEarnedToolTipHitTestFrame" template="PlayerRewardsPanel/EarnedToolTipHitTestFrameTemplate">
            <Anchor side="Top" relative="$parent/GoldCountingFrame/EarnedLabelAnchor" pos="Mid" offset="-100"/>
            <Anchor side="Bottom" relative="$parent/GoldCountingFrame/EarnedLabelAnchor" pos="Mid" offset="100"/>
            <Anchor side="Left" relative="$parent/GoldCountingFrame/EarnedLabelAnchor" pos="Mid" offset="-200"/>
            <Anchor side="Right" relative="$parent/GoldCountingFrame/EarnedLabelAnchor" pos="Mid" offset="150"/>

            <Frame type="TileList" name="EarnedToolTipFrame">
                <Anchor side="Left" relative="$parent" pos="Mid" offset="30"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="30"/>
                <Frame type="Label" name="ToolTipHeader">

                    <Text val="@UI/PlayerRewardsPanel/GoldEarnedToolTipHeader"/>
                </Frame>
            </Frame>
        </Frame>

        <Frame type="Frame" name="RankEarnedToolTipHitTestFrame" template="PlayerRewardsPanel/EarnedToolTipHitTestFrameTemplate">
            <Anchor side="Top" relative="$parent/RankCountingFrame/EarnedLabelAnchor" pos="Mid" offset="-100"/>
            <Anchor side="Bottom" relative="$parent/RankCountingFrame/EarnedLabelAnchor" pos="Mid" offset="100"/>
            <Anchor side="Left" relative="$parent/GoldEarnedToolTipHitTestFrame" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/XPEarnedToolTipHitTestFrame" pos="Min" offset="0"/>
            <Visible val="false"/>

            <Frame type="TileList" name="EarnedToolTipFrame">
                <Anchor side="Left" relative="$parent" pos="Mid" offset="20"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="20"/>
                <Frame type="Label" name="ToolTipHeader">
                    <Text val="@UI/PlayerRewardsPanel/RankEarnedToolTipHeader"/>
                </Frame>
            </Frame>
        </Frame>

        <!--=======================================================================================
        <! Sound hookup for ranked game
        <!======================================================================================-->
        <Animation name="StopLoopingSounds">
            <Event event="OnHidden" action="Restart,Play"/>
            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0.0" sound="*@UI_BNet_ProgressionExit_StopActor"/>
            </Controller>
        </Animation>

        <Animation name="RankedGameWon">
            <Event event="RankedGameWon" action="Restart,Play"/>
            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0.0" sound="@UI_Bnet_Ranked_Game_Won"/>
            </Controller>
        </Animation>

        <Animation name="RankedGameLoss">
            <Event event="RankedGameLoss" action="Restart,Play"/>
            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0.0" sound="@UI_Bnet_Ranked_Game_Loss"/>
            </Controller>
        </Animation>

        <Animation name="PlayPromotionWarning">
            <Event event="PlayPromotionWarning" action="Restart,Play"/>
            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0.0" sound="*@UI_BNet_Progression_Promotion"/>
            </Controller>
        </Animation>

        <Animation name="PlayDemotionWarning">
            <Event event="PlayDemotionWarning" action="Restart,Play"/>
            <Controller type="Sound" end="Stop">
                <Key type="Sound" time="0.0" sound="*@UI_BNet_Progression_Demotion"/>
            </Controller>
        </Animation>

        <StateGroup name="StartAsPromotionState">
            <State name="StartAsPromotionState">
                <When type="Property" frame="RankScoreFrame" PromotionState="Promotion"/>
                <When type="Property" frame="RankScoreFrame" inheritedVisible="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_Progression_Levelup_RankedBadge_Ascend_Start"/>
            </State>
            <State name="StartAsDemotionState">
                <When type="Property" frame="RankScoreFrame" PromotionState="Demotion"/>
                <When type="Property" frame="RankScoreFrame" inheritedVisible="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_Progression_Levelup_RankedBadge_Decline_Start"/>
            </State>
            <State name="EnterPromotionReady">
                <When type="Property" frame="RankScoreFrame" PromotionState="PromotionNextGame"/>
                <When type="Property" frame="RankScoreFrame" inheritedVisible="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_Progression_Promotion_Loop"/>
                <Action type="PlaySound" sound="*@UI_BNet_Progression_Promotion_Loop_End" on="Exit"/>
            </State>
            <State name="EnterDemotionReady">
                <When type="Property" frame="RankScoreFrame" PromotionState="DemotionNextGame"/>
                <When type="Property" frame="RankScoreFrame" inheritedVisible="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_Progression_Demotion_Loop"/>
                <Action type="PlaySound" sound="*@UI_BNet_Progression_Demotion_Loop_End" on="Exit"/>
            </State>
        </StateGroup>

        <StateGroup name="RankCountingSound">
            <State name="StartCountingUp">
                <When type="AnimationState" frame="RankScoreFrame" CountdownDirection="Up"/>
                <When type="Property" frame="RankCountingFrame/RankEarnedLabel" Counting="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Rank"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Rank_End" on="Exit"/>
                <Action type="SendEvent" frame="RankCountingFrame/RankEarnedLabel" event="EnlargeText" on="Exit"/>
            </State>
            <State name="StartCountingDown">
                <When type="AnimationState" frame="RankScoreFrame" CountdownDirection="Down"/>
                <When type="Property" frame="RankCountingFrame/RankEarnedLabel" Counting="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Demotion"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Demotion_End" on="Exit"/>
                <Action type="SendEvent" frame="RankCountingFrame/RankEarnedLabel" event="EnlargeText" on="Exit"/>
            </State>
        </StateGroup>

        <StateGroup name="GoldCountingSound">
            <State name="StartCounting">
                <When type="Property" frame="GoldCountingFrame/GoldEarnedLabel" Counting="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Gold"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Gold_End" on="Exit"/>
                <Action type="SendEvent" frame="GoldCountingFrame/GoldEarnedLabel" event="EnlargeText" on="Exit"/>
            </State>
        </StateGroup>

        <StateGroup name="XPCountingSound">
            <State name="StartCounting">
                <When type="Property" frame="XPCountingFrame/XPEarnedLabel" Counting="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_XP"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_XP_End" on="Exit"/>
                <Action type="SendEvent" frame="XPCountingFrame/XPEarnedLabel" event="EnlargeText" on="Exit"/>
            </State>
        </StateGroup>

        <StateGroup name="HeroProgressSound">
            <State name="StartCounting">
                <When type="Property" frame="XPCountingFrame/XPEarnedLabel" Counting="true"/>
                <When type="Property" frame="HeroScoreFrame" Visible="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Hero"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Hero_End" on="Exit"/>
            </State>
        </StateGroup>

        <StateGroup name="BrawlProgressBarSound">
            <State name="StartCounting">
                <When type="Property" frame="BrawlRewardScoreFrame" IsCounting="true"/>
                <When type="Property" frame="BrawlRewardScoreFrame" Visible="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Brawl"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Brawl_End" on="Exit"/>
            </State>
        </StateGroup>

        <StateGroup name="PlayerProgressSound">
            <State name="StartCounting">
                <When type="Property" frame="PlayerScoreFrame" IsCounting="true"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Rank"/>
                <Action type="PlaySound" sound="*@UI_BNet_ProgressionCounter_Rank_End" on="Exit"/>
            </State>
        </StateGroup>
    </Frame>

</Desc>
