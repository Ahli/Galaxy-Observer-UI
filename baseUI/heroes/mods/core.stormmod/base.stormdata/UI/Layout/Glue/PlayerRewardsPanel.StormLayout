                    <Event event="HeroLeague" action="DirectionReverse,Play" frame="$parent"/>

                    <Controller type="Text" end="Pause" frame="$this">
                        <Key type="Text" time="0" text="@UI/MatchmakingPanel/HeroLeagueTitle"/>
                        <Key type="Text" time="0.01" text="@UI/MatchmakingPanel/TeamLeagueTitle"/>
