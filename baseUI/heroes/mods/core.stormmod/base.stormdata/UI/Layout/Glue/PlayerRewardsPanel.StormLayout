            <Anchor side="Right" relative="$parent/HeroScoreFrame" pos="Mid" offset="-66"/>

            <Frame type="HeroContainerFrame" name="HeroContainerFrame">
                <Hero val="{$ancestor[@type=PlayerRewardsPanel]/@Hero[0]}"/>
            </Frame>
        </Frame>

        <Frame type="ScoreScreenPointReward" name="RoundScoreFrame1" template="PlayerRewardsPanel/RoundScoreFrameTemplate">
            <Anchor side="Top" relative="$parent/ScoreFramesAnchor" pos="Max" offset="-22"/>
            <Anchor side="Left" relative="$parent/RoundScoreFrame0" pos="Max" offset="-64"/>

            <Frame type="HeroContainerFrame" name="HeroContainerFrame">
                <Hero val="{$ancestor[@type=PlayerRewardsPanel]/@Hero[1]}"/>
            </Frame>
        </Frame>

        <Frame type="ScoreScreenPointReward" name="RoundScoreFrame2" template="PlayerRewardsPanel/RoundScoreFrameTemplate">
            <Anchor side="Top" relative="$parent/ScoreFramesAnchor" pos="Max" offset="-22"/>
            <Anchor side="Left" relative="$parent/RoundScoreFrame1" pos="Max" offset="-64"/>

            <Frame type="HeroContainerFrame" name="HeroContainerFrame">
                <Hero val="{$ancestor[@type=PlayerRewardsPanel]/@Hero[2]}"/>
            </Frame>
        </Frame>

        <!--=======================================================================================
        <! BRAWL SCORE FRAME
        <!======================================================================================-->
        <Frame type="ScoreScreenPointReward" name="BrawlRewardScoreFrame" template="PlayerRewardsPanel/BrawlRewardScoreFrameTemplate">
            <Visible val="false"/>
            <Anchor side="Left" relative="$parent/RankFrameAnchor" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/RankFrameAnchor" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/ScoreFramesAnchor" pos="Max" offset="0"/>

            <Animation name="CountUp">
                <Event event="CountdownStarting" action="Reset,Play"/>

                <!-- Start player/hero countdown labels -->
                <Controller type="Event" end="Pause" frame="$ancestor[@type=PlayerRewardsPanel]">
                    <Key type="Event" time="2.0" event="ResumeScoreAnimation"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="TileList" name="BrawlRewardTileList" template="PlayerRewardsPanel/BrawlUnlockTileList">
            <!-- This should stay hidden since rewards are already shown on the scoreframe -->
            <Visible val="false"/>
            <Anchor side="Top" relative="$parent/BrawlRewardScoreFrame" pos="Max" offset="-105"/>
            <Anchor side="Left" relative="$parent/BrawlRewardScoreFrame" pos="Max" offset="-140"/>
            <Width val="300"/>
            <Height val="100"/>
        </Frame>

        <!--=======================================================================================
        <! LEAGUE SCORE FRAME
        <!======================================================================================-->

        <Frame type="Frame" name="RankFrameAnchor">
            <Anchor side="Bottom" relative="$parent/ScoreFramesAnchor" pos="Max" offset="60"/>
            <Anchor side="Left" relative="$parent/ScoreFramesAnchor" pos="Min" offset="486"/>
            <Width val="10"/>
            <Height val="10"/>
        </Frame>

        <Frame type="Frame" name="XPScoreFrameAnchor">
            <Anchor side="Bottom" relative="$parent/RankFrameAnchor" pos="Max" offset="-10"/>
            <Anchor side="Left" relative="$parent/RankFrameAnchor" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/RankFrameAnchor" pos="Mid" offset="0"/>
            <Width val="650"/>
        </Frame>

        <Frame type="Frame" name="RankBadgePopupAnchor">
            <Anchor side="Top" relative="$parent/RankScoreFrame/AnchorFrame" pos="Min" offset="0" />
            <Anchor side="Left" relative="$parent/RankScoreFrame/AnchorFrame" pos="Min" offset="-20" />
            <Anchor side="Bottom" relative="$parent/RankScoreFrame/AnchorFrame" pos="Max" offset="0" />
            <Anchor side="Right" relative="$parent/RankScoreFrame/AnchorFrame" pos="Max" offset="20" />
            <TooltipAnchorPosition val="MiddleRight"/>
            <Frame type="AnchoredPopup" name="ScoreAnchoredPopup" template="PlayerRewardsPanel/ScoreAnchoredPopupTemplate"/>
        </Frame>

