                <Event event="RaiseRankBadgeAnchor" action="DirectionForward,Play"/>
                <Event event="ResetRankBadgeAnchor" action="DirectionReverse,Play"/>
                
                <Controller type="Anchor" end="Pause" side="Bottom" relative="False">
                    <Key type="Curve" time="0" value="0" inout="Linear,Step"/>
                    <Key type="Curve" time=".01" value="-40" inout="Step,Linear"/>
                </Controller>
