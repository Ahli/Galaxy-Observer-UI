            <BlendMode val="Add"/>
            <RenderType val="HDR"/>
            <Visible val="False"/>

            <Texture val="@UI/Storm_UI_Cloud_Mask" layer="0"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>
            <AlphaLayer val="True" layer="0"/>
            <WrapUV val="True" layer="0"/>

            <Texture val="@UI/Storm_UI_Store_QualityGlow_Legendary" layer="1"/>
            <TextureType val="Nineslice" layer="1"/>
            <WrapUV val="True" layer="1"/>

            <Animation name="ShowHide">
                <Event event="NotSkin" action="Stop" frame="$parent"/>
