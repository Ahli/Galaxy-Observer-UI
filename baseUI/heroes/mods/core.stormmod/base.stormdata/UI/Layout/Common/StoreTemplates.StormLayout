            </Animation>

            <Animation name="UVScroll">
                <Event event="Trigger" action="Restart,Play"/>
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time="1" value="255" in="Slow"/>
                    <Key type="Curve" time="4" value="255" out="Fast"/>
                    <Key type="Curve" time="5" value="0" in="Slow"/>
                </Controller>
                <Controller type="LayerUV" layer="0" side="Left" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Right" relative="False" sidelock="False" end="Loop" >

                    <Key type="Curve" time="0" value="1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Top" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
                <Controller type="LayerUV" layer="0" side="Bottom" relative="False" sidelock="False" end="Loop" >
                    <Key type="Curve" time="0" value="1" out="linear"/>
                    <Key type="Curve" time="5" value="0" in="linear" />
                </Controller>
            </Animation>
        </Frame>

        <!-- Filter is set based on the tile size. BigTile / SmallTile -->
        <Frame type="HeroCutsceneFrame" name="PortraitCutscene">
            <Anchor relative="$parent/CutsceneCoverImage"/>
            <Unclipped val="False"/>
            <RenderType val="HDR"/>
            <TimeScale val="0.5"/>
            <Visible val="True"/>
            <HeroCutsceneType val="ShopTileZoomed"/>
            <Animation name="ToggleHoverSpeed">
                <Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent"/>
                <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent"/>
                <Controller type="Cutscene" end="Pause">
                    <Key type="Cutscene" time="0.0" action="TimeScale" value=".5" out="Fast"/>
                    <Key type="Cutscene" time=".25" action="TimeScale" value="1.5" in="Slow"/>
                </Controller>
            </Animation>

            <Animation name="HoverLightOn">
                <Event event="OnMouseEnter" action="Play" frame="$parent"/>
                <Controller type="Animation" end="Stop" animation="HoverLightOff">
                    <Key type="Animation" time="0" action="Stop"/>
                </Controller>
                <Controller type="Cutscene" end="Stop" name="FilterOn">
                    <Key type="Cutscene" time="0.01" action="Bookmark" bookmark="Light On"/>
                    <Key type="Cutscene" time="0.01" action="Play"/>
                    <Key type="Cutscene" time="0.01" action="AddFilter" filter="LightOn"/>
                </Controller>
            </Animation>

            <Animation name="HoverLightOff">
                <Event event="OnMouseExit" action="Play" frame="$parent"/>
                <Controller type="Animation" end="Stop" animation="HoverLightOn">
                    <Key type="Animation" time="0" action="Stop"/>
                </Controller>
                <Controller type="Cutscene" end="Stop" name="FilterOff">
                    <Key type="Cutscene" time="0" action="Bookmark" bookmark="Light Off"/>
                    <Key type="Cutscene" time="0" action="Play"/>
                    <Key type="Cutscene" time="0.5" action="RemoveFilter" filter="LightOn"/>
                </Controller>
            </Animation>
        </Frame>

