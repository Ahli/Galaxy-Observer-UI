    <Frame type="Frame" name="FlareContainer" template="RankBadgeTemplates/FlareTemplate">
      <Visible val="False" />
    </Frame>
    <Frame type="Frame" name="RankIconContainer">
      <Frame type="Image" name="RankIconImage">
        <Width val="217" />
        <Height val="195" />
      </Frame>
      <Frame type="Image" name="PlacementIconImage">
        <Width val="217" />
        <Height val="195" />
      </Frame>
    </Frame>
    <Frame type="Frame" name="BadgeContainer">
      <Frame type="Label" name="LeagueLabelBlur">
        <Frame type="TextFormatFrame" name="FontStyleFormatter">
          <Replacement val="_Small" index="0" />
        </Frame>
      </Frame>
      <Frame type="Label" name="LeagueLabel">
        <Frame type="TextFormatFrame" name="FontStyleFormatter">
          <Replacement val="_Small" index="0" />
        </Frame>
      </Frame>
      <Frame type="Label" name="RankLabelBlur">
        <Frame type="TextFormatFrame" name="FontStyleFormatter">
          <Replacement val="_Small" index="0" />
        </Frame>
      </Frame>
      <Frame type="Label" name="RankLabel">
        <Frame type="TextFormatFrame" name="FontStyleFormatter">
          <Replacement val="_Small" index="0" />
        </Frame>
      </Frame>
    </Frame>
  </Frame>
  <Frame type="RankBadge" name="RankBadgeBaseTemplateNoOverlaySound" template="RankBadgeTemplates/RankBadgeFullsizeTemplate">
    <Animation name="RankIconIntroShake" template="RankBadgeTemplates/RankIconIntroShakeTemplate" />
    <Animation name="RankIconOutroShake" template="RankBadgeTemplates/RankIconOutroShakeTemplate" />
    <Animation name="LeagueDemotionTransition">
      <Event event="LeagueDemotion" action="Restart,Play" />
      <Event event="DivisionDemotion" action="Restart,Play" />
      <Controller type="Event" end="Pause">
        <Key type="Event" time="1.5" event="Complete" />
      </Controller>
    </Animation>
    <Animation name="LeagueDemotion">
      <Event event="LeagueDemotion" action="Restart,Play" />
      <Controller type="Sound" end="Stop">
        <Key type="Sound" time="0.0" sound="@UI_BNet_Ranked_Demotion_Generic" />
      </Controller>
    </Animation>
    <Animation name="DivisionDemotion">
      <Event event="DivisionDemotion" action="Restart,Play" />
      <Controller type="Sound" end="Stop">
        <Key type="Sound" time="0.0" sound="@UI_BNet_Ranked_Demotion_Generic" />
      </Controller>
    </Animation>
    <Animation name="CountStartingUp">
      <Event event="RankCounterAscend" action="Restart,Play" />
      <Controller type="Sound" end="Stop">
        <Key type="Sound" time="0.0" sound="@UI_BNet_ProgressionCounter_Rank_Ascend_Start" />
      </Controller>
    </Animation>
    <Animation name="CountStartingDown">
      <Event event="RankCounterDecline" action="Restart,Play" />
      <Controller type="Sound" end="Stop">
        <Key type="Sound" time="0.0" sound="@UI_BNet_ProgressionCounter_Rank_Decline_Start" />
      </Controller>
    </Animation>
  </Frame>
  <Frame type="RankBadge" name="RankBadgeTemplate" template="RankBadgeTemplates/RankBadgeBaseTemplateNoOverlaySound">
    <StateGroup name="PromotionState">
      <State name="EnterPromotionReady">
        <When type="Property" PromotionState="PromotionNextGame" />
        <When type="Property" inheritedVisible="true" />
        <Action type="PlaySound" sound="*@UI_BNet_Ranked_Promotion_Warning_Loop" />
        <Action type="PlaySound" sound="*@UI_BNet_Ranked_Promotion_Warning_Loop_End" on="Exit" />
      </State>
      <State name="EnterDemotionReady">
        <When type="Property" PromotionState="DemotionNextGame" />
        <When type="Property" inheritedVisible="true" />
        <Action type="PlaySound" sound="*@UI_BNet_Ranked_Demotion_Warning_Loop" />
        <Action type="PlaySound" sound="*@UI_BNet_Ranked_Demotion_Warning_Loop_End" on="Exit" />
      </State>
    </StateGroup>
  </Frame>
</Desc>