<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <DescFlags val="Locked"/>

    <!--===============================================================================================
    <! Constants
    <!==============================================================================================-->
    <Constant name="StoreTileItemSpacing" val="12"/>
    <Constant name="StoreTileItemVerticalSpacing" val="14"/>
    <Constant name="CollectionTileListTopMinOffset" val="144"/>
    <Constant name="CollectionTileListLeftMidOffset" val="-847"/>
    <Constant name="CollectionTileListRightMidOffset" val="855"/>
    <Constant name="CollectionCategoryTileListItemSpacing" val="-6"/>

    <!-- Constants for pages with a Collection Category Tile List -->
    <Constant name="CollectionCategoryNormalLeftOffset" val="-890"/>
    <Constant name="CollectionCategory4x3LeftOffset" val="-850"/>
    <Constant name="CollectionCategoryContentNormalLeftOffset" val="12"/>
    <Constant name="CollectionCategoryContent4x3LeftOffset" val="20"/>
    <Constant name="CollectionCategoryBigListNormalLeftOffset" val="66"/>
    <Constant name="CollectionCategoryBigList4x3LeftOffset" val="20"/>

    <!--===============================================================================================
    <! Page Selector Template
    <!==============================================================================================-->
    <Frame type="PageSelectorButtonList" name="PageSelectorTeamplate" template="StandardTemplates/PageSelectorButtonListTemplate">
        <Height val="600"/>
        <DisabledAlpha val="0"/>

        <Frame type="Button" name="DecrementButton" template="StandardTemplates/MiniArrowButtonTemplate_Left">
            <Anchor side="Top" relative="$parent/PageButton0" pos="Min" offset="-15"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Visible val="{$parent/PageButton0/@Visible}"/>
            <ClickSound val="@UI_BNet_Decrement"/>
        </Frame>

        <Frame type="Button" name="IncrementButton" template="StandardTemplates/MiniArrowButtonTemplate_Right">
            <Anchor side="Top" relative="$parent/DecrementButton" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/DecrementButton" pos="Max" offset="-16"/>
            <Visible val="{$parent/PageButton0/@Visible}"/>
            <ClickSound val="@UI_BNet_Increment"/>
        </Frame>

        <Frame type="Button" name="PageButton0" template="StandardTemplates/StandardPipButtonTemplate">
            <Anchor side="Top" relative="$parent/$parent/Item7" pos="Max" offset="15"/>
            <Anchor side="Left" relative="$parent/IncrementButton" pos="Max" offset="0"/>
            <CollapseLayout val="true"/>
        </Frame>
    </Frame>

    <!--===============================================================================================
    <! Section Header Template
    <!==============================================================================================-->
    <Frame type="Frame" name="SectionHeaderTemplate">

        <Frame type="Image" name="TitleIcon">
            <Anchor side="Left" relative="$parent" pos="Min" offset="-5"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="5"/>
            <Texture val="@UI/Storm_UI_Store_TitleIcon_Heroes"/>
        </Frame>

        <Frame type="Label" name="TitleLabel">
            <Anchor side="Top" relative="$parent/TitleIcon" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/TitleIcon" pos="Max" offset="9"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-9"/>
            <Anchor side="Bottom" relative="$parent/TitleIcon" pos="Mid" offset="0"/>
            <Text val="Featured Thing"/>
            <Style val="StoreSectionLabel"/>
        </Frame>
    </Frame>

    <!--===============================================================================================
    <! Filter Checkboxes Template
    <!==============================================================================================-->
    <Frame type="Frame" name="FilterOptionsTemplate">
        <Frame type="CheckBox" name="DebugShowInvalidCheckBox" template="StandardTemplates/StandardCheckBoxLabelTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/DebugShowHiddenCheckBox" pos="Min" offset="0"/>
            <Width val="225"/>
            <Height val="36"/>
            <CollapseLayout val="true"/>
            <Visible val="{$GlueUI/@DevMode}"/>

            <Frame type="Label" name="Label">
                <Anchor side="Left" relative="$parent/Button" pos="Max" offset="0"/>
                <Text val="showInvalidProducts"/>
                <Style val="StandardPulldownLabel"/>
            </Frame>
        </Frame>

        <Frame type="CheckBox" name="DebugShowHiddenCheckBox" template="StandardTemplates/StandardCheckBoxLabelTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/OwnershipFilterPulldown" pos="Min" offset="0"/>
            <Width val="225"/>
            <Height val="36"/>
            <CollapseLayout val="true"/>
            <Visible val="{$GlueUI/@DevMode}"/>

            <Frame type="Label" name="Label">
                <Anchor side="Left" relative="$parent/Button" pos="Max" offset="0"/>
                <Text val="showHiddenProducts"/>
                <Style val="StandardPulldownLabel"/>
            </Frame>
        </Frame>

        <Frame type="GameContentOwnershipFilterPulldown" name="OwnershipFilterPulldown" template="StandardTemplates/StandardPulldownTemplate">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Width val="256"/>
         </Frame>
    </Frame>

    <!--===============================================================================================
    <! Search Edit Box Template
    <!==============================================================================================-->
    <Frame type="EditBox" name="SearchEditBoxTemplate" template="StandardTemplates/StandardSearchEditBoxTemplate">
        <PreserveAnchorOffset val="False"/>
        <Width val="270"/>

        <Frame type="Image" name="FilterSearchImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            <Texture val="@UI/Storm_UI_Icon_SearchGlass_Dark"/>
            <Animation name="FilterSearchImage">
                <Event event="OnShown" action="DirectionForward" frame="$parent/ClearTextButton"/>
                <Event event="OnShown" action="Play" frame="$parent/ClearTextButton"/>
                <Event event="OnHidden" action="DirectionReverse" frame="$parent/ClearTextButton"/>
                <Event event="OnHidden" action="Play" frame="$parent/ClearTextButton"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="255" out="Slow"/>
                    <Key type="Curve" time=".1" value="0" in="Slow"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <!--===============================================================================================
    <! Main Tile List Template
    <!==============================================================================================-->
    <Frame type="GameContentTileList" name="ContentTileListTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="#CollectionTileListTopMinOffset"/>
        <Anchor side="Left" relative="$parent/BigTileList/Item2" pos="Max" offset="#CollectionCategoryContentNormalLeftOffset"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Height val="900"/>
        <ReportsStats val="true"/>
        <AcceptsMouse val="true"/>
        <CollapseLayout val="true"/>
        <FilterFlags val="Default|NoDupes|!NoValidProduct|!CanNotPurchase"/>
        <SearchFlags val=""/>
        <GenerationMode val="Store"/>
        <FilterString val="{$parent/SearchEditBox/@Text}"/>
        <SortMethod index="0" val="{$parent/SortPulldown/@SelectedSort}"/>
        <SortMethod index="1" val="Alphabetical"/>
        <ShowAsNewEnabled val="true"/>

        <MaxItemPerPage val="8"/>
        <ScrollStepValue val="4"/>

        <!-- Attribute Name is set by individual instances, if it needs to -->
        <ProductAttribute index="0" val="" />
        <ProductAttributeValue index="0" val="1" />
        <ProductAttributeCheck index="0" val="Value" />

        <Frame type="GameContentFrame" name="GameContentData">
            <Anchor relative="$parent"/>
            <GameContent val="{$parent/@SelectedGameContent}"/>
        </Frame>

        <StateGroup name="ShowInvalid">
            <State name="ShowInvalid">
                <When type="Property" frame="$parent/FilterOptions/DebugShowInvalidCheckBox" Checked="true"/>
                <Action type="SetProperty" FilterFlag_NoValidProduct="true"/>
            </State>
        </StateGroup>

        <StateGroup name="ShowHidden">
            <State name="ShowHidden">
                <When type="Property" frame="$parent/FilterOptions/DebugShowHiddenCheckBox" Checked="true"/>
                <Action type="SetProperty" FilterFlag_ProductHidden="true"/>
            </State>
        </StateGroup>

        <StateGroup name="OwnershipFilter">
            <State name="AllContent">
                <When type="Property" frame="$parent/FilterOptions/OwnershipFilterPulldown" SelectedFilter="AllContent"/>
                <Action type="SetProperty" FilterFlag_Owned="true"/>
                <Action type="SetProperty" FilterFlag_NotOwned="true"/>
            </State>
            <State name="OwnedContent">
                <When type="Property" frame="$parent/FilterOptions/OwnershipFilterPulldown" SelectedFilter="OwnedContent"/>
                <Action type="SetProperty" FilterFlag_Owned="true"/>
                <Action type="SetProperty" FilterFlag_NotOwned="false"/>
            </State>
            <State name="UnownedContent">
                <When type="Property" frame="$parent/FilterOptions/OwnershipFilterPulldown" SelectedFilter="UnownedContent"/>
                <Action type="SetProperty" FilterFlag_Owned="false"/>
                <Action type="SetProperty" FilterFlag_NotOwned="true"/>
            </State>
        </StateGroup>

        <Frame type="ScrollBar" name="ScrollBar" template="StandardTemplates/ScrollableBarTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="-2"/>
            <Anchor side="Right" relative="$parent/Item11" pos="Max" offset="26"/>
            <Anchor side="Bottom" relative="$parent/Item11" pos="Max" offset="2"/>
            <IncrementSound val=""/>
        </Frame>

        <!-- First row -->
        <Frame type="StoreTileListItem" name="Item0" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item1" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item2" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item1" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item3" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item2" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item2" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item4" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item3" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item3" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item5" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item4" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item4" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <!-- Second row -->
        <Frame type="StoreTileListItem" name="Item6" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Max" offset="#StoreTileItemVerticalSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item7" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item6" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item6" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item8" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item6" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item7" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item9" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item6" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item8" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item10" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item6" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item9" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item11" template="StoreTemplates/StoreTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item6" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item10" pos="Max" offset="#StoreTileItemSpacing"/>
        </Frame>

        <!-- State Group to adjust TileListItem anchoring based off of number shown -->
        <StateGroup name="VariousMaxItemStates">
            <State name="12TileListItems">
                <When type="Property" MaxItemPerPage="12"/>
                <Action type="SetProperty" ScrollStepValue="6"/>
                <Action type="SetAnchor" frame="Item4" side="Top" relative="$parent/Item3" pos="Min" offset="0"/>
                <Action type="SetAnchor" frame="Item4" side="Left" relative="$parent/Item3" pos="Max" offset="#StoreTileItemSpacing"/>
                <Action type="SetAnchor" frame="Item6" side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
            </State>
            <State name="8TileListItems">
                <When type="Property" MaxItemPerPage="8"/>
                <Action type="SetProperty" ScrollStepValue="4"/>
                <Action type="SetAnchor" frame="Item4" side="Top" relative="$parent/Item0" pos="Max" offset="#StoreTileItemVerticalSpacing"/>
                <Action type="SetAnchor" frame="Item4" side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
                <Action type="SetAnchor" frame="Item6" side="Left" relative="$parent/Item5" pos="Max" offset="#StoreTileItemSpacing"/>
                <Action type="SetAnchor" frame="ScrollBar" side="Right" relative="$parent/Item7" pos="Max" offset="26"/>
                <Action type="SetAnchor" frame="ScrollBar" side="Bottom" relative="$parent/Item7" pos="Max" offset="3"/>
            </State>
            <State name="4TileListItems">
                <When type="Property" MaxItemPerPage="4"/>
                <Action type="SetProperty" ScrollStepValue="2"/>
                <Action type="SetAnchor" frame="Item2" side="Top" relative="$parent/Item0" pos="Max" offset="#StoreTileItemVerticalSpacing"/>
                <Action type="SetAnchor" frame="Item2" side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
                <Action type="SetAnchor" frame="Item3" side="Left" relative="$parent/Item2" pos="Max" offset="#StoreTileItemSpacing"/>
                <Action type="SetAnchor" frame="ScrollBar" side="Right" relative="$parent/Item3" pos="Max" offset="26"/>
                <Action type="SetAnchor" frame="ScrollBar" side="Bottom" relative="$parent/Item3" pos="Max" offset="3"/>
            </State>
        </StateGroup>
    </Frame>

    <!--===============================================================================================
    <! Audio Preview Button
    <!==============================================================================================-->
    <Frame type="SoundButton" name="AudioPreviewButtonTemplate">
        <Width val="64"/>
        <Height val="64"/>
        <RenderPriority val="525"/>
        <DisplayType val="ImageLayer"/>
        <Visible val="false"/>

        <Frame type="Image" name="NormalImage">
            <Anchor relative="$parent"/>
            <LayerCount val="4"/>
            <StateCount val="1"/>
            <DesaturateOnDisable val="false"/>
            <TextureType val="Normal"/>
            <Texture val="@UI/Store/VOPreview_Normal" layer="0"/>
            <Texture val="@UI/Store/VOPreview_Pressed" layer="1"/>
            <Texture val="@UI/Store/VOPreview_Disabled" layer="3"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor relative="$parent"/>
            <LayerCount val="4"/>
            <StateCount val="1"/>
            <DesaturateOnDisable val="false"/>
            <TextureType val="Normal"/>
            <Texture val="@UI/Store/VOPreview_Hover" layer="0"/>
            <Texture val="@UI/Store/VOPreview_Pressed" layer="1"/>
            <Texture val="@UI/Store/VOPreview_Disabled" layer="3"/>
        </Frame>

        <Frame type="Image" name="IconNormalImage">
            <Anchor side="Top" relative="$parent" pos="0.234375" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="0.765625" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="0.234375" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="0.765625" offset="0"/>
            <Texture val="@UI/Store/VOPreview_Play_Normal"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="IconHoverImage">
            <Anchor side="Top" relative="$parent" pos="0.234375" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="0.765625" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="0.234375" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="0.765625" offset="0"/>
            <Texture val="@UI/Store/VOPreview_Play_Hover"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="IconMuteImage">
            <Anchor side="Top" relative="$parent" pos="0.234375" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="0.765625" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="0.234375" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="0.765625" offset="0"/>
            <Texture val="@UI/Store/VOPreview_Play_Disabled"/>
            <DesaturateOnDisable val="false"/>
            <Visible val="false"/>
        </Frame>

        <Frame type="Image" name="WaveAImage">
            <Anchor side="Top" relative="$parent" pos="0.109375" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="0.109375" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="0.890625" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="0.890625" offset="0"/>
            <Visible val="false"/>
            <Texture val="@UI/Store/VOPreview_WaveA"/>

            <Animation name="PulseAnimation">
                <Event event="OnShown" action="Reset, Play"/>
                <Event event="OnHidden" action="Stop"/>

                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" unclip="False" end="Loop">
                    <Key type="Curve" time="0" value="100"/>
                    <Key type="Curve" time=".2" value="45"/>
                    <Key type="Curve" time=".3" value="83"/>
                    <Key type="Curve" time=".4" value="21"/>
                    <Key type="Curve" time=".7" value="73"/>
                    <Key type="Curve" time=".8" value="50"/>
                    <Key type="Curve" time="1" value="100"/>
                    <Key type="Curve" time="1.2" value="38"/>
                    <Key type="Curve" time="1.3" value="100"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="WaveBImage">
            <Anchor side="Top" relative="$parent" pos="0.109375" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="0.109375" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="0.890625" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="0.890625" offset="0"/>
            <Visible val="false"/>
            <Texture val="@UI/Store/VOPreview_WaveB"/>

            <Animation name="PulseAnimation">
                <Event event="OnShown" action="Reset, Play"/>
                <Event event="OnHidden" action="Stop"/>

                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" unclip="False" end="Loop">
                    <Key type="Curve" time="0" value="100"/>
                    <Key type="Curve" time=".3" value="45"/>
                    <Key type="Curve" time=".5" value="97"/>
                    <Key type="Curve" time=".7" value="21"/>
                    <Key type="Curve" time="1" value="100"/>
                </Controller>
            </Animation>
        </Frame>

        <StateGroup name="WaveState">
            <DefaultState val="Off"/>
            <State name="Playing">
                <When type="Property" Playing="true"/>
                <When type="Property" Paused="false"/>
                <When type="Property" Enabled="true"/>
                <Action type="SetProperty" frame="WaveAImage" visible="true"/>
                <Action type="SetProperty" frame="WaveBImage" visible="true"/>
            </State>
            <State name="Paused">
                <When type="Property" Playing="true"/>
                <When type="Property" Paused="true"/>
                <When type="Property" Enabled="true"/>
            </State>
            <State name="Off"/>
        </StateGroup>

        <StateGroup name="SoundOptionState">
            <DefaultState val="Muted"/>

            <State name="Normal">
                <When type="Option" sound="true"/>
                <When type="Option" soundready="true"/>
                <When type="Option" soundvoices="true"/>
            </State>

            <State name="Muted">
                <Action type="SetProperty" enabled="false"/>
                <Action type="SetProperty" Tooltip="@UI/ScreenStore/SoundMuted"/>
            </State>
        </StateGroup>
        
        <StateGroup name="VisibleState">

            <State name="On">
                <When type="Property" frame="$parent/GameContentFrame" IsValid="true"/>
                <Action type="SetProperty" visible="true"/>
            </State>
        </StateGroup>

        <StateGroup name="IconState">
            <State name="Hover">
                <When type="Property" Hovering="true"/>
                <When type="Property" playing="false"/>
                <When type="Property" enabled="true"/>
                <Action type="SetProperty" frame="IconHoverImage" visible="true"/>
            </State>
            <State name="Normal">
                <When type="Property" Hovering="false"/>
                <When type="Property" playing="false"/>
                <When type="Property" enabled="true"/>
                <Action type="SetProperty" frame="IconNormalImage" visible="true"/>
            </State>
            <State name="Playing">
                <When type="Property" playing="true"/>
            </State>
            <State name="Disabled">
                <When type="Property" enabled="false"/>
                <Action type="SetProperty" frame="IconMuteImage" visible="true"/>
            </State>
        </StateGroup>
    </Frame>

    <!--===============================================================================================
    <! Big Tile List Template
    <!==============================================================================================-->
    <Frame type="GameContentTileList" name="BigTileListTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="#CollectionTileListTopMinOffset"/>
        <Anchor side="Left" relative="$parent/CategoryTileList" pos="Max" offset="#CollectionCategoryBigListNormalLeftOffset"/>
        <Anchor side="Right" relative="ScrollBar" pos="Max" offset="0"/>
        <Anchor side="Bottom" relative="Item0" pos="Max" offset="0"/>
        <Height val="900"/>
        <CollapseLayout val="true"/>
        <ReportsStats val="true"/>
        <AcceptsMouse val="true"/>
        <ShowAsNewEnabled val="true"/>
        <ScrollStepValue val="3"/>

        <ProductAttribute index="0" val="shop-section-featured-large"/>
        <ProductAttributeValue index="0" val="1" />
        <ProductAttributeCheck index="0" val="Value" />

        <RequireParentHeroFilter val="false"/>

        <FilterFlags val="Default|!ProductHidden|!NoValidProduct"/>
        <SearchFlags val=""/>
        <GenerationMode val="Store"/>
        <SortMethod index="0" val="ReleaseDate"/>
        <SortMethod index="1" val="Alphabetical"/>

        <Frame type="GameContentFrame" name="GameContentData">
            <Anchor relative="$parent"/>
            <GameContent val="{$parent/@SelectedGameContent}"/>
        </Frame>

        <Frame type="ScrollBar" name="ScrollBar" template="StandardTemplates/ScrollableBarTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="-2"/>
            <Anchor side="Right" relative="$parent/Item2" pos="Max" offset="30"/>
            <Anchor side="Bottom" relative="$parent/Item2" pos="Max" offset="2"/>
            <IncrementSound val=""/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item0" template="StoreTemplates/StoreBigTileListItemTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <CollapseLayout val="True"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item1" template="StoreTemplates/StoreBigTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Max" offset="#StoreTileItemSpacing"/>
            <CollapseLayout val="true"/>
        </Frame>

        <Frame type="StoreTileListItem" name="Item2" template="StoreTemplates/StoreBigTileListItemTemplate">
            <Anchor side="Top" relative="$parent/Item0" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Item1" pos="Max" offset="#StoreTileItemSpacing"/>
            <CollapseLayout val="true"/>
        </Frame>
     </Frame>

    <!--===============================================================================================
    <! Category Tile List Templates
    <!==============================================================================================-->
    <Frame type="CollectionCategoryTileList" name="CategoryTileListTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="139"/>
        <Anchor side="Left" relative="$parent" pos="Mid" offset="#CollectionCategoryNormalLeftOffset"/>
        <Anchor side="Right" relative="Item0" pos="Max" offset="10"/>
        <Anchor side="Bottom" relative="Item8" pos="Max" offset="0"/>
        <ReportsStats val="true"/>
        <AcceptsMouse val="True"/>
        <RenderPriority val="601"/>
        <ScrollStepValue val="1"/>
        <MaxItemPerPage val="9"/>
        <Height val="800"/>

        <RequireParentHeroFilter val="false"/>

        <RequireSelection val="false"/>
        <SelectedIndex val="#InvalidIndex"/>

        <FilterFlags val="Default|NoDupes|!ProductHidden|!NoValidProduct"/>
        <SearchFlags val=""/>
        <GenerationMode val="Store"/>
        <SortMethod index="0" val="Alphabetical"/>

        <Frame type="GameContentFrame" name="GameContentData">
            <Anchor relative="$parent"/>
            <GameContent val="{$parent/@SelectedGameContent}"/>
        </Frame>

        <Frame type="ScrollBar" name="ScrollBar" template="StandardTemplates/ScrollableBarTemplate">
            <Anchor side="Top" relative="$parent/FeaturedItem" pos="Min" offset="3"/>
            <Anchor side="Right" relative="$parent/Item0" pos="Max" offset="10"/>
            <Anchor side="Bottom" relative="$parent/Item8" pos="Max" offset="-3"/>
            <IncrementSound val=""/>
        </Frame>

        <Frame type="Button" name="FeaturedItem" template="StoreTemplates/FeaturedCategoryTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Toggled val="true"/>
            <CollapseLayout val="true"/>
        </Frame>

        <!-- Handle selecting first item when featured button is hidden -->
        <StateGroup name="FeaturedHidden">
            <State name="FeaturedIsHidden">
                <When type="Property" frame="FeaturedItem" Visible="false"/>
                <Action type="SetProperty" frame="FeaturedItem" Toggled="false" undo="false"/>
                <Action type="SetProperty" frame="$this" RequireSelection="true" undo="false"/>
            </State>
        </StateGroup>

        <!-- Handle toggling on/off featured tile list -->
        <StateGroup name="ToggleOffFeatured">
            <State name="ToggleOffFeatured">
                <When type="Property" frame="$this" SelectedIndex="#InvalidIndex" operator="NotEqual"/>
                <Action type="SetProperty" frame="FeaturedItem" Toggled="false" undo="false"/>
            </State>
        </StateGroup>

        <Frame type="CollectionCategoryTileListItem" name="Item0" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/FeaturedItem" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/FeaturedItem" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item1" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item0" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item2" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item1" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item3" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item2" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item4" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item3" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item5" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item4" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item6" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item5" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item7" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item6" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item8" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item7" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <Frame type="CollectionCategoryTileListItem" name="Item9" template="StoreTemplates/NormalCategoryTileListItem">
            <Anchor side="Top" relative="$parent/Item8" pos="Max" offset="#CollectionCategoryTileListItemSpacing"/>
            <Anchor side="Left" relative="$parent/Item0" pos="Min" offset="0"/>
        </Frame>

        <StateGroup name="FeaturedStates">
            <State name="FeaturedHidden">
                <When type="Property" frame="FeaturedItem" Visible="False"/>
                <Action type="SetProperty" MaxItemPerPage="10"/>
                <Action type="SetAnchor" frame="ScrollBar" side="Bottom" relative="$parent/Item9" pos="Max" offset="-3"/>
                <Action type="SetAnchor" frame="$this" side="Bottom" relative="Item9" pos="Max" offset="0"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="CollectionCategoryTileList" name="UniverseCategoryTileListTemplate" template="CollectionTemplates/CategoryTileListTemplate">

        <Frame type="CollectionCategoryTileListItem" name="Item0" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item1" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item2" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item3" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item4" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item5" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item6" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item7" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item8" template="StoreTemplates/UniverseCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item9" template="StoreTemplates/UniverseCategoryTileListItem"/>
    </Frame>

    <Frame type="CollectionCategoryTileList" name="NoImageCategoryTileListTemplate" template="CollectionTemplates/CategoryTileListTemplate">

        <Frame type="CollectionCategoryTileListItem" name="Item0" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item1" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item2" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item3" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item4" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item5" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item6" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item7" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item8" template="StoreTemplates/NoImageCategoryTileListItem"/>
        <Frame type="CollectionCategoryTileListItem" name="Item9" template="StoreTemplates/NoImageCategoryTileListItem"/>
    </Frame>

    <!-- Aspect Ratio Frame for Category Tile List Screens with NO Featured Category -->
    <Frame type="AspectRatioFrame" name="CategoryTileListAspectRatioFrameTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Height val="10"/>
        <Width val="10"/>
        <MinVisibleAspectRatio val="1.5"/>

        <StateGroup name="AspectRatioStates">
            <State name="Not4x3">
                <When type="Property" visible="True"/>
                <Action type="SetAnchor" frame="$parent/CategoryTileList" side="Left" relative="$parent" pos="Mid" offset="#CollectionCategoryNormalLeftOffset"/>
                <Action type="SetAnchor" frame="$parent/BigTileList" side="Left" relative="$parent/CategoryTileList" pos="Max" offset="#CollectionCategoryBigListNormalLeftOffset"/>
                <Action  type="SetAnchor" frame="$parent/ContentTileList" side="Left" relative="$parent/BigTileList/Item2" pos="Max" offset="#CollectionCategoryContentNormalLeftOffset"/>

            </State>
            <State name="Is4x3">
                <When type="Property" visible="False"/>
                <Action type="SetAnchor" frame="$parent/CategoryTileList" side="Left" relative="$parent" pos="Mid" offset="#CollectionCategory4x3LeftOffset"/>
                <Action type="SetAnchor" frame="$parent/BigTileList" side="Left" relative="$parent/CategoryTileList" pos="Max" offset="#CollectionCategoryBigList4x3LeftOffset"/>
                <Action  type="SetAnchor" frame="$parent/ContentTileList" side="Left" relative="$parent/BigTileList/Item2" pos="Max" offset="#CollectionCategoryContent4x3LeftOffset"/>
            </State>
        </StateGroup>
    </Frame>

    <!--===============================================================================================
    <! Main Game Content Panel Template
    <!==============================================================================================-->
    <Frame type="Frame" name="GameContentCollectionPanelTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="25"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

        <Frame type="Frame" name="BigSectionHeader" template="CollectionTemplates/SectionHeaderTemplate">
            <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="20"/>
            <Anchor side="Left" relative="$parent/CategoryTileList" pos="Min" offset="16"/>
            <Anchor side="Right" relative="$parent/SearchEditBox" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/CategoryTileList" pos="Min" offset="-19"/>
        </Frame>

        <Frame type="Frame" name="FilterOptions" template="CollectionTemplates/FilterOptionsTemplate">
            <Anchor side="Top" relative="$parent/SearchEditBox" pos="Min" offset="0"/>
            <Anchor side="Left" relative="DebugShowInvalidCheckBox" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/SortPulldown" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/SearchEditBox" pos="Max" offset="0"/>

            <Frame type="GameContentOwnershipFilterPulldown" name="OwnershipFilterPulldown" template="StandardTemplates/StandardPulldownTemplate">
                <GameContentType val="{$parent/$parent/ContentTileList/@GameContentType}"/>
            </Frame>
        </Frame>

        <Frame type="GameContentSortPulldown" name="SortPulldown" template="StandardTemplates/StandardPulldownTemplate">
            <Anchor side="Top" relative="$parent/SearchEditBox" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/SearchEditBox" pos="Min" offset="-5"/>
            <Width val="256"/>
         </Frame>

        <Frame type="EditBox" name="SearchEditBox" template="CollectionTemplates/SearchEditBoxTemplate">
            <Anchor side="Top" relative="$parent/BigSectionHeader/TitleLabel" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/BigSectionHeader/TitleLabel" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="822"/>
            <InputDelayTimeout val="#EditBoxInputTimeout"/>

            <StateGroup name="SearchHideContent">
                 <State name="SearchInactive">
                    <When type="Property" frame="$this" Text=""/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" SearchFlags=""/>
                </State>
                <State name="SearchActive">
                    <When type="Property" frame="$this" Text="" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$parent/CategoryTileList" Visible="false"/>
                    <Action type="SetProperty" frame="$parent/BigTileList" Visible="false"/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" CollectionCategoryFilter=""/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" ParentHeroFilter=""/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" ProductAttribute="" index="0"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="CollectionCategoryTileList" name="CategoryTileList" template="CollectionTemplates/CategoryTileListTemplate">
            <CountTextContentList val="$parent/ContentTileList"/>
            <GameContentType val="Hero"/>
            <CollapseLayout val="true"/>

            <StateGroup name="FeaturedToggle">
                <State name="ShowFeatured">
                    <When type="Property" frame="FeaturedItem" Toggled="true"/>
                    <Action type="SetProperty" frame="$this" RequireSelection="false" undo="false"/>
                    <Action type="SetProperty" frame="$this" SelectedIndex="#InvalidIndex" undo="false"/>
                    <Action type="SetProperty" frame="FeaturedItem" AcceptsMouse="false" undo="false"/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" CollectionCategoryFilter="" undo="false"/>
                </State>
                <State name="ShowNormal">
                    <When type="Property" frame="FeaturedItem" Toggled="false"/>
                    <Action type="SetProperty" frame="$this" RequireSelection="true" undo="false"/>
                    <Action type="SetProperty" frame="FeaturedItem" AcceptsMouse="true" undo="false"/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" CollectionCategoryFilter="{$parent/CategoryTileList/@SelectedCollectionCategory}" undo="false"/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" ProductAttribute="" index="0"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="GameContentTileList" name="BigTileList" template="CollectionTemplates/BigTileListTemplate">
            <CollapseLayout val="true"/>

            <MaxItemPerPage val="1"/>
            <Visible val="false"/>

            <StateGroup name="ShowBigTileList">
                <State name="ShowTileList">
                    <When type="Property" frame="$parent/CategoryTileList/FeaturedItem" Toggled="true"/>
                    <When type="Property" frame="$this" DataCount="0" operator="GreaterThan"/>
                    <Action type="SetProperty" frame="$this" Visible="true"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="GameContentTileList" name="ContentTileList" template="CollectionTemplates/ContentTileListTemplate">
            <CollectionCategoryFilter val="{$parent/CategoryTileList/@SelectedCollectionCategory}"/>

            <StateGroup name="ItemsPerPageStates">
                <State name="SearchInactive_WithBigTileList">
                    <When type="Property" frame="$parent/SearchEditBox" Text=""/>
                    <When type="Property" frame="$parent/BigTileList" Visible="true"/>
                    <Action type="SetProperty" frame="$this" MaxItemPerPage="4"/>
                </State>
                <State name="SearchInactive_NoBigTileList">
                    <When type="Property" frame="$parent/SearchEditBox" Text=""/>
                    <When type="Property" frame="$parent/BigTileList" Visible="false"/>
                    <Action type="SetProperty" frame="$this" MaxItemPerPage="8"/>
                </State>
                <State name="SearchActive">
                    <When type="Property" frame="$parent/SearchEditBox" Text="" operator="NotEqual"/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" MaxItemPerPage="12"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="AspectRatioFrame" name="4x3AspectRatioFrame" template="CollectionTemplates/CategoryTileListAspectRatioFrameTemplate"/>
    </Frame>

    <!--===============================================================================================
    <! Main Game Content Panel Template - Using Hero as category for filtering
    <!==============================================================================================-->
    <Frame type="Frame" name="GameContentCollectionPanelHeroCategoryTemplate" template="CollectionTemplates/GameContentCollectionPanelTemplate">
        <Frame type="Frame" name="FilterOptions" template="CollectionTemplates/FilterOptionsTemplate">
            <Frame type="CheckBox" name="DebugShowHiddenCheckBox" template="StandardTemplates/StandardCheckBoxLabelTemplate">
                <Anchor side="Right" relative="$parent/HeroOwnershipFilterPulldown" pos="Min" offset="0"/>
            </Frame>

            <Frame type="GameContentOwnershipFilterPulldown" name="HeroOwnershipFilterPulldown" template="StandardTemplates/StandardPulldownTemplate">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/OwnershipFilterPulldown" pos="Min" offset="0"/>
                <Width val="256"/>
                <GameContentType val="Hero"/>
             </Frame>
        </Frame>

        <Frame type="CollectionCategoryTileList" name="CategoryTileList" template="CollectionTemplates/CategoryTileListTemplate">
            <GameContentType val="Hero"/>
            <UseContentAsCategory val="true"/>

            <StateGroup name="FeaturedToggle">
                <State name="ShowFeatured">
                    <Action type="SetProperty" frame="$this" RequireSelection="false" undo="false"/>
                    <Action type="SetProperty" frame="$this" SelectedIndex="#InvalidIndex" undo="false"/>
                    <Action type="SetProperty" frame="FeaturedItem" AcceptsMouse="false" undo="false"/>
                </State>
                <State name="ShowNormal">
                    <Action type="SetProperty" frame="$this" RequireSelection="true" undo="false"/>
                    <Action type="SetProperty" frame="FeaturedItem" AcceptsMouse="true" undo="false"/>
                    <Action type="SetProperty" frame="$parent/ContentTileList" ProductAttribute="" index="0"/>
                </State>
            </StateGroup>

            <StateGroup name="HeroOwnershipFilter">
                <State name="AllHeroes">
                    <When type="Property" frame="$parent/FilterOptions/HeroOwnershipFilterPulldown" SelectedFilter="AllContent"/>
                    <Action type="SetProperty" FilterFlag_Owned="true"/>
                    <Action type="SetProperty" FilterFlag_NotOwned="true"/>
                </State>
                <State name="OwnedHeroes">
                    <When type="Property" frame="$parent/FilterOptions/HeroOwnershipFilterPulldown" SelectedFilter="OwnedContent"/>
                    <Action type="SetProperty" FilterFlag_Owned="true"/>
                    <Action type="SetProperty" FilterFlag_NotOwned="false"/>
                </State>
                <State name="UnownedHeroes">
                    <When type="Property" frame="$parent/FilterOptions/HeroOwnershipFilterPulldown" SelectedFilter="UnownedContent"/>
                    <Action type="SetProperty" FilterFlag_Owned="false"/>
                    <Action type="SetProperty" FilterFlag_NotOwned="true"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="GameContentTileList" name="ContentTileList" template="CollectionTemplates/ContentTileListTemplate">
            <ParentHeroFilter val="{$parent/CategoryTileList/GameContentData/@HeroLink}"/>
            <CollectionCategoryFilter val=""/>

            <StateGroup name="DontRequireParentHeroOnSearchOrFeatured">
                <State name="SearchActive">
                    <When type="Property" frame="$parent/SearchEditBox" Text="" operator="NotEqual"/>
                    <Action type="SetProperty" RequireParentHeroFilter="false"/>
                </State>
                <State name="FeaturedShown">
                    <When type="StateGroup" frame="$parent/CategoryTileList" FeaturedToggle="ShowFeatured"/>
                    <Action type="SetProperty" RequireParentHeroFilter="false"/>
                </State>
            </StateGroup>
        </Frame>
    </Frame>
</Desc>
