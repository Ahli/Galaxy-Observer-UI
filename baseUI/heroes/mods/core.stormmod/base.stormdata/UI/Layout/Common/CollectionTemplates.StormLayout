          <When type="Property" frame="$parent/FilterOptions/HeroOwnershipFilterPulldown" SelectedFilter="OwnedContent" />
          <Action type="SetProperty" FilterFlag_Owned="true" />
          <Action type="SetProperty" FilterFlag_NotOwned="false" />
        </State>
        <State name="UnownedHeroes">
          <When type="Property" frame="$parent/FilterOptions/HeroOwnershipFilterPulldown" SelectedFilter="UnownedContent" />
          <Action type="SetProperty" FilterFlag_Owned="false" />
          <Action type="SetProperty" FilterFlag_NotOwned="true" />
        </State>
      </StateGroup>
    </Frame>
    <Frame type="GameContentTileList" name="ContentTileList" template="CollectionTemplates/ContentTileListTemplate">
      <ParentHeroFilter val="{$parent/CategoryTileList/GameContentData/@HeroLink}" />
      <CollectionCategoryFilter val="" />
      <StateGroup name="FeaturedToggle">
        <State name="ShowNormal">
          <Action type="SetProperty" index="0" ProductAttribute="" />
        </State>
      </StateGroup>
      <StateGroup name="DontRequireParentHero">
        <State name="SearchActive">
          <When type="Property" frame="$parent/SearchEditBox" Text="" operator="NotEqual" />
          <Action type="SetProperty" RequireParentHeroFilter="false" />
        </State>
        <State name="NewlyAcquiredActive">
          <When type="Property" frame="$parent/FilterOptions/FilterNewlyAcquired" Toggled="true" />
          <Action type="SetProperty" RequireParentHeroFilter="false" />
        </State>
        <State name="FeaturedShown">
          <When type="StateGroup" frame="$parent/CategoryTileList" FeaturedToggle="ShowFeatured" />
          <Action type="SetProperty" RequireParentHeroFilter="false" />
        </State>
        <State name="AllSelectionChosen">
          <When type="Property" frame="$parent/CategoryTileList" SelectedIndex="#InvalidIndex" operator="NotEqual" />
          <When type="Property" frame="$parent/CategoryTileList/GameContentData" IsValid="false" />
          <Action type="SetProperty" RequireParentHeroFilter="false" />
        </State>
      </StateGroup>
    </Frame>
  </Frame>
</Desc>