    <!-- Spray Template -->

    <Frame type="Frame" name="SprayAnimationTemplate">
        <Height val="110"/>
        <Width val="92"/>

        
        <!-- <AcceptsMouse val="True"/>
        <Animation name="Click">
            <Event event="OnMouseDown" action="Restart,Play"/>
            <Controller type="Event" end="Stop">
                <Key type="Event" time="0" event="BossDefeated"/>
            </Controller>
        </Animation> -->
        
        <Frame type="Image" name="EnemyBossHudSlashCircle">
            <Anchor side="Top" relative="$parent" pos="Min" offset="3"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-2"/>
            <Texture val="@UI/Storm_UI_HUD_Minimap_Holdout_Strike_Circle_Blue"/>
            <Alpha val="0"/>
            <TextureType val="Circular"/>
            <Width val="70"/>
            <Height val="70"/>
            <StartAngle val="0"/>
            <EndAngleOffset val="360"/>
            <Animation name="BossDefeat2">
                <Event event="OnShown" action="Stop"/>
                <Event event="BossDefeat2" action="Restart, play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time=".1" value="0" inout="Linear"/>
                    <Key type="Curve" time=".13" value="255" inout="Slow,Linear"/>
                </Controller>
                <Controller type="EndAngleOffset" end="Stop" relative="false">
                    <Key type="Curve" time="0" value="0" inout="Linear,slow"/>
                    <Key type="Curve" time=".5" value="-360" inout="slow,Linear"/>
                </Controller>
                <Controller type="Animation" end="stop" frame="$parent/EnemyBossHudSlashDrips" Animation="BossDefeat3">
                    <Key type="Animation" time="0" action="reset"/>
                    <Key type="Animation" time=".3" action="play"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="EnemyBossHudSlashDrips">
            <Anchor side="Top" relative="$parent/EnemyBossHudSlashCircle" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/EnemyBossHudSlashCircle" pos="Min" offset="0"/>
            <Alpha val="0"/>
            <LayerCount val="2"/>

            <Texture val="@UI/Storm_UI_HUD_Minimap_Holdout_Strike_Drips_Mask" layer="0"/>
            <TextureCoords top="0" left="1" bottom="1" right="0" layer="0"/>
            <AlphaLayer val="true" layer="0"/>

            <Animation name="BossDefeat3">
                <Event event="OnShown" action="Stop"/>
                <Event event="BossDefeat3" action="Restart, Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <!-- <Key type="Curve" time=".1" value="0" inout="Linear"/> -->
                    <Key type="Curve" time=".4" value="255" inout="Slow,Linear"/>
                </Controller>
                <Controller type="LayerUV" layer="0" end="Pause" side="bottom" relative="false" sidelock="true">
                    <Key type="Curve" time="0" value="1.06" inout="Linear,Slow"/>
                    <Key type="Curve" time="1.5" value=".87" inout="Linear"/>
                </Controller>
            </Animation>

            <Texture val="@UI/Storm_UI_HUD_Minimap_Holdout_Strike_Drips_Blue" layer="1"/>

        </Frame>

        <Frame type="Image" name="EnemyBossHudLeftSlash1">
            <Anchor side="Top" relative="$parent/EnemyBossHudSlashCircle" pos="Min" offset="-0"/>
            <Anchor side="left" relative="$parent/EnemyBossHudSlashCircle" pos="Max" offset="-69"/>
            <Alpha val="0"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Storm_UI_HUD_Minimap_Holdout_Strike_Mask" layer="0"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>

            <AlphaLayer val="true" layer="0"/>
            <Animation name="BossDefeated">
                <Event event="OnShown" action="Stop"/>
                <Event event="BossDefeated" action="Restart, Play" frame="$parent"/>
                <Controller type="Fade" layer="0" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time=".03" value="255" inout="Linear"/>
                </Controller>
                <Controller type="LayerUV" layer="0" end="Pause" side="right" relative="false" sidelock="true">
                    <Key type="Curve" time="0" value="0" inout="Linear,slow"/>
                    <Key type="Curve" time=".3" value="1" inout="slow,Linear"/>
                </Controller>
                <Controller type="LayerUV" layer="0" end="Pause" side="top" relative="false" sidelock="true">
                    <Key type="Curve" time="0" value="1" inout="Linear,Slow"/>
                    <Key type="Curve" time=".3" value="0" inout="slow,Linear"/>
                </Controller>
                <Controller type="Animation" layer="0" end="stop" frame="$parent/EnemyBossHudLeftSlash2" Animation="BossDefeat4">
                    <Key type="Animation" time="0" action="reset"/>
                    <Key type="Animation" time=".2" action="play"/>
                </Controller>
            </Animation>

            <Texture val="@UI/Storm_UI_HUD_Minimap_Holdout_Strike_RL_Blue" layer="1"/>

        </Frame>

        <Frame type="Image" name="EnemyBossHudLeftSlash2">
            <Anchor side="Top" relative="$parent/EnemyBossHudSlashCircle" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/EnemyBossHudSlashCircle" pos="Min" offset="0"/>
            <Alpha val="0"/>
            <LayerCount val="2"/>

            <Texture val="@UI/Storm_UI_HUD_Minimap_Holdout_Strike_Mask" layer="0"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="0"/>

            <AlphaLayer val="true" layer="0"/>
            <Animation name="BossDefeat4">
                <Event event="OnShown" action="Stop"/>
                <Event event="BossDefeat4" action="Restart, Play" frame="$parent"/>
                <Controller type="Fade" end="Pause">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time=".1" value="0" inout="Linear"/>
                    <Key type="Curve" time=".13" value="255" inout="Slow,Linear"/>
                </Controller>
                <Controller type="LayerUV" layer="0" end="Pause" side="left" relative="false" sidelock="true">
                    <Key type="Curve" time="0" value="1" inout="Linear,slow"/>
                    <Key type="Curve" time=".3" value="0" inout="slow,Linear"/>
                </Controller>
                <Controller type="LayerUV" layer="0" end="Pause" side="top" relative="false" sidelock="true">
                    <Key type="Curve" time="0" value="1" inout="Linear,Slow"/>
                    <Key type="Curve" time=".3" value="0" inout="slow,Linear"/>
                </Controller>
                <Controller type="Animation" layer="0" end="stop" frame="$parent/EnemyBossHudSlashCircle" Animation="BossDefeat2">
                    <Key type="Animation" time="0" action="reset"/>
                    <Key type="Animation" time=".4" action="play"/>
                </Controller>
            </Animation>

            <Texture val="@UI/Storm_UI_HUD_Minimap_Holdout_Strike_LR_Blue" layer="1"/>

        </Frame>

    </Frame>

