<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Constant name="ControlYellow" val="f5df3d"/>
    
    <!--                                                                                                      -->
    <!--  CONTROL POINT PANEL                                                                                 -->
    <!--                                                                                                      -->
    <Frame type="Frame" name="VolskayaCapturePointPanel">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>

        <Width val="1000"/>
        
        <!-- animations -->
        <Animation name="SlideIn">
            <Event event="OnShown" action="Reset,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.3" value="255"/>
            </Controller>
        </Animation>
        
        <Animation name="SlideOut">
            <Event event="PlaySlideOut" action="Reset,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.00" value="255"/>
                <Key type="Curve" time="0.15" value="0"/>
            </Controller>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0.00" value="True"/>
                <Key type="Bool" time="0.15" value="False"/>
            </Controller>
        </Animation>
        
        <!-- colors -->
        <Frame type="UnitStatusColor" name="TeamOrderColors">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="3"/>
            <Height val="3"/>

            <ForcedTeam val="0"/>
            <ColorStyle val="VolskayaBars"/>
        </Frame>
        
        <Frame type="UnitStatusColor" name="TeamChaosColors">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TeamOrderColors" pos="Max" offset="0"/>
            <Width val="3"/>
            <Height val="3"/>

            <ForcedTeam val="1"/>
            <ColorStyle val="VolskayaBars"/>
        </Frame>
        
        <Frame type="UnitStatusColor" name="TeamOrderColorsBright">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TeamChaosColors" pos="Max" offset="0"/>
            <Width val="3"/>
            <Height val="3"/>

            <ForcedTeam val="0"/>
            <ColorStyle val="VolskayaBarsBright"/>
        </Frame>
        
        <Frame type="UnitStatusColor" name="TeamChaosColorsBright">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TeamOrderColorsBright" pos="Max" offset="0"/>
            <Width val="3"/>
            <Height val="3"/>

            <ForcedTeam val="1"/>
            <ColorStyle val="VolskayaBarsBright"/>
        </Frame>
        
        <!-- order control background layers -->
        <Frame type="Image" name="OrderControlPanelBack">
            <Anchor side="Top" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            
            <Width val="156"/>
            <Height val="80"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_bg.dds"/>
        </Frame>

        <Frame type="Image" name="OrderControlPanelStroke" template="VolskayaMapMechanic/VolskayaCapturePointPanel/OrderControlPanelBack">
            <Color val="{$ancestor[@name=VolskayaCapturePointPanel]/TeamOrderColors/@Color}"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_blue_bg_stroke.dds"/>
        </Frame>

        <Frame type="Image" name="OrderControlPanelFill" template="VolskayaMapMechanic/VolskayaCapturePointPanel/OrderControlPanelBack">
            <Color val="{$ancestor[@name=VolskayaCapturePointPanel]/TeamOrderColors/@Color}"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_blue_bg.dds"/>
        </Frame>

        <!-- order control label -->
        <Frame type="Label" name="OrderControlLabel">
            <Anchor side="Top" relative="$parent/OrderControlPanelBack" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/OrderControlPanelBack" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/OrderControlPanelBack" pos="Mid" offset="-12"/>
            <Anchor side="Right" relative="$parent/OrderControlPanelBack" pos="Mid" offset="-12"/>
            
            <Text val="88%"/>
            <Style val="Storm_Volskaya_ControlVictory"/>
        </Frame>

        <!-- chaos control background layers -->
        <Frame type="Image" name="ChaosControlPanelBack">
            <Anchor side="Top" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            
            <Width val="156"/>
            <Height val="80"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_bg.dds"/>
            <TextureCoords top="0" left="1" bottom="1" right="0" layer="0"/>
        </Frame>

        <Frame type="Image" name="ChaosControlPanelStroke" template="VolskayaMapMechanic/VolskayaCapturePointPanel/ChaosControlPanelBack">
            <Color val="{$ancestor[@name=VolskayaCapturePointPanel]/TeamChaosColors/@Color}"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_blue_bg_stroke.dds"/>
        </Frame>

        <Frame type="Image" name="ChaosControlPanelFill" template="VolskayaMapMechanic/VolskayaCapturePointPanel/ChaosControlPanelBack">
            <Color val="{$ancestor[@name=VolskayaCapturePointPanel]/TeamChaosColors/@Color}"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_blue_bg.dds"/>
        </Frame>

        <!-- chaos control label -->
        <Frame type="Label" name="ChaosControlLabel">
            <Anchor side="Top" relative="$parent/ChaosControlPanelBack" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/ChaosControlPanelBack" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/ChaosControlPanelBack" pos="Mid" offset="12"/>
            <Anchor side="Right" relative="$parent/ChaosControlPanelBack" pos="Mid" offset="12"/>
            
            <Text val="88%"/>
            <Style val="Storm_Volskaya_ControlVictory"/>
        </Frame> 

        <!-- center hex -->
        <Frame type="Image" name="CenterHex">
            <Anchor side="Bottom" relative="$parent/$parent/$parent/MinimapPanel" pos="Min" offset="60"/>
            <Anchor side="Left" relative="$parent/$parent/$parent/MinimapPanel" pos="Mid" offset="-68"/>
            
            <Width val="164"/>
            <Height val="164"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_frame.dds"/>
        </Frame>

        <!-- (circular) contest progress bar -->
        <Frame type="ProgressBar" name="ContestProgressBar">
            <Anchor side="Top" relative="$parent/CenterHex" pos="Min" offset="82"/>
            <Anchor side="Bottom" relative="$parent/CenterHex" pos="Min" offset="82"/>
            <Anchor side="Left" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            
            <Width val="164"/>
            <Height val="164"/>
            <MinValue val="0"/>
            <MaxValue val="1"/>
            <Value val="0"/>
            <Circular val="true"/>
            <CircularStart val="0"/>
            <CircularEndOffset val="360"/>
            <FillInset val="0"/> 

            <Frame type="Frame" name="FillImageContainer">
                <Anchor relative="$parent"/>

                <Frame type="Image" name="FillImage">
                    <Anchor relative="$parent"/> 
                    <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_blue_progress.dds"/>
                    <TextureType val="Circular"/>
                    
                    <StateGroup name="TeamColor">
                        <DefaultState val="Neutral"/>
                        <State name="Neutral">
                            <When type="AnimationState" TeamColor="Neutral"/>
                            <Action type="SetProperty" frame="$this" Color="e4d34c"/>
                        </State>
                        <State name="Order">
                            <When type="AnimationState" TeamColor="Order"/>
                            <Action type="SetProperty" frame="$this" Color="{$ancestor[@name=VolskayaCapturePointPanel]/TeamOrderColorsBright/@Color}"/>
                        </State>
                        <State name="Chaos">
                            <When type="AnimationState" TeamColor="Chaos"/>
                            <Action type="SetProperty" frame="$this" Color="{$ancestor[@name=VolskayaCapturePointPanel]/TeamChaosColorsBright/@Color}"/>
                        </State>
                    </StateGroup>
                </Frame>
            </Frame>
        </Frame>

        <!-- center circle & indicator -->
        <Frame type="Image" name="CenterCircle">
            <Anchor side="Top" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/CenterHex" pos="Mid" offset="0"/>
            
            <Width val="164"/>
            <Height val="164"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_blue.dds"/>
            
            <StateGroup name="TeamOwnershipColor">
                <DefaultState val="Neutral"/>
                <State name="Neutral">
                    <When type="AnimationState" TeamOwnershipColor="Neutral"/>
                    <Action type="SetProperty" frame="$this" Color="#ControlYellow"/>
                    <Action type="SetProperty" frame="$parent/OrderControlLabel" Style="Storm_Volskaya_ControlVictory"/>
                    <Action type="SetProperty" frame="$parent/OrderControlLabel" Color="{$ancestor[@name=VolskayaCapturePointPanel]/TeamOrderColors/@Color}"/>
                    <Action type="SetProperty" frame="$parent/ChaosControlLabel" Style="Storm_Volskaya_ControlVictory"/>
                    <Action type="SetProperty" frame="$parent/ChaosControlLabel" Color="{$ancestor[@name=VolskayaCapturePointPanel]/TeamChaosColors/@Color}"/>
                </State>
                <State name="Order">
                    <When type="AnimationState" TeamOwnershipColor="Order"/>
                    <Action type="SetProperty" frame="$this" Color="{$ancestor[@name=VolskayaCapturePointPanel]/TeamOrderColors/@Color}"/>
                    <Action type="SetProperty" frame="$parent/OrderControlLabel" Style="Storm_Volskaya_ControlVictory_Glow"/>
                    <Action type="SetProperty" frame="$parent/OrderControlLabel" Color="000000"/>
                    <Action type="SetProperty" frame="$parent/ChaosControlLabel" Style="Storm_Volskaya_ControlVictory"/>
                    <Action type="SetProperty" frame="$parent/ChaosControlLabel" Color="{$ancestor[@name=VolskayaCapturePointPanel]/TeamChaosColors/@Color}"/>
                </State>
                <State name="Chaos">
                    <When type="AnimationState" TeamOwnershipColor="Chaos"/>
                    <Action type="SetProperty" frame="$this" Color="{$ancestor[@name=VolskayaCapturePointPanel]/TeamChaosColors/@Color}"/>
                    <Action type="SetProperty" frame="$parent/OrderControlLabel" Style="Storm_Volskaya_ControlVictory"/>
                    <Action type="SetProperty" frame="$parent/OrderControlLabel" Color="{$ancestor[@name=VolskayaCapturePointPanel]/TeamOrderColors/@Color}"/>
                    <Action type="SetProperty" frame="$parent/ChaosControlLabel" Style="Storm_Volskaya_ControlVictory_Glow"/>
                    <Action type="SetProperty" frame="$parent/ChaosControlLabel" Color="000000"/>
                </State>
            </StateGroup>
        </Frame>

        <Frame type="Image" name="CenterPointIndicator">
            <Anchor side="Top" relative="$parent/CenterCircle" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/CenterCircle" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/CenterCircle" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/CenterCircle" pos="Mid" offset="0"/>
            
            <Width val="164"/>
            <Height val="164"/>
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_a.dds"/>
        </Frame>

        <!-- contested label -->
        <Frame type="Label" name="ContestedLabel">
            <Anchor side="Top" relative="$parent/CenterHex" pos="Max" offset="-35"/>
            <Anchor side="Left" relative="$parent/CenterHex" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/CenterHex" pos="Max" offset="0"/>
            
            <Width val="240"/>
            <Height val="50"/>
            <Style val="Storm_Volskaya_ContestedLabel"/>
            <Text val="@UI/MapMechanic/Volskaya/Contested"/>
        </Frame>
    </Frame>

    <!--                                                                                                      -->
    <!--  WARNING NOTIFICATION                                                                                -->
    <!--                                                                                                      -->
    <Frame type="Frame" name="VolskayaToastContainer" template="MapMechanicPanel/MechanicFrameTemplate">
        <Anchor side="Bottom" relative="$parent/$parent/MinimapPanel" pos="Min" offset="80"/>
        <Anchor side="Right" relative="$parent/$parent/MinimapPanel" pos="Max" offset="0"/>
        
        <Width val="600"/>
        <Height val="140"/>

        <Animation name="SlideIn">
            <Event event="OnShown" action="Reset,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.3" value="255"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Right" relative="true">
                <Key type="Curve" time="0.0" value="80"/>
                <Key type="Curve" time="0.3" value="0"/>
            </Controller>
        </Animation>
        
        <Animation name="SlideOut">
            <Event event="PlaySlideOut" action="Reset,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="255"/>
                <Key type="Curve" time="0.3" value="0"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Right" relative="true">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.3" value="80"/>
            </Controller>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0.0" value="True"/>
                <Key type="Bool" time="0.3" value="False"/>
            </Controller>
        </Animation>
        
        <Frame type="Frame" name="ToastNotification">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            
            <Width val="600"/>
            
            <!-- notification background -->
            <Frame type="Image" name="BackgroundImage">
                <Anchor side="Top" relative="$parent" pos="Min" offset="25"/>
                <Anchor side="Left" relative="$parent/InfoLabel" pos="Min" offset="-20"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                
                <Texture val="Assets\Textures\storm_ui_hud_volskaya_objective_bg.dds"/>
                <TextureType val="Nineslice"/>
                <Width val="0"/>
            </Frame>
            
            <!-- notification text -->
            <Frame type="Label" name="InfoLabel">
                <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="4"/>
                <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="4"/>
                <Anchor side="Right" relative="$parent/BackgroundImage" pos="Max" offset="-140"/>
                
                <Text val="@UI/MapMechanic/Volskaya/ControlPointWillUnlock"/>
                <Style val="Storm_MapMechanic_MechanicCounterVolskaya"/>
            </Frame>

            <!-- notification icon frame and letters -->
            <Frame type="Image" name="NotificationIcon">
                <Anchor side="Top" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/BackgroundImage" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/BackgroundImage" pos="Max" offset="0"/>
                
                <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_frame.dds"/>
            </Frame>
            
            <Frame type="Image" name="NotificationIconA">
                <Anchor side="Top" relative="$parent/NotificationIcon" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent/NotificationIcon" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/NotificationIcon" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/NotificationIcon" pos="Max" offset="0"/>
                
                <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_frame_a.dds"/>
            </Frame>
            
            <Frame type="Image" name="NotificationIconB" template="VolskayaMapMechanic/VolskayaToastContainer/ToastNotification/NotificationIconA">
                <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_frame_b.dds"/>
            </Frame>
            
            <Frame type="Image" name="NotificationIconC" template="VolskayaMapMechanic/VolskayaToastContainer/ToastNotification/NotificationIconA">
                <Texture val="Assets\Textures\storm_ui_hud_volskaya_control_frame_c.dds"/>
            </Frame>
            
            <!-- notification timer -->
            <Frame type="Label" name="TimerLabel">
                <Anchor side="Top" relative="$parent/NotificationIcon" pos="Mid" offset="50"/>
                <Anchor side="Left" relative="$parent/NotificationIcon" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent/NotificationIcon" pos="Max" offset="0"/>
                
                <Style val="Storm_Volskaya_ControlVictory"/>
            </Frame>
        </Frame>
        
    </Frame>

    <!--                                                                                                      -->
    <!--  OVERTIME PANEL                                                                                      -->
    <!--                                                                                                      -->
    <Frame type="Frame" name="VolskayaOvertimePanel">
        <Anchor side="Bottom" relative="$parent/VolskayaCapturePointPanel/CenterHex" pos="Min" offset="160"/>
        <Anchor side="Left" relative="$parent/VolskayaCapturePointPanel/CenterHex" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent/VolskayaCapturePointPanel/CenterHex" pos="Mid" offset="0"/>
        
        <Width val="500"/>
        <Height val="350"/>
        <Visible val="False"/>
        
        <Frame type="Image" name="OvertimeTextImage">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            
            <Texture val="@UI/Storm_UI_HUD_Volskaya_Overtime_Text"/>
        </Frame>

        <Frame type="Image" name="OvertimeProgressBarBG">
            <Anchor side="Top" relative="$parent/OvertimeTextImage" pos="Mid" offset="30"/>
            <Anchor side="Bottom" relative="$parent/OvertimeTextImage" pos="Mid" offset="30"/>
            <Anchor side="Left" relative="$parent/OvertimeTextImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/OvertimeTextImage" pos="Mid" offset="0"/>
            
            <Texture val="Assets\Textures\Storm_UI_HUD_Minimap_Hanamura_Brawl_Overtime_bg.dds"/>
            <RenderPriority val="1"/>
        </Frame>

        <Frame type="Image" name="OvertimeProgressBarBGGlow">
            <Anchor side="Top" relative="$parent/OvertimeTextImage" pos="Mid" offset="30"/>
            <Anchor side="Bottom" relative="$parent/OvertimeTextImage" pos="Mid" offset="30"/>
            <Anchor side="Left" relative="$parent/OvertimeTextImage" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/OvertimeTextImage" pos="Mid" offset="0"/>
            
            <RenderPriority val="0"/>
            <Texture val="Assets\Textures\Storm_UI_HUD_Minimap_Hanamura_Brawl_Overtime_bg_Glow.dds"/>

            <Animation name="Pulse">
                <Event event="OnShown" action="Reset,Play"/>
                <Controller type="Fade" end="PingPong">
                <Key type="Curve" time="0" value="255"/>
                <Key type="Curve" time="1" value="125"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="ProgressBar" name="OvertimeProgressBar">
            <Anchor side="Top" relative="$parent/OvertimeProgressBarBG" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/OvertimeProgressBarBG" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/OvertimeProgressBarBG" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/OvertimeProgressBarBG" pos="Mid" offset="0"/>
            
            <Value val="100"/>
            <Height val="36"/>
            <Width val="280"/>
            <RenderPriority val="2"/>
            <UseFillContainer val="true"/>

            <Frame type="Frame" name="FillImageContainer">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                <Anchor  side="Left" relative="$parent" pos="Min" offset="0"/>
                
                <Height val="36"/>
                <Width val="280"/>

                <Frame type="Image" name="FillImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor  side="Left" relative="$parent" pos="Min" offset="0"/>
                    
                    <Height val="36"/>
                    <Width val="280"/>
                    <Texture val="Assets\Textures\Storm_UI_HUD_Minimap_Hanamura_Brawl_Overtime_Bar.dds"/>
                </Frame>
            </Frame>
        </Frame>

        <Frame type="Image" name="OvertimeProgressBarIndicator">
            <Anchor side="Top" relative="$parent/OvertimeProgressBar/FillImageContainer" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/OvertimeProgressBar/FillImageContainer" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/OvertimeProgressBar/FillImageContainer" pos="Max" offset="-10"/>
            <Anchor side="Right" relative="$parent/OvertimeProgressBar/FillImageContainer" pos="Max" offset="-10"/>
            
            <Height val="128"/>
            <Width val="128"/>
            <RenderPriority val="4"/>
            <Texture val="Assets\Textures\Storm_UI_HUD_Minimap_Hanamura_Brawl_Overtime_Bar_Indicator.dds"/>

            <Animation name="Flash">
                <Event event="OnShown" action="Reset,Play"/>
                <Controller type="Fade" end="PingPong">
                <Key type="Curve" time="0" value="255"/>
                <Key type="Curve" time="0.2" value="200"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="CutsceneFrame" name="OvertimeBackgroundCutscene">
            <Anchor side="Top" relative="$parent/OvertimeProgressBarBG" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent/OvertimeProgressBarBG" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent/OvertimeProgressBarBG" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent/OvertimeProgressBarBG" pos="Mid" offset="0"/>
            
            <Width val="800"/>
            <Height val="300"/>
            <File val="Cutscenes/Overtime.StormCutscene"/>
            <RenderType val="HDR"/>
            <Unclipped val="True"/>
            <RenderPriority val="3"/>

            <Animation name="Show">
                <Event event="OnShown" action="Reset,Play"/>
                    
                <Controller type="Cutscene" end="Stop">
                    <Key type="Cutscene" time="0.001" action="Play"/>
                    <Key type="Cutscene" time="0.002" action="Bookmark" bookmark="0"/>
                </Controller>
            </Animation>
        </Frame>

    </Frame>

    <!--                                                                                                      -->
    <!--  ROBOT STATUS FRAME                                                                                  -->
    <!--                                                                                                      -->
    <Frame type="Frame" name="VolskayaRobotHealthSlideout">
        <Anchor side="Bottom" relative="$parent/$parent/MinimapPanel" pos="Min" offset="80"/>
        <Anchor side="Right" relative="$parent/$parent/MinimapPanel" pos="Max" offset="0"/>
        
        <Width val="520"/>
        <Height val="200"/>
        
        <Animation name="SlideIn">
            <Event event="OnShown" action="Reset,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.3" value="255"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Right" relative="true">
                <Key type="Curve" time="0.0" value="80"/>
                <Key type="Curve" time="0.3" value="0"/>
            </Controller>
        </Animation>
        
        <Animation name="SlideOut">
            <Event event="PlaySlideOut" action="Reset,Play"/>
            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0.0" value="255"/>
                <Key type="Curve" time="0.3" value="0"/>
            </Controller>
            <Controller type="Anchor" end="Pause" side="Right" relative="true">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.3" value="80"/>
            </Controller>
            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time="0.0" value="True"/>
                <Key type="Bool" time="0.3" value="False"/>
            </Controller>
        </Animation>

        <Frame type="UnitStatusColor" name="TeamOrderColorsBright">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Width val="3"/>
            <Height val="3"/>

            <ForcedTeam val="0"/>
            <ColorStyle val="VolskayaBarsBright"/>
        </Frame>
        
        <Frame type="UnitStatusColor" name="TeamChaosColorsBright">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/TeamOrderColorsBright" pos="Max" offset="0"/>
            <Width val="3"/>
            <Height val="3"/>

            <ForcedTeam val="1"/>
            <ColorStyle val="VolskayaBarsBright"/>
        </Frame>
        
        <!-- slideout -->
        <Frame type="Image" name="BackgroundImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="50"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            
            <Texture val="Assets\Textures\storm_ui_hud_volskaya_objective_bg.dds"/>
