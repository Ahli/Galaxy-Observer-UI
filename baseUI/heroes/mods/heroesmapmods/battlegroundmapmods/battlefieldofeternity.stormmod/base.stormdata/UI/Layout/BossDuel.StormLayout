                <Anchor side="Bottom" relative="$parent/$parent/BossHudLeft" pos="Mid" offset="148"/>
                <Anchor side="Left" relative="$parent/$parent/BossHudLeft" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/$parent/BossHudLeft" pos="Mid" offset="0"/>
                <Visible val="True"/>
                <ShowPercent val="False"/>
                <HideMax val="true"/>
                <Unclipped val="true"/>
                <ShowWhileInvulnerable val="True"/>
                    <Frame type="Label" name="StatusLabel">
                        <Anchor relative="$parent"/>
                        <Style val="Storm_MapMechanic_HealthPercentCentered_Blue"/>
                    </Frame>
