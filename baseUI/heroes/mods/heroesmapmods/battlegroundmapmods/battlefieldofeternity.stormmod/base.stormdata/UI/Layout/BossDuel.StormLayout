        <Frame type="UnitStatusFrame" name="AllyBossHealthBar" template="MapMechanicPanel/MapMechanicHealthBarTemplate">
            <Width val="0"/>
            <Height val="0"/>
            <Anchor side="Top" relative="$parent/HealthBarBgTop" pos="Min" offset="6"/>
            <Anchor side="Bottom" relative="$parent/HealthBarBgTop" pos="Max" offset="-7"/>
            <Anchor side="Left" relative="$parent/HealthBarBgTop" pos="Min" offset="2"/>
            <Anchor side="Right" relative="$parent/HealthBarBgTop" pos="Max" offset="-11"/>

            <Frame type="UnitStatusValidatorAnimElement" name="HealthBarController">
                <Anchor relative="$parent"/>
                <Validator val="BossDuelBossDuelComplete"/>
                <Validator val="BossDuelBossDuelIncomplete"/>

                <StateGroup name="IsDuelComplete">
                    <DefaultState val="DuelIncomplete"/>

                    <State name="DuelComplete">
                        <When type="AnimationState" frame="$this" UnitStatusValidatorState="BossDuelBossDuelComplete"/>
                        <Action type="SetProperty" frame="$parent/HealthLabel" VisibleToOwner="False"/>
                        <Action type="SetProperty" frame="$parent/HealthLabel" VisibleToAlly="False"/>
                        <Action type="SetProperty" frame="$parent/HealthLabel" VisibleToEnemy="False"/>
                    </State>
                    <State name="DuelIncomplete">
                        <!--  This is the implied behavior.  None of this should be required to be specified, unless other things change...
                        <When type="AnimationState" frame="$this" UnitStatusValidatorState="BossDuelBossDuelIncomplete"/>
                        <Action type="SetProperty" frame="$parent/HealthLabel" VisibleToOwner="True"/>
                        <Action type="SetProperty" frame="$parent/HealthLabel" VisibleToAlly="True"/>
                        <Action type="SetProperty" frame="$parent/HealthLabel" VisibleToEnemy="True"/>
                        ...END Expected default values -->
                    </State>
                </StateGroup>
            </Frame>
            
            
            <Frame type="UnitStatusHealthBar" name="HealthBar" template="UnitStatus/UnitStatusSegmentedHealthBarTemplate">
                <RenderPriority val="600"/>
                <Anchor relative="$parent"/>
