        <Frame type="Image" name="BossHudLeftGlowImage">
            <Anchor side="Top" relative="$parent/BossHudLeft" pos="Mid" offset="17"/>
            <Anchor side="Left" relative="$parent/BossHudLeft" pos="Mid" offset="-1"/>
            <Anchor side="Bottom" relative="$parent/BossHudLeft" pos="Mid" offset="17"/>
            <Anchor side="Right" relative="$parent/BossHudLeft" pos="Mid" offset="-1"/>
            <Texture val="@UI/HUD_MapMechanic_HexGlow_Add"/>
            <Color val="ff0000"/>
            <BlendMode val="Add"/>
            <AcceptsMouse val="True"/>
            <!--<Animation name="Click">
                <Event event="OnMouseDown" action="Restart,Play"/>
                <Controller type="Event" end="Stop" frame="$parent">
                    <Key type="Event" time="0" event="LeftBossDefeated"/>
                </Controller>
            </Animation>-->
            <Animation name="Pulse">
                <Controller type="Fade" end="PingPong">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time=".4" value="255" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" end="Loop">
                    <Key type="Curve" time="0" value="100" inout="Linear"/>
                    <Key type="Curve" time=".8" value="120" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Dimension" dimension="Height" relative="False" percentage="True" end="Loop">
                    <Key type="Curve" time="0" value="100" inout="Linear"/>
                    <Key type="Curve" time=".8" value="120" inout="Slow,Linear"/>
                </Controller>
            </Animation>
        </Frame>

