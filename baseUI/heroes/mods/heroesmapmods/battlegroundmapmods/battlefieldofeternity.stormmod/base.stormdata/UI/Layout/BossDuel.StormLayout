                <SubpixelRendering val="True"/>
                <RenderPriority val="#RenderPriority_Background"/>
                <ColorAdjustMode val="Desaturate"/>
                <LightAdjustColor val="{$parent/$parent/PrimaryHighlightColorFrame/@Color}"/>
