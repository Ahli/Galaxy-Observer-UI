    <Validator val="SourceHas2WarchiefsAlive" />
    <Validator val="SourceHas1WarchiefsAlive" />
    <Validator val="SourceHas0WarchiefsAlive" />
    <StateGroup name="WarchiefsAlive">
      <DefaultState val="3Alive" />
      <State name="3Alive">
        <When type="AnimationState" UnitStatusValidatorState="SourceHas3WarchiefsAlive" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield3/ShieldInside" Visible="True" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield2/ShieldInside" Visible="True" />
