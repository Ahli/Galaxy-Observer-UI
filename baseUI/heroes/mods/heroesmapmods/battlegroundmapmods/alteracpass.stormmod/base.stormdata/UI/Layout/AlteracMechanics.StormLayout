  <Frame type="UnitStatusUnitArmor" name="CoreArmorStatusDisplayTemplate">
    <Anchor side="Bottom" relative="StatusImage" pos="Max" offset="-5" />
    <Anchor side="Right" relative="StatusImage" pos="Max" offset="0" />
    <Frame type="Image" name="BackgroundImage">
      <Anchor relative="$parent/StatusImage" />
