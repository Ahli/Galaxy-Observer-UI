      </State>
      <State name="2Alive">
        <When type="AnimationState" UnitStatusValidatorState="SourceHas2WarchiefsAlive" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield3/ShieldInside" Visible="False" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield2/ShieldInside" Visible="True" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield1/ShieldInside" Visible="True" />
      </State>
      <State name="1Alive">
        <When type="AnimationState" UnitStatusValidatorState="SourceHas1WarchiefsAlive" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield3/ShieldInside" Visible="False" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield2/ShieldInside" Visible="False" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield1/ShieldInside" Visible="True" />
      </State>
      <State name="0Alive">
        <When type="AnimationState" UnitStatusValidatorState="SourceHas0WarchiefsAlive" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield3/ShieldInside" Visible="False" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield2/ShieldInside" Visible="False" />
        <Action type="SetProperty" frame="$parent/HealthBarShieldContainer/BossHealthBarShield1/ShieldInside" Visible="False" />
      </State>
