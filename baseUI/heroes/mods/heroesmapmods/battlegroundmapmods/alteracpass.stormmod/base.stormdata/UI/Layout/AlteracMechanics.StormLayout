      <Texture val="@UI/Storm_UI_Ingame_Armor_Icon" />
    </Frame>
    <Frame type="TextFormatFrame" name="ArmorLabelFormatSign">
      <FormatText val="+%0%" />
      <Replacement index="0" val="{$parent/@AllArmorBonus}" />
    </Frame>
    <Frame type="Label" name="ArmorLabel">
      <Anchor side="Top" relative="$parent/StatusImage" pos="Max" offset="-23" />
      <Anchor side="Left" relative="$parent/StatusImage" pos="Min" offset="0" />
      <Anchor side="Right" relative="$parent/StatusImage" pos="Max" offset="0" />
      <Height val="33" />
      <Text val="{$parent/ArmorLabelFormatSign/@Text}" />
      <Style val="UnitStatusPositiveArmor" />
    </Frame>
    <StateGroup name="ArmorBonusIconController">
      <DefaultState val="Neutral" />
      <State name="Buffed">
        <When type="Property" ShowArmorBuff="True" />
        <When type="Property" AllArmorBonus="0" operator="GreaterThanOrEqual" />
        <When type="Property" DurationMax="0" operator="GreaterThan" />
      </State>
      <State name="UnTimedBuffed">
        <When type="Property" ShowArmorBuff="True" />
        <When type="Property" AllArmorBonus="0" operator="GreaterThan" />
        <When type="Property" DurationMax="0" />
      </State>
      <State name="Debuffed">
        <When type="Property" AllArmorBonus="0" operator="LessThan" />
        <When type="Property" DurationMax="0" operator="GreaterThan" />
        <When type="Option" colorblind="False" />
        <Action type="SetProperty" frame="ArmorLabel" style="UnitStatusNegativeArmor" />
        <Action type="SetProperty" frame="ArmorLabelFormatSign" FormatText="%0%" />
        <Action type="SetProperty" frame="StatusImage" Texture="@UI/Storm_UI_Ingame_Armor_Icon_Negative" />
        <Action type="SetProperty" frame="DurationBar/FillImageContainer/FillImage" Texture="@UI/Storm_UI_Ingame_Armor_DurationBar_Negative" />
      </State>
      <State name="ColorblindDebuff">
        <When type="Property" AllArmorBonus="0" operator="LessThan" />
        <When type="Property" DurationMax="0" operator="GreaterThan" />
        <When type="Option" colorblind="True" />
        <Action type="SetProperty" frame="ArmorLabel" style="UnitStatusNegativeArmorColorblind" />
        <Action type="SetProperty" frame="ArmorLabelFormatSign" FormatText="%0%" />
        <Action type="SetProperty" frame="StatusImage" Texture="@UI/Storm_UI_Ingame_Armor_Icon_Negative_CB" />
        <Action type="SetProperty" frame="DurationBar/FillImageContainer/FillImage" Texture="@UI/Storm_UI_Ingame_Armor_DurationBar_Negative_CB" />
      </State>
      <State name="Neutral">
        <Action type="SetProperty" frame="StatusImage" visible="false" />
        <Action type="SetProperty" frame="ArmorLabel" visible="false" />
        <Action type="SetProperty" frame="BackgroundImage" visible="false" />
        <Action type="SetProperty" frame="DurationBar" visible="false" />
      </State>
    </StateGroup>
  </Frame>
