        <State name="DeadmansStandHeroic">
          <When type="Property" frame="$ScreenPlay_HeroNexusBrawlPanel" MapLink="DMSH" />
          <Action type="SetProperty" frame="NameLabel" Text="@UI/WeeklyRewardTooltip/WinsNotEarned" />
        </State>
        <State name="NotEarned">
          <Action type="SetProperty" frame="NameLabel" Text="@UI/WeeklyRewardTooltip/NotEarned" />
        </State>
      </StateGroup>
    </Frame>
    <Frame type="QuestFrame" name="QuestFrame" template="HeroNexusBrawlPanel/QuestFrameTemplate">
      <Anchor side="Bottom" relative="$parent" pos="Max" offset="-27" />
      <Anchor side="Left" relative="$parent" pos="Min" offset="45" />
      <Width val="215" />
      <Height val="300" />
      <Frame type="Label" name="TimeRemainingLabel">
        <Anchor side="Bottom" relative="$parent" pos="Min" offset="32" />
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0" />
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0" />
        <Width val="250" />
        <Style val="HeroesBrawlPanel_QuestTimeRemaining" />
        <Text val="{$ancestor[@type=HeroNexusBrawlPanel]/@RemainingDaysText}" />
        <Unclipped val="true" />
      </Frame>
      <Frame type="Label" name="Label">
        <Anchor side="Top" relative="$parent" pos="Min" offset="268" />
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0" />
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0" />
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0" />
        <Style val="HeroSelectPanelButtonLabel" />
        <Text val="@UI/ProfileDialog/WeeklyReward" />
      </Frame>
      <Frame type="Image" name="CompletedImage">
        <Anchor side="Top" relative="$parent/ValidQuest/ProgressLabel" pos="Mid" offset="-18" />
        <Anchor side="Bottom" relative="$parent/ValidQuest/ProgressLabel" pos="Mid" offset="-18" />
        <Anchor side="Right" relative="$parent/ValidQuest/ProgressLabel" pos="Mid" offset="0" />
        <Anchor side="Left" relative="$parent/ValidQuest/ProgressLabel" pos="Mid" offset="0" />
        <Width val="60" />
        <Height val="60" />
        <Texture val="@UI/Storm_UI_NewUser_IconComplete" />
      </Frame>
      <StateGroup name="WeeklyRewardState">
        <DefaultState val="NotEarned" />
        <State name="Earned">
          <When type="Property" frame="$ancestor[@type=HeroNexusBrawlPanel]" BrawlRewardState="Earned" />
          <Action type="SetProperty" frame="Label" Text="@UI/WeeklyReward/Complete" />
          <Action type="SetProperty" frame="ValidQuest/ProgressLabel" visible="false" />
          <Action type="SetProperty" frame="CompletedImage" visible="true" />
        </State>
        <State name="NotEarned">
          <Action type="SetProperty" frame="Label" Text="@UI/ProfileDialog/WeeklyReward" />
          <Action type="SetProperty" frame="ValidQuest/ProgressLabel" visible="true" />
          <Action type="SetProperty" frame="CompletedImage" visible="false" />
        </State>
      </StateGroup>
      <Frame type="Frame" name="ValidQuest">
        <Frame type="ProgressBar" name="QuestProgressBar">
          <AcceptsMouseTooltip val="true" />
          <TooltipAnchorPosition val="HCenterToCenter | BottomToTop" />
          <TooltipFrame val="BrawlRewardTooltip" />
        </Frame>
      </Frame>
    </Frame>
    <Frame type="Button" name="InfoButton" template="HeroSelectPanel/HeroAttributeButtonTemplate">
      <Anchor side="Bottom" relative="$parent" pos="Max" offset="0" />
      <Anchor side="Left" relative="$parent/QuestFrame" pos="Max" offset="0" />
      <Text val="@UI/HeroNexusBrawlPanel/BrawlInfo" />
      <Frame type="Image" name="BaseButtonImage">
        <Texture val="@UI/storm_ui_play_heroattributes_button_brawl_normal" />
      </Frame>
      <Frame type="Image" name="DisabledButtonImage">
        <Texture val="@UI/storm_ui_play_heroattributes_button_brawl_disabled" />
      </Frame>
      <Frame type="Image" name="HoverButtonImage">
        <Texture val="@UI/storm_ui_play_heroattributes_button_brawl_hover" />
      </Frame>
      <Frame type="Image" name="ToggledButtonImage">
        <Texture val="@UI/storm_ui_play_heroattributes_button_brawl_selected" />
      </Frame>
      <Frame type="Label" name="Label">
        <Anchor side="Top" relative="$parent/BaseButtonImage" pos="Max" offset="-25" />
        <Anchor side="Left" relative="$parent" pos="Min" offset="2" />
        <Anchor side="Right" relative="$parent" pos="Max" offset="0" />
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0" />
        <Style val="HeroSelectPanelButtonLabel" />
      </Frame>
      <StateGroup name="InfoButtonState">
        <DefaultState val="Disabled" />
        <State name="Disabled">
          <When type="Property" frame="$parent/InfoPanel" InfoAvailable="false" />
          <Action type="SetProperty" frame="$this" enabled="false" />
          <Action type="SendEvent" frame="$parent/InfoPanel" event="Hide" />
        </State>
        <State name="InfoAvailable">
          <When type="Property" frame="$parent/InfoPanel" InfoAvailable="true" />
          <When type="Property" frame="$parent" InfoPopupShown="false" />
          <Action type="SetProperty" enabled="true" />
          <Action type="SendEvent" frame="$parent/InfoPanel" event="Hide" />
        </State>
        <State name="InfoShown">
          <When type="Property" frame="$parent/InfoPanel" InfoAvailable="true" />
          <When type="Property" frame="$parent" InfoPopupShown="true" />
          <Action type="SetProperty" enabled="true" />
          <Action type="SendEvent" frame="$parent/InfoPanel" event="Show" />
        </State>
      </StateGroup>
      <StateGroup name="TutorialPopupState">
        <DefaultState val="Hide" />
        <State name="Show">
          <When type="Property" frame="$ancestor[@type=HeroNexusBrawlPanel]" TutorialPopupShown="true" />
          <Action type="SendEvent" frame="$parent/BrawlTutorialPanel" event="Show" />
        </State>
        <State name="Hide">
          <Action type="SendEvent" frame="$parent/BrawlTutorialPanel" event="Hide" />
        </State>
      </StateGroup>
    </Frame>
    <Frame type="Frame" name="BrawlTutorialPanel">
      <Anchor side="Left" relative="$parent/InfoPanel" pos="Min" offset="0" />
      <Anchor side="Right" relative="$parent/InfoPanel" pos="Max" offset="0" />
      <Anchor side="Bottom" relative="$parent/InfoPanel" pos="Min" offset="-5" />
      <Anchor side="Top" relative="TitleLabel" pos="Min" offset="-15" />
      <Alpha val="{$parent/InfoPanel/@Alpha}" />
      <VisibilityOnFade val="true" />
      <Visible val="false" />
      <Frame type="BlurFrame" name="BlurFrame">
        <Anchor side="Left" relative="$parent/Background" pos="Min" offset="4" />
        <Anchor side="Top" relative="$parent/Background" pos="Min" offset="4" />
        <Anchor side="Right" relative="$parent/Background" pos="Max" offset="-4" />
        <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="-4" />
        <AcceptsMouse val="True" />
        <BlurAmount val="0.8" />
        <Frame type="Image" name="LowImage">
          <Anchor relative="$parent" />
          <Texture val="@UI/Storm_Generic_Background_Low" />
        </Frame>
      </Frame>
      <Frame type="Image" name="BackgroundColorImage">
        <Anchor relative="$parent/BlurFrame" />
        <Color val="08081d" />
        <Alpha val="100" />
      </Frame>
      <Frame type="Image" name="Background">
        <Anchor relative="$parent" />
        <Texture val="@UI/Storm_Generic_SmallPanel_Border" />
        <TextureType val="Nineslice" />
      </Frame>
      <Frame type="Label" name="TitleLabel">
        <Anchor side="Bottom" relative="$parent/DescriptionLabel" pos="Min" offset="-10" />
        <Anchor side="Left" relative="$parent" pos="Min" offset="20" />
        <Anchor side="Right" relative="$parent" pos="Max" offset="-15" />
        <Style val="HeroesBrawlInfoPanel_TopTitle" />
        <Text val="@UI/BrawlInfoPanel/Title" />
      </Frame>
      <Frame type="Label" name="DescriptionLabel">
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="-15" />
        <Anchor side="Left" relative="$parent" pos="Min" offset="20" />
        <Anchor side="Right" relative="$parent" pos="Max" offset="-15" />
        <Style val="HeroesBrawlInfoPanel_TopDescription" />
        <Text val="@UI/BrawlInfoPanel/Description" />
      </Frame>
      <Frame type="Button" name="CloseButton" template="StandardTemplates/SmallPanelCloseButtonTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="4" />
        <Anchor side="Right" relative="$parent" pos="Max" offset="-6" />
        <Animation name="OnShown">
          <Event event="OnClick" action="Play" />
          <Controller type="Event" end="Stop" frame="$ancestor[@type=HeroNexusBrawlPanel]">
            <Key type="Event" time="0" event="DismissTutorialPopup" />
          </Controller>
        </Animation>
      </Frame>
      <Animation name="SlideOut">
        <Event event="Show" action="DirectionForward,Play" />
        <Event event="Hide" action="DirectionReverse,Play" />
        <Controller type="Fade" end="Pause">
          <Key type="Curve" time="0" value="0" inout="Linear,Slow" />
          <Key type="Curve" time=".175" value="255" inout="Auto,Linear" />
        </Controller>
        <Controller type="Anchor" end="Pause" side="Bottom" relative="true">
          <Key type="Curve" time="0" value="50" inout="Linear" />
          <Key type="Curve" time=".175" value="0" inout="Slow,Linear" />
        </Controller>
        <Controller type="Visibility" end="Pause">
          <Key type="Bool" time="0" value="false" />
          <Key type="Bool" time=".001" value="true" />
          <Key type="Bool" time=".175" value="true" />
        </Controller>
      </Animation>
    </Frame>
    <Frame type="BrawlInfoPanel" name="InfoPanel" template="HeroNexusBrawlPanel/InfoPanelTemplate">
      <Anchor side="Bottom" relative="$parent/InfoButton" pos="Min" offset="47" />
      <Anchor side="Left" relative="$parent/InfoButton" pos="Min" offset="-20" />
      <Frame type="Button" name="CloseButton">
        <Animation name="OnShown">
          <Event event="OnClick" action="Play" />
          <Controller type="Event" end="Stop" frame="$ancestor[@type=HeroNexusBrawlPanel]">
            <Key type="Event" time="0" event="DismissInfoPanel" />
          </Controller>
        </Animation>
      </Frame>
    </Frame>
  </Frame>
</Desc>